INSERT INTO BIOLOGIA VALUES(799,'799. Telesn\u00e9 proporcie novorodenca s\u00fa:','tak\u00e9 ist\u00e9 ako u dospel\u00e9ho jedinca','odli\u0161n\u00e9 od proporci\u00ed dospel\u00e9ho jedinca','relat\u00edvne mal\u00e1 hlava, dlh\u00fd trup a dlh\u00e9 kon\u010datiny','relat\u00edvne ve\u013ek\u00e1 hlava, kr\u00e1tky trup a kr\u00e1tke kon\u010datiny','relat\u00edvne ve\u013ek\u00e1 hlava, dlh\u00fd trup a kr\u00e1tke kon\u010datiny','relat\u00edvne mal\u00e1 hlava, kr\u00e1tky trup a dlh\u00e9 kon\u010datiny','relat\u00edvne mal\u00e1 hlava, dlh\u00fd trup a kr\u00e1tke kon\u010datiny','relat\u00edvne ve\u013ek\u00e1 hlava, kr\u00e1tky trup a dlh\u00e9 kon\u010datiny',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(800,'800. Pre doj\u010deneck\u00e9 obdobie je charakteristick\u00e9, \u017ee:','potravou v prv\u00fdch \u0161iestich mesiacoch je mlieko','na konci 1. roku \u017eivota je d\u013a\u017eka tela doj\u010da\u0165a obvykle 105 cm','e\u0161te neprebieha rozvoj nervovej s\u00fastavy','na lebke je u\u017e uzatvoren\u00e1 fontanela','prebieha intenz\u00edvny rozvoj nervovej s\u00fastavy','doch\u00e1dza k intenz\u00edvnemu telesn\u00e9mu rastu a rozvoju nervovej s\u00fastavy','sa rozv\u00edjaj\u00fa pohybov\u00e9 schopnosti','telesn\u00fd rast sa spoma\u013euje',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(801,'801. Pre pred\u0161kolsk\u00fd vek je typick\u00e9:','prv\u00e1 premena postavy','\u017ee je u\u017e uzatvoren\u00fd ve\u013ek\u00fd lupienok','\u017ee die\u0165a intenz\u00edvne rastie','\u017ee sa telesn\u00fd rast spoma\u013euje','\u017ee sa formuje spr\u00e1vanie','\u017ee sa za\u010d\u00edna prerez\u00e1va\u0165 trval\u00fd chrup','\u017ee za\u010d\u00edna sk\u00f4r u diev\u010dat','\u017ee sa kon\u010d\u00ed osifik\u00e1cia kost\u00ed',TRUE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(802,'802. Ozna\u010dte spr\u00e1vnu odpove\u010f:','pravdepodobne najstar\u0161ia p\u00edsomn\u00e1 zmienka o antikoncep\u010dnom prostriedku poch\u00e1dza z Egypta','najorigin\u00e1lnej\u0161\u00ed spis o antikoncepcii v obdob\u00ed antiky nap\u00edsal Soranus','medzi prirodzen\u00e9 met\u00f3dy antikoncepcie patr\u00ed rozli\u0161ovanie plodov\u00fdch dn\u00ed','Bilingsova antikoncep\u010dn\u00e1 met\u00f3da spo\u010d\u00edva v sledovan\u00ed zmien cervik\u00e1lneho hlienu','medzi bari\u00e9rov\u00e9 met\u00f3dy antikoncepcie patr\u00ed coitus interruptus','pou\u017e\u00edvanie vn\u00fatromaternicov\u00e9ho telieska sa ozna\u010duje ako coitus condomatus','hormon\u00e1lna antikoncepcia patr\u00ed medzi vo\u013enopredajn\u00e9 lieky','plodn\u00e9 dni je mo\u017en\u00e9 odhadn\u00fa\u0165 aj na z\u00e1klade palp\u00e1cie kr\u010dka matemice',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(803,'803. Medzi civiliza\u010dn\u00e9 choroby patr\u00ed:','AIDS','cukrovka','obezita','mal\u00e1ria','tuberkul\u00f3za','nervov\u00e9 a psychick\u00e9 ochorenia','n\u00e1dorov\u00e9 ochorenia','alergie',TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(804,'804. Alergia:','patr\u00ed medzi civiliza\u010dn\u00e9 ochorenia','je sp\u00f4soben\u00e1 precitlivelos\u0165ou organizmu na alerg\u00e9ny','m\u00f4\u017ee vies\u0165 a\u017e k smrti postihnut\u00e9ho','sa pren\u00e1\u0161a telesn\u00fdmi tekutinami','sa m\u00f4\u017ee prejavi\u0165 ako n\u00e1dcha, p\u0155h\u013eavka','sa m\u00f4\u017ee prejavi\u0165 ako opuch a ekz\u00e9m','je sp\u00f4soben\u00e1 v\u00edrusom HIV','v ak\u00fatnej forme sa naz\u00fdva fyziologick\u00fd \u0161ok',TRUE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
