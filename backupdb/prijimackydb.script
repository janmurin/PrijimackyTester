INSERT INTO BIOLOGIA VALUES(603,'603. O svaloch predlaktia \u010dloveka plat\u00ed, \u017ee:','zabezpe\u010duj\u00fa pohyby v ramennom k\u013abe','zabezpe\u010duj\u00fa pohyby v lak\u0165ovom k\u013abe','zabezpe\u010duj\u00fa pohyby v k\u013aboch z\u00e1p\u00e4stia','k nim patr\u00ed trojhlav\u00fd sval ramena','k nim patr\u00ed dvojhlav\u00fd sval ramena','k nim patr\u00ed povrchov\u00fd oh\u00fdba\u010d prstov','k nim patria oh\u00fdba\u010de, vystiera\u010de a privraca\u010de','zabezpe\u010duj\u00fa ohyb v lakti \u2014 napr. dvojhlav\u00fd sval ramena',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
