INSERT INTO BIOLOGIA VALUES(956,'956. Ktor\u00e9 tvrdenie je spr\u00e1vne:','ak pozn\u00e1me kod\u00f3n, pozn\u00e1me aminokyselinu','ak pozn\u00e1me aminokyselinu, v\u017edy pozn\u00e1me kod\u00f3n','ak pozn\u00e1me aminokyselinu, obvykle nepozn\u00e1me kod\u00f3n','ka\u017ed\u00e1 aminokyselina je k\u00f3dovan\u00e1 najmenej 2 kod\u00f3nmi','ka\u017ed\u00fd kod\u00f3n zodpoved\u00e1 aspo\u0148 za dve aminokyseliny','niektor\u00e9 aminokysel\u00edny maj\u00fa len jeden kod\u00f3n','niektor\u00e9 aminokyseliny maj\u00fa viac ako \u0161tyri kod\u00f3ny','niektor\u00e9 kod\u00f3ny nek\u00f3duj\u00fa aminokyselinu',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(957,'957. \u0160trukt\u00farny g\u00e9n:','k\u00f3duje polypeptid','k\u00f3duje enz\u00fdm','je riaden\u00fd regula\u010dn\u00fdm g\u00e9nom','sa neprepisuje do rRNA','sa prepisuje do mRNA','je ka\u017ed\u00fd g\u00e9n so zn\u00e1mou \u0161trukt\u00farou','ur\u010duje poradie aminokysel\u00edn v polypeptidovom re\u0165azci','k\u00f3duje prim\u00e1rnu \u0161trukt\u00faru tRNA',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(958,'958. \u0160trukt\u00farne g\u00e9ny:','ur\u010duj\u00fa poradie aminokysel\u00edn v polypeptidovom re\u0165azci','ur\u010duj\u00fa \u0161trukt\u00faru v\u0161etk\u00fdch ribonukleov\u00fdch kysel\u00edn','reprezentuj\u00fa podstatn\u00fa \u010das\u0165 jadrovej DNA','s\u00fa len tie g\u00e9ny, ktor\u00fdch \u0161trukt\u00fara je zn\u00e1ma','ur\u010duj\u00fa \u0161trukt\u00faru peptidovej v\u00e4zby','ur\u010duj\u00fa \u0161trukt\u00faru enz\u00fdmov','k\u00f3duj\u00fa prim\u00e1rnu \u0161trukt\u00faru bielkov\u00edn','k\u00f3duj\u00fa prim\u00e1rnu \u0161trukt\u00faru tRNA',TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(959,'959. Expresia \u0161trukt\u00farnych g\u00e9nov:','sa uskuto\u010d\u0148uje prostredn\u00edctvom replik\u00e1cie a transkripcie','je realizovan\u00e1 transkripciou a transl\u00e1ciou','je realiz\u00e1cia genetickej inform\u00e1cie','je regul\u00e1cia prepisu genetickej inform\u00e1cie','je realizovan\u00e1 pomocou mut\u00e1ci\u00ed','sa uskuto\u010d\u0148uje po\u010das mit\u00f3zy','sa uskuto\u010d\u0148uje po\u010das M f\u00e1zy','prebieha v G_1 f\u00e1ze',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(960,'960. Prokaryotick\u00e9 chromoz\u00f3my s\u00fa:','tvoren\u00e9 dvoma do kruhu uzatvoren\u00fdmi molekulami DNA','tvoren\u00e9 jednou do kruhu uzatvorenou dvojvl\u00e1knovou makromolekulou DNA','tvoren\u00e9 kruhovitou makromolekulou RNA','ulo\u017een\u00e9 v cytoplazme','chromoz\u00f3my s rovnak\u00fdm p\u00e1rovan\u00edm b\u00e1z ako eukaryotick\u00e9','zoraden\u00e9 do homologick\u00fdch p\u00e1rov','tvoren\u00e9 line\u00e1rnymi molekulami DNA','kruhov\u00e9 nukleoprote\u00ednov\u00e9 molekuly',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(961,'961. Plazmidy:','s\u00fa bunkov\u00e9 \u0161trukt\u00fary zelen\u00fdch rastl\u00edn','predstavuj\u00fa genetick\u00fa inform\u00e1ciu plastidov','m\u00f4\u017eu obsahova\u0165 g\u00e9ny pre rezistenciu na antibiotik\u00e1','s\u00fa makromolekuly DNA bakt\u00e9ri\u00ed','obsahuj\u00fa dodato\u010dn\u00fa genetick\u00fa inform\u00e1ciu','s\u00fa \u010dasto objektom genetick\u00e9ho v\u00fdskumu','sa rozli\u0161uj\u00fa pod\u013ea d\u013a\u017eky molekuly DNA','s\u00fa d\u00f4le\u017eitou \u0161trukt\u00farou mitochondri\u00ed',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(962,'962. O oper\u00f3ne plat\u00ed, \u017ee:','je to \u010das\u0165 molekuly DNA','je to \u010das\u0165 molekuly mRNA','je to s\u00favisl\u00fd re\u0165azec g\u00e9nov u bakt\u00e9ri\u00ed so spolo\u010dnou regul\u00e1ciou','napr. lakt\u00f3zov\u00fd reguluje metabolizmus lalct\u00f3zy','g\u00e9ny jedn\u00e9ho oper\u00f3nu spolu funk\u010dne s\u00favisia','sa prepisuje do viacer\u00fdch molek\u00fal mRNA','oper\u00f3nov\u00fd syst\u00e9m je ve\u013emi ekonomick\u00fd','jeho transkripcia nie je regulovan\u00e1',TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(963,'963. Plazmidy:','obsahuj\u00fa 2 a\u017e niekolko 100 g\u00e9nov','maj\u00fa jednovl\u00e1knov\u00fa kruhov\u00fa DNA','sa replikuj\u00fa v z\u00e1vislosti na chromoz\u00f3moch','sa m\u00f4\u017eu vyskytova\u0165 v bunke vo viacer\u00fdch k\u00f3pi\u00e1ch','m\u00f4\u017eu zodpoveda\u0165 za konjug\u00e1ciu bakt\u00e9ri\u00ed','znev\u00fdhod\u0148uj\u00fa pre\u017e\u00edvanie bakt\u00e9ri\u00ed','sa v bunk\u00e1ch vyskytuj\u00fa v\u017edy v homologick\u00fdch p\u00e1roch','sa m\u00f4\u017eu v bunke replikova\u0165 nez\u00e1visle na replik\u00e1cii bakteri\u00e1lneho chromoz\u00f3mu',TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(964,'964. Maj\u00fa plazmidy schopnos\u0165 replikova\u0165 sa samostatne:','nemaj\u00fa','maj\u00fa','niekedy','\u00e1no, v z\u00e1vislosti od stavu bakteri\u00e1lneho chromoz\u00f3mu','maj\u00fa, ale len plazmidy zodpovedn\u00e9 za fertilitu bakt\u00e9ri\u00ed','\u00e1no, ale len mimo bunkov\u00e9ho cyklu','\u00e1no, ale len v z\u00e1vislosti od lumkov\u00e9ho cyklu','\u00e1no, ale len v eukaryotick\u00fdch bunk\u00e1ch',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(965,'965. Genetick\u00e1 inform\u00e1cia plazmidov:','je pre bakteri\u00e1lnu bunku nepostr\u00e1date\u013en\u00e1','je pre bakteri\u00e1lnu bunku nev\u00fdhodn\u00e1','je pre bakteri\u00e1lnu bunku postr\u00e1date\u013en\u00e1','m\u00f4\u017ee by\u0165 pre bakteri\u00e1lnu bunku v\u00fdhodn\u00e1','v\u00e4\u010d\u0161inou je pre bunku nevyu\u017eite\u013en\u00e1','nek\u00f3duje polypeptidov\u00fd re\u0165azec','je toto\u017en\u00e1 s genetickou inform\u00e1ciou obsiahnutou v bakteri\u00e1lnom chromoz\u00f3me','je odli\u0161n\u00e1 od genetickej inform\u00e1cie obsiahnutej v bakteri\u00e1lnom chromoz\u00f3me',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(966,'966. Plazmidov\u00e9 g\u00e9ny:','s\u00fa usporiadan\u00e9 neline\u00e1rne','s\u00fa g\u00e9ny predov\u0161etk\u00fdm bakt\u00e9ri\u00ed','s\u00fa d\u00f4le\u017eit\u00e9 len pre \u0161pecifick\u00e9 \u017eivotn\u00e9 podmienky','m\u00f4\u017eu zabezpe\u010dova\u0165 konjug\u00e1ciu bakt\u00e9ri\u00ed','zabezpe\u010duj\u00fa nededi\u010dn\u00fa transform\u00e1ciu bakt\u00e9ri\u00ed','nie s\u00fa alelick\u00e9','obsahuj\u00fa nevyu\u017eite\u013en\u00fa genetick\u00fa inform\u00e1ciu','si bakt\u00e9rie m\u00f4\u017eu navz\u00e1jom odovzd\u00e1va\u0165 pri konjug\u00e1cii',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(967,'967. G\u00e9ny v plazmidoch s\u00fa usporiadan\u00e9:','cyklicky','nepravidelne','line\u00e1rne','pod\u013ea ve\u013ekosti','line\u00e1rne ako v bakteri\u00e1lnom chromoz\u00f3me','po skupin\u00e1ch bez ex\u00f3nov','v homologick\u00fdch p\u00e1roch','do oper\u00f3nov ako v eukaryotickom chromoz\u00f3me',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(968,'968. Pre plazmidy je charakteristick\u00e9, \u017ee:','maj\u00fa men\u0161iu genetick\u00fa plastickos\u0165','do ich \u0161trukt\u00fary sa Fahko v\u010dle\u0148uj\u00fa in\u00e9 g\u00e9ny','s\u00fa line\u00e1rne molekuly DNA','s\u00fa tvoren\u00e9 kruhovitou molekulou mRNA','sa rahko pren\u00e1\u0161aj\u00fa do in\u00fdch bakt\u00e9ri\u00ed','sa m\u00f4\u017eu v bakteri\u00e1lnej bunke vyskytova\u0165 v niekolic\u00fdch k\u00f3pi\u00e1ch','s\u00fa tvoren\u00e9 dvojvl\u00e1knovou molekulou RNA','obsahuj\u00fa g\u00e9ny, ktor\u00e9 bunku v ur\u010dit\u00fdch pr\u00edpadoch zv\u00fdhod\u0148uj\u00fa',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(969,'969. Konjuga\u010dn\u00e9 plazmidy:','maj\u00fa schopnos\u0165 sa replikova\u0165 samostatne','nemaj\u00fa schopnos\u0165 samostatnej replik\u00e1cie','riadia konjug\u00e1ciu v\u00edrusov','riadia konjug\u00e1ciu bakt\u00e9ri\u00ed','riadia pohlavn\u00e9 rozmno\u017eovanie bakt\u00e9ri\u00ed','riadia pohlavn\u00e9 rozmno\u017eovanie v\u00edrusov','riadia obojsmern\u00fa v\u00fdmenu genetick\u00e9ho materi\u00e1lu v procese konjug\u00e1cie','riadia jednosmern\u00fd prenos genetick\u00e9ho materi\u00e1lu v procese konjug\u00e1cie',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(970,'970. Konjug\u00e1cia bakt\u00e9ri\u00ed je:','spojenie bakt\u00e9ri\u00ed do mnohobunkov\u00fdch syst\u00e9mov','nepohlavn\u00e9 rozmno\u017eovanie prokaryotov','schopnos\u0165 bakt\u00e9ri\u00ed spoji\u0165 sa s inou bakt\u00e9riou cytoplazmatick\u00fdm most\u00edkom','sp\u00f4sob jednosmern\u00e9ho prenosu genetickej inform\u00e1cie','ve\u013emi \u010dast\u00e1 u plastidov','charakteristick\u00e1 pre niektor\u00e9 prokaryotick\u00e9 bunky','proces spojen\u00fd s prenosom genetickej inform\u00e1cie medzi prokaryotick\u00fdmi bunkami','proces ved\u00faci k diploidiz\u00e1cii bakt\u00e9ri\u00ed',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(971,'971. Pre prokaryotick\u00fd chromoz\u00f3m je charakteristick\u00e9, \u017ee:','pozost\u00e1va z line\u00e1rnej DNA stabilizovanej bielkovinov\u00fdmi zlo\u017ekami','jeho jednotliv\u00e9 \u010dasti s\u00fa funk\u010dne diferencovan\u00e9','neprech\u00e1dza mitotick\u00fdm delen\u00edm','neobsahuje v rade za sebou umiestnen\u00e9 g\u00e9ny','je skuto\u010dn\u00fd chromoz\u00f3m z genetick\u00e9ho h\u013eadiska','sa nereduplikuje','m\u00f4\u017ee obsahova\u0165 bloky \u0161trukt\u00farnych g\u00e9nov so spolo\u010dnou regul\u00e1ciou','po\u010das mitotick\u00e9ho delenia bunky prech\u00e1dza charakteristick\u00fdmi zmenami tvaru',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(972,'972. Prokaryotick\u00e1 bunka:','m\u00e1 len jednu alelu ka\u017ed\u00e9ho g\u00e9nu','je haploidn\u00e1','m\u00e1 dve alely ka\u017ed\u00e9ho g\u00e9nu','je v\u00fdvojovo star\u0161ia ako eukaryotick\u00e1','nem\u00e1 jadrov\u00fa membr\u00e1nu','obsahuje line\u00e1rne molekuly DNA','je v\u017edy heterotrofn\u00e1','m\u00e1 spravidla len jednu mitochondriu',TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(973,'973. V prokaryotick\u00fdch bunk\u00e1ch sa g\u00e9ny nach\u00e1dzaj\u00fa:','v mitochondri\u00e1ch','len v prokaryotick\u00fdch chromoz\u00f3moch','v molekule DNA ulo\u017eenej v cytoplazme','v plazmidoch','v mikrotubul\u00e1rnych \u0161trukt\u00farach','v jadrovej hmote','v chloroplastoch','v riboz\u00f3moch',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(974,'974. Jednosmern\u00fd prenos genetickej inform\u00e1cie medzi bunkami bakt\u00e9ri\u00ed m\u00f4\u017ee nasta\u0165:','v procese konjug\u00e1cie','prostredn\u00edctvom bakteriof\u00e1ga','prostredn\u00edctvom v\u00edrusu','pomocou kopula\u010dn\u00e9ho most\u00edka','pomocou konjuga\u010dn\u00e9ho mostfica','v procese oplodnenia','prostredn\u00edctvom vektora','prostredn\u00edctvom plazmidov',TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(975,'975. Nukleoid je:','z\u00e1kladn\u00e1 stavebn\u00e1 jednotka eukaryotick\u00e9ho chromoz\u00f3mu','z\u00e1kladn\u00e1 funk\u010dn\u00e1 jednotka eukaryotick\u00e9ho chromoz\u00f3mu','ozna\u010denie pre jadierko','ozna\u010denie pre jadro','ozna\u010denie miesta na chromoz\u00f3me zodpovedn\u00e9ho za organiz\u00e1ciu jadierka','DNA bakt\u00e9ri\u00ed','prokaryotick\u00fd chromoz\u00f3m','DNA bakt\u00e9ri\u00ed stabilizovan\u00e1 nieko\u013ek\u00fdmi molekulami bielkov\u00edn',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(976,'976. Karyotyp je:','s\u00fabor chromoz\u00f3mov diploidnej bunky v anaf\u00e1ze','s\u00fabor chromoz\u00f3mov jadra diploidnej bunky zoraden\u00fdch do homologick\u00fdch p\u00e1rov a morfologick\u00fdch skup\u00edn','s\u00fabor chromoz\u00f3mov zoraden\u00fdch do dvoch morfologick\u00fdch skup\u00edn: pohlavn\u00fdch chromoz\u00f3mov a gonoz\u00f3mov','s\u00fabor jadrov\u00fdch a nejadrov\u00fdch chromoz\u00f3mov','v\u00fdsledkom evol\u00facie','typ bunkov\u00e9ho jadra','s\u00fabor v\u0161etk\u00fdch g\u00e9nov v bunkovom jadre','tvar bunkov\u00e9ho jadra',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(977,'977. Homologick\u00e9 chromoz\u00f3my:','maj\u00fa rovnak\u00fd tvar a v\u017edy rovnak\u00e9 alely','maj\u00fa r\u00f4zny tvar a rovnak\u00e9 rozmery','maj\u00fa rovnak\u00fd tvar a rovnak\u00e9 g\u00e9ny','maj\u00fa rovnak\u00fa ve\u013ekos\u0165 a tvar ale r\u00f4zne g\u00e9ny','s\u00fa chromoz\u00f3my, ktor\u00e9 neobsahuj\u00fa g\u00e9ny ve\u013ek\u00e9ho \u00fa\u010dinku','maj\u00fa rovnak\u00fd tvar a na g\u00e9nov\u00fdch lokusoch maj\u00fa rovnak\u00e9 alebo odli\u0161n\u00e9 alely','s\u00fa pohlavn\u00e9 chromoz\u00f3my sam\u00edc vt\u00e1kov','s\u00fa chromoz\u00f3my, medzi ktor\u00fdmi nem\u00f4\u017ee doch\u00e1dza\u0165 ku crossing-overu',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(978,'978. Klon je s\u00fabor jedincov:','ktor\u00ed vznikli pri vegetat\u00edvnom rozmno\u017eovan\u00ed','ktor\u00ed s\u00fa genotypovo rozdielni a fenotypovo rovnak\u00ed','s rovnak\u00fdm genotypom','ktor\u00ed vznikli pr\u00edbuzensk\u00fdm kr\u00ed\u017een\u00edm','vzniknut\u00fd zo spolo\u010dnej bunky','ktor\u00ed vznikli mimotelov\u00fdm oplodnen\u00edm','ktor\u00ed s\u00fa potomkami jedn\u00e9ho rodi\u010dovsk\u00e9ho p\u00e1ru','ktor\u00ed s\u00fa geneticky zhodn\u00ed',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(979,'979. Pre nepohlavn\u00e9 rozmno\u017eovanie je charakteristick\u00e9, \u017ee:','nov\u00fd jedinec vznik\u00e1 z diploidn\u00fdch buniek mitotick\u00fdm delen\u00edm','nov\u00fd jedinec vznik\u00e1 z diploidn\u00fdch buniek mitotick\u00fdm alebo meiotick\u00fdm delen\u00edm','nov\u00fd jedinec vznik\u00e1 z haploidn\u00fdch buniek priamym delen\u00edm','nov\u00fd jedinec vznik\u00e1 amitoticky z haploidn\u00fdch buniek','potomstvo je geneticky identick\u00e9 s rodi\u010dovsk\u00fdm organizmom','nezvy\u0161uje variabilitu druhu','potomstvo netvor\u00ed pohlavn\u00e9 bunky','zvy\u0161uje variabilitu druhu',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(980,'980. V procese mei\u00f3zy doch\u00e1dza k:','tvorbe gam\u00e9t','zn\u00ed\u017eeniu po\u010dtu len niektor\u00fdch chromoz\u00f3mov','vzniku haploidnej sady chromoz\u00f3mov','redukcii po\u010dtu chromoz\u00f3mov na polovicu','strate chromoz\u00f3mov','tvorbe diploidn\u00fdch gam\u00e9t','rekombin\u00e1cii otcovsk\u00fdch a matersk\u00fdch chromoz\u00f3mov','replik\u00e1cii chromoz\u00f3mov',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(981,'981. Mei\u00f3za pozost\u00e1va z:','dvoch delen\u00ed bunky po jednej replik\u00e1cii DNA','dvoch delen\u00ed bunky a dvoch replik\u00e1ci\u00ed DNA','dvoch delen\u00ed bunky s odl\u00ed\u0161n\u00fdm priebehom','dvoch delen\u00ed, po\u010das ktor\u00fdch sa rozdelia homologick\u00e9 chromoz\u00f3my a sestersk\u00e9 chromat\u00eddy','jedn\u00e9ho delenia mitotick\u00e9ho a jedn\u00e9ho meiotick\u00e9ho','dvoch delen\u00ed po jednej replik\u00e1cii DNA','dvoch reduk\u010dn\u00fdch delen\u00ed','heterotypick\u00e9ho a homeotypick\u00e9ho delenia',TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(982,'982. Crossing-over je:','proces redukcie po\u010dtu chromoz\u00f3mov','p\u00e1rovanie heterologick\u00fdch chromoz\u00f3mov','v\u00fdmena \u010dast\u00ed medzi homologick\u00fdmi chromoz\u00f3mami','prekr\u00ed\u017eenie heterologick\u00fdch chromoz\u00f3mov','prekr\u00ed\u017eenie sestersk\u00fdch chromat\u00edd','v\u00fdznamn\u00fd epigenetick\u00fd proces','evolu\u010dne bezv\u00fdznamn\u00fd proces','proces prispievaj\u00faci k zabezpe\u010deniu vn\u00fatrodruhovej genetickej variability',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(983,'983. Crossing-over:','sa vyskytuje len medzi pohlavn\u00fdmi chromoz\u00f3mami u mu\u017eov','sa vyskytuje medzi nehomologick\u00fdmi chromoz\u00f3mami','sa vyskytuje medzi homologick\u00fdmi chromoz\u00f3mami','napom\u00e1ha segreg\u00e1cii chromoz\u00f3mov','sa vyskytuje pri reduk\u010dnom delen\u00ed','sa vyskytuje medzi nesestersk\u00fdmi chromat\u00eddami homologick\u00e9ho p\u00e1ru chromoz\u00f3mov','umo\u017e\u0148uje rekombin\u00e1ciu ne\u00fapine viazan\u00fdch g\u00e9nov','zabezpe\u010duje zhodu genotypu potomkov s rodi\u010dmi',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(984,'984. K segreg\u00e1cii matersk\u00fdch a otcovsk\u00fdch chromoz\u00f3mov pri mei\u00f3ze doch\u00e1dza tak, \u017ee:','gam\u00e9ta m\u00e1 n\u00e1hodn\u00fa kombin\u00e1ciu t\u00fdchto chromoz\u00f3mov','jedna gam\u00e9ta m\u00e1 v\u0161etky chromoz\u00f3my otcovsk\u00e9, druh\u00e1 matersk\u00e9','gam\u00e9ty maj\u00fa v\u017edy polovicu otcovsk\u00fdch a polovicu matersk\u00fdch chromoz\u00f3mov','preva\u017euj\u00fa gam\u00e9ty s otcovsk\u00fdmi chromoz\u00f3mami','nie je rozhoduj\u00faci ich p\u00f4vod','sa prie\u010dne rozdelia homologick\u00e9 chromoz\u00f3my','do ka\u017edej gam\u00e9ty sa dost\u00e1va len jeden chromoz\u00f3m z homologick\u00e9ho p\u00e1ru','do gam\u00e9t sa dost\u00e1vaj\u00fa p\u00e1ry homologick\u00fdch chromoz\u00f3mov n\u00e1hodne',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(985,'985. V ktorej f\u00e1ze meiotick\u00e9ho delenia nast\u00e1va kombinovanie alel:','na konci I. meiotick\u00e9ho delenia','na za\u010diatku anaf\u00e1zy I. meiotick\u00e9ho delenia','v prof\u00e1ze I. meiotick\u00e9ho delenia','pri segreg\u00e1cii chromoz\u00f3mov v II. meiotickom delen\u00ed','po\u010das replik\u00e1cie DNA','v anaf\u00e1ze II. meiotick\u00e9ho delenia','v metaf\u00e1ze heterotypick\u00e9ho delenia','v telof\u00e1ze homeotypick\u00e9ho delenia',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(986,'986. Gam\u00e9ta:','obsahuje kombin\u00e1ciu otcovsk\u00fdch a matersk\u00fdch chromoz\u00f3mov','m\u00f4\u017ee obsahova\u0165 nerovnak\u00fd po\u010det matersk\u00fdch a otcovsk\u00fdch chromoz\u00f3mov','obsahuje rekombinovan\u00e9 chromoz\u00f3my','vznik\u00e1 z diploidnej z\u00e1rodo\u010dnej bunky','v\u017edy obsahuje presne polovicu matersk\u00fdch a polovicu otcovsk\u00fdch chromoz\u00f3mov','je v\u00fdsledkom meiotick\u00e9ho delenia','je nediferencovan\u00e1 bunka','je vysoko \u0161pecializovan\u00fd typ bunky',TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(987,'987. Segreg\u00e1cia chromoz\u00f3mov po\u010das I. meiotick\u00e9ho delenia m\u00e1 za n\u00e1sledok:','prekr\u00ed\u017eenie homologick\u00fdch chromoz\u00f3mov','rovnak\u00fa genetick\u00fa vybavenos\u0165 v\u0161etk\u00fdch gam\u00e9t','genetick\u00fa variabilitu gam\u00e9t','kombin\u00e1ciu chromoz\u00f3mov matersk\u00e9ho p\u00f4vodu','redukciu po\u010dtu chromoz\u00f3mov na haploidn\u00fd','fenotypov\u00fa variabilitu potomstva','\u00fapln\u00fa zhodu potomstva s rodi\u010dmi','sterilitu jedinca',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(988,'988. Segreg\u00e1cia chromoz\u00f3mov je:','rozchod homologick\u00fdch chromoz\u00f3mov pri gametogen\u00e9ze','delenie chromoz\u00f3mov','separ\u00e1cia homologick\u00fdch chromoz\u00f3mov pri meiotickom delen\u00ed','proces zabezpe\u010duj\u00faci redukciu po\u010dtu chromoz\u00f3mov','rozklad chromoz\u00f3mov','tvorba chromoz\u00f3mov\u00fdch p\u00e1rov','proces v\u00fdmeny homologick\u00fdch \u010dast\u00ed chromoz\u00f3mov','nevyhnutn\u00fd predpoklad pre priebeh crossing-overu',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(989,'989. Po\u010det chromoz\u00f3mov v bunk\u00e1ch r\u00f4znych organizmov je:','r\u00f4zny a vo v\u00e4\u010d\u0161ine buniek jedincov dan\u00e9ho druhu premenliv\u00fd','rovnak\u00fd, preto\u017ee zlo\u017eenie DNA pre v\u0161etky organizmy je rovnak\u00e9','r\u00f4zny a l\u00ed\u0161i sa u jedincov jedn\u00e9ho druhu','rovnak\u00fd v haploidn\u00fdch a r\u00f4zny v diploidn\u00fdch bunk\u00e1ch','r\u00f4zny, av\u0161ak charakteristick\u00fd pre dan\u00fd druh','v\u00fdsledkom evolu\u010dn\u00e9ho v\u00fdvoja jedinca','v\u00fdsledkom evol\u00facie druhu','premenliv\u00fd v z\u00e1vislosti od aktivity buniek',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(990,'990. Pri segreg\u00e1cii chromoz\u00f3mov v procese mei\u00f3zy:','do gam\u00e9ty prech\u00e1dza n\u00e1hodn\u00fd diploidn\u00fd po\u010det chromoz\u00f3mov','sa do gam\u00e9ty dost\u00e1va z ka\u017ed\u00e9ho p\u00e1ru homologick\u00fdch chromoz\u00f3mov jeden chromoz\u00f3m','segreguj\u00fa alely g\u00e9nov','z ka\u017ed\u00e9ho p\u00e1ru alel sa do gam\u00e9ty dostane jedna alela','je proces rozchodu homologick\u00fdch chromoz\u00f3mov n\u00e1hodn\u00fd','m\u00f4\u017eu vznika\u0165 gen\u00f3mov\u00e9 mut\u00e1cie','vznikaj\u00fa dc\u00e9rske jadr\u00e1 geneticky identick\u00e9 s matersk\u00fdm','doch\u00e1dza k vzniku haploidn\u00fdch jadier, ktor\u00e9 s\u00fa navz\u00e1jom identick\u00e9',FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(991,'991. Eukaryotick\u00e9 chromoz\u00f3my s\u00fa tvoren\u00e9:','komplexom DNA, RNA a bielkov\u00edn','komplexom DNA a bielkov\u00edn','komplexom DNA, bielkov\u00edn a polysacharidov','komplexom RNA a bielkov\u00edn','chromat\u00ednom','len komplexom nukleov\u00fdch kysel\u00edn','komplexom DNA a fosfolipidov','komplexom DNA a polysacharidov',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(992,'992. Nukleoz\u00f3m je:','jadierko','jadro prokaryotickej bunky','membr\u00e1nov\u00e1 organela','z\u00e1kladn\u00e1 \u0161trukt\u00farna jednotka chromat\u00ednu','\u010das\u0165 chromoz\u00f3mu sp\u00e1jaj\u00faca sestersk\u00e9 chromat\u00eddy','organiz\u00e1tor jadierka','podjednotka nukleotidov','zlo\u017een\u00fd z DNA a hist\u00f3nov\u00fdch bielkov\u00edn',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(993,'993. Pod\u013ea polohy centrom\u00e9ry rozli\u0161ujeme chromoz\u00f3my:','somatick\u00e9 a pohlavn\u00e9','rovnoramenn\u00e9 a nerovnoramenn\u00e9','prokaryotick\u00e9 a eukaryotick\u00e9','metacentrick\u00e9, submetacentrick\u00e9 a akrocentrick\u00e9','mitotick\u00e9 a meiotick\u00e9','kr\u00e1tke a dlh\u00e9','dvojchromat\u00eddov\u00e9 a jednochromat\u00eddov\u00e9','monog\u00e9nne a polyg\u00e9nne',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(994,'994. Chromat\u00edn eukaryotick\u00fdch buniek je:','komplex DNA a bielkov\u00edn, jadro a chromoz\u00f3my s\u00fa ohrani\u010den\u00e9 jadrovou membr\u00e1nou','komplex DNA, RNA a bielkov\u00edn, jadro nem\u00e1 jadrov\u00fa membr\u00e1nu','komplex DNA, RNA a bielkov\u00edn v jadre s membr\u00e1nou','komplex DNA a bielkov\u00edn, jadro s chromoz\u00f3mami nem\u00e1 jadrov\u00fa membr\u00e1nu','nukleoprote\u00ednov\u00fd komplex - hmotn\u00fd z\u00e1klad chromoz\u00f3mov','zlo\u017een\u00fd z fragmentov chromoz\u00f3mov','bielkovina oba\u013euj\u00faca jadrov\u00fa membr\u00e1nu','hmota tvoriaca mitochondri\u00e1lnu matrix',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(995,'995. Chromoz\u00f3mov\u00e9 lokusy:','s\u00fa miesta, kde sa nach\u00e1dzaj\u00fa g\u00e9ny na chromoz\u00f3me','s\u00fa miesta prekr\u00ed\u017eenia chromoz\u00f3mov','s\u00fa na chromoz\u00f3moch usporiadan\u00e9 line\u00e1rne','s\u00fa oblasti chromoz\u00f3mov, kde sa nach\u00e1dza centrom\u00e9ra','svojou vzdialenos\u0165ou ur\u010duj\u00fa silu g\u00e9novej v\u00e4zby','ur\u010duj\u00fa karyotyp','s\u00fa oblasti chromoz\u00f3mov, ktor\u00e9 s\u00fa spojen\u00e9 s jadrovou membr\u00e1nou','s\u00fa oblasti chromoz\u00f3mov, ktor\u00e9 sa nikdy neprepisuj\u00fa do mRNA',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(996,'996. Heterologick\u00e9 chromoz\u00f3my:','maj\u00fa v\u017edy rovnak\u00e9 g\u00e9ny a r\u00f4zne alely','s\u00fa v\u017edy p\u00e1rov\u00e9 chromoz\u00f3my','s\u00fa chromoz\u00f3my odli\u0161n\u00fdch homologick\u00fdch p\u00e1rov u jedinca','maj\u00fa odli\u0161n\u00e9 g\u00e9ny aj tvar','s\u00fa pohlavn\u00e9 chromoz\u00f3my \u017eeny','s\u00fa chromoz\u00f3my rozli\u010dn\u00fdch biologick\u00fdch druhov','s\u00fa chromoz\u00f3my s identick\u00fdm obsahom genetickej inform\u00e1cie','s\u00fa chromoz\u00f3my mitochondri\u00ed a chloroplastov',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(997,'997. Chromoz\u00f3my s\u00fa najlep\u0161ie pozorovate\u013en\u00e9 v:','S-f\u00e1ze bunkov\u00e9ho cyklu','G_1 f\u00e1ze bunkov\u00e9ho cyklu','M-f\u00e1ze bunkov\u00e9ho cyklu','metaf\u00e1ze','anaf\u00e1ze','prof\u00e1ze','telof\u00e1ze','G_2 f\u00e1ze bunkov\u00e9ho cyklu',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(998,'998. Centrom\u00e9ra:','riadi konjug\u00e1ciu bakt\u00e9ri\u00ed','patr\u00ed k regula\u010dn\u00fdm g\u00e9nom','je typ plazmidu','je miesto uchytenia riboz\u00f3mov na tRNA v procese transl\u00e1cie','je miesto na uchytenie mikrotubulov deliaceho vretienka na chromoz\u00f3me','je miesto na uchytenie mikrofilamentov deliaceho vretienka na chromoz\u00f3me','sa ozna\u010duje tie\u017e ako sekund\u00e1rna konstrikcia','predstavuje z\u00fa\u017eenie chromoz\u00f3mu, za ktor\u00fdm nasleduje oblas\u0165 satelitu',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(999,'999. Chromom\u00e9ra:','je miesto uchytenia riboz\u00f3mov v procese transl\u00e1cie','je miesto uchytenia mikrotubulov deliaceho sa vretienka na chromoz\u00f3me','je miesto uchytenia mikrofilamentov deliaceho sa vretienka na chromoz\u00f3me','sa nach\u00e1dza v oblasti sekund\u00e1rnej konstrikcie chromoz\u00f3mu','je z\u00e1kladn\u00e1 stavebn\u00e1 jednotka eukaryotick\u00e9ho chromoz\u00f3mu','je typ hist\u00f3novej bielkoviny','je miesto intenz\u00edvnej\u0161ej \u0161piraliz\u00e1cie DNA pozorovate\u013en\u00e9 mikroskopom','je oblas\u0165 chromoz\u00f3mu zodpovedn\u00e1 za organiz\u00e1ciu jadierka',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1000,'1000. Satelit:','ozna\u010duje miesto intenz\u00edvnej\u0161ej \u0161piraliz\u00e1cie DNA','je miesto sekund\u00e1rnej konstrikcie chromoz\u00f3mu','je miesto prim\u00e1rnej konstrikcie chromoz\u00f3mu','je typ plazmidu','je zodpovedn\u00fd za organiz\u00e1ciu a funkciu jadierka','je koncov\u00e1 \u010das\u0165 molekuly tRNA','je koncov\u00e1 \u010das\u0165 niektor\u00fdch chromoz\u00f3mov oddelen\u00e1 sekund\u00e1rnou konstrikciou','je z\u00e1kladn\u00e1 \u0161trukt\u00farna jednotka chromoz\u00f3mu',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1001,'1001. Hist\u00f3nov\u00e9 bielkoviny:','maj\u00fa katalytick\u00fa funkciu','maj\u00fa \u0161trukt\u00farnu funkciu','nach\u00e1dzaj\u00fa sa predov\u0161etk\u00fdm v biologick\u00fdch membr\u00e1nach','s\u00fa pova\u017eovan\u00e9 za transportn\u00e9 bielkoviny','podie\u013eaj\u00fa sa na tvorbe nuldeoz\u00f3mov','s\u00fa s\u00fa\u010das\u0165ou chromat\u00ednu prokaryotickej bunky','s\u00fa s\u00fa\u010das\u0165ou chromat\u00ednu eukaryotickej bunky','zabezpe\u010duj\u00fa regul\u00e1ciu tvorby polypeptidov\u00fdch re\u0165azcov',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1002,'1002. Idiogram je:','z\u00e1znam z testovania inteligen\u010dn\u00e9ho kvocientu','chromoz\u00f3mov\u00e1 mapa bakt\u00e9ri\u00ed','chromoz\u00f3mov\u00e1 mapa eukaryotickej bunky','sch\u00e9matick\u00e9 zn\u00e1zornenie karyotypu ur\u010dit\u00e9ho jedinca','s\u00fabor chromoz\u00f3mov jednej somatickej bunky z\u00edskan\u00fdch napr. kultiv\u00e1ciou perif\u00e9rnej krvi','s\u00fabor dermatoglyfick\u00fdch odla\u010dkov prstov \u010dloveka','rodokme\u0148ov\u00e1 sch\u00e9ma','z\u00e1znam z kompletn\u00e9ho zmapovania rudsk\u00e9ho gen\u00f3mu',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1003,'1003. Chromoz\u00f3my \u010dloveka sa pod\u013ea ve\u013ekosti a tvaru zara\u010fuj\u00fa do:','3 skup\u00edn','4 skup\u00edn','5 skup\u00edn','6 skup\u00edn','7 skup\u00edn','8 skup\u00edn','9 skup\u00edn','10 skup\u00edn',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1004,'1004. Bivalent je:','typ chromoz\u00f3movej mut\u00e1cie','typ mut\u00e1cie, pri ktorej je zdvojn\u00e1soben\u00e1 cel\u00e1 chromoz\u00f3mov\u00e1 sada','tvoren\u00fd dvoma chromat\u00eddami','tvoren\u00fd \u0161tyrmi chromat\u00eddami','\u00fatvar zlo\u017een\u00fd z dvoch homologick\u00fdch chromoz\u00f3mov','\u00fatvar zlo\u017een\u00fd z dvoch heterologick\u00fdch chromoz\u00f3mov','s\u00faborn\u00e9 ozna\u010denie dvoch sestersk\u00fdch chromat\u00edd','zdvojenie \u00faseku chromoz\u00f3mu',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1005,'1005. Autoz\u00f3my s\u00fa:','nepohlavn\u00e9 chromoz\u00f3my','nep\u00e1rov\u00e9 chromoz\u00f3my','len v somatick\u00fdch bunk\u00e1ch','len v diploidn\u00fdch bunk\u00e1ch','somatick\u00e9 chromoz\u00f3my','chromoz\u00f3my, ktor\u00e9 neur\u010duj\u00fa pohlavie cicavcov','chromoz\u00f3my zodpovedn\u00e9 za ur\u010denie pohlavia cicavcov','mimojadrov\u00e9 chromoz\u00f3my',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1006,'1006. Ktor\u00e9 tvrdenie je spr\u00e1vne:','v pohlavnej bunke s\u00fa v\u017edy dve alely jedn\u00e9ho g\u00e9nu','s\u00fabor v\u0161etk\u00fdch g\u00e9nov organizmu sa naz\u00fdva genofond','konkr\u00e9tna forma alely sa naz\u00fdva g\u00e9n','v \u017eivom organizme s\u00fa v\u0161etky g\u00e9ny akt\u00edvne','v prokaryotick\u00fdch bunk\u00e1ch sa nevyskytuje syst\u00e9m alel','prokaryotick\u00e1 bunka m\u00e1 viac g\u00e9nov ako eukaryotick\u00e1 bunka','molekuly RNA v eukaryotickej bunke vznikaj\u00fa transkripciou pod\u013ea matrice DNA','fenotyp je s\u00fabor jadrov\u00fdch g\u00e9nov',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1007,'1007. Z\u00e1kladn\u00fd v\u00fdznam Mendelov\u00fdch objavov spo\u010d\u00edva v tom, \u017ee:','prv\u00fd dok\u00e1zal dedi\u010dnos\u0165 znakov','dok\u00e1zal materi\u00e1lnu podstatu g\u00e9nov','dok\u00e1zal, \u017ee g\u00e9ny sa pri kr\u00ed\u017een\u00ed nezmie\u0161avaj\u00fa','vy\u0161\u013eachtil nov\u00e9 odrody rastl\u00edn','presne stanovil \u0161tiepne pomery u heterozygotov','dok\u00e1zal, \u017ee u heterozygota sa do pohlavn\u00fdch buniek dost\u00e1vaj\u00fa nezmie\u0161an\u00e9 alely','dok\u00e1zal, \u017ee g\u00e9ny s\u00fa lokalizovan\u00e9 v chromoz\u00f3moch','dok\u00e1zal, \u017ee nosite\u013eom dedi\u010dnej inform\u00e1cie je DNA',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1008,'1008. Z\u00e1kladn\u00e1 genetick\u00e1 charakteristika homozygotov:','maj\u00fa rovnak\u00fd karyotyp','maj\u00fa rovnak\u00fd po\u010det g\u00e9nov','od rodi\u010dov zdedili rovnak\u00e9 alely ur\u010dit\u00e9ho g\u00e9nu','maj\u00fa rovnak\u00fd genotyp','na g\u00e9novom lokuse maj\u00fa rovnak\u00e9 alely','tvoria rovnak\u00fd po\u010det zygot','maj\u00fa alely dan\u00e9ho g\u00e9nu vz\u00e1jomne \u00faplne viazan\u00e9','tvoria gam\u00e9ty s odli\u0161n\u00fdmi alelami homozygotn\u00e9ho g\u00e9nu',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1009,'1009. Ako sa ozna\u010duje kr\u00ed\u017eenie, pri ktorom sledujeme prenos dvoch r\u00f4znych alel jedn\u00e9ho g\u00e9nu:','sp\u00e4tn\u00e9 kr\u00ed\u017eenie','somatick\u00e1 hybridiz\u00e1cia','recipro\u010dn\u00e9 kr\u00ed\u017eenie','monohybridizmus','pr\u00edbuzensk\u00e9 kr\u00ed\u017eenie','inbr\u00edding','dihybridizmus','parunixia',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1010,'1010. Z\u00e1kladn\u00e1 genetick\u00e1 charakteristika heterozygotov:','na g\u00e9novom lokuse maj\u00fa odli\u0161n\u00e9 alely','maj\u00fa odli\u0161n\u00fd genotyp aj fenotyp','nemaj\u00fa rovnak\u00fd genofond','vznikli kr\u00ed\u017een\u00edm fenotypovo odli\u0161n\u00fdch rodi\u010dov','od rodi\u010dov zdedili odli\u0161n\u00e9 alely dan\u00e9ho g\u00e9nu','tvoria odli\u0161n\u00e9 zygoty','tvoria gam\u00e9ty s odli\u0161n\u00fdmi alelami heterozygotn\u00e9ho g\u00e9nu','maj\u00fa alely dan\u00e9ho g\u00e9nu vz\u00e1jomne \u00fapine viazan\u00e9',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1011,'1011. Ako odl\u00ed\u0161ime pri \u00faplnej dominancii heterozygota od homozygota:','sp\u00e4tn\u00fdm testovac\u00edm kr\u00ed\u017een\u00edm','recipro\u010dn\u00fdm kr\u00ed\u017een\u00edm','ak kr\u00ed\u017eime heterozygota s reces\u00edvnym homozygotom','ned\u00e1 sa odl\u00ed\u0161i\u0165','anal\u00fdzou g\u00e9novej v\u00e4zby','molekul\u00e1mo-biologick\u00fdmi met\u00f3dami','na z\u00e1klade fenotypov\u00fdch odli\u0161nost\u00ed','na z\u00e1klade frekvencie crossing-overu',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1012,'1012. D\u00e1 sa pri ne\u00fapinej dominancii alely fenotypovo odl\u00ed\u0161i\u0165 heterozygot od dominantn\u00e9ho homozygota:','v niektor\u00fdch pr\u00edpadoch \u00e1no','m\u00e1lokedy','len recipro\u010dn\u00fdm kr\u00ed\u017een\u00edm','d\u00e1 sa','len sp\u00e4tn\u00fdm kr\u00ed\u017een\u00edm','pomocou pr\u00edbuzensk\u00e9ho kr\u00ed\u017eenia','len dihybridn\u00fdm kr\u00ed\u017een\u00edm','nie, preto\u017ee s\u00fa fenotypovo rovnak\u00ed',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1013,'1013. \u010co rozumieme v genetike pod pojmom intermediarita:','potla\u010denie dominantnej alely reces\u00edvnou alelou z in\u00e9ho lokusu','spolup\u00f4sobenie odli\u0161n\u00fdch alel g\u00e9nu na fenotypovom prejave','osobitn\u00fd pr\u00edpad ne\u00fapinej dominancie, pri\u010dom sa dominantn\u00e1 alela neprejav\u00ed vo fenotype heterozygota','pr\u00edpad ne\u00faplnej dominancie, kedy sa obidve alely prejavia vo fenotype rovnakou mierou','osobitn\u00fd pr\u00edpad ne\u00faplnej dominancie, kedy sa reces\u00edvna alela v\u00f4bec neprejav\u00ed vo fenotype heterozygota','pr\u00edpad, ke\u010f sa alely dan\u00e9ho g\u00e9nu navz\u00e1jom nepotl\u00e1\u010daj\u00fa','pr\u00edpad, ke\u010f je znak podmienen\u00fd dvoma spolup\u00f4sobiacimi g\u00e9nmi','pr\u00edpad, ke\u010f je prejav znaku ovplyvnen\u00fd aj negenetick\u00fdmi faktormi',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1014,'1014. Monog\u00e9nny znak u diploidn\u00e9ho organizmu je podmienen\u00fd:','dvoma g\u00e9nmi, jedn\u00fdm od matky, jedn\u00fdm od otca','dvoma alelami jedn\u00e9ho g\u00e9nu, jednou od matky a jednou od otca, ak ide o reces\u00edvny znak','dvoma alelami r\u00f4znych lokusov, jednou od otca a jednou od matky','dvoma alelov\u00fdmi p\u00e1rmi, jedn\u00fdm od otca a jedn\u00fdm od matky','dvoma dominantn\u00fdmi alebo reces\u00edvnymi g\u00e9nmi','v\u017edy jednou dominantnou alelou','dvoma lokusmi, jedn\u00fdm od matky a druh\u00fdm od otca','jednou sadou chromoz\u00f3mov',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1015,'1015. Ktor\u00e1 z genetick\u00fdch sch\u00e9m vyjadruje kr\u00ed\u017eenie dvoch reces\u00edvnych homozygotov:','AA x AA','AA x aa','aa x aa','AABb x aabb','aabbccddee x aabbccddee','AAbb x aaBB','AAbbCC x aaBBcc','AaBbCc x AaBbCc',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1016,'1016. Ktor\u00e1 z genetick\u00fdch sch\u00e9m vyjadruje kr\u00ed\u017eenie dvoch heterozygotov:','Cc x Cc','Aa x aa','AABB x AABb','AaBb x AaBb','AaBbCc x AaBbCc','AaBbCC x AaBbCc','aabbccddee x AABBCCDDEE','aa x AA',TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1017,'1017. Ktor\u00e9 z nasleduj\u00facich z\u00e1pisov neodporuj\u00fa Mendelov\u00fdm z\u00e1konom (rodi\u010dia - potomkovia):','AA x aa \u2014 AA, Aa, aa','AA x Aa \u2014 AA, AA, Aa, Aa','Aa x Aa \u2014 AA, Aa, Aa, aa','Aa x aa \u2014 Aa, Aa, aa, aa','AABB x AABb \u2014 AABB, AABb, AaBB, AaBb','aaBB x AAbb \u2014 AaBb, AAbb, aaBB','aa x aa \u2014 aa','aa x aa \u2014 aa, Aa, AA',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1018,'1018. Ktor\u00e9 z nasleduj\u00facich z\u00e1pisov nezodpovedaj\u00fa Mendelov\u00fdm z\u00e1konom (rodi\u010dia - potomkovia):','Aa x aa \u2014 Aa, Aa, aa, aa','AA x aa \u2014 AA, Aa, aa','bb x bb \u2014 bb','bb x bb \u2014 BB, Bb','AABB x AABb \u2014 AABB, AABb, AaBB, AaBb','AA x Aa \u2014 AA, AA, Aa, Aa','AA x Aa \u2014 AA, Aa, Aa, aa','aaBBccDD x AAbbCCdd \u2014 AaBbCcDd, AaBbCCDd, AaBbccDd',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1019,'1019. Sp\u00e4tn\u00e9 kr\u00ed\u017eenie:','je synonymum pre recipro\u010dn\u00e9 kr\u00ed\u017eenie','pou\u017e\u00edva sa len pri monohybridnom kr\u00ed\u017een\u00ed','je kr\u00ed\u017eenie dvoch reces\u00edvnych homozygotov','umo\u017e\u0148uje odl\u00ed\u0161i\u0165 dominantn\u00e9ho homozygota od heterozygota pri \u00faplnej dominancii','sa pou\u017e\u00edva v \u0161\u013eachtite\u013eskej praxi','sa uplat\u0148uje aj v genetike \u010dloveka','je synonymum pre inbr\u00edding','je kr\u00ed\u017eenie heterozygota s reces\u00edvnym homozygotom',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1020,'1020. Reces\u00edvna alela pri \u00faplnej dominancii:','m\u00e1 rovnak\u00fa kvalitu u heterozygota (Aa) ako u homozygota (aa)','m\u00e1 rozdielnu kvalitu u heterozygota (Aa) ako u homozygota (aa)','nem\u00e1 vplyv na fenotypov\u00fa variabilitu heterozygota','nie je ovplyv\u0148ovan\u00e1 prostred\u00edm','v gam\u00e9te vytvorenej heterozygotom m\u00e1 odli\u0161n\u00e9 vlastnosti ako v gam\u00e9te vytvorenej homozygotom','v homozygotnom stave je v\u017edy ovplyv\u0148ovan\u00e1 prostred\u00edm','sa vo fenotype prejav\u00ed len ak je v homozygotnom stave','m\u00f4\u017ee oslabi\u0165 vplyv dominantnej alely na fenotyp',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1021,'1021. Pri kr\u00ed\u017een\u00ed heterozygotov:','v potomstve z\u00edskavame dominantn\u00e9 aj reces\u00edvne fenotypy v ur\u010dit\u00fdch \u0161tiepnych pomeroch','sa v potomstve \u010dastej\u0161ie vyskytuj\u00fa dominantn\u00ed homozygoti ako heterozygoti','m\u00f4\u017ee by\u0165 pomer pohlavia v ich potomstve rovnak\u00fd','pri monohybridizme v potomstve dostaneme rovnak\u00fd pomer dominantn\u00fdch aj reces\u00edvnych homozygotov','sa v potomstve \u010dastej\u0161ie vyskytuj\u00fa reces\u00edvni homozygoti','pohlavie rodi\u010dov neovplyv\u0148uje fenotypy potomstva','sa v potomstve \u010dastej\u0161ie vyskytuj\u00fa heterozygoti ako dominantn\u00ed homozygoti','sa v potomstve nem\u00f4\u017eu vyskytova\u0165 homozygoti',TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1022,'1022. Kr\u00ed\u017eenie:','je vegetat\u00edvne rozmno\u017eovanie dvoch organizmov','je pohlavn\u00e9 rozmno\u017eovanie dvoch vybran\u00fdch jedincov ur\u010dit\u00e9ho druhu','je jednou zo z\u00e1kladn\u00fdch met\u00f3d genetick\u00e9ho v\u00fdskumu','pr\u00edbuzensk\u00e9 sa naz\u00fdva inbr\u00edding','ako met\u00f3du \u0161\u013eachtenia prv\u00fd pou\u017eil J. G. Mendel','je d\u00f4le\u017eit\u00fd proces vo v\u00fdvoji druhu','je jeden zo sp\u00f4sobov produkcie klonov','je jedna zo z\u00e1kladn\u00fdch met\u00f3d \u0161\u013eachtenia',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1023,'1023. Pre monohybridizmus je charakteristick\u00e9, \u017ee:','s\u00fa mo\u017en\u00e9 len tri odli\u0161n\u00e9 typy kr\u00ed\u017eenia','ak s\u00fa obaja rodi\u010dia odli\u0161n\u00ed homozygoti, vytv\u00e1raj\u00fa rovnak\u00fd typ gam\u00e9t','kr\u00ed\u017een\u00edm heterozygotov vznikaj\u00fa v potomstve tri genotypov\u00e9 skupiny','pri kr\u00ed\u017een\u00ed heterozygota s reces\u00edvnym homozygotom vznikaj\u00fa v potomstve dve fenotypov\u00e9 skupiny','kr\u00ed\u017een\u00edm homozygotov rovnak\u00e9ho typu vznik\u00e1 \u010dist\u00e1 l\u00ednia','z\u00e1kladn\u00e9 poznatky a pravidl\u00e1 sa daj\u00fa vyu\u017ei\u0165 v genetike \u010dloveka','potomstvo dvoch odli\u0161n\u00fdch homozygotov je genotypovo aj fenotypovo uniformn\u00e9','potomstvo dvoch odli\u0161n\u00fdch homozygotov vytv\u00e1ra dve fenotypov\u00e9 skupiny',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1024,'1024. Pri monohybridizme tvoria heterozygoti:','rovnak\u00e9 gam\u00e9ty s prevahou dominantnej alely','dva typy gam\u00e9t','gam\u00e9ty obsahuj\u00face len dominantn\u00fa alebo reces\u00edvnu alelu g\u00e9nu','gam\u00e9ty s alelami Aa a aA','polovicu gam\u00e9t s dominantnou a polovicu s reces\u00edvnou alelou','gam\u00e9ty s alelami AA a aa','jeden typ gam\u00e9t s alelami Aa','gam\u00e9ty s dominantnou alebo reces\u00edvnou alelou v pomere 3:1',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1025,'1025. Spr\u00e1vny z\u00e1pis pre parent\u00e1lnu gener\u00e1ciu pri monohybridizme je:','aa x aa','AA x aa','aa x Aa','AA x Aa','AAbb x AABB','Aa x Aa','AABB x aabb','AaBb x AaBb',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1026,'1026. Ur\u010dite genetick\u00fa sch\u00e9mu kr\u00ed\u017eenia dvoch odli\u0161n\u00fdch homozygotov:','Aa x AA','Aa x Aa','AA x aa','aa x Aa','AAbb x aaBB','AABb x Aabb','AA x BB','bb x BB',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1027,'1027. Ak\u00ed bud\u00fa jedinci v F, gener\u00e1cii pri monohybridnom kr\u00ed\u017een\u00ed s ne\u00faplnou dominanciou:','rovnak\u00ed fenotypovo ale rozdielni genotypovo','rovnak\u00ed fenotypovo aj genotypovo','rovnak\u00ed ako jeden z rodi\u010dov','navz\u00e1jom odli\u0161n\u00ed','heterozygotn\u00ed','uniformn\u00ed','odli\u0161n\u00ed od oboch rodi\u010dov','fenotypovo zhodn\u00ed s oboma rodi\u010dmi',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1028,'1028. Pri monohybridnom kr\u00ed\u017een\u00ed dvoch rovnak\u00fdch homozygotov v potomstve dostaneme:','fenotypovo aj genotypovo uniformn\u00fdch jedincov','polovicu jedincov s dominantn\u00fdm a polovicu jedincov s reces\u00edvnym znakom','jedincov s rovnakou kvalitou znaku a genotypu','genotypovo un\u00edformnych ale fenotypovo odli\u0161n\u00fdch jedincov','fenotypovo uniformn\u00fdch heterozygotn\u00fdch jedincov','\u010dist\u00fa l\u00edniu','dve kateg\u00f3rie odli\u0161n\u00fdch homozygotov','len homozygotn\u00fdch jedincov',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1029,'1029. Mendelove z\u00e1kony platia len za predpokladu, \u017ee:','ide o monog\u00e9nnu dedi\u010dnos\u0165','jeden g\u00e9n k\u00f3duje jeden znak','sledovan\u00e9 g\u00e9ny s\u00fa lokalizovan\u00e9 na pohlavn\u00fdch chromoz\u00f3moch','sledovan\u00e9 g\u00e9ny s\u00fa vo v\u00e4zbe','sa g\u00e9ny pre sledovan\u00e9 znaky nach\u00e1dzaj\u00fa na jednom chromoz\u00f3me','s\u00fa sledovan\u00e9 g\u00e9ny lokalizovan\u00e9 na r\u00f4znych autoz\u00f3moch','ak sledujeme viac ako jeden znak, ka\u017edy je determinovan\u00fd g\u00e9nom na inom chromoz\u00f3me','ak s\u00fa sledovan\u00e9 g\u00e9ny viazan\u00e9 na X chromoz\u00f3m',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1030,'1030. Genotypov\u00fd \u0161tiepny pomer:','vyjadruje zast\u00fapenie jednotliv\u00fdch genotypov v potomstve pri kr\u00ed\u017een\u00ed','vyjadruje zast\u00fapenie jednotliv\u00fdch vlastnosti alebo znakov v potomstve pri kr\u00ed\u017een\u00ed','je v\u017edy zhodn\u00fd s fenotypov\u00fdm \u0161tiepnym pomerom','m\u00f4\u017ee by\u0165 rovnak\u00fd ako fenotypov\u00fd \u0161tiepny pomer','je 1:2:1 pri kr\u00ed\u017een\u00ed monohybridov a \u00faplnej dominancii','je 1:2:1 pri kr\u00ed\u017een\u00ed monohybridov a ne\u00faplnej dominancii','je 9:3:3:1 pri kr\u00ed\u017een\u00ed dihybridov a \u00faplnej dominancii','je 3:1 pri kr\u00ed\u017een\u00ed monohybridov \u00faplnej dominancii',TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1031,'1031. Pri kr\u00ed\u017een\u00ed Aa x Aa a \u00faplnej dominancii plat\u00ed, \u017ee:','rodi\u010dia m\u00f4\u017eu produkova\u0165 3 typy gam\u00e9t - AA, Aa, aa','rodi\u010dia m\u00f4\u017eu produkova\u0165 1 typ gam\u00e9t - Aa','rodi\u010dia m\u00f4\u017eu produkova\u0165 2 typy gam\u00e9t - A, a','sa v potomstve nem\u00f4\u017ee objavi\u0165 reces\u00edvny homozygot','sa v potomstve m\u00f4\u017ee objavi\u0165 jedinec s reces\u00edvnym znakom','v potomstve bude viac jedincov s dominantn\u00fdm znakom ako s reces\u00edvnym znakom','v potomstve bude menej jedincov s dominantn\u00fdm znakom ako s reces\u00edvnym znakom','potomstvo bude uniformn\u00e9 a heterozygotn\u00e9',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1032,'1032. Pri kr\u00ed\u017een\u00ed AA x aa a \u00faplnej dominancii plat\u00ed, \u017ee:','sa v potomstve nem\u00f4\u017ee objavi\u0165 reces\u00edvny homozygot','sa v potomstve nem\u00f4\u017ee objavi\u0165 dominantn\u00fd homozygot','sa potomkovia bud\u00fa podoba\u0165 na rodi\u010da s dominantn\u00fdm znakom','sa potomkovia bud\u00fa podoba\u0165 na rodi\u010da s reces\u00edvnym znakom','cel\u00e9 potomstvo bude uniformn\u00e9 a heterozygotn\u00e9','cel\u00e9 potomstvo bude uniformn\u00e9 a dominantne homozygotn\u00e9','sa v potomstve m\u00f4\u017ee objavi\u0165 jedinec s reces\u00edvnym znakom','v potomstve bud\u00fa len jedinci s dominantn\u00fdm znakom',TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1033,'1033. Pri monohybridizme s ne\u00faplnou dominanciou nach\u00e1dzame v F_2 gener\u00e1cii nasledovn\u00fd po\u010det fenotypov:','2','1','2^3','3','men\u0161\u00ed ako po\u010det genotypov','rovnak\u00fd ako po\u010det genotypov','v\u00e4\u010d\u0161\u00ed ako po\u010det genotypov','3^2',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1034,'1034. Pri monohybridnom kr\u00ed\u017een\u00ed reces\u00edvneho homozygota s heterozygotom dostaneme \u0161tiepny pomer:','1:1 genotypov\u00fd aj fenotypov\u00fd a pri \u00faplnej dominancii 1/2 potomstva bude ma\u0165 dominantn\u00fd a 1/2 intermedi\u00e1rny znak','1:1 genotypov\u00fd aj fenotypov\u00fd a pri \u00faplnej dominancii bud\u00fa rovnako zast\u00fapen\u00ed jedinci s dominantn\u00fdm aj reces\u00edvnym znakom','2:1 genotypov\u00fd a 1:1 fenotypov\u00fd','1:1 fenotypov\u00fd a 1:2:1 genotypov\u00fd','3 : 1 fenotypov\u00fd aj genotypov\u00fd','pri ne\u00faplnej dominancii v\u0161etci jedinci bud\u00fa uniformn\u00ed','1:1 ako pri sp\u00e4tnom kr\u00ed\u017een\u00ed','1:1 genotypov\u00fd aj fenotypov\u00fd a pri ne\u00faplnej dominancii bud\u00fa rovnako zast\u00fapen\u00ed jedinci s dominantn\u00fdm aj reces\u00edvnym znakom',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1035,'1035. Pri kr\u00ed\u017een\u00ed dvoch dominantn\u00fdch homozygotov v ich potomstve:','nedoch\u00e1dza k \u0161tiepeniu fenotypov','sa vyskytuj\u00fa dominantn\u00ed homozygoti iba v F_1 gener\u00e1cii','v F_2 gener\u00e1cii doch\u00e1dza k \u0161tiepeniu fenotypov a genotypov','sa vyskytuj\u00fa fenotypovo aj genotypovo rovnak\u00ed jedinci','je pomer homozygotov a heterozygotov 1:1','sa znak manifestuje v\u00fdraznej\u0161ie ne\u017e u rodi\u010dov','s\u00fa pr\u00edslu\u0161n\u00edci Fi gener\u00e1cie uniformne heterozygotn\u00ed','sa vyskytuj\u00fa reces\u00edvne homozygotn\u00ed jedinci len v F_2 gener\u00e1cii',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1036,'1036. Pri \u00faplnej dominancii a kr\u00ed\u017een\u00ed homozygota s heterozygotom (AA x Aa):','potomkovia s\u00fa fenotypovo rovnak\u00ed','nach\u00e1dzame v potomstve fenotypov\u00fd \u0161tiepny pomer 1:1','v potomstve doch\u00e1dza k \u0161tiepeniu fenotypov v pomere 3:1','je potomstvo genotypovo uniformn\u00e9','v potomstve nedoch\u00e1dza k \u0161tiepeniu fenotypov','m\u00e1 \u0161tvrtina potomstva reces\u00edvny fenotyp','je polovica potomstva dominantne homozygotn\u00e1','je polovica potomstva heterozygotn\u00e1',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1037,'1037. Pri ne\u00faplnej dominancii pri kr\u00ed\u017een\u00ed dvoch r\u00f4znych homozygotov:','doch\u00e1dza k \u0161tiepeniu fenotypov v pomere 3:1','v\u0161etci potomkovia s\u00fa heterozygoti','doch\u00e1dza k \u0161tiepeniu fenotypov v pomere 1:1','potomkovia s\u00fa genotypovo rovnak\u00ed','potomkovia s\u00fa fenotypovo rovnak\u00ed, l\u00ed\u0161ia sa genotypmi','je potomstvo fenotypovo zhodn\u00e9 s dominantn\u00fdm rodi\u010dom','v\u0161etci potomkovia s\u00fa fenotypovo rovnak\u00ed a zhodn\u00ed s dominantn\u00fdm rodi\u010dom','v\u0161etci potomkovia s\u00fa fenotypovo rovnak\u00ed a odli\u0161n\u00ed od oboch rodi\u010dov',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
