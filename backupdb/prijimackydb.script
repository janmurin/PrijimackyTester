INSERT INTO BIOLOGIA VALUES(131,'131. V\u00edrusy:','maj\u00fa jednoduch\u00fa prokaryotick\u00fa bunku','s\u00fa najjednoduch\u0161ie jednobunkov\u00e9 organizmy','s\u00fa vn\u00fatrobunkov\u00e9 parazity','s\u00fa zlo\u017een\u00e9 z nukleov\u00fdch kysel\u00edn a lipidov\u00e9ho kapsidu','\u0161tuduje mikrobiol\u00f3gia','\u0161tuduje virol\u00f3gia','s\u00fa nebunkov\u00e9 organizmy','maj\u00fa ako prirodzen\u00fd rezervo\u00e1r niektor\u00e9 bezstavovce',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(132,'132. V\u00edrus chr\u00edpky:','m\u00e1 ve\u013ekos\u0165 50 \u2014 80 nm','m\u00f4\u017eeme pozorova\u0165 len elektr\u00f3nov\u00fdm mikroskopom','m\u00e1 kapsid zlo\u017een\u00fd z bielkovinov\u00fdch monom\u00e9rov','m\u00f4\u017eeme pozorova\u0165 optick\u00fdm mikroskopom','m\u00e1 ve\u013ekos\u0165 15 \u2014 300 nm','m\u00e1 tukov\u00fd obal','m\u00e1 nukleoid zlo\u017een\u00fd z bielkovinov\u00fdch monom\u00e9rov','m\u00e1 nukleokapsid uzavret\u00fd v tukovom obale',TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
