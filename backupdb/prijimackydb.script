INSERT INTO BIOLOGIA VALUES(777,'777. K vn\u00fatorn\u00fdm \u017eensk\u00fdm pohlavn\u00fdm org\u00e1nom patria:','mal\u00e9 pysky ohanbia','predsie\u0148 po\u0161vy','maternica','vaje\u010dn\u00edky','clitoris','ovarium','uterus','ovulum',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(778,'778. Vyberte spr\u00e1vne tvrdenie o \u017eensk\u00fdch pohlavn\u00fdch org\u00e1noch:','vaj\u00ed\u010dkovod je p\u00e1rov\u00e1 \u017e\u013eaza mand\u013eovit\u00e9ho tvaru','na vaje\u010dn\u00edku rozli\u0161ujeme na povrchu k\u00f4ru a vo vn\u00fatri dre\u0148','maternica je nep\u00e1rov\u00fd dut\u00fd org\u00e1n zakon\u010den\u00fd strapcovit\u00fdmi v\u00fdbe\u017ekami','hymen sa nach\u00e1dza na za\u010diatku matemice \u017eien, ktor\u00e9 nemali pohlavn\u00fd styk','mons pubis m\u00e1 podobn\u00fd erek\u010dn\u00fd mechanizmus ako mu\u017esk\u00fd pohlavn\u00fd org\u00e1n','uterus je svalovo-v\u00e4zivov\u00e1 trubica vystlan\u00e1 riasinkovym epitelom','labia minora pudendi s\u00fa ko\u017en\u00e9 valy kryt\u00e9 chlpmi','po\u0161va je \u017eensk\u00fdm kopula\u010dn\u00fdm org\u00e1nom',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
