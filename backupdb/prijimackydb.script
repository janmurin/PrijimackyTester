INSERT INTO BIOLOGIA VALUES(1334,'1334. Klepietkavce:','maj\u00fa zlo\u017een\u00e9 o\u010di','d\u00fdchaj\u00fa p\u013e\u00facami alebo vzdu\u0161nicami','maj\u00fa klepietka premenen\u00e9 na hmatadl\u00e1','sa zvliekaj\u00fa z panciera','d\u00fdchaj\u00fa vzdu\u0161nicami alebo p\u013e\u00facnymi vakmi','s\u00fa oddelen\u00e9ho pohlavia','maj\u00fa hryzadl\u00e1','sa nezvliekaj\u00fa z panciera',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1335,'1335. Ozna\u010dte spr\u00e1vne odpovede:','klie\u0161\u0165 oby\u010dajn\u00fd nepatr\u00ed medzi rozto\u010de','Ixodes ricinus m\u00f4\u017ee pren\u00e1\u0161a\u0165 p\u00f4vodcov z\u00e1palu mozgov\u00fdch bl\u00e1n a boreli\u00f3zy','z\u00e1ko\u017eka svrabov\u00e1 patr\u00ed medzi rozto\u010de','rozto\u010de pren\u00e1\u0161aj\u00fa v\u00edrusov\u00e9 ochorenia','rozto\u010de maj\u00fa telo rozdelen\u00e9 na hlavu, hru\u010f a bru\u0161ko','rozto\u010de maj\u00fa celistv\u00e9 telo','klie\u0161\u0165 oby\u010dajn\u00fd m\u00f4\u017ee pren\u00e1\u0161a\u0165 hepatit\u00eddu, boreli\u00f3zu a bilharzi\u00f3zu','klie\u0161te v na\u0161ich podmienkach m\u00f4\u017eu pren\u00e1\u0161a\u0165 p\u00f4vodcov encefalit\u00eddy a boreli\u00f3zy',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1336,'1336. Pri k\u00f4rovcoch:','ve\u013emi pokro\u010dila diferenci\u00e1cia kon\u010datin','sa kon\u010datiny nach\u00e1dzaj\u00fa aj na bru\u0161ku','sa kon\u010datiny nach\u00e1dzaj\u00fa len na hlavohrudi','sa nach\u00e1dza statocysta','u\u017e nach\u00e1dzame rovnov\u00e1\u017eny org\u00e1n','s\u00fa kon\u010datiny dvojvetvov\u00e9','niektor\u00e9 druhy v nepriazniv\u00fdch podmienkach vytv\u00e1raj\u00fa statocystu','na rozdiel od klepietkavcov nedoch\u00e1dza k diferenci\u00e1cii kon\u010dat\u00edn',TRUE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1337,'1337. Ozna\u010dte nespr\u00e1vne odpovede:','larvy blchy rudskej sa vyv\u00edjaj\u00fa mimo tela \u010dloveka','Yersinia pestis je p\u00f4vodcom moru','blcha \u013eudsk\u00e1 je endoparazit','sami\u010dka v\u0161i detskej nakladie po\u010das \u017eivota asi 3 000 vaj\u00ed\u010dok','plo\u0161tica poste\u013en\u00e1 patr\u00ed medzi bzdochy','plo\u0161tica poste\u013en\u00e1 sa \u017eiv\u00ed odumret\u00fdmi bunkami \u013eudskej poko\u017eky','iba sam\u010deky kom\u00e1ra pisk\u013eav\u00e9ho sa \u017eivia paraziticky','sami\u010dky kom\u00e1ra pisk\u013eav\u00e9ho sa \u017eivia rastlinn\u00fdmi \u0161\u0165avami',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1338,'1338. Hmyz:','s\u00fa preva\u017ene obojpohlavn\u00e9 \u017eivo\u010d\u00edchy','m\u00e1 pevn\u00fa vonkaj\u0161iu kostru','nem\u00e1 e\u0161te dobre vyvinut\u00e9 komunika\u010dn\u00e9 schopnosti','m\u00e1 kr\u00eddla analogick\u00e9 s kr\u00eddlami vt\u00e1kov','s\u00fa \u017eivo\u010d\u00edchy s pevnou vn\u00fatomou chit\u00ednoznou kostrou','je oddelen\u00e9ho pohlavia','m\u00e1 kr\u00eddla homologick\u00e9 s kr\u00eddlami vt\u00e1kov','m\u00e1 vysok\u00fd stupe\u0148 in\u0161tinkt\u00edvneho spr\u00e1vania',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1339,'1339. Mimikry:','s\u00fa miniat\u00farne vaj\u00ed\u010dka niektor\u00fdch druhov morsk\u00fdch r\u00fdb','s\u00fa ikry, ktor\u00e9 sa vyu\u017e\u00edvaj\u00fa v potravin\u00e1rskom priemysle ako kavi\u00e1r','je napr schopnos\u0165 napodobni\u0165 hrnyz nebezpe\u010dn\u00fd pre dravca','s\u00fa napr. podobnos\u0165 kr\u00eddla muchy a vt\u00e1ka','vytv\u00e1raj\u00fa len dospel\u00e9 jedince hmyzu','je sp\u00f4sob maskovania','vytv\u00e1raj\u00fa aj larv\u00e1lne \u0161t\u00e1dia hmyzu','s\u00fa sami\u010die gam\u00e9ty r\u00fdb',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1340,'1340. Chrob\u00e1ky:','prech\u00e1dzaj\u00fa po\u010das svojho v\u00fdvinu \u00faplnou premenou','maj\u00fa slabo vyvinut\u00e9, zakrpaten\u00e9 tykadl\u00e1','maj\u00fa druh\u00fd p\u00e1r kr\u00eddel blanit\u00fd','maj\u00fa prv\u00fd p\u00e1r kr\u00eddel premenen\u00fd na krovky','sa po\u010das svojho v\u00fdvinu zakuk\u013euj\u00fa','patria k hmyzu s ne\u00faplnou premenou','maj\u00fa druh\u00fd p\u00e1r kr\u00eddel premenen\u00fd na krovky','maj\u00fa vzdu\u0161nice umiestnen\u00e9 i v kr\u00eddlach',TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE)
