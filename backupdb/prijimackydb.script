INSERT INTO BIOLOGIA VALUES(127,'127. V\u00edrusy sa rozmno\u017euj\u00fa:','mei\u00f3zou','mit\u00f3zou','amit\u00f3zou','v \u017eivo\u010d\u00ed\u0161nych bunk\u00e1ch tak, \u017ee do nich prenikaj\u00fa pinocyt\u00f3zou','pu\u010dan\u00edm','nepriamym delen\u00edm','v hostite\u013eskej bunke, ktor\u00e1 vytv\u00e1ra nov\u00e9 viri\u00f3ny','tak, \u017ee v hostite\u013eskej bunke prebieha replik\u00e1cia a realiz\u00e1cia ich genetickej inform\u00e1cie',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(128,'128. V\u00edrusy sa odli\u0161uj\u00fa od ostatn\u00fdch mikroorganizmov t\u00fdm, \u017ee:','maj\u00fa len jeden typ nukleovej kyseliny','s\u00fa najmen\u0161ie prokaryotick\u00e9 organizmy','s\u00fa to najmen\u0161ie infek\u010dn\u00e9 \u010dastice \u2014 maj\u00fa rozmery 15 \u2014 300 nm','s\u00fa to najmen\u0161ie infek\u010dn\u00e9 \u010dastice \u2014 maj\u00fa rozmery 15 \u2014 300 pm','s\u00fa to nebunkov\u00e9 organizmy','sp\u00f4sobuj\u00fa infek\u010dn\u00e9 ochorenia','maj\u00fa bi\u010d\u00edlcy','nemaj\u00fa vlastn\u00fd genetick\u00fd materi\u00e1l',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
