INSERT INTO BIOLOGIA VALUES(660,'660. O ne\u0161pecifickej imunite plat\u00ed, \u017ee:','je vroden\u00e1','je z\u00edskan\u00e1','je zalo\u017een\u00e1 na tvorbe ne\u0161pecifick\u00fdch protil\u00e1tok B lymfocytmi','je zalo\u017een\u00e1 na tvorbe \u0161pecifick\u00fdch protil\u00e1tok B lymfocytmi','je zalo\u017een\u00e1 na vytvoren\u00ed pam\u00e4\u0165ov\u00fdch buniek','je zabezpe\u010dovan\u00e1 bunkami, ktor\u00e9 s\u00fa schopn\u00e9 fagocyt\u00f3zy','je zabezpe\u010dovan\u00e1 usmrcovan\u00edm po\u0161koden\u00fdch buniek','je zabezpe\u010dovan\u00e1 usmrcovan\u00edm buniek napadnut\u00fdch v\u00edrusmi',TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(661,'661. V obdob\u00ed systoly kom\u00f4r srdca zdrav\u00e9ho \u010dloveka:','sa komory plnia krvou','je krv vypuden\u00e1 do ve\u013ek\u00fdch tepien','sa otv\u00e1ra mitr\u00e1lna chlop\u0148a','sa uzatv\u00e1raj\u00fa polmesia\u010dikovit\u00e9 chlopne','s\u00fa uzatvoren\u00e9 c\u00edpovit\u00e9 chlopne','sa otv\u00e1raj\u00fa polmesia\u010dikovit\u00e9 chlopne','sa zmr\u0161\u0165uje svalovina kom\u00f4r','je systola predsien\u00ed u\u017e ukon\u010den\u00e1',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(662,'662. C\u00edpovit\u00e9 chlopne v \u013eudskom srdci:','zabra\u0148uj\u00fa sp\u00e4tn\u00e9mu toku krvi z kom\u00f4r do predsien\u00ed','s\u00fa medzi predsie\u0148ami a komorami','s\u00fa na za\u010diatku srdcovnice a p\u013e\u00facnej tepny','zabra\u0148uj\u00fa sp\u00e4tn\u00e9mu toku krvi z predsien\u00ed do kom\u00f4r','zabra\u0148uj\u00fa sp\u00e4tn\u00e9mu toku krvi z kom\u00f4r do ve\u013ek\u00fdch tepien','s\u00fa pevnou nepriepustnou prep\u00e1\u017ekou medzi komorami a predsie\u0148ami','s\u00fa dve, jedna odde\u013euje prav\u00fa a \u013eav\u00fa komoru, druh\u00e1 predsiene','s\u00fa \u0161tyri',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(663,'663. V krvnom obehu \u010dloveka prech\u00e1dza krv z pravej komory:','p\u013e\u00facnymi \u017eilami do p\u013e\u00fac','p\u013e\u00facnicou do p\u013e\u00fac','p\u013e\u00facnicou do cel\u00e9ho tela','cez mitr\u00e1lnu chlop\u0148u do p\u013e\u00fac','p\u013e\u00facnicou do pravej predsiene','srdcovnicou do p\u013e\u00fac','hornou a dolnou dutou \u017eilou do p\u013e\u00fac','p\u013e\u00facnym kme\u0148om do p\u013e\u00fac',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
