INSERT INTO BIOLOGIA VALUES(567,'567. Kompletn\u00fd hrudn\u00edk \u010dloveka tvoria tieto kosti:','12 hrudn\u00edkov\u00fdch stavcov, 7 p\u00e1rov prav\u00fdch rebier, ktor\u00e9 sa prip\u00e1jaj\u00fa priamo v\u00e4zivom na hrudn\u00fa kos\u0165','hrudn\u00e1 kos\u0165 a rebr\u00e1','12 hrudn\u00edkov\u00fdch stavcov, 14 p\u00e1rov rebier a hrudn\u00e1 kos\u0165','hrudn\u00edkov\u00e9 stavce, rebr\u00e1 a hrudn\u00e1 kos\u0165','12 hrudn\u00edkov\u00fdch stavcov a hrudn\u00e1 kos\u0165','12 hrudn\u00edkov\u00fdch stavcov, 12 p\u00e1rov rebier a hrudn\u00e1 kos\u0165','hrudn\u00edkov\u00e9 stavce, rebr\u00e1, hrudn\u00e1 kos\u0165 a lopatka','12 hrudn\u00edkov\u00fdch stavcov, 7 p\u00e1rov prav\u00fdch rebier, 5 p\u00e1rov neprav\u00fdch rebier a sternum',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
