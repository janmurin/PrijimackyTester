INSERT INTO BIOLOGIA VALUES(1088,'1088. Heterochromoz\u00f3my s\u00fa:','chromoz\u00f3my s nerovnak\u00fdmi g\u00e9nmi u \u017eeny','pohlavn\u00e9 chromoz\u00f3my','len v pohlavn\u00fdch bunk\u00e1ch','odli\u0161ne sa farbiace chromoz\u00f3my u \u017eeny','zodpovedn\u00e9 za v\u00fdvin pohlavia cicavcov','chromoz\u00f3my, na ktor\u00fdch s\u00fa len g\u00e9ny ur\u010duj\u00face pohlavie','mimojadrov\u00e9 chromoz\u00f3my','s\u00fa chromoz\u00f3my neobsahuj\u00face chromat\u00edn',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1089,'1089. Chromoz\u00f3m Y:','b\u00fdva oby\u010dajne v\u00e4\u010d\u0161\u00ed ako X chromoz\u00f3m','u niektor\u00fdch druhov sa nevyskytuje','patr\u00ed medzi gonoz\u00f3my','sa nach\u00e1dza v polovici sami\u010d\u00edch gam\u00e9t cicavcov','musia ma\u0165 v\u0161etci jedinci ur\u010dit\u00e9ho druhu','obsahuje menej g\u00e9nov ako X chromoz\u00f3m','sa nach\u00e1dza v somatick\u00fdch bunk\u00e1ch sam\u00edc vt\u00e1kov','sa vyskytuje Ien v bunk\u00e1ch jedincov homogametick\u00e9ho pohlavia',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
