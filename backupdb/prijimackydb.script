INSERT INTO BIOLOGIA VALUES(942,'942. Degenerovanos\u0165 genetick\u00e9ho k\u00f3du znamen\u00e1 \u017ee:','v\u0161etky organizmy maj\u00fa rovnak\u00fd sp\u00f4sob k\u00f3dovania genetickej inform\u00e1cie','ka\u017ed\u00fd nukleotid je s\u00fa\u010das\u0165ou viacer\u00fdch kod\u00f3nov','ka\u017ed\u00fd nukleotid je s\u00fa\u010das\u0165ou jedn\u00e9ho kod\u00f3nu','jednu aminokyselinu m\u00f4\u017ee k\u00f3dova\u0165 viac kod\u00f3nov','na z\u00e1pis genetickej inform\u00e1cie s\u00fa potrebn\u00e9 len \u0161tyri k\u00f3dov\u00e9 p\u00edsmen\u00e1','na z\u00e1pis genetickej inform\u00e1cie s\u00fa potrebn\u00e9 len tri k\u00f3dov\u00e9 p\u00edsmen\u00e1','okrem \u0161trukt\u00farnych g\u00e9nov k\u00f3duje aj g\u00e9ny regula\u010dn\u00e9','je tripletov\u00fd',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(943,'943. O regula\u010dn\u00fdch g\u00e9noch vo v\u0161eobecnosti plat\u00ed:','s\u00fa podriaden\u00e9 \u0161trukt\u00farnym g\u00e9nom','s\u00fa nadraden\u00e9 \u0161trukt\u00farnym g\u00e9nom','s\u00fa umiestnen\u00e9 v oblasti oper\u00f3nu','s\u00fa umiestnen\u00e9 mimo oblasti oper\u00f3nu','v\u00fdsledkom ich expresie m\u00f4\u017ee by\u0165 molekula represora','v\u00fdsledkom ich expresie m\u00f4\u017ee by\u0165 molekula prom\u00f3tora','ich transkripciou vznik\u00e1 molekula mRNA','ich transkripciou vznik\u00e1 molekula represora',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(944,'944. Vyzna\u010dte spr\u00e1vne tvrdenie o molekule tRNA:','je dvojvl\u00e1knov\u00e1','je jednovl\u00e1knov\u00e1','jej \u0161trukt\u00fara pripom\u00edna \u010fatelinov\u00fd list','jej \u0161trukt\u00fara pripom\u00edna lipov\u00fd list','koncov\u00fd triplet ramena tRNA sa naz\u00fdva antikod\u00f3n','koncov\u00fd triplet ramena tRNA sa naz\u00fdva kod\u00f3n','na koncov\u00fd triplet sa via\u017ee aktivovan\u00e1 aminokyselina','\u0161pecifick\u00e1 tRNA je schopn\u00e1 prenosu akejko\u013evek aminokyseliny po jej aktiv\u00e1cii',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(945,'945. Vyzna\u010dte spr\u00e1vne tvrdenie o molekule mRNA:','je dvojvl\u00e1knov\u00e1','je jednovl\u00e1knov\u00e1','jej \u0161trukt\u00fara pripom\u00edna \u010fatelinov\u00fd list','jej \u0161trukt\u00fara pripom\u00edna lipov\u00fd list','v prokaryotick\u00fdch bunk\u00e1ch sa nenach\u00e1dza','nach\u00e1dza sa aj v prokaryotick\u00fdch bunk\u00e1ch','jej synt\u00e9zu katalyzuje enz\u00fdm DNA-polymer\u00e1za','na koncov\u00fd triplet sa via\u017ee aktivovan\u00e1 aminokyselina',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(946,'946. O synt\u00e9ze DNA v procese replik\u00e1cie plat\u00ed, \u017ee:','prebieha v smere 5 \u2014>3','prebieha v smere 3--->5','je katalyzovan\u00e1 enz\u00fdmom RNA polymer\u00e1za','prebieha pod\u013ea predlohy dc\u00e9rskeho re\u0165azca','prebieha podra predlohy matersk\u00e9ho re\u0165azca','ako matersk\u00fd re\u0165azec m\u00f4\u017ee sl\u00fa\u017ei\u0165 len jedno vl\u00e1kno DNA','ako matersk\u00e9 re\u0165azce sl\u00fa\u017eia obidve vl\u00e1kna p\u00f4vodnej DNA','je to proces vzniku vod\u00edkovej v\u00e4zby medzi susedn\u00fdmi nukleotidmi',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(947,'947. Vz\u00e1jomn\u00e9 sp\u00e1janie aminokysel\u00edn v procese transl\u00e1cie:','prebieha bez dodania energie','si vy\u017eaduje dodanie energie','sa deje na z\u00e1klade komplementarity aminokysel\u00edn','sa uskuto\u010d\u0148uje prostredn\u00edctvom vod\u00edkovej v\u00e4zby','sa uskuto\u010d\u0148uje prostredn\u00edctvom peptidovej v\u00e4zby','prebieha prostredn\u00edctvom riboz\u00f3mov','prebieha prostredn\u00edctvom plazmidov','je prv\u00fdm stup\u0148om expresie g\u00e9nov',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(948,'948. Ozna\u010dte nespr\u00e1vnu odpove\u010f:','prim\u00e1rna \u0161trukt\u00fara DNA je dan\u00e1 jej d\u013a\u017ekou, t.j. po\u010dtom b\u00e1z','nosite\u013ekou genetickej inform\u00e1cie je v\u017edy DNA','polyg\u00e9nny syst\u00e9m je reprezentovan\u00fd g\u00e9nmi mal\u00e9ho \u00fa\u010dinku','riboz\u00f3mov\u00e1 RNA je syntetizovan\u00e1 v riboz\u00f3moch','pr\u00edkladom kvalitat\u00edvneho znaku je farba o\u010d\u00ed drozofily','na DNA plazmidov sa transkribuje aj tRNA','prim\u00e1rna \u0161trukt\u00fara DNA je dan\u00e1 porad\u00edm nukleotidov','transferov\u00e1 RNA vznik\u00e1 v procese transl\u00e1cie',TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
