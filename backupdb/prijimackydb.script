INSERT INTO BIOLOGIA VALUES(780,'780. Ozna\u010dte spr\u00e1vnu odpove\u010f o reproduk\u010dnom cykle \u017eeny:','cyklick\u00e9 zmeny prebiehaj\u00face v maternici ozna\u010dujeme ako uterinny cyklus','obdobie pohlavnej aktivity \u017eeny sa za\u010d\u00edna objaven\u00edm prv\u00e9ho men\u0161trua\u010dn\u00e9ho krv\u00e1cania','reproduk\u010dn\u00fd cyklus \u017eeny spravidla ust\u00e1va okolo 60. roku \u017eivota \u017eeny','obdobie \u017eivota \u017eeny, v ktorom sa reproduk\u010dn\u00fd cyklus zastavil, sa naz\u00fdva klimakt\u00e9rium','men\u0161trua\u010dn\u00e9 krv\u00e1canie \u017eeny naz\u00fdvane tie\u017e menopauza','tehotenstvo zastavuje reproduk\u010dn\u00fd cyklus spravidla na obdobie 10 lun\u00e1rnych mesiacov','cyklick\u00e9 zmeny prebiehaj\u00face vo vaje\u010dn\u00edku naz\u00fdvame men\u0161trua\u010dn\u00fd cyklus','klimakt\u00e9rium za\u010d\u00edna pribli\u017ene v 45. a\u017e 50. roku \u017eivota',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(781,'781. Semenn\u00edky dospel\u00e9ho mu\u017ea:','produkuj\u00fa mu\u017esk\u00e9 pohlavn\u00e9 bunky - spermie','produkuj\u00fa horm\u00f3n progester\u00f3n','produkuj\u00fa najm\u00e4 estrog\u00e9ny','produkuj\u00fa testoster\u00f3n','produkuj\u00fa najm\u00e4 gestag\u00e9ny','pracuj\u00fa pod vplyvom hypof\u00fdzy','maj\u00fa aj endokrinn\u00fa funkciu','maj\u00fa aj in\u00fa ako endokrinn\u00fa funkciu',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(782,'782. Ischemick\u00e1 f\u00e1za uterinn\u00e9ho cyklu spravidla nast\u00e1va:','v 1. a\u017e 4. de\u0148 cyklu','v 5. a\u017e 9. de\u0148 cyklu','v 10. a\u017e 14. de\u0148 cyklu','v 14. a\u017e 18. de\u0148 cyklu','v 18. de\u0148 cyklu','v 21. de\u0148 cyklu','v 24. de\u0148 cyklu','v 28. de\u0148 cyklu',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(783,'783. Prolifera\u010dn\u00e1 f\u00e1za men\u0161trua\u010dn\u00e9ho cyklu sa vyzna\u010duje t\u00fdm \u017ee:','zanik\u00e1 \u017elt\u00e9 teliesko a kles\u00e1 produkcia pohlavn\u00fdch horm\u00f3nov','sliznica maternice sa st\u00e1va kyprou, dobre prekrvenou a presiaknutou tekutinou','sliznica maternice je pripraven\u00e1 na prijatie oplodnen\u00e9ho vaj\u00ed\u010dka','za\u010d\u00edna odl\u00fapen\u00edm a odstr\u00e1nen\u00edm sliznice m\u00e1ternice','baz\u00e1lna vrstva sliznice maternice sa pokryje regenerovan\u00fdm epitelom','zrel\u00fd Graafov folikul praskne a doch\u00e1dza k uvo\u013eneniu vaj\u00ed\u010dka','bunky obalu zrel\u00e9ho Graafovho folikulu vylu\u010duj\u00fa progester\u00f3n','cievy z\u00e1sobuj\u00face sliznicu maternice sa stiahnu a sliznica sa za\u010dne odlupova\u0165',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
