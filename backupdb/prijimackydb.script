INSERT INTO BIOLOGIA VALUES(1326,'1326. U z\u00e1stupcov kme\u0148a m\u00e4kk\u00fd\u0161e (Mollusca):','sa nach\u00e1dza hemolymfa','sa v \u00fastach nach\u00e1dza ozuben\u00fd str\u00fahac\u00ed org\u00e1n \u2014 planula','sa nach\u00e1dza krvomiazga','sa u\u017e vyskytuje lymfa','nach\u00e1dzame rozpt\u00fdlen\u00fa nervov\u00fa s\u00fastavu','sa vyskytuje vonkaj\u0161ie oplodnenie','sa v \u00fastach nach\u00e1dza ozuben\u00fd str\u00fahac\u00ed org\u00e1n \u2014 radula','sa vyskytuje vn\u00fatorn\u00e9 oplodnenie',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE)
