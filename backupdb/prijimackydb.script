INSERT INTO BIOLOGIA VALUES(933,'933. G\u00e9ny pre RNA k\u00f3duj\u00fa:','prim\u00e1rnu \u0161trukt\u00faru regula\u010dn\u00fdch g\u00e9nov','poradie nukleotidov molek\u00fal rRNA','len poradie nukleotidov rRNA','poradie nukleotidov mRNA','poradie nukleotidov molek\u00fal tRNA','prim\u00e1rnu \u0161trukt\u00faru riboz\u00f3mov','prim\u00e1rnu \u0161trukt\u00faru tRNA','enz\u00fdmy, potrebn\u00e9 pre synt\u00e9zu RNA',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(934,'934. Pod\u013ea \u00faseku CCA GGA CCT AGT CTA v DNA vznikne mRNA:','CCU GGU CCT UGT CTU','GGU CCU GGA UCA GAU','GGT CCT GGU TCU GUT','GGU CCU GGA TCA GAT','GGT CCT GGU TCA GAT','GCT CCT GGU TCU GUT','CCT GGT CCU TGU CUT','GGT CCT GCU TCU GUT',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(935,'935. V jadre bunky sa syntetizuj\u00fa:','len mRNA a rRNA','len mRNA','tRNA pod\u013ea mRNA','v\u0161etky typy RNA','polyribonukleotidov\u00e9 molekuly','molekuly ATP potrebn\u00e9 pre transkripciu','molekuly riboz\u00f3movej RNA','polydeoxyribonukleotidov\u00e9 vl\u00e1kna pri transkripcii',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(936,'936. Kod\u00f3n je:','triplet b\u00e1z v mRNA a tRNA, ktor\u00fd ur\u010duje zaradenie aminokyseliny do polypeptidov\u00e9ho re\u0165azca','jednotka genetick\u00e9ho k\u00f3du','trojica nukleotidov v tRNA','triplet b\u00e1z v DNA alebo mRNA ur\u010duj\u00faci zaradenie jednej aminokyseliny do polypeptidov\u00e9ho re\u0165azca','triplet b\u00e1z v DNA a tRNA ur\u010duj\u00faci zaradenie aminokyseliny do polypeptidov\u00e9ho re\u0165azca','k\u00f3duj\u00faca \u010das\u0165 rRNA','z\u00e1kladn\u00e1 informa\u010dn\u00e1 jednotka tRNA','poradie nukleotidov v re\u0165azci rRNA',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(937,'937. V\u00e4zba vo\u013en\u00fdch aminokysel\u00edn na tRNA nast\u00e1va:','v jadre','v jadierku','v cytoplazme','na endoplazmatickom retikule','na z\u00e1klade \u0161pecifickosti tRNA','po ich biochemickej aktiv\u00e1cii','v Golgiho apar\u00e1te','na riboz\u00f3moch',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(938,'938. Materi\u00e1lom pre synt\u00e9zu polypeptidov\u00e9ho re\u0165azca s\u00fa:','jednoduch\u0161ie pept\u00eddy a ATP','ATP a enz\u00fdmy','vo\u013en\u00e9 aminokyseliny','ribonukleov\u00e9 kyseliny','nukleotidy','triplety DNA','fosfolipidy a ATP','nukleoprote\u00edny',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(939,'939. Po\u010das transl\u00e1cie genetickej inform\u00e1cie:','doch\u00e1dza k do\u010dasn\u00e9mu komplement\u00e1rnemu sp\u00e1janiu kod\u00f3nu mRNA a antikod\u00f3nu tRNA','doch\u00e1dza k do\u010dasn\u00e9mu komplement\u00e1rnemu sp\u00e1janiu kod\u00f3nov DNA s antikod\u00f3nmi tRNA','rastie polypeptidov\u00fd re\u0165azec na riboz\u00f3me r\u00fdchlos\u0165ou asi 1000 aminokysel\u00edn za min\u00fatu','m\u00f4\u017ee by\u0165 inform\u00e1cia jednej mRNA prekladan\u00e1 na viacer\u00fdch riboz\u00f3moch s\u00fa\u010dasne','je na pripojenie jednej aminokyseliny do polypeptidu potrebn\u00e1 energia pribli\u017ene 150 kJ/mol','je pri pripojen\u00ed jednej aminokyseliny do polypeptidu uvo\u013enen\u00e1 energia pribli\u017ene 150 kJ/mol','doch\u00e1dza k do\u010dasn\u00e9mu komplement\u00e1rnemu sp\u00e1janiu kod\u00f3nov rRNA s antikod\u00f3nmi tRNA','s\u00fa matricou na tvorbu polypeptidov\u00fdch re\u0165azcov triplety nukleotidov rRNA',TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(940,'940. Reverzn\u00e1 transkript\u00e1za je enz\u00fdm:','katalyzuj\u00faci synt\u00e9zu rRNA','katalyzuj\u00faci synt\u00e9zu tRNA','katalyzuj\u00faci replik\u00e1ciu DNA v prokaryotickej bunke','katalyzuj\u00faci synt\u00e9zu mRNA','katalyzuj\u00faci prepis genetickej inform\u00e1cie z RNA do DNA','katalyzuj\u00faci prepis genetickej inform\u00e1cie z DNA do RNA','ktor\u00fd bol po prv\u00fdkr\u00e1t objaven\u00fd vo v\u00edrusoch','ktor\u00fd bol po prv\u00fdkr\u00e1t objaven\u00fd v bakt\u00e9riach',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
