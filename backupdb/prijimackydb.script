INSERT INTO BIOLOGIA VALUES(806,'806. Medzi \u017e\u013eazy s vn\u00fatorn\u00fdm vylu\u010dovan\u00edm u \u010dloveka patr\u00ed:','nadobli\u010dka','slezina','hypof\u00fdza','epif\u00fdza','Graafov folikul','neurohypof\u00fdza','epiglottis','glandula thyroidea',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(807,'807. Ozna\u010dte spr\u00e1vne tvrdenie:','hypof\u00fdza \u010dloveka je anatomicky aj funk\u010dne rozdelen\u00e1 na laloky odli\u0161n\u00e9ho p\u00f4vodu','hypof\u00fdza \u010dloveka je anatomicky aj funk\u010dne rozdelen\u00e1 na laloky rovnak\u00e9ho p\u00f4vodu','hypof\u00fdza \u010dloveka je ulo\u017e en\u00e1 v zadnom mozgu','hypof\u00fdza \u010dloveka je ulo\u017een\u00e1 v medzimozgu','adenohypof\u00fdza \u010dloveka vznik\u00e1 z mezodermy','adenohypof\u00fdza \u010dloveka vznik\u00e1 z ektodermy','\u0161u\u0161kovit\u00e9 teleso produkuje horm\u00f3n melan\u00edn','\u0161u\u0161kovit\u00e9 teleso produkuje horm\u00f3n tyrox\u00edn',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(808,'808. Funkciou horm\u00f3nov v \u013eudskom organizme je vo v\u0161eobecnosti:','regulova\u0165 metabolick\u00e9 procesy','regulova\u0165 tvorbu a v\u00fddaj in\u00fdch horm\u00f3nov','ovplyv\u0148ova\u0165 rast a v\u00fdvoj tkan\u00edv i organizmu ako celku','prenos tzv. chemick\u00fdch inform\u00e1ci\u00ed k cie\u013eov\u00fdm tkaniv\u00e1m','ovplyv\u0148ova\u0165 \u010dinnos\u0165 jednotliv\u00fdch buniek','dod\u00e1va\u0165 energiu pre \u0161pecifick\u00e9 bunkov\u00e9 mechanizmy','riadi\u0165 vn\u00fatrobunkov\u00e9 deje pod\u013ea potrieb organizmu','regulova\u0165 ne\u0161pecifick\u00e9 funkcie',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(809,'809. Medzi tkanivov\u00e9 horm\u00f3ny \u010dloveka patria:','najm\u00e4 l\u00e1tky, ktor\u00e9 vznikaj\u00fa aj p\u00f4sobia v tom istom tkanive','horm\u00f3ny produkovan\u00e9 tkanivami \u0161pecializovan\u00fdch endokrinn\u00fdch \u017eliaz','horm\u00f3ny nadobli\u010diek','\u017e\u013eazov\u00e9 horm\u00f3ny','chemicky r\u00f4znorod\u00e9 l\u00e1tky','niektor\u00e9 mozgov\u00e9 peptidy','horm\u00f3ny, ktor\u00e9 sa vytv\u00e1raj\u00fa v sliznici \u017eal\u00fadka a tenk\u00e9ho \u010dreva','horm\u00f3ny, ktor\u00e9 sa vylu\u010duj\u00fa hlavne do \u017e\u013eazov\u00fdch v\u00fdvodov',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(810,'810. Ktor\u00e1 \u017e\u013eaza nie je pravou \u017e\u013eazou s vn\u00fatornou sekr\u00e9ciou:','stredn\u00fd lalok hypof\u00fdzy','hypotalamus','\u0161t\u00edtna \u017e\u013eaza','pod\u017eal\u00fadkov\u00e1 \u017e\u013eaza','neurohypof\u00fdza','adenohypof\u00fdza','zadn\u00fd lalok hypof\u00fdzy','predn\u00fd lalok hypof\u00fdzy',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(811,'811. Horm\u00f3ny vznikaj\u00face v hypotalame \u010dloveka priamo riadia:','\u010dinnos\u0165 vaje\u010dn\u00edkov','\u010dinnos\u0165 predn\u00e9ho laloku hypof\u00fdzy','\u010dinnos\u0165 maternice','tvorbu ml ieka','premenu bielkov\u00edn','\u010dinnos\u0165 k\u00f4ry nadobli\u010diek','metabolizmus v\u00e1pnika','hospod\u00e1renie s vodou v organizme',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(812,'812. Predn\u00fd lalok hypof\u00fdzy \u010dloveka podlieha priamemu riadiacemu vplyvu:','hypotalamu, ktor\u00fd je s\u00fa\u010das\u0165ou medzimozgu','podl\u00f4\u017eka','zadn\u00e9ho laloku hypof\u00fdzy','pri\u0161t\u00edtnych teliesok','epif\u00fdzy','talamu','pred\u013a\u017eenej miechy','stredn\u00e9ho mozgu',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(813,'813. Adenohypof\u00fdza \u010dloveka produkuje:','horm\u00f3ny bielkovinovej povahy','tyrox\u00edn','tyreotropn\u00fd horm\u00f3n','antidiuretick\u00fd horm\u00f3n','horm\u00f3ny steroidnej povahy','kalciferol','vazopres\u00edn','folikulostimula\u010dn\u00fd horm\u00f3n',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(814,'814. Horm\u00f3nom adenohypof\u00fdzy, ktor\u00fd ovplyv\u0148uje funkciu \u017elt\u00e9ho telieska u \u010dloveka, je:','prolakt\u00edn','folikuly stimuluj\u00faci','luteiniza\u010dn\u00fd','progester\u00f3n','tyreotropn\u00fd','estradiol','estriol','oxytoc\u00edn',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(815,'815. Horm\u00f3n adenohypof\u00fdzy reguluj\u00faci funkciu \u013eudsk\u00fdch vaje\u010dn\u00edkov\u00fdch folikulov je:','prolakt\u00edn','folikuly stimuluj\u00faci','tyrox\u00edn','adiuret\u00edn','tyreotropn\u00fd','glandotropn\u00fd','estriol','gon\u00e1dotropn\u00fd',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(816,'816. Rastov\u00fd horm\u00f3n \u010dloveka :','podporuje tvorbu a transport bielkov\u00edn','sa tvor\u00ed v prednom laloku hypof\u00fdzy spolu s luteiniza\u010dn\u00fdm horm\u00f3nom','podporuje synt\u00e9zu RNA v bunk\u00e1ch','sa tvor\u00ed v hypotalame spolu s oxytoc\u00ednom','sa tvor\u00ed spolu s tyreotropn\u00fdm horm\u00f3nom v prednom laloku hypof\u00fdzy','m\u00e1 v priebehu \u017eivota klesaj\u00facu koncentr\u00e1ciu','m\u00e1 v priebehu \u017eivota ust\u00e1len\u00fa koncentr\u00e1ciu','pri nadprodukcii v dospelosti sp\u00f4sobuje akromeg\u00e1liu',TRUE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(817,'817. Nadmern\u00fd rast - gigantizmus u \u013eud\u00ed je sp\u00f4soben\u00fd:','nadbytkom rastov\u00e9ho horm\u00f3nu','nadbytkom somatotrop\u00ednu','nadbytkom potravy','nedostato\u010dn\u00fdm v\u00fddajom energie','po\u017e\u00edvan\u00edm stravy bohatej na tuky','nadbytkom tyrox\u00ednu','nedostatkom tyrox\u00ednu','nedostatkom rastov\u00e9ho horm\u00f3nu',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(818,'818. Tyreotropn\u00fd horm\u00f3n \u010dloveka sa tvor\u00ed:','v \u0161t\u00edtnej \u017e\u013eaze','v adenohypof\u00fdze','v hypotalame','v prednom laloku hypof\u00fdzy','v neurohypof\u00fdze','v k\u00f4re nadobli\u010diek','v pe\u010deni','v pankrease',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(819,'819. Tyreotropn\u00fd horm\u00f3n \u010dloveka:','patr\u00ed medzi glandotropn\u00e9 horm\u00f3ny','podporuje rast a \u010dinnos\u0165 pri\u0161t\u00edtnych teliesok','sa tvor\u00ed v pri\u0161t\u00edtnych telieskach pri poklese hladiny tyrox\u00ednu','patr\u00ed medzi gon\u00e1dotropn\u00e9 horm\u00f3ny','sa tvor\u00ed v epif\u00fdze','podporuje rast a \u010dinnos\u0165 \u0161t\u00edtnej \u017efazy','podnecuje tvorbu horm\u00f3nov v nadobli\u010dk\u00e1ch','sa tvor\u00ed v adenohypof\u00fdze',TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(820,'820. V zadnom laloku hypof\u00fdzy \u010dloveka sa syntetizuje:','adrenal\u00edn','kalciferol','luteiniza\u010dn\u00fd horm\u00f3n','adrenal\u00edn','antidiuretick\u00fd horm\u00f3n','kortizol','oxytoc\u00edn','vazopres\u00edn',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(821,'821. Oxytoc\u00edn u \u010dloveka','riadi \u010dinnos\u0165 predn\u00e9ho laloku hypof\u00fdzy','p\u00f4sob\u00ed na hladk\u00e9 svaly mliekovodov','p\u00f4sob\u00ed na dozrievanie folikulov','ur\u00fdch\u013euje premenu \u017eiv\u00edn','podnecuje kontrakcie svalov matemice','zvy\u0161uje oxid\u00e1ciu tkan\u00edv','p\u00f4sob\u00ed na rozvoj mlie\u010dnej \u017e\u013eazy','spolu s \u010fal\u0161\u00edmi mechanizmami vyvol\u00e1va ovul\u00e1ciu',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(822,'822. Antidiureticky horm\u00f3n a oxytoc\u00edn sa u \u010dloveka tvor\u00ed:','v talame','v\u00fdlu\u010dne mimo hypof\u00fdzy','v hypotalame','na podnet rastov\u00e9ho horm\u00f3nu','v epif\u00fdze','v neurohypof\u00fdze','v adenohypof\u00fdze','v zadnom laloku hypof\u00fdzy',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(823,'823. Adiuret\u00edn p\u00f4sob\u00ed v \u013eudskych obli\u010dk\u00e1ch na:','sp\u00e4tn\u00e9 vstreb\u00e1vanie drasl\u00edka','sp\u00e4tn\u00e9 vstreb\u00e1vanie gluk\u00f3zy','steny tepni\u010diek, kde zni\u017euje tlak','sp\u00e4tn\u00e9 vstreb\u00e1vanie sod\u00edka','sp\u00e4tn\u00e9 vstreb\u00e1vanie vody','sp\u00e4tn\u00e9 vstreb\u00e1vanie aminokysel\u00edn a hor\u010d\u00edka','hospod\u00e1renie s vodou','steny tepni\u010diek, kde zvy\u0161uje tlak',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(824,'824. Tvorba horm\u00f3nov \u0161t\u00edtnej \u017e\u013eazy u \u010dloveka je z\u00e1visl\u00e1 najm\u00e4 na pr\u00edvode:','drasl\u00edka','hor\u010d\u00edka','mang\u00e1nu','zinku','\u017eeleza','kobaltu','v\u00e1pnika','j\u00f3du',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(825,'825. Tyrox\u00edn u \u010dloveka produkuje:','glandula thyroidea','hypof\u00fdza','adenohypof\u00fdza','\u0161t\u00edtna \u017e\u013eaza','hypotalamo-hypof\u00fdzov\u00fd syst\u00e9m','pri\u0161t\u00edtne telieska','podmozgov\u00e1 \u017e\u013eaza','placenta',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(826,'826. Kret\u00e9nizmus u \u010dloveka:','vznik\u00e1 prechodne po u\u017eit\u00ed drogy','vznik\u00e1 pri nedostatku parathorm\u00f3nu','sa vyzna\u010duje vysok\u00fdm vzrastom a zn\u00ed\u017een\u00fdm intelektom','vznik\u00e1 v d\u00f4sledku nedostatku vitam\u00ednov','je sp\u00f4soben\u00fd nedostatkom antidiuretick\u00e9ho horm\u00f3nu','vznik\u00e1 pri nedostato\u010dnej \u010dinnosti \u0161t\u00edtnej \u017e\u013eazy u novorodencov a mal\u00fdch det\u00ed','vznik\u00e1 najm\u00e4 v starobe','vznik\u00e1 pri nadprodukcii horm\u00f3nov \u0161t\u00edtnej \u017e\u013eazy',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(827,'827. Vylu\u010dovanie parathorm\u00f3nu v \u013eudskom organizme je regulovan\u00e9:','\u010dinnos\u0165ou hypotalamu a talamu','adenohypof\u00fdzou','neurohypof\u00fdzou','mineralokortikoidmi','humor\u00e1lnymi sign\u00e1lmi z kostn\u00fdch buniek','\u010dinnos\u0165ou hypof\u00fdzy','hladinou v\u00e1pnika v lymfatickej tekutine','hladinou v\u00e1pnika v plazme',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(828,'828. Hospod\u00e1renie s v\u00e1pnikom v \u013eudskom organizme je riaden\u00e9 horm\u00f3nom:','predn\u00e9ho laloku hypof\u00fdzy','parathorm\u00f3nom','antidiuretick\u00fdm','testoster\u00f3nom','pri\u0161t\u00edtnych teliesok','prolakt\u00ednom','k\u00f4ry nadobli\u010diek','aux\u00ednom',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(829,'829. Z ktor\u00fdch \u010dast\u00ed sa skladaj\u00fa nadobli\u010dky dospel\u00e9ho \u010dloveka:','z drene a prisp\u00f4soben\u00e9ho tkaniva','z obli\u010dky, nadobli\u010dky a vlastnej endokrinnej \u017e\u013eazy','z adrenal\u00ednu a noradrenal\u00ednu','zo sivej a bielej hmoty','z drene a \u0161u\u0161kovit\u00e9ho telieska','z k\u00f4ry a horm\u00f3nov','z k\u00f4ry a drene','z kortexu a drene',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(830,'830. Horm\u00f3ny drene nadobli\u010diek u \u010dloveka:','zni\u017euj\u00fa hladinu gluk\u00f3zy v krvi','del\u00edme na mineralokortikoidy, glukokortikoidy a androg\u00e9nne horm\u00f3ny','podnecuj\u00fa zadr\u017eiavanie sod\u00edka v organizme','zvy\u0161uj\u00fa hladinu krvn\u00e9ho cukru','ovplyv\u0148uj\u00fa \u010dinnos\u0165 srdca','zvy\u0161uj\u00fa hladinu krvn\u00e9ho cukru, okrem gluk\u00f3zy','s\u00fa aldoster\u00f3n a antidiuretick\u00fd horm\u00f3n','sa naz\u00fdvaj\u00fa aj stresov\u00fdmi horm\u00f3nmi',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(831,'831. Mineralokortikoidy v \u013eudskom organizme :','mobilizuj\u00fa zdroje energie pri z\u00e1\u0165a\u017ei','zasahuj\u00fa do premeny \u017eiv\u00edn, najm\u00e4 bielkov\u00edn','vpl\u00fdvaj\u00fa na hospod\u00e1renie so sod\u00edkom a drasl\u00edkom','v\u00fdrazne menia priemer ciev a zvy\u0161uj\u00fa \u010dinnos\u0165 srdca','podnecuj\u00fa zadr\u017eiavanie sod\u00edka v organizme a zvy\u0161uj\u00fa vylu\u010dovanie drasl\u00edka','vpl\u00fdvaj\u00fa na metabolimus gluk\u00f3zy','vpl\u00fdvaj\u00fa na hospod\u00e1renie so sod\u00edkom a hor\u010d\u00edkom','ovplyv\u0148uj\u00fa vlastnosti mimobunkovej tekutiny',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(832,'832. Sekr\u00e9cia glukokortikoidov je regulovan\u00e1:','z medzimozgu cez neurohypof\u00fdzu','z medzimozgu cez predn\u00fd lalok hypof\u00fdzy','hladinou adrenokortikotropn\u00e9ho horm\u00f3nu','hormon\u00e1lne','l\u00e1tkovo','z medzimozgu cez zadn\u00fd lalok hypof\u00fdzy','z hypotalamo-hypof\u00fdzov\u00e9ho komplexu','z medzimozgu cez adenohypof\u00fdzu',FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(833,'833. Glukokortikoidy:','s\u00fa produkovan\u00e9 Langerhansov\u00fdmi ostrov\u010dekmi','s\u00fa horm\u00f3ny drene nadobli\u010diek','s\u00fa horm\u00f3ny k\u00f4ry nadobli\u010diek','ovplyv\u0148uj\u00fa metabolizmus bielkov\u00edn','zvy\u0161uj\u00fa odolnos\u0165 organizmu proti stresu','ovplyv\u0148uj\u00fa premenu bielkov\u00edn na cukry','tlmia z\u00e1palov\u00e9 procesy','p\u00f4sobia stimuluj\u00faco na tvorbu bielkov\u00edn',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(834,'834. \u013dudsky inzul\u00edn:','podporuje prienik gluk\u00f3zy do buniek','zvy\u0161uje premenu bielkov\u00edn na cukry','podporuje tvorbu bielkov\u00edn','zvy\u0161uje hladinu gluk\u00f3zy v krvi','zni\u017euje hladinu gluk\u00f3zy v krvi','je syntetizovan\u00fd v pankrease','je steroidn\u00fd horm\u00f3n','je bielkovinovej povahy',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(835,'835. Nedostatok inzul\u00ednu u \u010dloveka sp\u00f4sobuje:','zn\u00ed\u017eenie hladiny gluk\u00f3zy v krvi','nanizmus','kret\u00e9nizmus','zv\u00fd\u0161enie hladiny sachar\u00f3zy v krvi','hyperglyk\u00e9miu','oneskorenie telesn\u00e9ho a du\u0161evn\u00e9ho v\u00fdvinu len u \u017eien','cukrovku','zv\u00fd\u0161enie hladiny gluk\u00f3zy v krvi',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(836,'836. Ktor\u00fd horm\u00f3n v \u013eudskom tele riadi hospod\u00e1renie s vodou:','glukag\u00f3n','oxytoc\u00edn','antidiuretick\u00fd horm\u00f3n','tyreotropn\u00fd','vazopres\u00edn','tyrox\u00edn','adiuret\u00edn','kortizol',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(837,'837. Ktor\u00e9 horm\u00f3ny zvy\u0161uj\u00fa u \u013eud\u00ed hladinu gluk\u00f3zy v krvi:','noradrenal\u00edn a kortikotrop\u00edn','horm\u00f3ny drene nadobli\u010diek','glukokortikoidy','inzul\u00edn','adrenal\u00edn a kortikotrop\u00edn','adrenal\u00edn a noradrenal\u00edn','adrenal\u00edn a inzul\u00edn','glukag\u00f3n',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(838,'838. Glukag\u00f3n u \u010dloveka:','ovplyv\u0148uje koncentr\u00e1ciu cukru v krvi','ovplyv\u0148uje glyk\u00e9miu','patr\u00ed medzi glukokortikoidy','patr\u00ed medzi glandotropn\u00e9 horm\u00f3ny','pri zv\u00fd\u0161enej produkcii m\u00f4\u017ee zhor\u0161i\u0165 priebeh cukrovky','patr\u00ed medzi tr\u00e1viace enz\u00fdmy','tvor\u00ed sa v pri\u0161t\u00edtnych telieskach','sp\u00f4sobuje akromeg\u00e1lie',TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(839,'839. Ozna\u010dte spr\u00e1vnu odpove\u010f:','hypotalamus sa po slovensky naz\u00fdva podl\u00f4\u017eko','neurohypof\u00fdza nie je prav\u00e1 \u017e\u013eaza s vn\u00fatorn\u00fdm vylu\u010dovan\u00edm','inzul\u00edn je vylu\u010dovan\u00fd bunkami Langerhansov\u00fdch ostrov\u010dekov \u017eal\u00fadkovej \u017e\u013eazy','adrenokortikotropn\u00fd horm\u00f3n riadi sekr\u00e9ciu glukokortikoidov','testoster\u00f3n riadi dozrievanie folikulov vo vaje\u010dn\u00edkoch','adenohypof\u00fdza nie je prav\u00e1 \u017e\u013eaza s vn\u00fatorn\u00fdm vylu\u010dovan\u00edm','neurit je obalen\u00fd myel\u00ednovou po\u0161vou','rastov\u00fd horm\u00f3n sa tvor\u00ed v hypotalame',TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(840,'840. Medzi gon\u00e1dotropn\u00e9 horm\u00f3ny patr\u00ed:','tyreotropn\u00fd horm\u00f3n','folikuly stimuluj\u00faci horm\u00f3n','FSH','somatotropn\u00fd horm\u00f3n','adrenokortikotropn\u00fd horm\u00f3n','luteiniza\u010dn\u00fd horm\u00f3n','oxytoc\u00edn','antidiuretick\u00fd horm\u00f3n',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(841,'841. Pri\u0161t\u00edtne telieska \u010dloveka produkuj\u00fa:','parathorm\u00f3n','tyrox\u00edn','oxytoc\u00edn','horm\u00f3n ovplyv\u0148uj\u00faci nervovosvalov\u00fa dr\u00e1\u017edivos\u0165','horm\u00f3n reguluj\u00faci obsah sod\u00edka v krvi','horm\u00f3n reguluj\u00faci obsah drasl\u00edka v krvi','horm\u00f3n reguluj\u00faci obsah v\u00e1pnika v krvi','horm\u00f3n, ktor\u00e9ho nadprodukcia sp\u00f4sobuje Basedowovu chorobu',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(842,'842. Basedowova choroba:','je sp\u00f4soben\u00e1 nadprodukciou horm\u00f3nu pri\u0161t\u00edtnych teliesok','je sp\u00f4soben\u00e1 nadprodukciou horm\u00f3nov \u0161t\u00edtnej \u017e\u013eazy','sa prejavuje zn\u00ed\u017een\u00fdm metabolizmom','sa prejavuje zv\u00fd\u0161en\u00fdm metabolizmom','vznik\u00e1 v d\u00f4sledku nedostatku j\u00f3du','je sp\u00f4soben\u00e1 nadprodukciou tyrox\u00ednu','je sp\u00f4soben\u00e1 nadprodukciou adiuret\u00ednu','je sp\u00f4soben\u00e1 zn\u00ed\u017eenou produkciou adiuret\u00ednu',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(843,'843. Oblas\u0165 dendritov neur\u00f3nu u \u013eud\u00ed:','pren\u00e1\u0161a vzruchy, inform\u00e1cie k nervov\u00fdm zakon\u010deniam','je \u010dasto bohato vetven\u00e1','m\u00e1 podporn\u00fa a vy\u017eivovaciu funkciu','prij\u00edma inform\u00e1ciu v podobe vzruchu','sa ozna\u010duje ako vstupn\u00fd \u00fasek','sa ozna\u010duje ako v\u00fdstupn\u00fd \u00fasek','zabezpe\u010duj\u00fa vedenie senzit\u00edvnych vzruchov','je obalen\u00e1 myel\u00ednovou po\u0161vou',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(844,'844. Vz\u00e1jomn\u00e9 spojenie nervov\u00fdch buniek v mozgu \u010dloveka naz\u00fdvame:','sylabizmus','syngamia','synapsia','symbi\u00f3za','systola','syndezm\u00f3za','synergia','synoda',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(845,'845. \u013dudsk\u00fd neur\u00f3n:','je bezjadrov\u00e1 bunka so v\u0161etk\u00fdmi organelami typick\u00fdmi pre \u017eivo\u010d\u00ed\u0161nu bunku','obsahuje jadro, jadierko ako aj v\u0161etky ostatn\u00e9 organely typick\u00e9 pre \u017eivo\u010d\u00ed\u0161nu bunku','bol prv\u00fdkr\u00e1t pop\u00edsan\u00fd J.E. Purkynem','okrem tela m\u00e1 po\u010detn\u00e9 ax\u00f3ny a dlh\u00fd v\u00fdbe\u017eok dendrit','je cel\u00fd obalen\u00fd myel\u00ednovou po\u0161vou','je \u0161pecializovan\u00e1 bunka zablokovan\u00e1 v G_0 f\u00e1ze bunkov\u00e9ho cyklu','je schopn\u00fd sa deli\u0165 po cel\u00fd \u017eivot','je podporovan\u00fd a vy\u017eivovan\u00fd gliov\u00fdmi bunkami',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(846,'846. Ozna\u010dte spr\u00e1vnu odpove\u010f:','synapsia zabezpe\u010duje prenos nervov\u00fdch vzruchov','nervov\u00e1 synapsia je funk\u010dn\u00fd kontakt medzi membr\u00e1nami dvoch buniek, z ktor\u00fdch aspo\u0148 jedna je neur\u00f3n','u \u010dloveka je synaptick\u00fd prenos sprostredkovan\u00fd chemicky','najd\u00f4le\u017eitej\u0161ie synaptick\u00e9 pren\u00e1\u0161a\u010de u \u010dloveka s\u00fa adrenal\u00edn a noradrenal\u00edn','po\u010det synapsi\u00ed v mozgu \u010dloveka je rovn\u00fd po\u010dtu neur\u00f3nov','nervov\u00e9 vzruchy p\u00f4sobia len povzbudivo','nervov\u00e1 synapsia sa vytv\u00e1ra len medzi membr\u00e1nami nervov\u00fdch buniek','reflexn\u00fd dej u \u010dloveka je zvy\u010dajne nekontrolovan\u00fd',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(847,'847. Mozog dospel\u00e9ho \u010dloveka m\u00e1 priemern\u00fa hmotnos\u0165:','2500 g','1300 g','250 g','500 g','5 kg','25 g','0,8 kg','750 g',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(848,'848. Vn\u00fatorn\u00e1 \u010das\u0165 hemisf\u00e9r predn\u00e9ho mozgu \u010dloveka je tvoren\u00e1:','sivou hmotou','bielou hmotou','bledou hmotou','hlavne mno\u017estvom nervov\u00fdch vl\u00e1kien','mno\u017estvom nervov\u00fdch buniek','retikul\u00e1rnou form\u00e1ciou','mozgovomiechov\u00fdm mokom','hlavne baz\u00e1lnymi gangliami',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(849,'849. Ozna\u010dte spr\u00e1vne tvrdenie:','mozgov\u00e1 k\u00f4ra riadi vedom\u00fa \u010dinnos\u0165','l\u00f4\u017eko medzimozgu je s\u00eddlom riadenia \u010dinnosti vn\u00fatorn\u00fdch org\u00e1nov','baz\u00e1lne gangli\u00e1 sa z\u00fa\u010dast\u0148uj\u00fa na riaden\u00ed pohybov tela','\u00fastredie nepodmienen\u00fdch sluchov\u00fdch reflexov le\u017e\u00ed v mozo\u010dku','re\u010dov\u00e9 centrum je umiestnen\u00e9 v dominantnej hemisf\u00e9re','v \u013eudskom mozgu je viac synapsi\u00ed ako neur\u00f3nov','po\u010det synapsi\u00ed v \u013eudskom mozgu sa odhaduje na 10^14','\u013eudsk\u00fd mozog obsahuje asi miliardu synapsi\u00ed',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(850,'850. O mozgov\u00fdch komor\u00e1ch \u010dloveka plat\u00ed \u017ee:','je ich 5','s\u00fa 4','s\u00fa 3','I., II. a III. mozgov\u00e1 komora s\u00fa v obidvoch hemisf\u00e9rach predn\u00e9ho mozgu','I. a II. mozgov\u00e1 komora s\u00fa v obidvoch hemisf\u00e9rach predn\u00e9ho mozgu','len I. mozgov\u00e1 komora je ulo\u017een\u00e1 v prednom mozgu','len III. mozgov\u00e1 komora je ulo\u017een\u00e1 v oblasti medzimozgu','medzi mozgovomiechov\u00fdm mokom v komor\u00e1ch a krvou neprebieha v\u00fdmena l\u00e1tok',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(851,'851. V\u00fdkonn\u00e9 funkcie nervovej s\u00fastavy sa rozde\u013euj\u00fa na:','\u00fastredn\u00e9 a obvodov\u00e9','\u00fastredn\u00e9 a perif\u00e9rne','somatick\u00e9 a vegetat\u00edvne','somatick\u00e9 a auton\u00f3mne','auton\u00f3mne a vegetat\u00edvne','mozgov\u00e9 a miechov\u00e9','vn\u00fatom\u00e9 a vonkaj\u0161ie','hybn\u00e9 a motorick\u00e9',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(852,'852. Asocia\u010dn\u00e9 oblasti mozgovej k\u00f4ry u \u013eud\u00ed:','maj\u00fa vz\u0165ah k vytv\u00e1raniu pam\u00e4\u0165ov\u00fdch st\u00f4p','s\u00fa miestom zakon\u010denia vzostupn\u00fdch dr\u00e1h receptorov','sa nach\u00e1dzaj\u00fa v cortex cerebri','s\u00fa najvy\u0161\u0161\u00edm centrom vegetat\u00edvneho riadenia','s\u00fa tvoren\u00e9 baz\u00e1lnymi gangliami','s\u00fa miestom koordin\u00e1ci\u00ed nervov\u00fdch funkci\u00ed','s\u00fa miestom formovania vedomia','zabezpe\u010duj\u00fa homeost\u00e1zu',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(853,'853. Hlavn\u00e9 \u00fastredie pre riadenie telesnej teploty je u \u010dloveka v:','potn\u00fdch \u017e\u013eaz\u00e1ch','hypotalame','pe\u010deni','mozo\u010dku','auton\u00f3mnych gangli\u00e1ch','pred\u013a\u017eenej mieche','podl\u00f4\u017eku','ko\u017ei',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(854,'854. Retikul\u00e1rna form\u00e1cia mozgu \u010dloveka je tvoren\u00e1:','rozsiahlym s\u00faborom posp\u00e1jan\u00fdch nervov\u00fdch buniek roztr\u00fasen\u00fdch v bielej hmote, ktor\u00fd prestupuje pred\u013a\u017eenou miechou, mostom a stredn\u00fdm mozgom a\u017e do medzimozgu','nervov\u00fdmi vl\u00e1knami, ktor\u00e9 umo\u017e\u0148uj\u00fa prenos vzruchu medzi mozgom a miechou','sie\u0165ovitou sple\u0165ou vl\u00e1kien siahaj\u00facou od miechy a\u017e do medzimozgu','len vzostupn\u00fdmi nervov\u00fdmi vl\u00e1knami','nervov\u00fdmi bunkami, ktor\u00e9 riadia podmienen\u00e9 reflexy','dr\u00e1hami a nervami, ktor\u00e9 inervuj\u00fa okohybn\u00e9 svaly','rozpt\u00fdlen\u00fdm miazgov\u00fdm tkanivom','len zostupn\u00fdmi nervov\u00fdmi vl\u00e1knami',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(855,'855. Retikul\u00e1rna form\u00e1cia mozgu \u010dloveka je d\u00f4le\u017eit\u00e1 hlavne pri:','udr\u017ean\u00ed rovnov\u00e1hy','prepojen\u00ed v\u0161etk\u00fdch oddielov centr\u00e1lnej nervovej s\u00fastavy','riaden\u00ed obrann\u00fdch reflexov (ka\u0161e\u013e a k\u00fdchanie)','pri prenose zvukov\u00fdch v\u013an kosti\u010dkami stredn\u00e9ho ucha','ovplyv\u0148ovan\u00ed miechov\u00fdch reflexov','v\u00f4\u013eov\u00fdch pohyboch o\u010d\u00ed','udr\u017ean\u00ed pozornosti','prebuden\u00ed zo sp\u00e1nku',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(856,'856. Stredn\u00fd mozog \u010dloveka:','je ulo\u017een\u00fd v tesnom susedstve mostu','je \u00fastred\u00edm podmienen\u00fdch \u010duchov\u00fdch a sluchov\u00fdch reflexov','obsahuje centr\u00e1 V. a VI. hlavov\u00e9ho nervu','je \u00fastred\u00edm podmienen\u00fdch zrakov\u00fdch a sluchov\u00fdch reflexov','je \u00fastred\u00edm nepodmienen\u00fdch zrakov\u00fdch a sluchov\u00fdch reflexov','vytv\u00e1ra \u0161tvorhrbolie','je \u00fastred\u00edm nepodmienen\u00fdch \u010duchov\u00fdch a sluchov\u00fdch reflexov','obsahuje centr\u00e1 III. a IV. hlavov\u00e9ho nervu',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(857,'857. Mozo\u010dek \u010dloveka:','sa podie\u013ea na udr\u017eovan\u00ed rovnov\u00e1hy tela a na koordin\u00e1cii pohybov','m\u00e1 na povrchu bielu a vo vn\u00fatri siv\u00fa hmotu','je centrom d\u00fdchania','dost\u00e1va inform\u00e1cie z mozgovej k\u00f4ry','riadi a ovl\u00e1da \u010dinnos\u0165 mozgovej k\u00f4ry','m\u00e1 na povrchu siv\u00fa a vo vn\u00fatri bielu hmotu','je najvy\u0161\u0161\u00edm centrom vegetat\u00edvneho riadenia','dost\u00e1va inform\u00e1cie zo statokinetick\u00e9ho receptora, z ko\u017en\u00fdch mechanoreceptorov, svalov\u00fdch a \u0161\u013eachov\u00fdch receptorov',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(858,'858. Hlavnou funkciou \u013eudsk\u00e9ho mozo\u010dka je:','produkcia horm\u00f3nov','anal\u00fdza obrazovej inform\u00e1cie','regul\u00e1cia svalov\u00e9ho nap\u00e4tia','formovanie vedomej \u010dinnosti','uvedomenie si \u00famyseln\u00fdch pohybov','udr\u017eiavanie rovnov\u00e1hy tela','sprostredkovanie obrann\u00fdch reflexov','koordin\u00e1cia pohybov\u00fdch funkci\u00ed',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(859,'859. V pred\u013a\u017eenej mieche \u010dloveka s\u00fa \u017eivotne d\u00f4le\u017eit\u00e9 \u00fastredia:','udr\u017eiavania st\u00e1lej teploty','zrakov\u00e9ho syst\u00e9mu','rovnov\u00e1hy','niektor\u00fdch nepodmienen\u00fdch reflexov','riadenia v\u0161etk\u00fdch svalov\u00fdch funkci\u00ed a d\u00fdchania','koordin\u00e1cie krvn\u00e9ho obehu a psychick\u00fdch funkci\u00ed','riadenia nervov\u00fdch a svalov\u00fdch funkci\u00ed','obehu krvi a d\u00fdchania',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(860,'860. \u00dastredie d\u00fdchac\u00edch a srdcovo-cievnych funkci\u00ed sa u \u010dloveka nach\u00e1dza v:','mozo\u010dku','talame','pred\u013a\u017eenej mieche','chrbticovej mieche','hypotalame','mozgovej k\u00f4re','hypof\u00fdze','strednom mozgu',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(861,'861. Thalamus \u010dloveka:','sa po slovensky ozna\u010duje ako podl\u00f4\u017eko','je s\u00fa\u010das\u0165ou medzimozgu','je s\u00fa\u010das\u0165ou diencephala','je s\u00fa\u010das\u0165ou mesencephala','je najvy\u0161\u0161\u00edm centrom riadenia \u010dinnosti vn\u00fatom\u00fdch org\u00e1nov','m\u00e1 v dolnej \u010dasti umiestnen\u00e9 centrum re\u010di','je s\u00eddlom III. a IV. hlavov\u00e9ho nervu','riadi cel\u00fa \u010dinnos\u0165 endokrinn\u00fdch \u017eliaz',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(862,'862. Zu\u017eovanie a roz\u0161irovanie ciev je u \u010dloveka nervovo riaden\u00e9 z \u00fastredia v:','medzimozgu','pred\u013a\u017eenej mieche','hypof\u00fdze','chrbtovej mieche','dominantnej hemisf\u00e9re','aorte','retikul\u00e1rnej form\u00e1cii','mozgovej k\u00f4re',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(863,'863. Ko\u013eko p\u00e1rov hlavov\u00fdch nervov je u \u010dloveka:','7','9','14','16','31','33','11','12',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(864,'864. Nervov\u00e9 vl\u00e1kna v mieche \u010dloveka:','tvoria bielu hmotu obklopen\u00fa sivou hmotou','tvoria siv\u00fa hmotu obklopuj\u00facu bielu hmotu','sa funk\u010dne delia na motorick\u00e9 a senzit\u00edvne','tvoria po\u010detn\u00e9 kan\u00e1liky','tvoria hust\u00fa sple\u0165 vyzeraj\u00facu ako kompaktn\u00e1 hmota','tvoria bielu hmotu obklopuj\u00facu siv\u00fa hmotu','tvoria siv\u00fa hmotu obklopen\u00fa bielou hmotou','sa norm\u00e1lne nevyskytuj\u00fa',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(865,'865. Po\u010det p\u00e1rov miechov\u00fdch nervov \u010dloveka je:','12','31','11','14','7','60','nespo\u010detn\u00e9 mno\u017estvo','22',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(866,'866. K obvodovej, t.j. perif\u00e9rnej nervovej s\u00fastave \u010dloveka patria:','pred\u013a\u017een\u00e1 miecha a perif\u00e9rne nervy','bedrov\u00e1 miecha a centr\u00e1lna miecha','kr\u00ed\u017eov\u00e9 a kostr\u010dov\u00e9 nervy','kr\u00ed\u017eov\u00e9 a kr\u010dn\u00e9 nervy','driekov\u00e9 a hrudn\u00edkov\u00e9 nervy','hlavov\u00e9 nervy','miechov\u00e9 nervy','hlavov\u00e9 a miechov\u00e9 nervy',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(867,'867. Mozog a miechu \u010dloveka oba\u013euje;','m\u00e4kk\u00e1 plena','tvrd\u00e1 plena','pav\u00fa\u010dnica','cievnatka','pia mater','medulla','arachnoidea','dura mater',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(868,'868. Pav\u00fa\u010dnica u \u010dloveka:','je v\u00e4zivov\u00e1 bohato prekrven\u00e1 blana','je v\u00e4zivov\u00e1 m\u00e1lo prekrven\u00e1 blana','je jemn\u00e1 bezcievna blana','je obal tesne priliehaj\u00faci na povrch mozgu a miechy','je blana sp\u00e1jaj\u00faca z\u00e1hyby CNS','vytv\u00e1ra svorov\u00e9 teleso','sa naz\u00fdva aj \u201ebr\u00e1nou vedomia"','sa latinsky naz\u00fdva arachnoidea',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(869,'869. V\u00fdvojovo najmlad\u0161ou \u010das\u0165ou mozgu \u010dloveka je:','zadn\u00fd mozog','stredn\u00fd mozog','most','mozo\u010dek','pred\u013a\u017een\u00e1 miecha','predn\u00fd mozog','thalamus','mesencephalon',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(870,'870. Proprioreceptory u \u010dloveka sa rozhoduj\u00facim sp\u00f4sobom uplat\u0148uj\u00fa pri:','vn\u00edman\u00ed chu\u0165ov\u00fdch vnemov, patria k interoreceptorom','vn\u00edman\u00ed bolesti, patria k exteroreceptorom','vn\u00edman\u00ed pohybov a polohy tela, patria k interoreceptorom','vn\u00edman\u00ed chladu a tepla, patria k interoreceptorom','vn\u00edman\u00ed pohybov a polohy tela, patria k exteroreceptorom','vn\u00edman\u00ed pohybov a polohy tela, patria k stredn\u00e9mu uchu','vn\u00edman\u00ed polohy a pohybov tela, nach\u00e1dzaj\u00fa sa v k\u013aboch','vn\u00edman\u00ed polohy a pohybov tela, nach\u00e1dzaj\u00fa sa vo svaloch a v \u0161\u013each\u00e1ch',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(871,'871. Motorick\u00fa inerv\u00e1ciu prie\u010dne pruhovan\u00fdch kostrov\u00fdch svalov \u010dloveka zabezpe\u010duj\u00fa nervy:','vegetat\u00edvne','statoakustick\u00e9','miechov\u00e9','hlavov\u00e9','somatick\u00e9','motorick\u00e9','auton\u00f3mne','senzit\u00edvne',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(872,'872. Z\u00e1kladnou funk\u010dnou jednotkou riadiacej \u010dinnosti nervovej s\u00fastavy \u010dloveka je:','receptor','neurit','impulz','ax\u00f3n','reflex','vzruch','stres','efektor',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(873,'873. Reflexn\u00fd obl\u00fak u \u010dloveka:','je v\u017edy ulo\u017een\u00fd v mieche','je priame nervov\u00e9 spojenie receptora s efektorom','nem\u00e1 nikdy \u010das\u0165 dr\u00e1hy ulo\u017een\u00fa v mozgu','m\u00e1 dostrediv\u00fa a odstrediv\u00fa \u010das\u0165','je nervov\u00e9 spojenie receptora a efektora cez \u00fastredn\u00fa nervov\u00fa s\u00fastavu','vznik\u00e1 ako odpove\u010f organizmu na dr\u00e1\u017edenie receptorov','obvykle zabezpe\u010duje i kontrolu reflexn\u00e9ho deja na princ\u00edpe sp\u00e4tnej v\u00e4zby','je jednoduch\u0161\u00ed pri ni\u017e\u0161ej a zlo\u017eitej\u0161\u00ed pri vy\u0161\u0161ej nervovej \u010dinnosti',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(874,'874. Medzi deje a prejavy vy\u0161\u0161ej nervovej \u010dinnosti patr\u00ed:','d\u00e1viv\u00fd reflex','podmienen\u00fd reflex','nepodmienen\u00fd reflex','u\u010denie','stereotypn\u00e9 in\u0161tinkt\u00edvne reakcie','em\u00f3cie','umeleck\u00e1 tvorba','myslenie a abstrakcia',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(875,'875. Prejavom ni\u017e\u0161ej nervovej \u010dinnosti u \u010dloveka:','s\u00fa vroden\u00e9 reakcie na podnety','je lenivos\u0165','s\u00fa nepodmienen\u00e9 reflexy','s\u00fa krimin\u00e1lne \u010diny','je alkoholizmus','je umeleck\u00e1 tvorivos\u0165','je pl\u00e1novanie','je predv\u00eddanie',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(876,'876. Hlavn\u00fdm \u00fastred\u00edm auton\u00f3mnych funkci\u00ed \u010dloveka je:','plexus solaris','talamus','podl\u00f4\u017eko','vegetat\u00edvne nervstvo hrudn\u00edka a brucha','hypotalamus','bl\u00fadiv\u00fd nerv','mozgov\u00e1 k\u00f4ra','pred\u013a\u017een\u00e1 miecha',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(877,'877. Sympatikus u \u010dloveka:','vystupuje z hrudn\u00edkovej a driekovej miechy','nepodie\u013ea sa na tvorbe miechov\u00fdch nervov, prebieha mimo nich','vystupuje z mozgu a miechy v kr\u00ed\u017eovej oblasti','podporuje vylu\u010dovanie sl\u00edn','podporuje vylu\u010dovanie potu','zni\u017euje krvn\u00fd tlak','vyvol\u00e1va zv\u00fd\u0161enie krvn\u00e9ho tlaku','vyvol\u00e1va zr\u00fdchlenie srdcovej \u010dinnosti',TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(878,'878. Sympatikov\u00fd syst\u00e9m u \u010dloveka podporuje \u010di vyvol\u00e1va:','r\u00fdchlu anal\u00fdzu sympati\u00ed v centr\u00e1ch mozo\u010dka','vylu\u010dovanie inzul\u00ednu','vylu\u010dovanie adrenal\u00ednu','akomod\u00e1ciu oka','roz\u0161\u00edrenie zrenice','pre\u017e\u00edvanie sympati\u00ed k \u013eu\u010fom','existenciu em\u00f3ci\u00ed','vylu\u010dovanie horm\u00f3nov drene nadobli\u010diek',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(879,'879. Parasympatikus u \u010dloveka:','vystupuje iba z mozgu a miechy v kr\u00ed\u017eovej oblasti','podporuje zosilnenie pohybu \u010driev','vystupuje z hrudn\u00edkovej a driekovej miechy','podporuje vylu\u010dovanie sl\u00edn','podporuje spomalenie srdcovej \u010dinnosti','inervuje v\u00e4\u010d\u0161inu org\u00e1nov spolo\u010dne so sympatikom, ale nie je to pravidlom','vyvol\u00e1va obvykle efekty protichodn\u00e9 vo\u010di sympatiku','zni\u017euje krvn\u00fd tlak',TRUE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(880,'880. \u010cuchov\u00e9 \u00fastredie \u010dloveka sa nach\u00e1dza:','v sp\u00e1nkovom laloku mozgovej k\u00f4ry','v mozgovej k\u00f4re','v telencefale','v bielej hmote mozgu','v sliznici hornej \u010dasti nosovej dutiny','v strednom mozgu','v \u010delovom laloku mozgovej k\u00f4ry','v k\u00f4re temenn\u00e9ho laloka',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(881,'881. Z\u00e1kladn\u00e9 chu\u0165ov\u00e9 vnemy u \u013eud\u00ed s\u00fa:','sladko, horko, kyslo, slano','sladko, kyslo, hor\u00faco, slano','sladko, hor\u00faco, kyslo, horko','sladko a slano, ostatn\u00e9 s\u00fa odvoden\u00e9','kyslo a slano, ostatn\u00e9 s\u00fa odvoden\u00e9','kyslo, horko, studeno, teplo','horko, hor\u00faco, kyslo, slano','sladko, slano, studeno, teplo',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(882,'882. Vlastn\u00fd chu\u0165ov\u00fd receptor \u010dloveka:','je stimulovan\u00fd l\u00e1tkami rozpusten\u00fdmi vo vode','neobsahuje \u010duchov\u00e9 bunky, ktor\u00e9 sa z\u00fa\u010dast\u0148uj\u00fa na tvorbe v\u00fdsledn\u00e9ho vnemu','s\u00fa chu\u0165ov\u00e9 bunky v chu\u0165ov\u00fdch poh\u00e1rikoch','je stimulovan\u00fd l\u00e1tkami rozpusten\u00fdmi v hlienoch','sa nach\u00e1dza v sliznici m\u00e4kk\u00e9ho podnebia a hltana','neposkytuje inform\u00e1ciu o teplote a konzistencii potravy','neobsahuje tepeln\u00e9 receptory','je menej citliv\u00fd ako \u010duchov\u00e9 bunky',TRUE,TRUE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(883,'883. Sign\u00e1lom pre sluch \u010dloveka s\u00fa zvukov\u00e9 vlny s frekvenciou:','500 Hz','1000 a\u017e 3000 Hz','20 a\u017e 22 000 Hz','20 a\u017e 23 000 Hz','do 1000 Hz','16 a\u017e 20 000 Hz','14 a\u017e 700 Hz','8 a\u017e 12 000 Hz',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(884,'884. Dostrediv\u00e9 sluchov\u00e9 nervov\u00e9 vl\u00e1kna \u010dloveka:','s\u00fa aktivovan\u00e9 priamo zvukov\u00fdmi vinami','ved\u00fa vzruchy do mozgovej k\u00f4ry','ved\u00fa vzruchy zo sluchov\u00e9ho apar\u00e1tu ulo\u017eenom vo vn\u00fatomom uchu','s\u00fa aktivovan\u00e9 mechanick\u00fdm podnetom','s\u00fa aktivovan\u00e9 chemick\u00fdm podnetom','s\u00fa aktivovan\u00e9 pohybom vl\u00e1skov\u00fdch buniek Cortiho platne','prij\u00edmaj\u00fa priame podnety z u\u0161n\u00e9ho bubienka','ved\u00fa vzruchy zo zmyslov\u00fdch buniek kinetick\u00e9ho receptora',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(885,'885. Ozna\u010dte spr\u00e1vne tvrdenie:','organus statoakusticus sa anatomicky del\u00ed na vonkaj\u0161ie, stredn\u00e9 a vn\u00fatom\u00e9 ucho','organus statoakusticus sa anatomicky del\u00ed na kladivko, strmienok a n\u00e1kovku','medzi sluchov\u00e9 kosti\u010dky patr\u00ed: maleus, incus a strapes','sluchov\u00fdmi kosti\u010dkami sa pren\u00e1\u0161aj\u00fa kmity bubienka na stredn\u00e9 ucho','sluchov\u00fdmi kosti\u010dkami sa pren\u00e1\u0161aj\u00fa kmity bubienka na vn\u00fatorn\u00e9 ucho','k stredn\u00e9mu uchu patr\u00ed: bubienkov\u00e1 dutina, sluchov\u00e9 kosti\u010dky a slim\u00e1k','k stredn\u00e9mu uchu patr\u00ed : bubienok, sluchov\u00e9 kosti\u010dky a Cortiho plat\u0148a','Eustachova trubica patr\u00ed k vonkaj\u0161iemu uchu',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(886,'886. Statokinetick\u00fd receptor:','je tvoren\u00fd dvoma anatomicky aj funk\u010dne odli\u0161n\u00fdmi typmi receptorov','je ulo\u017een\u00fd v sp\u00e1nkovej \u010dasti lebky','je ulo\u017een\u00fd vo vonkaj\u0161om uchu','je d\u00f4le\u017eit\u00fd pre vn\u00edmanie intenzity a smeru zvuku','je d\u00f4le\u017eit\u00fd pre udr\u017eanie vzpriamen\u00e9ho postoja a rovnov\u00e1hy','sa sklad\u00e1 z kinetick\u00e9ho a statick\u00e9ho receptora','je d\u00f4le\u017eit\u00fd pre rozl\u00ed\u0161enie zvukov a t\u00f3nov','m\u00f4\u017ee ma\u0165 poruchy naz\u00fdvan\u00e9 refrak\u010dn\u00e9 chyby',TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(887,'887. Krauseho teliesko je receptorom pre vn\u00edmanie:','tlaku a vibr\u00e1cie','chladu','bolesti','zvuku','tepla','polohy','pohybu','pachu',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(888,'888. Ruffiniho teliesko je receptorom pre vn\u00edmanie:','zvuku','tepla','polohy','pohybu','pachu','tlaku a vibr\u00e1cie','chladu','bolesti',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(889,'889. Boles\u0165 u \u010dloveka:','je vn\u00edman\u00e1 hmatov\u00fdmi telieskami','je vn\u00edman\u00e1 vo\u013en\u00fdmi nervov\u00fdmi zakon\u010deniami','je zvy\u010dajne sprev\u00e1dzan\u00e1 zmenami krvn\u00e9ho tlaku','je zvy\u010dajne sprev\u00e1dzan\u00e1 spomalen\u00edm srdcovej \u010dinnosti','je vn\u00edman\u00e1 nervov\u00fdmi zakon\u010deniami, ktor\u00fdch je v ko\u017ei pribli\u017ene 13 na cm2','je vn\u00edman\u00e1 nervov\u00fdmi zakon\u010deniami, ktor\u00fdch je v ko\u017ei pribli\u017ene 50 - 150 na cm2','je vn\u00edman\u00e1 telieskami, ktor\u00fdch je v ko\u017ei pribli\u017ene 50 - 150 na cm2','je vn\u00edman\u00e1 tzv. Ruffiniho telieskom',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(890,'890. Statick\u00fd receptor u \u010dloveka sa nach\u00e1dza:','v labyrinte vn\u00fatorn\u00e9ho ucha','v polkruhovit\u00fdch kan\u00e1likoch','v mozo\u010dku','vo vonkaj\u0161om zvukovode','v gu\u013eovitom vrec\u00fa\u0161ku a ov\u00e1lnom va\u010dku','v svalov\u00fdch vretienkach','v ov\u00e1lnych kan\u00e1likoch a gu\u013eovitom vrec\u00fa\u0161ku','v ko\u017ei stupaj\u00ed',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(891,'891. Pr\u00faden\u00edm endolymfy v polkruhovit\u00fdch chodbi\u010dk\u00e1ch vn\u00fatorn\u00e9ho ucha \u010dloveka:','s\u00fa dr\u00e1\u017eden\u00e9 zmyslov\u00e9 bunky, ktor\u00fdch vl\u00e1sky vy\u010dnievaj\u00fa do statick\u00e9ho receptora','je dr\u00e1\u017eden\u00fd kinetick\u00fd receptor','je rozkmit\u00e1van\u00e1 Cortiho plat\u0148a','je rozkmit\u00e1van\u00fd bubienok','je dr\u00e1\u017eden\u00fd gu\u013eovit\u00fd va\u010dok, v ktorom s\u00fa nahromaden\u00e9 zmyslov\u00e9 bunky','je dr\u00e1\u017eden\u00fd kinetick\u00fd a statick\u00fd receptor','je dr\u00e1\u017eden\u00fd sp\u00e1nkov\u00fd lalok mozgovej k\u00f4ry','s\u00fa dr\u00e1\u017eden\u00e9 zmyslov\u00e9 bunky kinetick\u00e9ho receptora, ktor\u00fdch vl\u00e1sky vy\u010dnievaj\u00fa do dr\u00e1hy endolymfy',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(892,'892. Astigmatizmus je sp\u00f4soben\u00fd:','ch\u00fdban\u00edm rohovicy','akomod\u00e1ciou \u0161o\u0161ovky','nerovnomern\u00fdm zakalen\u00edm \u0161o\u0161ovky','splynut\u00edm slepej a \u017eltej \u0161kvrny','rovnomern\u00fdm pravideln\u00fdm zakriven\u00edm rohovky','ch\u00fdban\u00edm \u017eltej \u0161kvrny','ch\u00fdban\u00edm slepej \u0161kvrny','nerovnomern\u00fdm zakriven\u00edm rohovky',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(893,'893. Akomod\u00e1cia \u0161o\u0161ovky u \u010dloveka:','umo\u017e\u0148uje perif\u00e9rne videnie','reguluje mno\u017estvo svetla vstupuj\u00face do oka','je ovl\u00e1dan\u00e1 z\u00e1vesn\u00fdm apar\u00e1tom vr\u00e1skovca','je reflexn\u00e1, prebieha automaticky','umo\u017e\u0148uje farebn\u00e9 videnie','je reakciou na zmenu osvetlenia','s\u00favis\u00ed so zmenou jej lomivosti','je prisp\u00f4sobenie sa oka intenzite osvetlenia',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(894,'894. Refrak\u010dn\u00e9 chyby oka \u010dloveka m\u00f4\u017eu spo\u010d\u00edva\u0165:','v nedostatku vitam\u00ednu D s n\u00e1slednou \u0161eroslepotou','v poruch\u00e1ch optick\u00e9ho apar\u00e1tu oka','v zmen\u00e1ch dr\u00e1\u017edivosti sklovca','v predozadnom pred\u013a\u017een\u00ed alebo skr\u00e1ten\u00ed o\u010dnej gule','vo zv\u00fd\u0161enej optickej lomivosti sietnice','v nedostatku vitam\u00ednu C','v nedostatku rodops\u00ednu','v nerovnomernom zakriven\u00ed rohovky s n\u00e1sledn\u00fdm znejasnen\u00edm obrazu',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(895,'895. Podnetom pre zrakov\u00fd analyz\u00e1tor \u010dloveka s\u00fa sveteln\u00e9 vlny s v pribli\u017enom rozsahu:','v\u00fdlu\u010dne 400 - 500 nm','40 - 70 nm','400 - 700 nm','40 - 60 nm','4 - 70 nm','od 400 nm a vy\u0161\u0161ie','do 300 nm','zhodnom ako absorbuj\u00fa asimila\u010dn\u00e9 farbiv\u00e1 rastl\u00edn',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
