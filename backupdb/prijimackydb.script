INSERT INTO BIOLOGIA VALUES(1142,'1142. Mut\u00e1cie s\u00fa:','naj\u010dastej\u0161ie dominantn\u00e9 a kodominantn\u00e9','mieny genetickej inform\u00e1cie','zdrojom roz\u0161\u00edrenia variability','pr\u00ed\u010dinou vroden\u00fdch por\u00fach metabolizmu','naj\u010dastej\u0161ie dominantn\u00e9 so zn\u00ed\u017eenou aktivitou','zmeny tvaru bunkov\u00e9ho jadra','nededi\u010dn\u00e9 zmeny','vyvol\u00e1van\u00e9 selekciou',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1143,'1143. Indukovan\u00e9 mut\u00e1cie:','s\u00fa cielen\u00e9, predv\u00eddate\u013en\u00e9 zmeny g\u00e9nov','vznikaj\u00fa po z\u00e1mernom p\u00f4soben\u00ed mutag\u00e9nov','s\u00fa n\u00e1hle zmeny genetick\u00e9ho materi\u00e1lu','m\u00f4\u017eu by\u0165 gametick\u00e9 alebo somatick\u00e9','vznikaj\u00fa len v deliacich sa bunk\u00e1ch','nevznikaj\u00fa v gam\u00e9tach','m\u00f4\u017eu by\u0165 pre organizmus prospe\u0161n\u00e9','m\u00f4\u017eu by\u0165 pre organizmus \u0161kodliv\u00e9',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1144,'1144. Ktor\u00e9 tvrdenie je spr\u00e1vne:','preva\u017en\u00e1 v\u00e4\u010d\u0161ina mut\u00e1ci\u00ed zlep\u0161uje vlastnosti organimiu','preva\u017en\u00e1 v\u00e4\u010d\u0161ina mut\u00e1ci\u00ed je \u0161kodliv\u00e1','mut\u00e1cie m\u00f4\u017eu zmeni\u0165 biochemick\u00e9 vlastnosti bunky','mut\u00e1cie maj\u00fa v\u00fdznam len ak sa vyskytn\u00fa v gam\u00e9te','autoz\u00f3my s\u00fa \u010dastej\u0161ie postihovan\u00e9 mut\u00e1ciami ako heterochromoz\u00f3my','mut\u00e1cie maj\u00fa v\u00fdznam pre evol\u00faciu druhov','o \u0161\u00edren\u00ed mutovan\u00fdch g\u00e9nov v popul\u00e1cii rozhoduje pr\u00edrodn\u00fd v\u00fdber','mut\u00e1cie nem\u00f4\u017eu ovplyvni\u0165 reproduk\u010dn\u00fa schopnos\u0165 organizmov',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1145,'1145. Mut\u00e1cie m\u00f4\u017eu by\u0165:','dedi\u010dn\u00e9 zmeny','dedi\u010dn\u00e9 a nededi\u010dn\u00e9 zaneny','dominantn\u00e9 a reces\u00edvne','fenotypov\u00e9 a genotypov\u00e9','spont\u00e1nne a nededi\u010dn\u00e9','pr\u00ed\u010dinou n\u00e1dorov\u00fdch ochoren\u00ed','g\u00e9nov\u00e9 a chromoz\u00f3mov\u00e9','gen\u00f3mov\u00e9 a chromat\u00ednov\u00e9',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1146,'1146. Spont\u00e1nne mut\u00e1cie:','nie s\u00fa n\u00e1hodn\u00fdm javom vzh\u013eadom na jednotliv\u00e9 bunky','s\u00fa n\u00e1hodn\u00fdm javom vzh\u013eadom na chromoz\u00f3my bunky','s\u00fa t\u00fdm \u010dastej\u0161ie, \u010d\u00edm je v\u00e4\u010d\u0161\u00ed gen\u00f3m bunky','sa vyskytuj\u00fa v z\u00e1vislosti od d\u013a\u017eky bunkov\u00e9ho cyklu','sa vyskytuj\u00fa v relat\u00edvne ve\u013ekom po\u010dte','nemaj\u00fa v\u00fdznam pre dedi\u010dnos\u0165 \u010dloveka','vznikaj\u00fa bez z\u00e1sahov \u010dloveka','prispievaj\u00fa k stabilite genofondu',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1147,'1147. Indukovan\u00e9 mut\u00e1cie:','vznikaj\u00fa v podstate in\u00fdmi mechanizmami ako spont\u00e1nne','s\u00fa v\u00fdsledkom p\u00f4sobenia mutag\u00e9nov','sp\u00f4soben\u00e9 chemick\u00fdmi l\u00e1tkami sa naz\u00fdvaj\u00fa chemomutag\u00e9ny','\u010dastej\u0161ie vznikaj\u00fa p\u00f4soben\u00edm organick\u00fdch ako anorganick\u00fdch l\u00e1tok','nie s\u00fa zn\u00e1me u prokaryotick\u00fdch buniek','s\u00fa v\u00fdsledkom p\u00f4sobenia napr\u00edklad ionizuj\u00faceho \u017eiarenia','postihuj\u00fa cielene len zvolen\u00e9 \u0161trukt\u00farne g\u00e9ny','m\u00f4\u017eu by\u0165 vyvolan\u00e9 len v jadrov\u00fdch g\u00e9noch',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1148,'1148. V\u00fdznam mut\u00e1ci\u00ed spo\u010d\u00edva v tom, \u017ee:','roz\u0161iruj\u00fa spektrum alel v genofonde popul\u00e1cie','v\u017edy zlep\u0161uj\u00fa genetick\u00fa v\u00fdbavu mutantov','roz\u0161iruj\u00fa len fenotypov\u00fa premenlivos\u0165','s\u00fa zdrojom dedi\u010dnej premenlivosti','v\u00e4\u010d\u0161inou s\u00fa nev\u00fdhodn\u00e9 pre svojho nosite\u013ea','s\u00fa predpokladom p\u00f4sobenia pr\u00edrodn\u00e9ho v\u00fdberu','umo\u017e\u0148uj\u00fa cielene opravi\u0165 po\u0161koden\u00fd g\u00e9n','umo\u017e\u0148uj\u00fa spr\u00e1vnu diferenci\u00e1ciu buniek po\u010das ontogen\u00e9zy',TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1149,'1149. Ozna\u010dte spr\u00e1vne odpovede:','pod\u013ea stup\u0148a organiz\u00e1cie genetickej inform\u00e1cie rozli\u0161ujeme 4 typy mut\u00e1ci\u00ed','zmena po\u010dtu chromoz\u00f3mov v bunke sa naz\u00fdva gen\u00f3mov\u00e1 mut\u00e1cia','zmena \u0161trukt\u00fary chromoz\u00f3mu sa naz\u00fdva chromoz\u00f3mov\u00e1 aber\u00e1cia','mut\u00e1cie del\u00edme na dominantn\u00e9 a reces\u00edvne pod\u013ea \u010dinite\u013eov, ktor\u00e9 ich vyvol\u00e1vaj\u00fa','g\u00e9nov\u00e9 mut\u00e1cie s\u00fa menej v\u00fdznamn\u00e9','mut\u00e1cie m\u00f4\u017eu by\u0165 spont\u00e1nne alebo indukovan\u00e9','mut\u00e1cie vznikaj\u00fa len v \u0161trukt\u00farnych g\u00e9noch','mut\u00e1cie nevznikaj\u00fa v mimojadrov\u00fdch molekul\u00e1ch DNA',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1150,'1150. G\u00e9nov\u00e9 mut\u00e1cie:','maj\u00fa v niektor\u00fdch pr\u00edpadoch molekulov\u00fa podstatu','s\u00fa \u010dastej\u0161ie v\u00fdsledkom z\u00e1meny norm\u00e1lnej b\u00e1zy za neprirodzen\u00fa','m\u00f4\u017eu vznikn\u00fa\u0165 stratou nieko\u013ek\u00fdch nukleotidov','m\u00f4\u017eu vznikn\u00fa\u0165 zaraden\u00edm nadpo\u010detn\u00e9ho nukleotidu','s\u00fa v\u00e4\u010d\u0161inou dominantn\u00e9','u \u010dloveka s\u00fa \u010dasto zodpovedn\u00e9 za metabolick\u00e9 poruchy','s\u00fa zmeny po\u010dtu chromoz\u00f3mov v jadre','m\u00f4\u017eu vznika\u0165 aj v mitochondri\u00e1lnej DNA',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1151,'1151. Chromoz\u00f3mov\u00e9 aber\u00e1cie:','patria medzi gen\u00f3mov\u00e9 mut\u00e1cie','predstavuj\u00fa chromoz\u00f3mov\u00e9 mut\u00e1cie','m\u00f4\u017eu by\u0165 prek\u00e1\u017ekou bunkov\u00e9ho delenia','u \u010dloveka s\u00fa relat\u00edvne \u010dast\u00e9','nemaj\u00fa v\u00fdznam pre pre\u017e\u00edvanie zygoty','u \u010dloveka sp\u00f4sobuj\u00fa patologick\u00e9 syndr\u00f3my','s\u00fa typick\u00e9 najm\u00e4 pre prokaryotick\u00e9 organizmy','vznikaj\u00fa naj\u010dastej\u0161ie v mitochondri\u00e1ch a chloroplastoch',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1152,'1152. Vznik chromoz\u00f3movej mut\u00e1cie je podmienen\u00fd:','chybnou transkripciou RNA','p\u00f4soben\u00edm mutag\u00e9nov','nerovnomern\u00fdm rozostupen\u00edm chromoz\u00f3mov po\u010das mei\u00f3zy','zlomom chromoz\u00f3mu','stratou chromoz\u00f3mu','\u0161trukt\u00farnou poruchou chromoz\u00f3mu','poruchou segreg\u00e1cie chromoz\u00f3mov v heterotypickom delen\u00ed mei\u00f3zy','poruchou cytokin\u00e9zy',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1153,'1153. Chromoz\u00f3mov\u00e1 translok\u00e1cia:','je strata \u010dasti chromoz\u00f3mu','je oto\u010denie ur\u010dit\u00e9ho \u00faseku chromoz\u00f3mu o 180\u00b0','je v\u00fdmena \u00fasekov dvoch homologick\u00fdch chromoz\u00f3mov','je v\u00fdmena \u00fasekov dvoch nehomologick\u00fdch chromoz\u00f3mov','je chromoz\u00f3mov\u00e1 aber\u00e1cia, pri ktorej sa men\u00ed po\u010det g\u00e9nov v gen\u00f3me','je zmena, ktor\u00e1 men\u00ed v\u00e4zbov\u00e9 skupiny g\u00e9nov','je mut\u00e1cia meniaca po\u010det chromoz\u00f3mov\u00fdch s\u00e1d','je strata chromoz\u00f3mu',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1154,'1154. Mut\u00e1cia pri ktorej doch\u00e1dza k strate vn\u00fatorn\u00e9ho \u00faseku chromoz\u00f3mu, sa naz\u00fdva:','translok\u00e1cia','inverzia','duplik\u00e1cia','monoz\u00f3mia','haploidia','del\u00e9cia','chromoz\u00f3mov\u00e1 mut\u00e1cia','g\u00e9nov\u00e1 mut\u00e1cia',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1155,'1155. Mut\u00e1cie maj\u00fa v\u00fdznam:','pozit\u00edvny len v niektor\u00fdch pr\u00edpadoch','pri dedi\u010dnej premenlivosti','pre fylogenetick\u00fd v\u00fdvoj','pre vznik dedi\u010dn\u00fdch ochoren\u00ed','v\u017edy negat\u00edvny','v\u017edy pozit\u00edvny','v \u0161\u013eachtite\u013estve','ako sp\u00f4sob lie\u010denia dedi\u010dn\u00fdch ochoren\u00ed',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1156,'1156. Polyploidia znamen\u00e1:','zv\u00fd\u0161enie po\u010dtu len homologick\u00fdch chromoz\u00f3mov','zv\u00fd\u0161enie po\u010dtu chromoz\u00f3mov\u00fdch s\u00e1d','zv\u00fd\u0161en\u00fd po\u010det regula\u010dn\u00fdch a \u0161trukt\u00farnych g\u00e9nov','nadmerne ve\u013ek\u00e9 chromoz\u00f3my','napr. triploidiu alebo tetraploidiu','zv\u00fd\u0161enie po\u010dtu pohlavn\u00fdch chromoz\u00f3mov','pre cicavcov neschopnos\u0165 pre\u017e\u00edvania','zmenu tvaru chromoz\u00f3mov',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1157,'1157. Haploidia:','je typick\u00e1 pre somatick\u00e9 bunky v\u010dely robotnice','je typick\u00e1 pre pohlavn\u00e9 bunky \u010dloveka','je typick\u00e1 pre somatick\u00e9 bunky \u010dloveka','znamen\u00e1, \u017ee v gen\u00f3me jeden chromoz\u00f3m ch\u00fdba','je pre rastliny v\u0161eobecne nezlu\u010dite\u013en\u00e1 so \u017eivotom','je pre \u010dloveka nezlu\u010dite\u013en\u00e1 so \u017eivotom','je pre cicavce nezlu\u010dite\u013en\u00e1 so \u017eivotom','je typick\u00e1 pre somatick\u00e9 bunky tr\u00fada',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1158,'1158. Naj\u010dastej\u0161ie sa vyskytuj\u00facou aneuploidiou v na\u0161ej popul\u00e1cii je:','hemof\u00edlia','cystick\u00e1 fibr\u00f3za','monoz\u00f3mia chromoz\u00f3mu 21','Turnerov syndr\u00f3m','Klinefelterov syndr\u00f3m','Patauov syndr\u00f3m','Downov syndr\u00f3m','triz\u00f3mia chromoz\u00f3mu 21',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1159,'1159. Pri gen\u00f3mov\u00fdch mut\u00e1ci\u00e1ch:','sa menia g\u00e9ny gen\u00f3mu bunky','sa menia g\u00e9ny gen\u00f3mu jedinca','sa nemenia jednotliv\u00e9 g\u00e9ny ale \u0161trukt\u00fara chromoz\u00f3mov','sa men\u00ed po\u010det chromoz\u00f3mov v bunke','somatick\u00e1 bunka cicavcov nem\u00e1 diploidn\u00fd po\u010det chromoz\u00f3mov','vznikaj\u00fa aneuploidn\u00e9 bunky','vznikaj\u00fa bezjadrov\u00e9 bunky','doch\u00e1dza k vzniku nov\u00fdch v\u00e4zbov\u00fdch skup\u00edn g\u00e9nov',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1160,'1160. Gam\u00e9ty s chromoz\u00f3movou mut\u00e1ciou:','nemaj\u00fa zn\u00ed\u017een\u00fa oplod\u0148ovaciu schopnos\u0165','m\u00f4\u017eu utv\u00e1ra\u0165 ne\u017eivotaschopn\u00e9 zygoty','m\u00f4\u017eu by\u0165 pr\u00ed\u010dinou sterility','nemaj\u00fa vplyv na fertilitu','sa vyskytuj\u00fa len pri jedincoch heterogametick\u00e9ho pohlavia','m\u00f4\u017eu by\u0165 v\u00fdsledkom vplyvu mutag\u00e9nov','m\u00f4\u017eu by\u0165 pr\u00ed\u010dinou v\u00e1\u017eneho po\u0161kodenia plodu','m\u00f4\u017eu vznika\u0165 spont\u00e1nne bez z\u00e1sahu \u010dloveka',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1161,'1161. Triploidn\u00ed jedinci pri cicavcoch s\u00fa:','eliminovan\u00ed pr\u00edrodn\u00fdm v\u00fdberom','neschopn\u00ed pre\u017e\u00edva\u0165','hospod\u00e1rsky v\u00fdhodn\u00ed pre ich vy\u0161\u0161iu \u00fa\u017eitkovos\u0165','v\u00fdsledkom poruchy pri tvorbe gam\u00e9t rodi\u010dov','plodnej\u0161\u00ed','m\u00e1lo odoln\u00ed vo\u010di vplyvom vonkaj\u0161ieho prostredia','z\u00edskavan\u00ed medzidruhovou hybridiz\u00e1ciou','cielene \u0161\u013eachten\u00ed',TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1162,'1162. Aneuploidn\u00e1 bunka obsahuje:','2n-1 alebo 2n+1 chromoz\u00f3mov','3n chromoz\u00f3mov','2n+1n chromoz\u00f3mov','zn\u00ed\u017een\u00fd alebo zv\u00fd\u0161en\u00fd po\u010det len niektor\u00fdch chromoz\u00f3mov','2n-n, t.j. haploidn\u00fd po\u010det chromoz\u00f3mov','napr. trizomick\u00fd karyotyp','triploidn\u00fd po\u010det chromoz\u00f3mov','viac ako dve chromoz\u00f3mov\u00e9 sady',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1163,'1163. Zmena \u0161trukt\u00fary chromoz\u00f3mu:','nikdy nie je spojen\u00e1 so zmenou poradia g\u00e9nov','m\u00f4\u017ee by\u0165 spojen\u00e1 so zmenou po\u010dtu g\u00e9nov','je podmienen\u00e1 zlomom chromoz\u00f3mu','m\u00f4\u017ee by\u0165 spojen\u00e1 so zmenou poradia g\u00e9nov','sp\u00f4sobuje zmenu po\u010dtu chromoz\u00f3mov','vo v\u00e4\u010d\u0161ine pr\u00edpadov sa prejavuje negat\u00edvnym sp\u00f4sobom','je chromoz\u00f3mov\u00e1 mut\u00e1cia','je gen\u00f3mov\u00e1 mut\u00e1cia',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1164,'1164. Podiel vonkaj\u0161\u00edch faktorov na prenose monog\u00e9nne podmienen\u00fdch dedi\u010dn\u00fdch chor\u00f4b:','je takmer nulov\u00fd','je ve\u013emi vysok\u00fd','sa d\u00e1 vyjadri\u0165 koeficientom dedivosti 0,5','je vysok\u00fd pri dominantnej dedi\u010dnosti ochorenia','je v\u00fdznamn\u00fd pri reces\u00edvnej dedi\u010dnosti ochorenia','je podmienen\u00fd pohlav\u00edm','je v\u00fdznamn\u00fd napr. pri hemof\u00edlii','je nev\u00fdznamn\u00fd napr. pri daltonizme',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1165,'1165. Podstatou Klinefelterovho syndr\u00f3mu je:','triz\u00f3mia X chromoz\u00f3mu','monoz\u00f3mia X chromoz\u00f3mu','triz\u00f3mia Y chromoz\u00f3mu','monoz\u00f3mia Y chromoz\u00f3mu','pr\u00edtomnos\u0165 jedn\u00e9ho X chromoz\u00f3mu navy\u0161e v karyotype mu\u017ea','pr\u00edtomnos\u0165 jedn\u00e9ho X chromoz\u00f3mu navy\u0161e v karyotype \u017eeny','kombin\u00e1cia heterochromoz\u00f3mov XXY v karyotype jedinca','pr\u00edtomnos\u0165 len jedn\u00e9ho X chromoz\u00f3mu v karyotype \u017eeny',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1166,'1166. Podstatou Turnerovho syndr\u00f3mu je:','triz\u00f3mia X chromoz\u00f3mu','monoz\u00f3mia X chromoz\u00f3mu','triz\u00f3mia Y chromoz\u00f3mu','monoz\u00f3mia Y chromoz\u00f3mu','pr\u00edtomnos\u0165 jedn\u00e9ho X chromoz\u00f3mu navy\u0161e v karyotype mu\u017ea','pr\u00edtomnos\u0165 len jedn\u00e9ho X chromoz\u00f3mu v karyotype \u017eeny','kombin\u00e1cia heterochromoz\u00f3mov XXY','gen\u00f3mov\u00e1 mut\u00e1cia s karyotypom 45,X0',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1167,'1167. Kvantitat\u00edvne znaky s\u00fa:','podmienen\u00e9 len dedi\u010dne - polyg\u00e9nne','podmienen\u00e9 polyg\u00e9nne a ovplyvnen\u00e9 aj nededi\u010dn\u00fdmi faktormi','podmienen\u00e9 viacer\u00fdmi mut\u00e1ciami','podmienen\u00e9 g\u00e9nmi ve\u013ek\u00e9ho \u00fa\u010dinku','podmienen\u00e9 kvantitat\u00edvnymi g\u00e9nmi','podmienen\u00e9 g\u00e9nmi mal\u00e9ho \u00fa\u010dinku','podmienen\u00e9 jedn\u00fdm g\u00e9nom ve\u013ek\u00e9ho \u00fa\u010dinku','nededi\u010dn\u00e9',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1168,'1168. Dedi\u010dnos\u0165 kvalitat\u00edvnych znakov u \u010dloveka je zv\u00e4\u010d\u0161a:','polyg\u00e9nna','monog\u00e9nna','podmienen\u00e1 g\u00e9nmi, ktor\u00fdch \u00fa\u010dinky sa s\u010d\u00edtavaj\u00fa','podmienen\u00e1 jedn\u00fdm g\u00e9nom ve\u013ek\u00e9ho \u00fa\u010dinku','viacer\u00fdmi g\u00e9nmi, ktor\u00e9 s\u00fa vo v\u00e4zbe','autoz\u00f3mov\u00e1','podmienen\u00e1 velk\u00fdm po\u010dtom g\u00e9nov z r\u00f4znych lokusov','podmienen\u00e1 g\u00e9nmi mal\u00e9ho \u00fa\u010dinku',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1169,'1169. G\u00e9ny ve\u013ek\u00e9ho \u00fa\u010dinku:','s\u00fa v\u00fdrazne z\u00e1visl\u00e9 od vonkaj\u0161ieho prostredia','podmie\u0148uj\u00fa fenotypov\u00fa hodnotu ur\u010dit\u00e9ho kvantitat\u00edvneho znaku','maj\u00fa v\u00fdrazn\u00fd fenotypov\u00fd \u00fa\u010dinok','vytv\u00e1raj\u00fa polyg\u00e9nny syst\u00e9m dedi\u010dnosti','sa uplat\u0148uj\u00fa napr. pri determin\u00e1cii krvnoskupinov\u00e9ho syst\u00e9mu AB0','kontroluj\u00fa synt\u00e9zu enz\u00fdmov','zodpovedaj\u00fa za niektor\u00e9 monog\u00e9nne podmienen\u00e9 dedi\u010dn\u00e9 ochorenia','nek\u00f3duj\u00fa \u0161trukt\u00faru bielkov\u00edn',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1170,'1170. G\u00e9ny mal\u00e9ho \u00fa\u010dinku:','podmie\u0148uj\u00fa v\u00e4\u010d\u0161inou kvalitat\u00edvne znaky','sa vyskytuj\u00fa len na X chromoz\u00f3me','vytv\u00e1raj\u00fa polyg\u00e9nny syst\u00e9m dedi\u010dnosti','podmie\u0148uj\u00fa znaky m\u00e1lo z\u00e1visl\u00e9 od vonkaj\u0161ieho prostredia','sa uplat\u0148uj\u00fa napr. pri dedi\u010dnosti telesnej v\u00fd\u0161ky','v genetike \u010dloveka maj\u00fa mal\u00fd v\u00fdznam','sa nevyskytuj\u00fa na gonoz\u00f3moch','nek\u00f3duj\u00fa \u0161trukt\u00faru bielkov\u00edn',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1171,'1171. Pojem akt\u00edvna alela sa pou\u017e\u00edva:','ako synonymum pre dominantn\u00fa alelu','pri polyg\u00e9nnej dedi\u010dnosti','pre ozna\u010denie alely zodpovedaj\u00facej za synt\u00e9zu enz\u00fdmu','pre alelu, na ktor\u00fa nep\u00f4sob\u00ed selekcia','ak alela zvy\u0161uje hodnotu kvantitat\u00edvneho znaku','pri intermedi\u00e1rnej dedi\u010dnosti','pri kodom\u00ednantnej dedi\u010dnosti','pri dedi\u010dnosti monog\u00e9nnych znakov viazan\u00fdch na pohlavie',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1172,'1172. V polyg\u00e9nnom syst\u00e9me rozli\u0161ujeme alely:','akt\u00edvne','pas\u00edvne','neutr\u00e1lne','negat\u00edvne','pozit\u00edvne','dominantn\u00e9','reces\u00edvne','kvalitat\u00edvne',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1173,'1173. Priemern\u00e9 hodnoty kvantitat\u00edvneho znaku v popul\u00e1cii:','sa vyskytuj\u00fa s rovnakou frekvenciou ako vysok\u00e9 hodnoty','sa vyskytuj\u00fa \u010dastej\u0161ie ako n\u00edzke hodnoty','sa vyskytuj\u00fa menej \u010dasto ako vysok\u00e9 hodnoty','m\u00f4\u017eu by\u0165 modifikovan\u00e9 vonkaj\u0161\u00edm prostred\u00edm','s\u00fa podmienen\u00e9 polyg\u00e9nne','sa vyskytuj\u00fa sporadicky','s\u00fa podmienen\u00e9 monog\u00e9nne','maj\u00fa len homozygotn\u00ed jedinci',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1174,'1174. Premenlivos\u0165 organizmov v r\u00e1mci biologick\u00e9ho druhu je:','nez\u00e1visl\u00e1 od dedi\u010dnosti','podmienen\u00e1 len geneticky','v\u00fdsledkom dedi\u010dnosti a \u00fa\u010dinkov prostredia','zdrojom pre p\u00f4sobenie pr\u00edrodn\u00e9ho v\u00fdberu','charakteristick\u00e1 pre biologick\u00fd druh','podmienen\u00e1 geneticky - len g\u00e9nmi ve\u013ek\u00e9ho \u00fa\u010dinku','variabiln\u00fd prejav fenotypu jedincov rovnak\u00e9ho druhu','vyvolan\u00e1 len vplyvom prostredia',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1175,'1175. Vn\u00fatrodruhov\u00e1 fenotypov\u00e1 variabilita:','sa tie\u017e naz\u00fdva modifik\u00e1cia','je podmienen\u00e1 genotypom a vonkaj\u0161\u00edm prostred\u00edm','je v\u00fdznamn\u00e1 z h\u013eadiska evol\u00facie','je podmienen\u00e1 len vplyvom prostredia','je vyvolan\u00e1 len indukovan\u00fdmi mut\u00e1ciami','je ovplyv\u0148ovan\u00e1 aj mut\u00e1ciami','je odrazom variability genofondu','nem\u00e1 v\u00fdznam pre zachovanie druhu',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1176,'1176. Dedivos\u0165:','vyjadruje podiel fenotypu na celkovej genotypovej premenlivosti','vyjadruje podiel genotypu na celkovej fenotypovej premenlivosti','ak je nulov\u00e1, vtedy je znak ovplyvnen\u00fd len prostred\u00edm','ak je nulov\u00e1, vtedy je znak ovplyvnen\u00fd v\u00fdlu\u010dne genotypom','ak m\u00e1 hodnotu h^2= 1, vtedy je znak ovplyvnen\u00fd len prostred\u00edm','ak m\u00e1 hodnotu h^2= 1, vtedy je znak ovplyvnen\u00fd v\u00fdlu\u010dne genotypom','ak m\u00e1 hodnotu h^2= 0,25, vtedy 75 % premenlivosti znaku podmie\u0148uj\u00fa rozdielne podmienky prostredia','ak m\u00e1 hodnotu h^2 = 0,25, vtedy 25 % premenlivosti znaku podmie\u0148uj\u00fa rozdielne podmienky prostredia',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1177,'1177. Koeficient dedivosti:','sa m\u00f4\u017ee pohybova\u0165 od -1 do +1','m\u00e1 v\u00fdznam pri polyg\u00e9nnej dedi\u010dnosti','sa uplat\u0148uje pri posudzovan\u00ed dedi\u010dnosti du\u0161evn\u00fdch vlastnost\u00ed','sa pohybuje od 0 do 1','nie je z\u00e1visl\u00fd od vplyvu faktorov vonkaj\u0161ieho prostredia','ud\u00e1va podiel dedi\u010dnej zlo\u017eky na celkovej fenotypovej premenlivosti znaku','pri dedi\u010dn\u00fdch znakoch je v\u017edy v\u00e4\u010d\u0161\u00ed ako 1','ud\u00e1va silu v\u00e4zby g\u00e9nov',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1178,'1178. Genofond je:','s\u00fabor v\u0161etk\u00fdch g\u00e9nov v bunke','s\u00fabor v\u0161etk\u00fdch alel biologick\u00e9ho rodu','s\u00fabor v\u0161etk\u00fdch alel ur\u010ditej popul\u00e1cie','s\u00fabor karyotypov biologick\u00e9ho druhu','genetick\u00e1 inform\u00e1cia popul\u00e1cie','genetick\u00fd fond jedinca','z\u00e1sobn\u00e1 genetick\u00e1 inform\u00e1cia','s\u00fabor v\u0161etk\u00fdch chromoz\u00f3mov v bunke',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1179,'1179. Hardyho-Weinbergov z\u00e1kon plat\u00ed:','predov\u0161etk\u00fdm v mal\u00fdch popul\u00e1ci\u00e1ch','vo ve\u013ek\u00fdch popul\u00e1ci\u00e1ch pri inbr\u00eddingu','vo velkej popul\u00e1cii pri panmixii','vo ve\u013ekej popul\u00e1cii pri panmixii a zv\u00fd\u0161enej indukcii mut\u00e1ci\u00ed','vo v\u0161etk\u00fdch popul\u00e1ci\u00e1ch, ktor\u00e9 sp\u013a\u0148aj\u00fa ur\u010dit\u00e9 podmienky','ak je vysok\u00fd v\u00fdskyt pozit\u00edvnych mut\u00e1ci\u00ed v popul\u00e1cii','v mal\u00fdch popul\u00e1ci\u00e1ch pri inbr\u00eddingu','vo ve\u013ekej popul\u00e1cii pri panmixii a pozit\u00edvnej selekcii',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1180,'1180. Panmixia je:','mie\u0161anie g\u00e9nov v popul\u00e1cii pri inbr\u00eddingu','n\u00e1hodn\u00fd v\u00fdber partnerov pri kr\u00ed\u017een\u00ed','jav vyskytuj\u00faci sa \u010dastej\u0161ie v mal\u00fdch popul\u00e1ci\u00e1ch','jav vyskytuj\u00faci sa \u010dastej\u0161ie pri autogamii','d\u00f4le\u017eit\u00e1 podmienka pre platnos\u0165 Hardyho-Weinbergovho z\u00e1kona','m\u00e1lo v\u00fdznamn\u00fd faktor pre popula\u010dn\u00fa genetiku','sp\u00f4sob kr\u00ed\u017eenia, ktor\u00fdm sa z\u00edskavaj\u00fa \u010dist\u00e9 l\u00ednie','pohlavn\u00fd v\u00fdber',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1181,'1181. K procesom nar\u00fa\u0161aj\u00facim genetick\u00fa rovnov\u00e1hu v popul\u00e1cii patr\u00ed:','panmixia','inbr\u00edding','rekombin\u00e1cie','selekcia','mut\u00e1cie','geografick\u00e1 izol\u00e1cia','migr\u00e1cia','n\u00e1hodn\u00e9 kr\u00ed\u017eenie',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1182,'1182. Popul\u00e1cia je v genetickej rovnov\u00e1he, ak:','je zlo\u017een\u00e1 zo sam\u00fdch heterozygotov','je dostato\u010dne mal\u00e1','v nej doch\u00e1dza ku g\u00e9nov\u00e9mu driftu','v nej p\u00f4sob\u00ed selekcia','je dostato\u010dne ve\u013ek\u00e1 a panmiktick\u00e1','je dostato\u010dne velk\u00e1 a autogamn\u00e1','s\u00fa genotypov\u00e9 frekvencie z gener\u00e1cie na gener\u00e1ciu rovnak\u00e9','ak sa z gener\u00e1cie na gener\u00e1ciu menia g\u00e9nov\u00e9 frekvencie',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1183,'1183. V malej popul\u00e1cii vplyvom g\u00e9nov\u00e9ho posunu sa m\u00f4\u017ee po ur\u010ditom po\u010dte gener\u00e1ci\u00ed:','meni\u0165 genofond popul\u00e1cie v zmysle vymiznutia niektor\u00fdch alel a zv\u00fd\u0161enia frekvencie in\u00fdch','genofond sta\u0165 variabiln\u00fd, frekvencia v\u0161etk\u00fdch alel st\u00fapa a prib\u00fada homozygotov v d\u00f4sledku panmixie','genofond sta\u0165 variabiln\u00fd, pri\u010dom st\u00fapa po\u010det heterozygotov','meni\u0165 genofond popul\u00e1cie v zmysle vymiznutia niektor\u00fdch alel pri zv\u00fd\u0161en\u00ed po\u010dtu heterozygotov','zv\u00fd\u0161i\u0165 po\u010det homozygotov','zv\u00fd\u0161i\u0165 po\u010det len dominantn\u00fdch homozygotov','zv\u00fd\u0161i\u0165 po\u010det heterozygotov','zn\u00ed\u017ei\u0165 po\u010det heterozygotov',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1184,'1184. K zmene frekvencie alel v popul\u00e1cii doch\u00e1dza:','pri panmixii','v z\u00e1vislosti od v\u00fdskytu chromoz\u00f3mov\u00fdch mut\u00e1ci\u00ed v somatick\u00fdch bunk\u00e1ch','vplyvom selekcie','len vo ve\u013ek\u00fdch popul\u00e1ci\u00e1ch','vplyvom genetickej izol\u00e1cie','v d\u00f4sledku zvy\u0161ovania po\u010dtu heterozygotov','v d\u00f4sledku mut\u00e1ci\u00ed','v d\u00f4sledku nerovnakej \u017eivotaschopnosti r\u00f4znych genotypov',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1185,'1185. O selekcii plat\u00ed, \u017ee:','len od jej intenzity z\u00e1vis\u00ed r\u00fdchlos\u0165 zmeny po\u010detnosti alel v popul\u00e1cii','m\u00f4\u017ee by\u0165 negat\u00edvna alebo pozit\u00edvna','jej v\u00fdsledok z\u00e1vis\u00ed aj od toho, \u010di je alela dominantn\u00e1 alebo reces\u00edvna','ak je nev\u00fdhodn\u00e1 reces\u00edvna alela, selekcia p\u00f4sob\u00ed len vo\u010di reces\u00edvnym homozygotom','po\u010detnos\u0165 reces\u00edvnej alely podmie\u0148uj\u00facej nev\u00fdhodn\u00fd znak kles\u00e1 v popul\u00e1cii ve\u013emi pomaly','eliminuje m\u00e1lo prisp\u00f4sobiv\u00fdch jedincov','zvy\u0161uje v popul\u00e1cii po\u010detnos\u0165 alel, ktor\u00e9 podmie\u0148uj\u00fa v\u00fdhodn\u00e9 znaky','eliminuje m\u00e1lo prisp\u00f4sobiv\u00fdch jedincov, ale nemen\u00ed frekvenciu alel v popul\u00e1cii',FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1186,'1186. Pre mut\u00e1cie vo vz\u0165ahu ku genofondu popul\u00e1cie plat\u00ed, \u017ee:','po\u010detnos\u0165 spont\u00e1nnych mut\u00e1ci\u00ed sa vyjadruje po\u010detnos\u0165ou mut\u00e1ci\u00ed na jeden g\u00e9n za jednu gener\u00e1ciu','po\u010detnos\u0165 mut\u00e1ci\u00ed v popul\u00e1cii je v\u0161eobecne vysok\u00e1','po\u010das jednej gener\u00e1cie mut\u00e1cie vyvolaj\u00fa len bezv\u00fdznamn\u00e9 zmeny po\u010detnosti alel','mut\u00e1cie bez selekcie nemaj\u00fa ve\u013ek\u00fd v\u00fdznam pre zmenu po\u010detnosti alel v popul\u00e1cii','umo\u017e\u0148uj\u00fa uplatnenie selekcie v popul\u00e1cii','vyvol\u00e1vaj\u00fa zmeny genofondu','mut\u00e1cie neovplyv\u0148uj\u00fa genofond popul\u00e1cie','v mal\u00fdch popul\u00e1ci\u00e1ch je v\u00fdznam mut\u00e1ci\u00ed men\u0161\u00ed ako vo ve\u013ek\u00fdch',TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1187,'1187. V\u00fdvoj genofondu popul\u00e1cie:','z\u00e1vis\u00ed od ve\u013ekosti popul\u00e1cie','z\u00e1vis\u00ed od kvality alel','z\u00e1vis\u00ed od selek\u010dnej hodnoty znaku','m\u00f4\u017ee by\u0165 ovplyvnen\u00fd inbr\u00eddingom','nie je ovplyv\u0148ovan\u00fd sp\u00f4sobom kr\u00ed\u017eenia','je ovplyv\u0148ovan\u00fd muta\u010dnou frekvenciou','sa v s\u00fa\u010dasnosti neprejavuje','nez\u00e1vis\u00ed od intenzity selekcie',TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1188,'1188. O popul\u00e1cii plat\u00ed, \u017ee:','jedinci, ktor\u00ed ju tvoria, musia ma\u0165 rovnak\u00fd genotyp','je tvoren\u00e1 jedincami rovnak\u00e9ho biologick\u00e9ho druhu, ktor\u00ed sa navz\u00e1jom kr\u00ed\u017eia a v danom \u010dase os\u00edd\u013euj\u00fa ur\u010dit\u00e9 \u00fazemie','genofond jednotliv\u00fdch popul\u00e1ci\u00ed ur\u010dit\u00e9ho druhu sa m\u00f4\u017ee navz\u00e1jom odli\u0161ova\u0165','navz\u00e1jom sa odli\u0161uj\u00face popul\u00e1cie dan\u00e9ho druhu vznikli selekt\u00edvnym prisp\u00f4soben\u00edm sa prostrediu','n\u00e1hodn\u00fd g\u00e9nov\u00fd posun nem\u00e1 v\u00fdznam pre mal\u00fa popul\u00e1ciu','jej genofond nie je ovplyv\u0148ovan\u00fd inbr\u00eddingom','je tvoren\u00e1 jedincami r\u00f4znych biologick\u00fdch druhov, ktor\u00ed v danom \u010dase os\u00edd\u013euj\u00fa ur\u010dit\u00e9 \u00fazemie','je tvoren\u00e1 jedincami rovnak\u00e9ho biologick\u00e9ho druhu, ktor\u00ed v danom \u010dase os\u00edd\u013euj\u00fa viacer\u00e9 geograficky izolovan\u00e9 \u00fazemia',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1189,'1189. N\u00e1hodn\u00fd g\u00e9nov\u00fd posun:','m\u00e1 v\u00e4\u010d\u0161\u00ed v\u00fdznam pre ve\u013ek\u00fa popul\u00e1ciu','je \u010dastej\u0161\u00ed v malej popul\u00e1cii','m\u00f4\u017ee sp\u00f3sobi\u0165 zv\u00fd\u0161enie po\u010detnosti niektor\u00fdch alel v popul\u00e1cii','m\u00f4\u017ee sp\u00f4sobi\u0165 vymiznutie niektor\u00fdch alel v popul\u00e1cii','vo ve\u013ek\u00fdch popul\u00e1ci\u00e1ch je menej \u010dast\u00fd','pre rudsk\u00e9 popul\u00e1cie nem\u00e1 v\u00fdznam','z genetick\u00e9ho h\u013eadiska je bezv\u00fdznamn\u00fd','nem\u00e1 \u017eiaden vplyv na frekvenciu alel v genofonde',FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1190,'1190. V malej popul\u00e1cii vplyvom g\u00e9nov\u00e9ho posunu sa m\u00f4\u017ee meni\u0165:','variabilita genofondu, frekvencia alel st\u00fapa a zvy\u0161uje sa po\u010det heterozygotov','genofond tak, \u017ee sa vytr\u00e1caj\u00fa niektor\u00e9 alely a zvy\u0161uje sa po\u010det in\u00fdch','variabilita genofondu tak, \u017ee st\u00fapa po\u010det hemizygotov','genofond popul\u00e1cie tak, \u017ee sa vytratia niektor\u00e9 alely a frekvencia in\u00fdch sa zv\u00fd\u0161i, pri\u010dom narastie po\u010det homozygotov','po\u010det heterozygotov tak, \u017ee sa ich frekvencia zvy\u0161uje','frekvencia niektor\u00fdch genotypov','frekvencia alel','druhov\u00e9 zlo\u017eenie popul\u00e1cie',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1191,'1191. Hardyho-Weinbergov z\u00e1kon mo\u017eno vyjadri\u0165 z\u00e1pisom:','p + q = 1','p + 2pq + q = 1','p^2 + 2pq + q^2 = 1','p^2 + q^2 =','p - q = 1','(p + q)^2 = 1','(p + q)^2 = p^2 + 2pq + q^2','p = q',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1192,'1192. Hardyho-Weinbergov z\u00e1kon sa uplat\u0148uje:','v popula\u010dnej genetike','v geneal\u00f3gii','v mal\u00fdch popul\u00e1ci\u00e1ch pri inbr\u00eddingu','v gemelol\u00f3gii','aj v genetike \u010dloveka','v panmiktick\u00fdch popul\u00e1ci\u00e1ch za ur\u010dit\u00fdch podmienok','v popul\u00e1ci\u00e1ch vystaven\u00fdch selekcii','v cytogenetike',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
