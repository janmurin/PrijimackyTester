INSERT INTO BIOLOGIA VALUES(1331,'1331. Pijavice:','maj\u00fa slinn\u00e9 \u017e\u013eazy obsahuj\u00face l\u00e1tku podporuj\u00facu zr\u00e1\u017eanie krvi','maj\u00fa slinn\u00e9 \u017e\u013eazy obsahuj\u00face hirud\u00edn','sa pou\u017e\u00edvali na lek\u00e1rske \u00fa\u010dely na ods\u00e1vanie krvi pri n\u00edzkom tlaku','s\u00fa drav\u00e9 i parazituj\u00face','patria medzi mnoho\u0161tetinavce','patria medzi m\u00e1lo\u0161tetinavce','\u017eij\u00fa v moriach i sladk\u00fdch vod\u00e1ch','maj\u00fa \u010drevo s bo\u010dn\u00fdmi v\u00fdrastkami na ukladanie krvi do z\u00e1soby',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1332,'1332. \u010cl\u00e1nkono\u017ece maj\u00fa:','na ka\u017edom \u010dl\u00e1nku p\u00e1r \u010dl\u00e1nkovan\u00fdch kon\u010dat\u00edn','uzatvoren\u00fa cievnu s\u00fastavu','hydrolymfu','otvoren\u00fa cievnu s\u00fastavu','homon\u00f3mnu \u010dl\u00e1nkovanos\u0165 tela','na ka\u017edom \u010dl\u00e1nku p\u00e1r tykadiel','heteron\u00f3mnu \u010dl\u00e1nkovanos\u0165 tela','hemolymfu',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
