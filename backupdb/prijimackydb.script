INSERT INTO BIOLOGIA VALUES(114,'114. Mikrotubuly s\u00fa v bi\u010d\u00edku usporiadan\u00e9:','do dev\u00e4\u0165 troj\u00edc po obvode','prie\u010dne','pod\u013ea sch\u00e9my 9 + 2','do dev\u00e4\u0165 dvoj\u00edc po obvode a jedn\u00e9ho centr\u00e1lneho p\u00e1ru','do jeden\u00e1s\u0165 obvodov\u00fdch dvoj\u00edc','n\u00e1hodne','pod\u013ea sch\u00e9my 9 + 3','v centr\u00e1lnej rovine',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
