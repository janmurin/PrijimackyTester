INSERT INTO BIOLOGIA VALUES(199,'199. Mixotrofia:','je autotrofno-heterotrofn\u00fd sp\u00f4sob v\u00fd\u017eivy','je prisp\u00f4sobenie sa rastl\u00edn stanovi\u0161\u0165u s v\u00fdrazn\u00fdm deficitom dus\u00edka','je parazitick\u00fd sp\u00f4sob \u017eivota','je schopnos\u0165 niektor\u00fdch rastl\u00edn z\u00edskava\u0165 molekul\u00e1rny dus\u00edk zo vzduchu','je \u0161pecifick\u00fd sp\u00f4sob symbi\u00f3zy','je autotrofno-saprofytick\u00fd sp\u00f4sob v\u00fd\u017eivy','je typick\u00e1 pre m\u00e4so\u017erav\u00e9 rastliny','znamen\u00e1, \u017ee rastliny sa vy\u017eivuj\u00fa anorganick\u00fdmi l\u00e1tkami a m\u00f4\u017eu s\u00fa\u010dasne prij\u00edma\u0165 aj organick\u00e9 l\u00e1tky',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
