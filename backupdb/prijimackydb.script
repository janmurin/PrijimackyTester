INSERT INTO BIOLOGIA VALUES(560,'560. Detsk\u00fd chrup m\u00e1:','20 zubov, prv\u00e9 sa pravidelne prerez\u00e1vaj\u00fa stoli\u010dky vo veku okolo jedn\u00e9ho roka','20 zubov, prv\u00e9 sa oby\u010dajne prerez\u00e1vaj\u00fa rez\u00e1ky vo veku okolo 6. mesiaca','22 zubov, prv\u00e9 sa oby\u010dajne prerez\u00e1vaj\u00fa stoli\u010dky vo veku okolo jedn\u00e9ho roka','20 zubov: 8 rez\u00e1kov, 4 o\u010dn\u00e9 zuby a 8 stoli\u010diek','18 zubov, prv\u00e9 sa pravidelne prerez\u00e1vaj\u00fa rez\u00e1ky vo veku okolo 6. mesiaca','v\u0161etky zuby do\u010dasn\u00e9, tzv. mlie\u010dne','20 zubov: 8 rez\u00e1kov, 4 \u010drenov\u00e9 zuby a 8 stoli\u010diek','24 zubov, prv\u00e9 sa oby\u010dajne prerez\u00e1vaj\u00fa rez\u00e1ky vo veku okolo 6. mesiaca',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
