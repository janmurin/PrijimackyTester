INSERT INTO BIOLOGIA VALUES(923,'923. Replik\u00e1cia DNA sa uskuto\u010d\u0148uje pod\u013ea:','poradia b\u00e1z oboch jej oddelen\u00fdch vl\u00e1kien','jednovl\u00e1knov\u00fdch matr\u00edc DNA, ka\u017ed\u00e1 z nov\u00fdch molek\u00fal DNA m\u00e1 obe vl\u00e1kna novo nasyntetizovan\u00e9','dvojvI\u00e1knov\u00fdch matr\u00edc DNA, ka\u017ed\u00e1 z nov\u00fdch molek\u00fal m\u00e1 po jednom novom vl\u00e1kne','dvojvl\u00e1knov\u00fdch matr\u00edc DNA, ka\u017ed\u00e1 z nov\u00fdch molek\u00fal DNA m\u00e1 obe komplement\u00e1rne vl\u00e1kna novo nasyntetizovan\u00e9','dvoch jednovl\u00e1knov\u00fdch matr\u00edc DNA, ka\u017ed\u00e1 z nov\u00fdch molek\u00fal m\u00e1 po jednom novo nasyntetizovanom vl\u00e1kne','dvojvl\u00e1knov\u00fdch matr\u00edc DNA, ka\u017ed\u00e1 z nov\u00fdch molek\u00fal m\u00e1 po jednom novom vl\u00e1kne, proces je riaden\u00fd DNA polymer\u00e1zou','dvojvl\u00e1knov\u00fdch matr\u00edc RNA, ka\u017ed\u00e1 z nov\u00fdch molek\u00fal m\u00e1 po jednom novom vl\u00e1kne, proces je riaden\u00fd RNA polymer\u00e1zou','dvoch dvojvl\u00e1knov\u00fdch matr\u00edc DNA',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
