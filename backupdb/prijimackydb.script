INSERT INTO BIOLOGIA VALUES(665,'665. Srdcov\u00fd cyklus \u010dloveka je:','proces postupn\u00e9ho \u0161\u00edrenia podnetov srdcov\u00fdm vodiv\u00fdm tkanivom v priebehu diastoly','za norm\u00e1lnych okolnost\u00ed opakovan\u00fd v pravidelnom rytme','cel\u00fd proces od naplnenia predsien\u00ed krvou a\u017e po vypudenie krvi z kom\u00f4r do obehu','celkov\u00e9 mno\u017estvo krvi, ktor\u00e9 srdce pre\u010derp\u00e1 za jednu min\u00fatu','po\u010det kontrakci\u00ed srdca za jednu min\u00fatu','riaden\u00fd auton\u00f3mnym nervov\u00fdm syst\u00e9mom a vodivou svalovinou srdca','posluchov\u00fd n\u00e1lez nad bij\u00facim srdcom','vlastn\u00fd krvn\u00fd obeh srdca tvoren\u00fd vencovit\u00fdmi cievami',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(666,'666. Srdcov\u00e9 chlopne:','s\u00fa na za\u010diatku p\u013e\u00facnych \u017e\u00edl','zabra\u0148uj\u00fa sp\u00e4tn\u00e9mu toku krvi','s\u00fa c\u00edpovit\u00e9 a polmesia\u010dikovit\u00e9','zabra\u0148uj\u00fa mie\u0161aniu okysli\u010denej a odkysli\u010denej krvi','s\u00fa medzi predsie\u0148ami','s\u00fa medzi komorami','zabra\u0148uj\u00fa sp\u00e4tn\u00e9mu toku krvi z predsien\u00ed do kom\u00f4r a z ve\u013ek\u00fdch tepien do kom\u00f4r','zabra\u0148uj\u00fa sp\u00e4tn\u00e9mu toku krvi z kom\u00f4r do predsien\u00ed a z ve\u013ek\u00fdch tepien do kom\u00f4r',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(667,'667. \u010cinnos\u0165 srdca sa m\u00f4\u017ee prejavi\u0165:','zmenou elektrick\u00fdch pr\u00fadov, ktor\u00e9 sa nedaj\u00fa mera\u0165','srdcov\u00fdmi ozvami','tepom \u2014 u zdrav\u00e9ho \u010dloveka je po\u010det tepov 70/min','zmenou elektrick\u00fdch pr\u00fadov, ktor\u00e9 sa zaznamen\u00e1vaj\u00fa ako EKG','vazokonstrikciou','tepom, pri\u010dom u \u017eien je tepov\u00e1 frekvencia ni\u017e\u0161ia ako u mu\u017eov','systolickou a diastolickou ozvou','zmenou elektrick\u00fdch pr\u00fadov, ktor\u00e9 sa zaznamen\u00e1vaj\u00fa ako EEG',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
