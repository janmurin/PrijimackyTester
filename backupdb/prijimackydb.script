INSERT INTO BIOLOGIA VALUES(676,'676. Srdcov\u00e1 svalovina:','sa ozna\u010duje ako epikard','sa ozna\u010duje ako endokard','m\u00e1 schopnos\u0165 kontrakcie','pracuje vo f\u00e1zach \u2014 systola a diastola','sa zmr\u0161\u0165uje rytmicky','sa zmr\u0161\u0165uje a neochabuje','je vy\u017eivovan\u00e1 vodivou svalovinou srdca','sa zmr\u0161\u0165uje arytmicky',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(677,'677. Podstatou infarktu myokardu je:','upchatie niektorej z vencovit\u00fdch tepien','nedomykavos\u0165 chlopn\u00ed','upchatie srdcovnice','upchatie p\u013e\u00facnice','upchatie niektorej z predsien\u00ed alebo kom\u00f4r','m\u00e1lokrvnos\u0165','z\u00e1pal myokardu','upchatie niektorej z koron\u00e1mych tepien',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(678,'678. \u017dily na rozdiel od tepien \u010dloveka:','priv\u00e1dzaj\u00fa krv do srdca','odv\u00e1dzaj\u00fa krv zo srdca','maj\u00fa tenk\u00fa vrstvu svaloviny','maj\u00fa hrub\u00fa vrstvu svaloviny','s\u00fa viac elastick\u00e9 ako tepny','s\u00fa menej elastick\u00e9 ako tepny','vstupuj\u00fa do srdca','vystupuj\u00fa zo srdca',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(679,'679. V d\u00f4sledku straty pru\u017enosti stien ciev v starobe \u010dloveka:','sa zni\u017euje systolick\u00fd krvn\u00fd tlak','sa zvy\u0161uje systolick\u00fd krvn\u00fd tlak','vznik\u00e1 hypotenzn\u00e9 ochorenie','vznik\u00e1 hypertenzn\u00e9 ochorenie','sa zvy\u0161uj\u00fa hodnoty systolick\u00e9ho tlaku nad 20 kPa','sa zvy\u0161uj\u00fa hodnoty systolick\u00e9ho tlaku nad 160 torr','sa zvy\u0161uj\u00fa hodnoty diastolick\u00e9ho tlaku nad 160 torr','sa zvy\u0161uj\u00fa hodnoty diastolick\u00e9ho tlaku nad 20 kPa',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(680,'680. Na pohybe krvi v \u017eil\u00e1ch sa podie\u013ea:','tlak sp\u00f4soben\u00fd systolou \u013eavej komory','sacia sila srdca','v zna\u010dnej miere tlak krvi v \u017eil\u00e1ch','v malej miere tlak krvi v \u017eil\u00e1ch','svalov\u00e1 pumpa','stl\u00e1\u010danie \u017e\u00edl prie\u010dne pruhovan\u00fdmi svalmi','stl\u00e1\u010danie \u017e\u00edl hladk\u00fdmi svalmi','d\u00fdchanie \u2014 po\u010das n\u00e1dychu st\u00fapa vn\u00fatrohrudn\u00edkov\u00fd tlak a krv je nas\u00e1van\u00e1 do dut\u00fdch \u017e\u00edl a pravej komory',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(681,'681. U \u010dloveka pritek\u00e1 krv do p\u013e\u00fac bezprostredne z:','p\u013e\u00facnych \u017e\u00edl','\u013eavej komory','p\u013e\u00facnice, ktor\u00e1 odstupuje z pravej komory','pravej predsiene a p\u013e\u00facnice','vencovit\u00fdch tepien','p\u013e\u00facnice, ktor\u00e1 odstupuje z \u013eavej komory','aorty, ktor\u00e1 odstupuje z \u013eavej komory','\u013eavej komory a p\u013e\u00facnice',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(682,'682. Okysli\u010den\u00fa krv z p\u013e\u00fac \u010dloveka odv\u00e1dzaj\u00fa:','p\u013e\u00facnica','dve p\u013e\u00facne \u017eily, ktor\u00e9 \u00fastia do \u013eavej predsiene','\u0161tyri p\u013e\u00facne tepny','p\u013e\u00facne \u017eily','\u0161tyri p\u013e\u00facne \u017eily, ktor\u00e9 \u00fastia do pravej predsiene','\u0161tyri p\u013e\u00facne \u017eily, ktor\u00e9 \u00fastia do \u013eavej predsiene','p\u013e\u00facnica, ktor\u00e1 \u00fasti do \u013eavej predsiene','p\u013e\u00facne tepny, ktor\u00e9 \u00fastia do \u013eavej predsiene',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(683,'683. Svalov\u00e1 vrstva v stene ciev:','sa naz\u00fdva epikard','sa naz\u00fdva myokard','je v \u017eil\u00e1ch hrub\u0161ia ako v tepn\u00e1ch','je v tepn\u00e1ch hrub\u0161ia ako v \u017eil\u00e1ch','je z prie\u010dne pruhovan\u00fdch svalov','je z hladk\u00fdch svalov','je \u0161pecifick\u00e1','je v \u017eil\u00e1ch ten\u0161ia ako v tepn\u00e1ch',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(684,'684. V p\u013e\u00facnych \u017eil\u00e1ch \u010dloveka pr\u00fadi krv:','odkysli\u010den\u00e1','smerom z p\u013e\u00fac do pravej srdcovej predsiene','okysli\u010den\u00e1 z p\u013e\u00fac do pravej srdcovej predsiene','okysli\u010den\u00e1 z p\u013e\u00fac do \u013eavej srdcovej predsiene','odkysli\u010den\u00e1 do pravej srdcovej predsiene','okysli\u010den\u00e1','okysli\u010den\u00e1 z \u013eavej srdcovej komory do p\u013e\u00fac','odkysli\u010den\u00e1 z \u013eavej srdcovej komory do p\u013e\u00fac',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(685,'685. Srdcov\u00fd infarkt u \u010dloveka vznik\u00e1:','krv\u00e1can\u00edm do srdcov\u00e9ho svalu','naj\u010dastej\u0161ie ako d\u00f4sledok art\u00e9rioskler\u00f3zy','po preru\u0161en\u00ed dod\u00e1vky kysl\u00edka a/alebo \u017eiv\u00edn do srdcovej svaloviny','odtrhnut\u00edm koron\u00e1rnej tepny od p\u013e\u00facnice','uzavret\u00edm koron\u00e1rnej tepny alebo jej vetvy','pri prekro\u010den\u00ed vn\u00fatorn\u00e9ho srdcov\u00e9ho objemu','uzavret\u00edm vencovitej tepny alebo jej vetvy','odumret\u00edm \u010dasti svaloviny srdca',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(686,'686. Arterioskler\u00f3za:','je zhrubnutie tepnovej steny sp\u00f4soben\u00e9 ukladan\u00edm polysacharidov','vznik\u00e1 v d\u00f4sledku zn\u00ed\u017een\u00e9ho z\u00e1sobovania tkan\u00edv kysl\u00edkom','je sprievodn\u00fdm javom starnutia','m\u00e1 za n\u00e1sledok zn\u00ed\u017een\u00fd prietok krvi tepnami','je hypertenzn\u00e9 ochorenie','je degenerat\u00edvne ochorenie tepien','je strata pam\u00e4te','je degenerat\u00edvne ochorenie \u017e\u00edl',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(687,'687. Prevodov\u00fd syst\u00e9m srdca u \u010dloveka:','je tvoren\u00fd vencovit\u00fdmi tepnami','zabezpe\u010duje y\u00fd\u017eivu srdca','je \u0161peci\u00e1lne tkanivo, ktor\u00e9 sa stavbou podob\u00e1 svalov\u00e9mu a funk\u010dne nervov\u00e9mu','tvor\u00ed tzv. vodiv\u00e1 svalovina srdca','je \u0161peci\u00e1lne nervov\u00e9 tkanivo tvoren\u00e9 neur\u00f3nmi','je \u0161peci\u00e1lny obehov\u00fd syst\u00e9m srdca, ktor\u00fd m\u00e1 vlastnosti svaloviny a nervov\u00e9ho tkaniva','vytla\u010d\u00ed ka\u017ed\u00fdm tepom 70 ml krvi do tela','vysiela asi 70 elektrick\u00fdch impulzov za min\u00fatu',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(688,'688. Miazgov\u00e1 s\u00fastava \u010dloveka:','je anatomicky vetven\u00e1 odli\u0161ne od srdcovocievneho syst\u00e9mu','z\u00e1sobuje tkaniv\u00e1 tkanivov\u00fdm mokom','je tvoren\u00e1 miazgov\u00fdmi cievami','odv\u00e1dza z tkan\u00edv nadbyto\u010dn\u00fd tkanivov\u00fd mok','predstavuje podporn\u00fd dren\u00e1\u017eny syst\u00e9m, ktor\u00fdm pr\u00fadi lymfa','m\u00e1 vlastn\u00e9 srdce','vytv\u00e1ra vo svojich cievach vysok\u00fd tlak','odv\u00e1dza z tkan\u00edv odpadov\u00e9 l\u00e1tky',TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(689,'689. Lymfa \u010dloveka:','m\u00e1 podobn\u00e9 zlo\u017eenie ako krvn\u00e1 plazma','obsahuje krvinky','neobsahuje krvinky','z\u00e1sobuje tkaniv\u00e1 v\u00fd\u017eivn\u00fdmi l\u00e1tkami','je filtrovan\u00e1 v miazgov\u00fdch uzlin\u00e1ch','pr\u00fadi do tkan\u00edv pod n\u00edzkym tlakom','obsahuje imunitn\u00e9 bunky','pr\u00fadi v miazgov\u00fdch cievach',TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(690,'690. Miazgov\u00e9 uzliny:','produkuj\u00fa miazgu','filtruj\u00fa miazgu','filtruj\u00fa krv','produkuj\u00fa lymfocyty','s\u00fa napr. mandle','zachyt\u00e1vaj\u00fa cudzorod\u00e9 mikroorganizmy','zachyt\u00e1vaj\u00fa tox\u00edny a odpadov\u00e9 l\u00e1tky','s\u00fa s\u00fa\u010dastou cievnej s\u00fastavy',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(691,'691. Miazga \u010dloveka:','vznik\u00e1 filtr\u00e1ciou v miazgov\u00fdch uzlin\u00e1ch','odv\u00e1dza \u010das\u0165 tkanivov\u00e9ho moku do krvi','vznik\u00e1 v kapil\u00e1rach','obieha v uzavretom miazgovom rie\u010disku','obsahuje ve\u013ea lymfocytov','je mimobunkovou tekutinou','norm\u00e1lne neobsahuje erytrocyty','obsahuje v\u00e4\u010d\u0161ie mno\u017estvo bielkov\u00edn ako krvn\u00e1 plazma',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(692,'692. Aglutin\u00edny \u010dloveka s\u00fa:','antig\u00e9ny erytrocytov','hociktor\u00e9 protil\u00e1tky v krvnej plazme','aglutinog\u00e9ny leukocytov','produkty aglutinog\u00e9nov','spoluzodpovedn\u00e9 za zhlukovanie erytrocytov pri nespr\u00e1vnej transf\u00fazii','ur\u010dit\u00fdm druhom protil\u00e1tok v krvi','pou\u017e\u00edvan\u00e9 pri vy\u0161etren\u00ed krvnej skupiny AB0 krvn\u00e9ho syst\u00e9mu','antig\u00e9ny pr\u00edtomn\u00e9 na bunk\u00e1ch \u017elt\u00e9ho telieska',FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(693,'693. Aglutin\u00edny \u010dloveka:','s\u00fa antig\u00e9ny \u010derven\u00fdch krviniek','s\u00fa protil\u00e1tky v krvnej plazme, ktor\u00e9 zhlukuj\u00fa napr. \u010derven\u00e9 krvinky inej krvnej skupiny','vznikaj\u00fa pri zr\u00e1\u017ean\u00ed krvi premenou rozpustn\u00fdch aglutinog\u00e9nov krvnej plazmy','sa uvo\u013e\u0148uj\u00fa z gran\u00fal neutrofiln\u00fdch leukocytov','s\u00fa protil\u00e1tky v krvnej plazme, ktor\u00e9 zhlukuj\u00fa napr. bakt\u00e9rie','s\u00fa antig\u00e9ny v krvnej plazme','patria medzi globul\u00edny','patria medzi album\u00edny',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(694,'694. Aglutinog\u00e9ny \u013eudskych \u010derven\u00fdch krviniek:','s\u00fa z\u00edskan\u00e9','nie s\u00fa dedi\u010dn\u00e9','s\u00fa vroden\u00e9 a dedi\u010dn\u00e9','sa menia po\u010das \u017eivota','sa z\u00edskavaj\u00fa iba po transf\u00fazii','vyvol\u00e1vaj\u00fa tvorbu protil\u00e1tok','patria medzi plazmatick\u00e9 bielkoviny','s\u00fa zodpovedn\u00e9 za rozl\u00ed\u0161enie krvn\u00fdch skup\u00edn',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(695,'695. Krvn\u00e9 skupiny \u010dloveka s\u00fa podmienen\u00e9:','typom antig\u00e9nu pr\u00edtomn\u00e9ho v krvnej plazme','schopnos\u0165ou erytrocytov tvori\u0165 protil\u00e1tky','individu\u00e1lnym typom plazmatick\u00fdch bielkov\u00edn u ka\u017ed\u00e9ho jedinca','pr\u00edtomnos\u0165ou \u0161pecifick\u00fdch antig\u00e9nov v membr\u00e1ne \u010derven\u00fdch krviniek','pr\u00edtomnos\u0165ou protil\u00e1tok v krvnej plazme','geneticky','pr\u00edtomnos\u0165ou imunoglobul\u00ednov v membr\u00e1ne \u010derven\u00fdch krviniek','pr\u00edtomnos\u0165ou gamaglobul\u00ednov v membr\u00e1ne \u010derven\u00fdch krviniek',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(696,'696. Krvn\u00e1 plazma \u010dloveka s krvnou skupinou A obsahuje:','antig\u00e9n A','ant\u00edg\u00e9n B','aglutin\u00edn anti-A','aglutin\u00edn anti-B','protil\u00e1tku anti-A','protil\u00e1tku anti-B','antig\u00e9ny A, B','aglutinog\u00e9n anti-A',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(697,'697. Krv \u010dloveka s krvnou skupinou AB neobsahuje:','antig\u00e9n B','antig\u00e9n A','antig\u00e9n A, B','protil\u00e1tku anti-A','protil\u00e1tku anti-B','aglutin\u00edny proti vlastn\u00fdm aglutinog\u00e9nom','aglutin\u00edn anti-A','aglutin\u00edn anti-B',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(698,'698. Ktor\u00e1 kombin\u00e1cia krvn\u00fdch skup\u00edn u jedinca je v na\u0161ej popul\u00e1cii naj\u010dastej\u0161ia:','AB, Rh^+','A, Rh^-','B, Rh^+','A, Rh+','0, Rh^-','0, Rh^+','AB, Rh^-','B, Rh^-',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(699,'699. \u010clovek s krvnou skupinou B m\u00e1:','aglutinog\u00e9n B a aglutin\u00edn anti-A','aglutinog\u00e9n A a aglutin\u00edn anti-B','aglutinog\u00e9n B v krvnej plazme a aglutin\u00edn anti-A na povrchu erytrocytov','aglutinog\u00e9n A na povrchu erytrocytov a aglutin\u00edn anti-B v krvnej plazme','aglutinog\u00e9n B na povrchu erytocytov a aglutin\u00edn anti-A v krvnej plazme','aglutinog\u00e9n B a \u017eiadny aglutin\u00edn','aglutinog\u00e9n AB na povrchu erytrocytov','aglutin\u00edn anti-A a anti-B v krvnej plazme',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(700,'700. \u010clovek s krvnou skupinou 0:','m\u00e1 aglutinog\u00e9ny A a B, ktor\u00e9 sa nach\u00e1dzaj\u00fa v krvnej plazme','nem\u00e1 \u017eiadne aglutin\u00edny','m\u00e1 aglutin\u00edny anti-A i anti-B, ktor\u00e9 sa nach\u00e1dzaj\u00fa v krvnej plazme','m\u00e1 aglutin\u013any anti-A a anti-B, kt\u00f3r\u00e9 sa nach\u00e1dzaj\u00fa na povrchu erytrocytov','m\u00e1 antig\u00e9ny A aj B','m\u00e1 protil\u00e1tky proti antig\u00e9nom A aj B','m\u00e1 v s\u00e9re aglutin\u00edny anti-A i anti-B','m\u00f4\u017ee ma\u0165 na povrchu erytrocytov in\u00e9 antig\u00e9ny',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(701,'701. Osoba s krvnou skupinou AB:','m\u00e1 antig\u00e9ny A aj B','nem\u00e1 aglutin\u00edny anti-A a anti-B','m\u00e1 aglutin\u00edn anti-A','m\u00e1 aglutin\u00edn anti-B','m\u00e1 aglutin\u00edny anti-A aj anti-B','nem\u00f4\u017ee dosta\u0165 nikdy transf\u00e1ziu krvi','nem\u00e1 protil\u00e1tky anti-A a anti-B','je v\u017edy Rh pozit\u00edvna',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(702,'702. Ktor\u00fa z uveden\u00fdch transf\u00fazi\u00ed mo\u017eno vykona\u0165 bez toho, aby bol ohrozen\u00fd \u017eivot pr\u00edjemcu:','krv 0 Rh- je podan\u00e1 pr\u00edjemcovi s krvnou skupinou 0 Rh+','krv B Rh+ je podan\u00e1 prijemcovi s krvnou skupinou A Rh+','krv A Rh- je podan\u00e1 pr\u00edjemcovi s krvnou skupinou AB Rh-','krv B Rh- je podan\u00e1 pr\u00edjemcovi s krvnou skupinou AB Rh-','krv A Rh+ je podan\u00e1 pr\u00edjemcovi s krvnou skupinou A Rh-','krv 0 Rh+ je podan\u00e1 pr\u00edjemcovi s krvnou skupinou B Rh-','krv AB Rh- je podan\u00e1 pr\u00edjemcovi s krvnou skupinou 0 Rh-','krv A Rh- je podan\u00e1 pr\u00edjemcovi s krvnou skupinou A Rh+',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(703,'703. O krvnom syst\u00e9me Rh plat\u00ed, \u017ee:','jeho ozna\u010denie je odvoden\u00e9 od opice vre\u0161\u0165ana hrdzav\u00e9ho','protil\u00e1tky anti-Rh nie s\u00fa vroden\u00e9','protil\u00e1tky anti-Rh s\u00fa vroden\u00e9','protil\u00e1tka anti-Rh sa za\u010dne tvori\u0165, ak sa do krvi Rh negat\u00edvneho \u010dloveka dostan\u00fa Rh pozit\u00edvne krvinky','protil\u00e1tka anti-Rh sa za\u010dne tvori\u0165, ak sa do krvi Rh pozit\u00edvneho \u010dloveka dostan\u00fa Rh negat\u00edvne krvinky','v tomto syst\u00e9me existuj\u00fa prirodzen\u00e9 protil\u00e1tky','protil\u00e1tky v tomto syst\u00e9me vznikaj\u00fa a\u017e imuniz\u00e1ciou','protil\u00e1tky anti-Rh vznikaj\u00fa v tele Rh+ matky proti Rh- plodu',TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(704,'704. \u0160pecifick\u00e1 imunita \u013eudsk\u00e9ho organizmu:','je zalo\u017een\u00e1 na schopnosti organizmu tolerova\u0165 cudzorod\u00fd antig\u00e9nny materi\u00e1l','sa u ka\u017ed\u00e9ho rozv\u00edja individu\u00e1lne po\u010das ontogen\u00e9zy','je zabezpe\u010dovan\u00e1 hlavne aktivitou fagocytuj\u00facich buniek','m\u00f4\u017ee by\u0165 bunkov\u00e1 zabezpe\u010dovan\u00e1 T-lymfocytmi','je zabezpe\u010dovan\u00e1 hlavne lymfocytmi','zah\u0155\u0148a tvorbu protil\u00e1tok','je u v\u0161etk\u00fdch zdrav\u00fdch \u013eud\u00ed namieren\u00e1 proti rovnak\u00fdm infekci\u00e1m','je zabezpe\u010dovan\u00e1 monocytmi',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(705,'705. Trval\u00e1 imunita z\u00edskan\u00e1 po prekonan\u00ed niektor\u00fdch infek\u010dn\u00fdch ochoren\u00ed je:','v\u017edy ne\u0161pecifick\u00e1','\u0161pecifick\u00e1','napr\u00edklad protil\u00e1tkov\u00e1','pas\u00edvna','akt\u00edvna','zabezpe\u010dovan\u00e1 neutrofiln\u00fdmi granulocytmi','zabezpe\u010dovan\u00e1 Blymfocytmi, ktor\u00e9 sa aktivovali pri styku s antig\u00e9nom','zabezpe\u010dovan\u00e1 pam\u00e4\u0165ov\u00fdmi bunkami',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(706,'706. K prejavom ne\u0161pecifickej imunity \u010dloveka patr\u00ed:','o\u010dkovanie','tvorba protil\u00e1tok','fagocyt\u00f3za','alergia','imuniz\u00e1cia','enzymatick\u00e9 usmrcovanie bakt\u00e9ri\u00ed v slz\u00e1ch a slin\u00e1ch','p\u00f4sobenie lyz\u00ednu','aktivita lyzoz\u00fdmu',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(707,'707. Za chorobn\u00e9 sa u dospel\u00e9ho \u010dloveka pova\u017euj\u00fa hodnoty:','systolick\u00e9ho tlaku nad 13,3 kPa','systolick\u00e9ho tlaku nad 20 kPa','systolick\u00e9ho tlaku nad 100 torr','systolick\u00e9ho tlaku nad 160 torr','diastolick\u00e9ho tlaku nad 13,3 kPa','diastolick\u00e9ho tlaku pod 13,3 kPa','diastolick\u00e9ho tlaku nad 100 torr, vtedy hovor\u00edme o hypertenznom ochoren\u00ed','diastolick\u00e9ho tlaku nad 160 torr, vtedy hovor\u00edme o hypotenznom ochoren\u00ed',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(708,'708. Srdcov\u00fd infarkt u \u010dloveka:','vznik\u00e1 v d\u00f4sledku upchatia srdcovnice','vznik\u00e1 v hypertenznom ochoren\u00ed','vznik\u00e1 v d\u00f4sledku prasknutia vencovit\u00fdch tepien','vznik\u00e1 v d\u00f4sledku upchatia vencovit\u00fdch tepien','vznik\u00e1 v d\u00f4sledku upchatia koron\u00e1rnych tepien','vznik\u00e1 v d\u00f4sledku upchatia prevodov\u00e9ho syst\u00e9mu srdca','v\u00e4\u010d\u0161ieho rozsahu m\u00f4\u017ee sp\u00f4sobi\u0165 smr\u0165 jedinca','je \u010dastej\u0161\u00ed u \u017eien ako u mu\u017eov',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(709,'709. Rizikov\u00fdmi faktormi ateroskler\u00f3zy s\u00fa:','bul\u00edmia','vysok\u00fd tlak krvi','faj\u010denie','infarkt myokardu','zn\u00ed\u017een\u00e1 \u010dinnos\u0165 obli\u010diek','obezita','an\u00e9mia','zv\u00fd\u0161en\u00e1 sedimet\u00e1cia',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(710,'710. K d\u00fdchacej s\u00fastave \u010dloveka patria:','pa\u017eer\u00e1k','hltan','p\u013e\u00faca','d\u00fdchacie cesty','pharynx','hrtan','priedu\u0161nica','larynx',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(711,'711. D\u00fdchanie \u010dloveka:','je riaden\u00e9 d\u00fdchac\u00edm centrom ulo\u017een\u00fdm v mieche','je riaden\u00e9 d\u00fdchac\u00edm centrom ulo\u017een\u00fdm v pred\u013a\u017eenej mieche','je riaden\u00e9 d\u00fdchac\u00edm centrom','je riaden\u00e9 d\u00fdchac\u00edm centrom v p\u013e\u00facach','sa prisp\u00f4sobuje potreb\u00e1m organizmu automaticky','je riaden\u00e9 d\u00fdchac\u00edm centrom, ktor\u00e9 je ovplyv\u0148ovan\u00e9 nervov\u00fdmi podnetmi','je riaden\u00e9 d\u00fdchac\u00edm centrom, ktor\u00e9 je ovplyv\u0148ovan\u00e9 chemick\u00fdmi podnetmi','del\u00edme na horn\u00e9 a doln\u00e9 d\u00fdchanie',FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(712,'712. Prav\u00e9 p\u013e\u00faca \u010dloveka:','maj\u00fa dva laloky','maj\u00fa tri laloky','s\u00fa pokryt\u00e9 pohrudnicou','s\u00fa pokryt\u00e9 pop\u013e\u00facnicou','s\u00fa zrkadlov\u00fdm obrazom \u013eav\u00fdch p\u013e\u00fac','maj\u00fa o jeden lalok menej ako \u013eav\u00e9','maj\u00fa o jeden lalok viac ako \u013eav\u00e9','maj\u00fa o dva laloky viac ako \u013eav\u00e9',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(713,'713. O d\u00fdchac\u00edch cest\u00e1ch \u010dloveka plat\u00ed:','sa delia na horn\u00e9 a doln\u00e9','sa delia na vonkaj\u0161ie a vn\u00fatorn\u00e9','s\u00fa vlastn\u00fdm org\u00e1nom vonkaj\u0161ieho d\u00fdchania','horn\u00e9 d\u00fdchacie cesty tvor\u00ed nosov\u00e1 dutina a hltan','horn\u00e9 d\u00fdchacie cesty tvor\u00ed nosov\u00e1 dutina a nosohltan','k nim patria p\u013e\u00faca, ktor\u00e9 s\u00fa rozdelen\u00e9 z\u00e1rezmi na laloky','k nim patria priedu\u0161ky, ktor\u00e9 s\u00fa pokryt\u00e9 respira\u010dn\u00fdm epitelom','priedu\u0161ky s\u00fa pokryt\u00e9 riasinkov\u00fdm epitelom, pri\u010dom riasink\u00fd kmitaj\u00fa smerom k p\u013e\u00facam',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(714,'714. Pri pokojnom d\u00fdchan\u00ed sa za min\u00fatu v p\u013e\u00facach dospel\u00e9ho \u010dloveka vymen\u00ed pribli\u017ene:','500 ml vzduchu','16 \u2014 18 l vzduchu','7 \u2014 9 l vzduchu','u mu\u017eov 3 500 a\u017e 5 000 ml vzduchu','u \u017eien 2 500 a\u017e 4 000 ml vzduchu','250 ml vzduchu','7 000 a\u017e 9 000 ml vzduchu','7 \u2014 9 ml vzduchu',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(715,'715. K v\u00fdmene d\u00fdchac\u00edch plynov u \u010dloveka doch\u00e1dza:','v horn\u00fdch d\u00fdchac\u00edch cest\u00e1ch','v doln\u00fdch d\u00fdchac\u00edch cest\u00e1ch','v p\u013e\u00facnych mech\u00farikoch \u2014 ide o vonkaj\u0161ie d\u00fdchanie','v p\u013e\u00facnych mech\u00farikoch \u2014 ide o vn\u00fatorn\u00e9 d\u00fdchanie','medzi krvou a bunkami \u2014 ide o vonkaj\u0161ie d\u00fdchanie','medzi krvou a bunkami \u2014 ide o vn\u00fatom\u00e9 d\u00fdchanie','v p\u013e\u00facach \u2014 ide o vn\u00fatorn\u00e9 d\u00fdchanie','v p\u013e\u00facnych mech\u00far\u00edkoch \u2014 ide o tkanivov\u00e9 d\u00fdchanie',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(716,'716. Vniknutie atmosf\u00e9rick\u00e9ho vzduchu medzi pohrudnicu a pop\u013e\u00facnicu u \u010dloveka vol\u00e1me:','\u0161ok','pneumothorax','kolaps','kes\u00f3nov\u00e1 choroba','p\u013e\u00facna ventil\u00e1cia','faryngit\u00edda','pohrudnicov\u00fd infarkt','vonkaj\u0161ie d\u00fdchanie',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(717,'717. Pohrudnica \u010dloveka:','je jemn\u00e1 v\u00e4zivov\u00e1 blana','je jemn\u00e1 v\u00e4zivov\u00e1 blana, ktor\u00e1 rozdefuje p\u013e\u00faca na laloky','je hrudn\u00edkov\u00e1 \u010das\u0165 aorty','povlieka aj srdce a p\u013e\u00faca','dot\u00fdka sa p\u013e\u00fac povle\u010den\u00fdch pop\u013e\u00facnicou','je synonymum pre pop\u013e\u00facnicu','vystiela vn\u00fatorn\u00fa plochu hrudn\u00edkovej dutiny','prebieha z bru\u0161nej dutiny do hrudn\u00edka',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(718,'718. Pop\u013e\u00facnica \u010dloveka:','je pokra\u010dovan\u00edm p\u013e\u00facnice po vnoren\u00ed do p\u013e\u00fac','je z\u00e1palov\u00e9 ochorenie povrchu p\u013e\u00fac','je jemn\u00e1 elastick\u00e1 chrupka na povrchu p\u013e\u00fac','je jemn\u00e1 v\u00e4zivov\u00e1 blana povliekaj\u00faca p\u013e\u00faca','je svalov\u00e1 prep\u00e1\u017eka odde\u013euj\u00faca p\u013e\u00faca od bru\u0161nej dutiny','je pokra\u010dovan\u00edm priedu\u0161iek','prech\u00e1dza na vn\u00fatorn\u00fa plochu hrudn\u00edkovej dutiny ako pohrudnica','prech\u00e1dza na povrch srdca ako pohrudnica',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(719,'719. Pop\u013e\u00facnica a pohrudnica \u010dloveka:','s\u00fa dvoma \u010das\u0165ami tej istej blany','spolu ohrani\u010duj\u00fa a uzavieraj\u00fa \u0161trbinov\u00fa pohrudnicov\u00fa dutinu','s\u00fa klzk\u00e9 a leskl\u00e9','s\u00fa dve odli\u0161n\u00e9 cievy','s\u00fa dve r\u00f4zne \u010dasti tej istej cievy','sa navz\u00e1jom dot\u00fdkaj\u00fa ve\u013ekou \u010das\u0165ou svojho povrchu','k\u013a\u017eu sa po sebe pri d\u00fdchac\u00edch pohyboch','vylu\u010duj\u00fa maz\u013eav\u00fa \u010d\u00edru tekutinu',TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(720,'720. Vdych \u010dloveka zabezpe\u010duj\u00fa:','respira\u010dn\u00e9 epitely','steny p\u013e\u00facnych alveol','vonkaj\u0161ie medzirebrov\u00e9 svaly a pohyb br\u00e1nice nadol','vn\u00fatorn\u00e9 medzirebrov\u00e9 svaly a pohyb br\u00e1nice nahor','vonkaj\u0161ie medzirebrov\u00e9 svaly a pohyb br\u00e1nice nahor','vonkaj\u0161ie a vn\u00fatorn\u00e9 medzirebrov\u00e9 svaly','vn\u00fatorn\u00e9 medzirebrov\u00e9 svaly a pohyb br\u00e1nice nadol','svaly nosohltanu, hrtanu, chrbtov\u00e9 svalstvo, v\u0161etky medzirebrov\u00e9 svaly a br\u00e1nica',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(721,'721. Hrtan:','je s\u00fa\u010das\u0165ou horn\u00fdch d\u00fdchac\u00edch ciest','je s\u00fa\u010das\u0165ou doln\u00fdch d\u00fdchac\u00edch ciest','sl\u00fa\u017ei na respir\u00e1ciu a fon\u00e1ciu','sa na konci rozvetvuje na dve priedu\u0161ky','je spevnen\u00fd s\u00faborom chrupiek, svalov a v\u00e4zov','m\u00e1 pri vchode pr\u00edchlopku \u2014 glottis','m\u00e1 pri vchode pr\u00edchlopku \u2014 epiglottis','je dut\u00fd org\u00e1n na zadnej strane krku',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(722,'722. Vstup do hrtanu uzatv\u00e1ra:','hltanov\u00e1 pr\u00edchlopka','hrtanov\u00e1 pr\u00edchlopka','vr\u00e1tnikov\u00fd kruhov\u00fd zviera\u010d','epiglottis','glottis','chrupkovit\u00e1 pr\u00edchlopka','hrtanov\u00e1 pr\u00edchlopka, ktorej z\u00e1klad tvor\u00ed hyal\u00ednna chrupka','hrtanov\u00e1 pr\u00edchlopka, ktorej z\u00e1klad tvor\u00ed elastick\u00e1 chrupka',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(723,'723. K doln\u00fdm d\u00fdchac\u00edm cest\u00e1m patria:','p\u013e\u00faca','nosohltan','priedu\u0161ky','hltan','hrtan','pulmo','trachea','riasinky',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(724,'724. Norm\u00e1lna dychov\u00e1 frekvencia \u010dloveka v pokojovom stave je:','70 dychov za min\u00fatu','40 dychov za min\u00fatu','16 \u2014 18 dychov za min\u00fatu','7 \u2014 8 dychov za min\u00fatu','120 dychov za min\u00fatu','80 \u2014 120 dychov za min\u00fatu','ovplyv\u0148ovan\u00e1 vekom, pohlav\u00edm, v\u00fd\u017eivou','ovplyv\u0148ovan\u00e1 teplotou prostredia, dennou dobou, aktivitou organizmu',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(725,'725. Ozna\u010dte spr\u00e1vne tvrdenie o d\u00fdchan\u00ed \u010dloveka:','min\u00fatov\u00e1 dychov\u00e1 frekvencia \u010dloveka v pokojovom stave je 12 \u2014 13 dychov za min\u00fatu','min\u00fatov\u00e1 dychov\u00e1 frekvencia \u010dloveka v pokojovom stave je 16 \u2014 18 dychov za min\u00fatu','dychov\u00fd objem v pokoji je asi 1,5 l','dychov\u00fd objem v pokoji je asi 500 ml','dychov\u00fd objem je objem vzduchu vytla\u010den\u00fd z p\u013e\u00fac maxim\u00e1lnym v\u00fddychom','vn\u00fatorn\u00e9 d\u00fdchanie sa ozna\u010duje ako tkanivov\u00e9','v\u00fdmena d\u00fdchac\u00edch plynov prebieha od \u00farovne priedu\u0161nice','na v\u00fddychu sa podie\u013eaj\u00fa vn\u00fatorn\u00e9 medzirebrov\u00e9 svaly a pohyb br\u00e1nice nadol',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(726,'726. Striedanie vdychu a v\u00fddychu:','je v\u00fdsledkom zmien objemu hrudn\u00edka','je v\u00fdsledkom zmien tlaku v pohrudnicovej dutine','je dole\u017eit\u00e9 pre v\u00fdmenu vzduchu medzi p\u013e\u00facami a prostred\u00edm','sa ozna\u010duje ako min\u00fatov\u00e1 ventil\u00e1cia','sa meria spirometrom alebo spirografom','patr\u00ed medzi obrann\u00e9 d\u00fdchacie reflexy','sa uskuto\u010d\u0148uje zv\u00e4\u010d\u0161ovan\u00edm a zmen\u0161ovan\u00edm bru\u0161nej dutiny','zabezpe\u010duj\u00fa vdychov\u00e9 a v\u00fddychov\u00e9 svaly',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(727,'727. Objem vzduchu vytla\u010den\u00fd z p\u013e\u00fac maxim\u00e1lnym v\u00fddychom, ktor\u00fd nasleduje po maxim\u00e1lnom vdychu, sa naz\u00fdva:','vit\u00e1lna kapacita p\u013e\u00fac','dychov\u00fd objem','min\u00fatov\u00e1 ventil\u00e1cia','p\u013e\u00facna ventil\u00e1cia','vn\u00fatorn\u00e9 d\u00fdchanie','inspirium','expirium','respir\u00e1cia',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(728,'728. Min\u00fatov\u00e1 ventil\u00e1cia \u010dloveka v k\u013eude:','u mu\u017eov sa pohybuje medzi 3 500 a\u017e 5 000 ml','je 7 - 9 l vzduchu','u \u017eien sa pohybuje medzi 2 500 a\u017e 4 000 ml','je asi 500 ml vzduchu','je mno\u017estvo vzduchu vd\u00fdchnut\u00e9ho a vyd\u00fdchnut\u00e9ho pri pokojovom d\u00fdchan\u00ed za min\u00fatu','sa meria sprirometrom','sa meria sprirografom','je s\u00fa\u010det dychov\u00fdch objemov pri pokojnom d\u00fdchan\u00ed za min\u00fatu',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(729,'729. Ozna\u010dte spr\u00e1vne tvrdenie o \u013eudsk\u00fdch p\u013e\u00facach:','prav\u00e9 p\u013e\u00faca maj\u00fa dva laloky','prav\u00e9 p\u013e\u00faca maj\u00fa tri laloky','na ich povrchu sa nach\u00e1dza pop\u013e\u00facnica','\u013eav\u00e9 p\u013e\u00faca maj\u00fa tri laloky','je to nep\u00e1rov\u00fd org\u00e1n ulo\u017een\u00fd v hrudn\u00edkovej dutine','v p\u013e\u00facach je ni\u017e\u0161\u00ed tlak ako v pohrudnicovej dutine','po vniknut\u00ed atmosf\u00e9rick\u00e9ho vzduchu do p\u013e\u00fac d\u00f4jde k pneumotoraxu','pri vstupe do p\u013e\u00fac sa priedu\u0161nica vetv\u00ed na priedu\u0161ky',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(730,'730. Vit\u00e1lna kapacita \u013eudsk\u00fdch p\u013e\u00fac:','v pokojovom stave je 7 \u2014 9 litrov vzduchu','u mu\u017eov sa pohybuje medzi 3 500 a\u017e 5 000 ml vzduchu','je individu\u00e1lne ve\u013ek\u00e1 u ka\u017ed\u00e9ho jedinca','v pokoji je asi 500 ml vzduchu','je ukazovate\u013eom v\u00fdkonnosti p\u013e\u00fac','je 16 \u2014 18 dychov za min\u00fatu','je v priemere v\u00e4\u010d\u0161ia u mu\u017eov','u \u017eien sa pohybuje medzi 2 500 a\u017e 4 000 ml vzduchu',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(731,'731. Pr\u00edstroj na meranie vit\u00e1lnej kapacity p\u013e\u00fac sa naz\u00fdva:','tomograf','sonograf','spirometer','spirograf','tonometer','refraktometer','potenciometer','voltmeter',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(732,'732. Hlasivkov\u00e9 v\u00e4zy a pr\u00edchlopka odde\u013euj\u00faca d\u00fdchaciu s\u00fastavu od tr\u00e1viacej sa nach\u00e1dza v:','nosovej dutine','nosohltane','hltane','hrtane','priedu\u0161nici','p\u013e\u00facach','\u0161t\u00edtnej chrupke','epiglottis',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(733,'733. Vonkaj\u0161ie d\u00fdchanie \u010dloveka:','je v\u00fdmena d\u00fdchac\u00edch plynov medzi krvou a bunkami cel\u00e9ho tela','je v\u00fdmena d\u00fdchac\u00edch plynov medzi atmosf\u00e9rick\u00fdm vzduchom a krvou','je v\u00fdmena 0_2 a CO_2 medzi krvou a tkanivami formou dif\u00fazie','zah\u0155\u0148a p\u013e\u00facnu ventil\u00e1ciu, distrib\u00faciu a dif\u00faziu d\u00fdchac\u00edch plynov','je tzv. p\u013e\u00facne','je riaden\u00e9 z d\u00fdchacieho centra v mieche','je tzv. tkanivov\u00e9','je oxygen\u00e1cia hemoglob\u00ednu spojen\u00e1 so zmenou farby na tmavomodr\u00fa',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(734,'734. Medzi obrann\u00e9 d\u00fdchacie reflexy patr\u00ed:','ka\u0161e\u013e','d\u00e1venie','smrkanie','zvracanie','k\u00fdchanie','potenie','defeka\u010dn\u00fd reflex','nap\u00ednac\u00ed reflex',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(735,'735. Vn\u00fatorn\u00e9 d\u00fdchanie \u010dloveka:','zabezpe\u010duje distrib\u00faciu vd\u00fdchnut\u00e9ho vzduchu do v\u0161etk\u00fdch mech\u00farikov','je v\u00fdmena 0_2 a CO_2 medzi krvou a tkanivami formou dif\u00fazie','je vlastn\u00e1 v\u00fdmena vzduchu medzi p\u013e\u00facami a prostred\u00edm','prebieha v p\u013e\u00facnych alveol\u00e1ch','je tzv. p\u013e\u00facne','zah\u0155\u0148a p\u013e\u00facnu ventil\u00e1ciu, distrib\u00faciu a dif\u00faziu plynov','je tzv. tkanivov\u00e9','je v\u00fdmena d\u00fdchac\u00edch plynov medzi krvou a tkanivami formou pas\u00edvneho transportu',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(736,'736. Pr\u00edtomnos\u0165 dusi\u010dnanov a dusitanov v potrave ovplyv\u0148uje d\u00fdchanie tak, \u017ee:','sp\u00f4sobuje oxid\u00e1ciu Fe^2+ v hemoglob\u00edne na Fe^3+','sp\u00f4sobuje vznik methemoglob\u00ednu, ktor\u00fd nie je schopn\u00fd pren\u00e1\u0161a\u0165 kysl\u00edk','sp\u00f4sobuje oxid\u00e1ciu Fe^3+ v hemoglob\u00edne na Fe^2+','sp\u00f4sobuje vznik oxyhemoglob\u00ednu, ktor\u00fd nie je schopn\u00fd pren\u00e1\u0161a\u0165 kysl\u00edk','sp\u00f4sobuje rakovinu p\u013e\u00fac','sp\u00f4sobuje bronchit\u00eddu','sp\u00f4sobuje po\u0161kodenie respira\u010dn\u00e9ho epitelu','vyvol\u00e1va obrann\u00e9 reflexy \u2014 k\u00fdchanie a ka\u0161e\u013e',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(737,'737. K\u00fdchanie \u010dloveka je vyvol\u00e1van\u00e9:','podr\u00e1\u017eden\u00edm sliznice \u00fastnej dutiny','podr\u00e1\u017eden\u00edm sliznice hrtana','podr\u00e1\u017eden\u00edm sliznice nosa','podr\u00e1\u017eden\u00edm sliznice priedu\u0161nice','podr\u00e1\u017eden\u00edm sliznice priedu\u0161iek','dr\u00e1\u017eden\u00edm d\u00fdchac\u00edch ciest nahromaden\u00fdm hlienom','dr\u00e1\u017eden\u00edm d\u00fdchac\u00edch ciest pevn\u00fdmi \u010diasto\u010dkami','dr\u00e1\u017eden\u00edm nervov\u00fdch zakon\u010den\u00ed v d\u00fdchac\u00edch cest\u00e1ch',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(738,'738. Podr\u00e1\u017edenie sliznice hrtana, priedu\u0161nice a priedu\u0161iek vyvol\u00e1va u \u010dloveka:','ka\u0161e\u013e','n\u00e1dchu','k\u00fdchanie','zvracanie','obrann\u00e9 reflexy','nepr\u00edjemn\u00fd pocit','chr\u00edpku','nauzeu',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(739,'739. Medzi naj\u010dastej\u0161ie ochorenia d\u00fdchac\u00edch ciet u \u010dloveka patr\u00ed:','bronchit\u00edda \u2014 z\u00e1pal p\u013e\u00fac','faryngit\u00edda \u2014 z\u00e1pal hrtana','laryngit\u00edda \u2014 z\u00e1pal hltana','bronchit\u00edda \u2014 z\u00e1pal priedu\u0161iek','faryngit\u00edda \u2014 z\u00e1pal hltana','laryngit\u00edda \u2014 z\u00e1pal hrtana','k\u00fdchanie','kvap\u00f4\u010dkov\u00e1 infekcia',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(740,'740. O infek\u010dn\u00fdch ochoreniach d\u00fdchac\u00edch ciest u \u010dloveka plat\u00ed, \u017ee:','v\u00e4\u010d\u0161ina t\u00fdchto ochoren\u00ed sa pren\u00e1\u0161a kvap\u00f4\u010dkovou infekciou','v\u00e4\u010d\u0161ina t\u00fdchto ochoren\u00ed sa pren\u00e1\u0161a k\u00fdchan\u00edm a ka\u0161\u013ean\u00edm','m\u00f4\u017eu by\u0165 v\u00edrusov\u00e9ho p\u00f4vodu \u2014 napr. chr\u00edpka','m\u00f4\u017eu by\u0165 bakteri\u00e1lneho p\u00f4vodu \u2014 napr. tuberkul\u00f3za','m\u00f4\u017eu by\u0165 v\u00edrusov\u00e9ho p\u00f4vodu \u2014 napr. tuberkul\u00f3za','m\u00f4\u017eu by\u0165 bakteri\u00e1lneho p\u00f4vodu \u2014 napr. chr\u00edpka','sa pren\u00e1\u0161aj\u00fa naj\u010dastej\u0161ie nakazen\u00fdmi potravinami','k nim patr\u00ed alergia',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(741,'741. Cel\u00fa mimobunkov\u00fa tekutinu \u013eudsk\u00e9ho tela m\u00f4\u017eeme rozdeli\u0165 takto:','krv a krvn\u00e1 plazma','krv, tkanivov\u00fd mok a cytoplazma','tkanivov\u00fd mok a krvn\u00e1 plazma','miazgov\u00fd mok a krv','krv, miazga, tkanivov\u00fd mok a in\u00e9 telesn\u00e9 tekutiny','krv, plazma a lymfa','krv, lymfa, mimocievne tekutiny a in\u00e9 telesn\u00e9 tekutiny','intracelul\u00e1rna a extracelul\u00e1rna',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(742,'742. Bielkovina kerat\u00edn, ktor\u00e1 je pr\u00edtomn\u00e1 v povrchov\u00fdch bunk\u00e1ch poko\u017eky, sp\u00f4sobuje, \u017ee ko\u017ea:','je pru\u017en\u00e1','neprep\u00fa\u0161\u0165a ultrafialov\u00e9 \u017eiarenie','m\u00e1 schopnos\u0165 regener\u00e1cie','je pre vodu nepriepusm\u00e1','neprep\u00fa\u0161\u0165a r\u00e1dioakt\u00edvne \u017eiarenie','prep\u00fa\u0161\u0165a infra\u010derven\u00e9 \u017eiarenie','prep\u00fa\u0161\u0165a ultrafialov\u00e9 \u017eiarenie','m\u00e1 primeran\u00e9 sfarbenie',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(743,'743. Ku ko\u017en\u00fdm deriv\u00e1tom \u010dloveka patria:','akn\u00e9','nechty','mazov\u00e9 \u017e\u013eazy','papil\u00e1rne li\u0161ty','mlie\u010dne \u017e\u013eazy','vlasy','po\u010detn\u00e9 receptory v zam\u0161i','potn\u00e9 \u017e\u013eazy',FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(744,'744. Ko\u017ea \u010dloveka chr\u00e1ni organizmus pred:','\u00fa\u010dinkami r\u00e1dioakt\u00edvneho \u017eiarenia','\u00fa\u010dinkami infra\u010derven\u00e9ho \u017eiarenia','\u00fa\u010dinkami ultrafialov\u00e9ho \u017eiarenia','vniknut\u00edm mikroorganizmov','stratami tepla','tvorbou akn\u00e9','rakovinou ko\u017ee','prehriat\u00edm',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(745,'745. Tukov\u00e9 z\u00e1soby sa v tele \u010dloveka ukladaj\u00fa:','v poko\u017eke','v lymfatickom dren\u00e1\u017enom syst\u00e9me','v zam\u0161i','do podko\u017en\u00e9ho v\u00e4ziva','do pr\u00eddatn\u00fdch org\u00e1nov ko\u017ee','v mazov\u00fdch \u017e\u013eaz\u00e1ch','do podko\u017en\u00e9ho v\u00e4ziva, \u010d\u00edm sa ko\u017ea zvl\u00e1\u010d\u0148uje','do podko\u017en\u00e9ho v\u00e4ziva, \u010d\u00edm sa zabezpe\u010duje st\u00e1los\u0165 vn\u00fatom\u00e9ho prostredia',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(746,'746. Potn\u00e9 \u017e\u013eazy \u010dloveka:','v po\u010dte asi 2 mili\u00f3ny s\u00fa v ko\u017ei rozlo\u017een\u00e9 pravidelne','v po\u010dte asi 2 mili\u00f3ny s\u00fa v ko\u017ei rozlo\u017een\u00e9 nepravidelne','produkuj\u00fa pot, ktor\u00fd udr\u017euje ko\u017eu vla\u010dnu a vlhk\u00fa','produkuj\u00fa maz, ktor\u00fd zvl\u00e1\u010d\u0148uje ko\u017eu','ch\u00fdbaj\u00fa v ko\u017ei dlan\u00ed a stupaj\u00ed','produkuj\u00fa pot, ktor\u00fd sl\u00fa\u017ei na ochladzovanie organizmu a vylu\u010dovanie \u0161kodliv\u00edn','s\u00fa ulo\u017een\u00e9 v zam\u0161i','s\u00fa vo ve\u013ekom mno\u017estve na \u010dele, dlaniach a stupaj\u00e1ch',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(747,'747. V zam\u0161i (corium) \u010dloveka s\u00fa ulo\u017een\u00e9:','mazov\u00e9 \u017e\u013eazy','potn\u00e9 \u017e\u013eazy','krvn\u00e9 cievy','nervy','pigment melan\u00edn','kerat\u00edn','po\u010detn\u00e9 receptory','podko\u017en\u00e9 v\u00e4zivo',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(748,'748. Plocha povrchu ko\u017ee priemern\u00e9ho dospel\u00e9ho \u010dloveka meria pribli\u017ene:','1,6 \u2014 2 m2','2,5 \u2014 3,5 m2','1,0 m2','3,1 \u2014 3,5 m2','0,8 \u2014 1,1 m2','0,5 m2','0,9 m2','2,7 m2',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(749,'749. Pot \u010dloveka obsahuje:','mo\u010dovinu','kreatin\u00edn','kyselinu mlie\u010dnu','vodu','kyselinu mo\u010dov\u00fa','kerat\u00edn','kyselinu pantot\u00e9nov\u00fa','mastn\u00e9 kyseliny',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(750,'750. Riziko vzniku rakoviny ko\u017ee \u010dloveka sa zvy\u0161uje:','ke\u010f je poko\u017eka vystavovan\u00e1 nadmern\u00e9mu ultrafialov\u00e9mu \u017eiareniu','pri albinizme','ke\u010f je poko\u017eka vystavovan\u00e1 nadmern\u00e9mu infra\u010derven\u00e9mu \u017eiareniu','pri nadmernom opa\u013eovan\u00ed','pri zv\u00fd\u0161en\u00ed po\u010dtu mut\u00e1ci\u00ed v bunk\u00e1ch poko\u017eky','pri zv\u00fd\u0161enej hormon\u00e1lnej aktivite v puberte','pri zv\u00fd\u0161enej tvorbe akn\u00e9','pri zn\u00ed\u017eenej funkcii obli\u010diek',TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(751,'751. Protil\u00e1tky u \u010dloveka:','reaguj\u00fa s antig\u00e9nom na princ\u00edpe komplementarity b\u00e1z','sa vytv\u00e1raj\u00fa v plazmatick\u00fdch bunk\u00e1ch ktor\u00e9 vznikli premenou B-lymfocytov','s\u00fa bielkoviny a reaguj\u00fa len s ur\u010dit\u00fdm antig\u00e9nom','s\u00fa do krvi produkovan\u00e9 najm\u00e4 T-lymfocytmi','s\u00fa napr\u00edklad aglutin\u00edny','s\u00fa do krvi produkovan\u00e9 T-Iymfocytmi po styku s antig\u00e9nom','s\u00fa naz\u00fdvan\u00e9 aj ako aglutinog\u00e9ny','s\u00fa polynukleotidy',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(752,'752. Imunitn\u00e1 reakcia \u013eudsk\u00e9ho organizmu:','je v celom rozsahu reaktivity vroden\u00e1','sa u ka\u017ed\u00e9ho rozv\u00edja individu\u00e1lne po\u010das ontogen\u00e9zy','je zabezpe\u010dovan\u00e1 hlavne aktivitou fagocytuj\u00facich buniek','m\u00f4\u017ee by\u0165 pr\u00ed\u010dinou alergi\u00ed','je zabezpe\u010dovan\u00e1 hlavne lymfocytmi','zah\u0155\u0148a tvorbu protil\u00e1tok','je u v\u0161etk\u00fdch zdrav\u00fdch \u013eud\u00ed namieren\u00e1 proti rovnak\u00fdm infekci\u00e1m','je zabezpe\u010den\u00e1 \u0161pecifick\u00fdm zlo\u017een\u00edm v\u00fdlu\u010dkov ko\u017en\u00fdch \u017eliaz',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(753,'753. Bunkov\u00fd typ \u0161pecifickej imunity u \u013eud\u00ed sprostredk\u00favaj\u00fa:','lymfocyty B','neutrofiln\u00e9 granulocyty','lymfocyty T','plazmatick\u00e9 bunky','monocyty','v\u0161etky leukocyty','erytrocyty','trombocyty',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(754,'754. Na plazmatick\u00e9 bunky sa menia:','T-Iymfocyty','B-lymfocyty','monocyty','erytrocyty','granulocyty','plam\u00f3di\u00e1','niektor\u00e9 lymfocyty','niektor\u00e9 agranulocyty',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(755,'755. \u013dudsk\u00e9 T-lymfocyty:','zabezpe\u010duj\u00fa protil\u00e1tkov\u00fa obranu','produkuj\u00fa protil\u00e1tky do krvi','zabezpe\u010duj\u00fa pas\u00edvnu imunitu','s\u00fa postihnut\u00e9 v\u00edrusom HIV pri AIDS','zabezpe\u010duj\u00fa bunkov\u00fa obranu','zabezpe\u010duj\u00fa protil\u00e1tkov\u00fa imunitn\u00fa reakciu','fagocyt\u00f3zou zne\u0161kod\u0148uj\u00fa cudzorod\u00e9 l\u00e1tky','vytv\u00e1raj\u00fa imunoglobul\u00edny',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(756,'756. K imunitn\u00e9mu syst\u00e9mu \u010dloveka rad\u00edme:','miazgov\u00e9 cievy','lymfatick\u00e9 cievy','miazgov\u00e9 tkanivo v \u0161\u013each\u00e1ch','miazgov\u00e9 tkanivo v stene mo\u010dovej a tr\u00e1viacej s\u00fastavy','t\u00fdmus, slezinu a mandle','uzliny','miazgov\u00e9 org\u00e1ny','Iymfatick\u00e9 org\u00e1ny',TRUE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(757,'757. Fagocyt\u00f3za bunkami imunitn\u00e9ho syst\u00e9mu \u010dloveka spo\u010d\u00edva v:','tvorbe protil\u00e1tok','\u010dinnosti lymfocytov v tkaniv\u00e1ch','pohlcovan\u00ed cudzorod\u00fdch l\u00e1tok','produkcii imunoglobul\u00ednov','pohlcovan\u00ed mikroorganizmov','pomno\u017een\u00ed pam\u00e4\u0165ov\u00fdch buniek v tkaniv\u00e1ch','premene lymfocytov na pam\u00e4\u0165ov\u00e9 bunky','fagocyt\u00e1rnej \u010dinnosti \u0161peci\u00e1lnych buniek v tkaniv\u00e1ch',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(758,'758. Bakteric\u00eddna l\u00e1tka prirodzene obsiahnut\u00e1 v slin\u00e1ch \u010dloveka sa naz\u00fdva:','amyl\u00e1za','lyz\u00edn','antibiotikum','lyzoz\u00f3m','bakt\u00e9riof\u00e1g','lyzoz\u00fdm','ptyal\u00edn','fagoz\u00f3m',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(759,'759. Diaped\u00e9za u \u010dloveka je:','pohlcovanie choroboplodn\u00fdch mikroorganizmov','schopnos\u0165 ch\u00f4dze na dvoch kon\u010datin\u00e1ch','premena B lymfocytov na plazmatick\u00e9 bunky','filtr\u00e1cia krvnej plazmy v glomerule','prestup bielych krviniek cez stenu vl\u00e1so\u010dn\u00edc','vzpriamen\u00e1 ch\u00f4dza pomocou doln\u00fdch kon\u010dat\u00edn','sekre\u010dn\u00e1 \u010dinnos\u0165 lymfocytov','v\u00fdstup \u010derven\u00fdch krviniek z vl\u00e1so\u010drt\u00edc',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(760,'760. Chemotaxia znamen\u00e1:','pohlcovanie choroboplodn\u00fdch mikroorganizmov','opustenie krvn\u00e9ho rie\u010di\u0161\u0165a bielymi krvinkami','roz\u0161\u00edrenie krvn\u00fdch ciev','pohybov\u00fa reakciu na chemick\u00e9 podnety','aktiviz\u00e1ciu komplementov\u00e9ho syst\u00e9mu','zr\u00e1\u017eanie krvi','zhlukovanie \u010derven\u00fdch krviniek','\u00fapravu bakt\u00e9ri\u00ed pred fagocyt\u00f3zou',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(761,'761. Pohlcovanie mikroorganizmov \u013eudskou imunitnou bunkou sa naz\u00fdva :','pinocyt\u00f3za','exocyt\u00f3za','diaped\u00e9za','fagocyt\u00f3za','plazmol\u00fdza','endocyt\u00f3za','disjunkcia','chemotaxia',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(762,'762. Detsk\u00e1 \u017e\u013eaza \u010dloveka:','pln\u00ed funkciu centr\u00e1lneho org\u00e1nu lymfatick\u00e9ho syst\u00e9mu','je najlep\u0161ie vyvinut\u00e1 u dospel\u00fdch jedincov','je najlep\u0161ie vyvinut\u00e1 u mal\u00fdch det\u00ed','je org\u00e1nom, kde dozrievaj\u00fa B-lymfocyty','je org\u00e1nom, kde dozrievaj\u00fa T-Iymfocyty','b\u00fdva v dospelosti nahr\u00e1dzan\u00e1 svalov\u00fdm tkanivom','je v dospelosti nahraden\u00e1 tukov\u00fdm tkanivom','je najva\u010d\u0161\u00ed lymfatick\u00fd org\u00e1n',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(763,'763. Mandle \u010dloveka:','s\u00fa najv\u00e4\u010d\u0161\u00edm lymfatick\u00fdm org\u00e1nom','s\u00fa centr\u00e1lnym org\u00e1nom lymfatick\u00e9ho syst\u00e9mu','s\u00fa pova\u017eovan\u00e9 za imunologick\u00fd filter krvi','sa nach\u00e1dzaj\u00fa v sliznici hrtana','sa nach\u00e1dzaj\u00fa v sliznici hltana','b\u00fdvaj\u00fa v dospelosti nahraden\u00e9 tukov\u00fdm tkanivom','s\u00fa miestom tvorby lymfocytov','s\u00fa \u017eivotne d\u00f4le\u017eit\u00fdm org\u00e1nom',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(764,'764. Mikroorganizmy sa u \u010dloveka zvy\u010dajne zachyt\u00e1vaj\u00fa:','v tkanivovom moku','v krvnej plazme a lymfatick\u00fdch uzlin\u00e1ch','v krvnej plazme a miazgov\u00fdch uzlin\u00e1ch','v miazgov\u00fdch uzlin\u00e1ch','v lymfatick\u00fdch uzlin\u00e1ch','na povrchu ko\u017ee','na povrchu slizn\u00edc','v hliene d\u00fdchadch ciest',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(765,'765. Ne\u0161pecifick\u00fa fagocyt\u00e1rnu obranu u \u010dloveka zabezpe\u010duj\u00fa:','eozinofiln\u00e9 granulocyty','leukocyty a erytrocyty','neutrofiln\u00e9 granulocyty','B-Iymfocyty','T-lymfocyty','monocyty','ur\u010dit\u00e9 bunky v krvi i tkaniv\u00e1ch','niektor\u00e9 typy leukocytov',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(766,'766. Men\u0161trua\u010dn\u00fd cyklus \u017eeny rozde\u013eujeme na:','detsk\u00fd vek, pubertu, dospelos\u0165, obdobie prechodu a starobu','dospelos\u0165 a klimax','4 f\u00e1zy','ovul\u00e1ciu, tehotenstvo a \u0161estonedelie','3 f\u00e1zy','ovula\u010dn\u00fa a men\u0161trua\u010dn\u00fa f\u00e1zu','men\u0161trua\u010dn\u00fa, prolifera\u010dn\u00fa, sekre\u010dn\u00fa, ischemick\u00fa f\u00e1zu','5 f\u00e1z',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
