INSERT INTO BIOLOGIA VALUES(212,'212. Katabolick\u00e9 procesy v rastlin\u00e1ch s\u00fa:','disimila\u010dn\u00e9','asimila\u010dn\u00e9','napr. typu fotosynt\u00e9zy','napr. typu d\u00fdchania','spojen\u00e9 so spotrebou energie','spojen\u00e9 s uvo\u013e\u0148ovan\u00edm energie chemick\u00fdch v\u00e4zieb','endergonick\u00e9','tak\u00e9, pri ktor\u00fdch rastliny \u0161tiepia zlo\u017eitej\u0161ie organick\u00e9 l\u00e1tky na jednoduch\u0161ie',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(213,'213. M\u00e4so\u017erav\u00e9 rastliny maj\u00fa praktick\u00fd v\u00fdznam:','preto\u017ee s\u00fa schopn\u00e9 viaza\u0165 vzdu\u0161n\u00fd dus\u00edk','vo farmaceutickom priemysle','v medic\u00edne','pri \u201ezelenom hnojen\u00ed"','pri lie\u010dbe n\u00e1dorov\u00fdch ochoren\u00ed','ako reducenty organick\u00fdch l\u00e1tok','v biologickom boji proti \u0161kodcom','preto\u017ee mnoh\u00e9 z nich s\u00fa ve\u013emi vz\u00e1cne lie\u010div\u00e9 rastliny',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(214,'214. Synt\u00e9za ATP v rastlinn\u00fdch bunk\u00e1ch prebieha:','na vn\u00fatornej membr\u00e1ne mitochondri\u00ed','na membr\u00e1ne tylakoidov v chloroplastoch','v jadre bunky','na vonkaj\u0161ej membr\u00e1ne mitochondri\u00ed','na drsnom endoplazmatickom retikule','na mitochondri\u00e1lnych krist\u00e1ch','v str\u00f3me chloroplastov','preva\u017ene v cytoplazme',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(215,'215. Kysl\u00edk uvo\u013e\u0148ovan\u00fd pri fotosynt\u00e9ze do vzduchu poch\u00e1dza:','z vody','z CO_2','z gluk\u00f3zy','z NADP^+','z fotol\u00fdzy vody','z asimila\u010dn\u00fdch farb\u00edv','pri C3 rastlin\u00e1ch z ribul\u00f3za 1,5-bisfosf\u00e1tu','pri C4 rastlin\u00e1ch z fosfoenolpyruv\u00e1tu',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(216,'216. Sekund\u00e1rne procesy fotosynt\u00e9zy:','vyu\u017e\u00edvaj\u00fa chemick\u00fa energiu na redukciu CO_2 na sacharidy','s\u00fa charakterizovan\u00e9 dvoma sp\u00f4sobmi viazania CO_2','nemusia prebieha\u0165 na svetle','vy\u017eaduj\u00fa akceptor CO_2 acetylkoenz\u00fdm A','vy\u017eaduj\u00fa akceptor CO_2 fosfoenolpyruv\u00e1t \u2014 C3 rastliny','vy\u017eaduj\u00fa akceptor CO_2 ribul\u00f3za 1,5-bisfosf\u00e1t \u2014 C4 rastliny','vy\u017eaduj\u00fa akceptor CO_2 fosfoenolpyruv\u00e1t \u2014 C4 rastliny','vy\u017eaduj\u00fa akceptor CO_2 ribul\u00f3za 1,5-bisfosf\u00e1t \u2014 C3 rastliny',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(217,'217. Fotochemick\u00e1 f\u00e1za fotosynt\u00e9zy zah\u0155\u0148a procesy:','ktor\u00e9 m\u00f4\u017eu prebieha\u0165 len na svetle','ktor\u00e9 m\u00f4\u017eu prebieha\u0165 na svetle aj v tme','ktor\u00e9 s\u00fa spojen\u00e9 s fotofosforyl\u00e1ciou','spojen\u00e9 s premenou l\u00e1tok','spojen\u00e9 s fotol\u00fdzou vody','fotol\u00fdzy CO2','tvorby ATP','\u0161tiepenia ATP',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(218,'218. Fotol\u00fdza vody:','je z\u00e1visl\u00e1 od \u017eiarivej energie','patr\u00ed k fotobiologick\u00fdm procesom','patr\u00ed k sekund\u00e1rnym procesom fotosynt\u00e9zy','prebieha len na svetle','je prim\u00e1rny proces syntetickej f\u00e1zy fotosynt\u00e9zy','je disoci\u00e1cia vody vplyvom sveteln\u00e9ho \u017eiarenia','prebieha v cytoplazme rastlinn\u00fdch buniek','m\u00f4\u017ee by\u0165 cyklick\u00e1 a necyklick\u00e1',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(219,'219. Pri fotosynt\u00e9ze:','vznik\u00e1 CO_2','vznik\u00e1 0_2','vznik\u00e1 ATP','sa \u0161tiepi ATP vo fotochemickej f\u00e1ze','sa spotreb\u00fava 0_2','vznik\u00e1 36 molek\u00fal ATP','vznik\u00e1 NADPH +H^+','vznik\u00e1 gluk\u00f3za',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(220,'220. D\u00fdchac\u00ed re\u0165azec, ktor\u00e9ho kone\u010dn\u00fdm produktom je voda:','je lokalizovan\u00fd v mitochondri\u00e1lnej matrix','je lokalizovan\u00fd v str\u00f3me chloroplastov','je lokalizovan\u00fd vo vn\u00fatornej membr\u00e1ne chloroplastov','je sled reakci\u00ed, v ktorom vznik\u00e1 ATP','je sled reakci\u00ed, v ktorom sa \u0161tiepia makroergick\u00e9 v\u00e4zby ATP','je lokalizovan\u00fd vo vn\u00fatornej mitochondri\u00e1lnej membr\u00e1ne','je Iokalizovan\u00fd vo v\u0161etk\u00fdch bunk\u00e1ch v cytoplazme','je sled reakci\u00ed, v ktorom sa uvo\u013enen\u00e1 energia uklad\u00e1 do asimil\u00e1tov',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(221,'221. Rastliny d\u00fdchaj\u00fa:','iba v noci','iba cez de\u0148','aj cez de\u0148, aj v noci','aj cez de\u0148, aj v noci, ale d\u00fdchanie cez de\u0148 je \u201ezastret\u00e9" fotosynt\u00e9zou','pri\u010dom prij\u00edmaj\u00fa CO_2 a uvo\u013e\u0148uj\u00fa 0_2 do prostredia','pri\u010dom prij\u00edmaj\u00fa 0_2 a vyd\u00e1vaj\u00fa CO_2 do prostredia','pri\u010dom premie\u0148aj\u00fa energiu fot\u00f3nov slne\u010dn\u00e9ho \u017eiarenia','pri\u010dom spotreb\u00favaj\u00fa organick\u00e9 a anorganick\u00e9 l\u00e1tky',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(222,'222. Celkov\u00fd energetick\u00fd zisk pri odb\u00faravan\u00ed jednej molekuly gluk\u00f3zy na H_20 a CO_2:','30 molek\u00fal ATP','2 molekuly ATP','36 molek\u00fal ATP','38 molek\u00fal ATP','zn\u00ed\u017een\u00fd v d\u00f4sledku fotorespir\u00e1cie','rovnak\u00fd ako pri fotosynt\u00e9ze','n\u00edzky, preto\u017ee rastlina nepred\u00fdcha v\u0161etky produkty fotosynt\u00e9zy','tak\u00fd, \u017ee to posta\u010d\u00ed na vytvorenie 36 makroergick\u00fdch v\u00e4zieb',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(223,'223. Kyselina pyrohroznov\u00e1:','je z\u00e1kladn\u00fd produkt \u00fapinej oxid\u00e1cie organick\u00fdch l\u00e1tok','je z\u00e1kladn\u00fd produkt glykol\u00fdzy','podlieha oxida\u010dnej dekarboxyl\u00e1cii v mitochondri\u00e1ch','podlieha oxida\u010dnej dekarboxyl\u00e1cii v cytoplazme eukaryotick\u00fdch buniek','je transportovan\u00e1 do cytoplazmy','je transportovan\u00e1 do mitochondri\u00e1lnej matrix','bezprostredne vstupuje do Krebsovho cyklu','bezprostredne vstupuje do Calvinovho cyklu',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(224,'224. Pri sekund\u00e1rnych procesoch fotosynt\u00e9zy sa CO_2 via\u017ee na:','vhodn\u00fd organick\u00fd substr\u00e1t','vhodn\u00fd anorganick\u00fd substr\u00e1t','polysacharid','ribul\u00f3za 1,5-bisfosf\u00e1t','rib\u00f3za 1,5-fosf\u00e1t pri C3 rastlin\u00e1ch','ox\u00e1lacet\u00e1t','fosfoenolpyruv\u00e1t pri C4 rastlin\u00e1ch','fosfoenolpyruv\u00e1t pri C3 rastlin\u00e1ch',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(225,'225. Fotoautotrofia je:','synt\u00e9za jednoduch\u00fdch anorganick\u00fdch l\u00e1tok pomocou svetla','synonymum pre fotosynt\u00e9zu','synonymum pre fotorespir\u00e1ciu','pr\u00edrodn\u00fd proces tvorby organick\u00fdch l\u00e1tok a kysl\u00edka rastlinami','proces spa\u013eovania l\u00e1tok a uvo\u013e\u0148ovania nahromadenej energie','proces disimil\u00e1cie anorganick\u00fdch l\u00e1tok','s\u00e9ria biochemick\u00fdch reakci\u00ed \u0161tiepenia organick\u00fdch l\u00e1tok spojen\u00e1 s uvo\u013e\u0148ovan\u00edm energie','proces oxid\u00e1cie anorganick\u00e9ho substr\u00e1tu a redukcie CO_2 na sacharidy',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(226,'226. Oxida\u010dn\u00e1 fosforyl\u00e1cia je proces:','\u0161tiepenia ATP za pr\u00edstupu kysl\u00edka','anaer\u00f3bnej oxid\u00e1cie organick\u00fdch l\u00e1tok a fosforyl\u00e1cie ADP','aer\u00f3bnej oxid\u00e1cie organick\u00fdch l\u00e1tok a fosforyl\u00e1cie ADP','aer\u00f3bnej oxid\u00e1cie organick\u00fdch l\u00e1tok a fosforyl\u00e1cie ATP','pri ktorom vznik\u00e1 energia vo forme ATP','pri ktorom je vod\u00edk oxidovan\u00fd na vodu a fosforylovan\u00fd ATP','pri ktorom je vod\u00edk gluk\u00f3zy oxidovan\u00fd na peroxidy','ktor\u00fd prebieha v mitochondri\u00e1ch',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(227,'227. Glykol\u00fdza:','prebieha v mitochondri\u00e1ch','je prv\u00e1 etapa biologickej oxid\u00e1cie','je prav\u00e9 kvasenie','prebieha v cytoplazme','prebieha v anaer\u00f3bnych organizmoch','prebieha v aer\u00f3bnych organizmoch','je druh\u00e1 etapa biologickej oxid\u00e1cie','je ne\u00fapin\u00e1 oxid\u00e1cia',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(228,'228. Prav\u00e9 kvasenie:','prebieha za pr\u00edstupu vonkaj\u0161ieho kysl\u00edka','ur\u00fdch\u013euje pr\u00edtomnos\u0165 kysl\u00edka','ur\u00fdch\u013euj\u00fa bakt\u00e9rie, kvasinky a plesne','kon\u010d\u00ed vznikom CO_2 a H_20','kon\u010d\u00ed vznikom kyseliny pyrohroznovej','je biologick\u00fd rozklad sacharidov pomocou mikroorganizmov','uskuto\u010dhuj\u00fa napr. kvasinky pivn\u00e9','v bunk\u00e1ch \u017eivo\u010d\u00edchov a \u010dloveka kon\u010d\u00ed vznikom kyseliny mlie\u010dnej',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(229,'229. Oxida\u010dn\u00e1 dekarboxyl\u00e1cia kyseliny pyrohroznovej:','je prv\u00e1 etapa biologickej oxid\u00e1cie','prebieha v mitochondri\u00e1ch','je proces vzniku kyseliny pyrohroznovej','poskytuje vstupn\u00fd produkt citr\u00e1tov\u00e9ho cyklu','d\u00e1va vznik acetyl-koenz\u00fdmu A','prebieha v cytoplazme v\u0161etk\u00fdch buniek','je poslednou f\u00e1zou biologickej oxid\u00e1cie','d\u00e1va vznik 30 molekul\u00e1m ATP',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(230,'230. Energia z asimil\u00e1tov sa v rastlinnej bunke uvo\u013e\u0148uje:','glykol\u00fdzou','procesom glukoneogen\u00e9zy','v d\u00fdchacom re\u0165azci','oxida\u010dnou fosforyl\u00e1ciou','\u0161tiepen\u00edm uhl\u00edkov\u00e9ho re\u0165azca gluk\u00f3zy','biologick\u00fdmi oxid\u00e1ciami','fotosynt\u00e9zou','fotofosforyl\u00e1ciou',TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(231,'231. Izogamia je spl\u00fdvanie gam\u00e9t:','morfologicky a fyziologicky rovnak\u00fdch','morfologicky rovnak\u00fdch a fyziologicky odl\u00ed\u0161en\u00fdch','geneticky identick\u00fdch','ktor\u00e9 sa l\u00ed\u0161ia tvarom, organiz\u00e1ciou a pohyblivos\u0165ou','haploidn\u00fdch','rovnak\u00e9ho pohlavn\u00e9ho typu','odli\u0161n\u00e9ho pohlavn\u00e9ho typu','typu makrogam\u00e9ta a mikrogam\u00e9ta',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(232,'232. Anizogamia je spl\u00fdvanie gam\u00e9t:','morfologicky odl\u00ed\u0161en\u00fdch','geneticky identick\u00fdch','haploidn\u00fdch','rovnak\u00e9ho pohlavn\u00e9ho typu','ktor\u00e9 sa l\u00ed\u0161ia tvarom, organiz\u00e1ciou alebo pohyblivos\u0165ou','typu makrogam\u00e9ta a mikrogam\u00e9ta','fyziologicky odli\u0161n\u00fdch','geneticky odli\u0161n\u00fdch',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(233,'233. Rodozmena je:','metabi\u00f3za','striedanie pohlavnej a nepohlavnej gener\u00e1cie v ontogen\u00e9ze rastl\u00edn','metagen\u00e9za','mutagen\u00e9za','striedanie gametofytu a sporofytu v ontogen\u00e9ze rastl\u00edn','gametogen\u00e9za','partenogen\u00e9za pri rastlin\u00e1ch','striedanie haploidnej (n \u2014 sporofyt) a diploidnej (2n \u2014 gametofyt) gener\u00e1cie',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(234,'234. Gametofyt pri machorastoch:','je v\u017edy haploidn\u00e1 stielka','je diploidn\u00e1 stielka','je kormus','tvor\u013a v\u00fdtrusnice','vznik\u00e1 mitotick\u00fdm delen\u00edm v\u00fdtrusu','vznik\u00e1 meiotick\u00fdm delen\u00edm v\u00fdtrusu','vznik\u00e1 mitotick\u00fdm delen\u00edm zygoty','vznik\u00e1 z jednobunkov\u00e9ho haploidn\u00e9ho \u00fatvaru',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(235,'235. Nepohlavn\u00e9 rozmno\u017eovanie rastl\u00edn:','je vegetat\u00edvne','je asexu\u00e1lne','je sexu\u00e1lne pri vy\u0161\u0161\u00edch rastlin\u00e1ch','je v\u00fdvojovo star\u0161\u00ed sp\u00f4sob rozmno\u017eovania','je v\u00fdvojovo mlad\u0161\u00ed sp\u00f4sob rozmno\u017eovania','znamen\u00e1, \u017ee nov\u00fd jedinec vznik\u00e1 z telov\u00fdch buniek rodi\u010dovsk\u00e9ho organizmu','v\u00fdtrusn\u00fdch je sporog\u00f3nia','v\u00fdtrusn\u00fdch je gametog\u00f3nia',TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(236,'236. V\u00fdtrusy s\u00fa:','pohlavn\u00e9 rozmno\u017eovacie \u010dastice ni\u017e\u0161\u00edch rastl\u00edn','pohlavn\u00e9 rozmno\u017eovacie \u010dastice vy\u0161\u0161\u00edch rastl\u00edn','nepohlavn\u00e9 rozmno\u017eovacie \u010dastice','v\u00fdsledkom mitotick\u00e9ho delenia sp\u00f3r v sp\u00f3rangi\u00e1ch','v\u00fdsledkom reduk\u010dn\u00e9ho delenia','rozmno\u017eovacie \u010dastice machorastov','rozmno\u017eovacie \u010dastice papra\u010forastov','haploidn\u00e9 a nepohybliv\u00e9',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(237,'237. Sporofyt machorastov:','je diploidn\u00fd','je v\u017edy haploidn\u00fd','reprezentuje nepohlavn\u00fa gener\u00e1ciu','nie je schopn\u00fd fotosynt\u00e9zy','je prvokl\u00edk','je v\u017edy izomorfn\u00fd','je v\u017edy heteromorfn\u00fd','v\u00fd\u017eivou z\u00e1vis\u00ed od gametofytu',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(238,'238. R\u00f4znotvar\u00e1 rodozmena:','je izomorfn\u00e1','je heteromorfn\u00e1','znamen\u00e1, \u017ee gametofyt a sporofyt s\u00fa morfologicky rovnak\u00e9','znamen\u00e1, \u017ee gametofyt a sporofyt sa morfologicky l\u00ed\u0161ia','je charakteristick\u00e1 pre vy\u0161\u0161ie rastliny','je charakteristick\u00e1 pre ni\u017e\u0161ie rastliny','znamen\u00e1, \u017ee sam\u010die a sami\u010die gametangi\u00e1 s\u00fa na r\u00f4znych rastlin\u00e1ch','je len pri dvojdom\u00fdch rastlin\u00e1ch',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(239,'239. V ktorej gener\u00e1cii po\u010das rodozmeny doch\u00e1dza k mei\u00f3ze:','v gametofyte','v haploidnej','v sporofyte','v diploidnej','v izomorfnej','v heteromorfnej','v sam\u010dej','v sami\u010dej',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(240,'240. V \u017eivotnom cykle semenn\u00fdch rastl\u00edn:','je v\u00fdrazn\u00e1 prevaha sporofytu','je v\u00fdrazn\u00e1 prevaha garnetofytu','sa gametofyt vyv\u00edja ako s\u00fa\u010das\u0165 sporofytu','sa sporofyt vyv\u00edja ako s\u00fa\u010das\u0165 gametofytu','sporofyt pln\u00ed vy\u017eivovaciu funkciu','sporofyt reprezentuje vlastn\u00fa rastlinu','gametofyt pln\u00ed vy\u017eivovaciu funkciu','gametofyt reprezentuje vlastn\u00fa rastlinu',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(241,'241. V ktorej gener\u00e1cii machorastov vznikaj\u00fa gam\u00e9ty:','v haploidnej','v diploidnej','v pohlavnej','v nepohlavnej','v gametofyte','v sporofyte','v izomorfmej','v heteromorfnej',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(242,'242. Gametofyt sa od sporofytu machorastov odli\u0161uje:','morfologicky','cytologicky \u2014 gametofyt je haploidn\u00fd, sporofyt je diploidn\u00fd','cytologicky \u2014 gametofyt je diploidn\u00fd, sporofyt je haploidn\u00fd','fyziologicky \u2014 sporofyt v\u00fd\u017eivou z\u00e1vis\u00ed od gametofytu','fyziologicky \u2014 gametofyt v\u00fd\u017eivou z\u00e1vis\u00ed od sporofytu','pohyblivos\u0165ou','t\u00fdm, \u017ee nesie gametangi\u00e1 a sporofyt stopku s v\u00fdtrusnicou','t\u00fdm, \u017ee nesie stopku s v\u00fdtrusnicou a sporofyt gametangi\u00e1',TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(243,'243. V\u00fddaj vody rastlinou sa uskuto\u010d\u0148uje:','transpir\u00e1ciou \u2014 preva\u017ene v noci','transpir\u00e1ciou \u2014 cez de\u0148','gut\u00e1ciou \u2014 preva\u017ene cez de\u0148','gut\u00e1ciou \u2014 preva\u017ene v noci a nad r\u00e1nom','prieduchov\u00fdm v\u00fdtlakom','koh\u00e9znymi silami','vyparovan\u00edm vody cez prieduchy','kutikul\u00e1rnou transpir\u00e1ciou',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(244,'244. Na veden\u00ed vody v drevnej \u010dasti cievnych zv\u00e4zkov rastliny sa z\u00fa\u010dast\u0148uje:','koh\u00e9zia vody','adh\u00e9zia vody','kapilarita vody','transpir\u00e1cia','kore\u0148ov\u00fd v\u00fdtlak','v\u00fdrazne gut\u00e1cia','osmotick\u00e1 aktivita p\u00f4dneho roztoku','obsah kysl\u00edka v p\u00f4de',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(245,'245. Medzi rastov\u00e9 stimul\u00e1tory rastl\u00edn patria:','kyselina abscisov\u00e1','etyl\u00e9n','fenoly','aux\u00edny','kyselina ox\u00e1loctov\u00e1','giberel\u00edny','cytokin\u00edny','z\u00e1brann\u00e9 l\u00e1tky',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(246,'246. \u017divotn\u00fd cyklus rastliny:','je synonymum pre ontogen\u00e9zu','je fylogenetick\u00fd v\u00fdvoj rastlinn\u00e9ho druhu','je obdobie \u017eivota rastliny od vzniku semena po semeno','je obdobie \u017eivota rastliny od vzniku v\u00fdtrusu po v\u00fdtrus','je \u017eivot jedinca od za\u010diatku jeho zrodu a\u017e po smr\u0165','m\u00e1 typick\u00fd priebeh pre pr\u00edslu\u0161n\u00fa skupinu rastl\u00edn','a jeho typick\u00fd priebeh je jedn\u00fdm zo znakov dosiahnutej fylogenetickej \u00farovne','je v\u00fdvin rastliny od vykl\u00ed\u010denia po vytvorenie rozmno\u017eovacich org\u00e1nov',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(247,'247. Ozna\u010dte spr\u00e1vnu odpove\u010f:','ekologick\u00e9 sucho vznik\u00e1, ke\u010f je n\u00edzky obsah kysl\u00edka v p\u00f4de','fyziologick\u00e9 sucho vznik\u00e1, ke\u010f je slab\u00fd kore\u0148ov\u00fd v\u00fdtlak','ekologick\u00e9 sucho vznik\u00e1 pri mrazoch','fyziologick\u00e9 sucho vznik\u00e1 v \u00edlovit\u00fdch, mastn\u00fdch a kysl\u00fdch p\u00f4dach','kyprenie p\u00f4dy sa ozna\u010duje ako such\u00e9 zalievanie','pr\u00edjem vody rastlinou ovplyv\u0148uje aj obsah 0_2 v p\u00f4de','vy\u0161\u0161\u00ed osmotick\u00fd potenci\u00e1l p\u00f4dneho roztoku u\u013eah\u010duje pr\u00edjem vody rastlinou','osmoticky napnut\u00e1 bunka je \u0161pongiov\u00e1',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(248,'248. Transpira\u010dn\u00fd koeficient:','vyjadruje mno\u017estvo vody vydan\u00e9 rastlinou za jednotku \u010dasu','vyjadruje mno\u017estvo vody prijat\u00e9 rastlinou za jednotku \u010dasu','sa pou\u017e\u00edva na vyjadrenie intenzity transpir\u00e1cie','sa pou\u017e\u00edva na vyjadrenie produkcie biomasy rastl\u00edn','sa pou\u017e\u00edva na vyjadrenie n\u00e1rokov rastl\u00edn na vodu','vyjadruj\u00e9 mno\u017estvo vody v litroch spotrebovan\u00e9 rastlinou za vegeta\u010dn\u00e9 obdobie na 1 kg suchej hmotnosti','vyjadruje vodn\u00fa bilanciu rastliny','vyjadruje hospod\u00e1renie rastliny s vodou',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(249,'249. Respira\u010dn\u00fd kvocient:','je pomer vyprodukovan\u00e9ho CO_2 k spotrebovan\u00e9mu 0_2 pri bunkov\u00fdch oxid\u00e1ci\u00e1ch','je pomer prijat\u00e9ho 0_2 k vydan\u00e9mu CO_2 pri bunkov\u00fdch oxid\u00e1ci\u00e1ch','je pomer vydan\u00e9ho 0_2 k pr\u00edjat\u00e9mu CO_2 pri bunkov\u00fdch oxid\u00e1ci\u00e1ch','vyjadruje intenzitu d\u00fdchania','vyjadruje intenz\u00edtu fotosynt\u00e9zy','pre gluk\u00f3zu m\u00e1 hodnotu 1','je pomer koncentr\u00e1cie 0_2 a CO_2 v pr\u00facach','vyjadruje intenzitu glykol\u00fdzy',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(250,'250. Aux\u00edny:','s\u00fa chemick\u00e9 prostriedky na ni\u010denie rastl\u00edn','s\u00fa rastov\u00e9 regul\u00e1tory','sa vyu\u017e\u00edvaj\u00fa v pornohospod\u00e1rstve na ni\u010denie buriny','s\u00fa najstar\u0161ie zn\u00e1me rastov\u00e9 l\u00e1tky','podiel aj\u00fa sa na regula\u010dn\u00fdch procesoch rastl\u00edn','sa uplat\u0148uj\u00fa v apik\u00e1lnych merist\u00e9moch kore\u0148ov','podporuj\u00fa rast bo\u010dn\u00fdch kore\u0148ov','podporuj\u00fa zakore\u0148ovanie odrezkov',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(251,'251. Hlavn\u00fdmi z\u00e1sobn\u00fdmi l\u00e1tkami \u017eivo\u010d\u00ed\u0161nych buniek s\u00fa:','bielkoviny','tuky','\u0161krob','polysacharidy','enz\u00fdmy','glykog\u00e9n','celul\u00f3za','integumenty',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(252,'252. Homeost\u00e1za je:','st\u00e1los\u0165 vn\u00fatorn\u00e9ho prostredia organizmu','zabezpe\u010dovan\u00e1 \u010dinnos\u0165ou regula\u010dn\u00fdch mechanizmov','porucha zr\u00e1\u017eania krvi','obrann\u00fd mechanizmus','nervov\u00e1 regul\u00e1cia','podobnos\u0165 org\u00e1nov vykon\u00e1vaj\u00facich rovnak\u00fa funkciu','nerozl\u00ed\u0161en\u00e1 pohlavnos\u0165','ovplyv\u0148ovan\u00e1 vonkaj\u0161\u00edm prostred\u00edm',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(253,'253. Morfologickou a funk\u010dnou jednotkou hladk\u00e9ho svalu je:','viacjadrov\u00e9 svalov\u00e9 vl\u00e1kno','svalov\u00e9 vl\u00e1kno tvoren\u00e9 jednou bunkou','svalov\u00e1 bunka s aspo\u0148 piatimi jadrami','myofibrila','myoz\u00edn','akt\u00edn','intermedi\u00e1rny filament','aktinomyoz\u00edn',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(254,'254. K spojiv\u00e1m patria:','epitely, kosti, tkaniv\u00e1 svalov\u00e9 a nervov\u00e9','v\u00e4zivo, tkaniv\u00e1 chrupkov\u00e9 a kostn\u00e9','kosti, v\u00e4zivo, tkaniv\u00e1 krvn\u00e9','v\u00e4zivo a kostn\u00e9 tkanivo','epitely, v\u00e4zivo, tkaniv\u00e1 chrupkov\u00e9','v\u00e4zivo, tkaniv\u00e1 svalov\u00e9 a kostn\u00e9','resorp\u010dn\u00fd epitel a okostica','chrupka a kutikula',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(255,'255. \u017d\u013eazovy epitel:','obsahuje bunky \u0161pecializovan\u00e9 na sekr\u00e9ciu','pokr\u00fdva povrch tela \u017eivo\u010d\u00edchov','pokr\u00fdva povrch kost\u00ed','je funk\u010dn\u00fdm z\u00e1kladom \u017eliaz','obsahuje bunky schopn\u00e9 meni\u0165 vonkaj\u0161ie podnety na nervov\u00fd vzruch','je tvoren\u00fd bezjadrov\u00fdmi bunkami','m\u00e1 podporn\u00fa funkciu','m\u00e1 resorp\u010dn\u00fa funkc\u00edu',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(256,'256. Epitelov\u00e9 tkaniv\u00e1:','maj\u00fa typick\u00e9 vrstevnat\u00e9 ulo\u017eenie buniek','nemaj\u00fa vlastn\u00e9 cievne z\u00e1sobovanie','maj\u00fa vlastn\u00e9 cievne z\u00e1sobovanie','s\u00fa tvoren\u00e9 mal\u00fdmi bunkami s ve\u013eic\u00fdm mno\u017estvom medzibunkovej hmoty','maj\u00fa schopnos\u0165 obnovy po po\u0161koden\u00ed','obsahuj\u00fa mineralizovan\u00fa medzibunkov\u00fa hmotu','m\u00f4\u017eu zabezpe\u010dova\u0165 pr\u00edjem a vylu\u010dovanie l\u00e1tok','m\u00f4\u017eu ma\u0165 ochrann\u00fa funkciu',TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(257,'257. Deriv\u00e1tom poko\u017eky cicavcov s\u00fa:','ostne','\u0161up\u00edny r\u00fdb','kopyt\u00e1','pri vajcorodcoch perie','ko\u017en\u00e9 \u017e\u013eazy','chit\u00ednov\u00e1 kutikula','pelikula','anorganick\u00e9 schr\u00e1nky',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(258,'258. Medzi v\u00fdstelkov\u00e9 tkaniv\u00e1 patr\u00ed:','kryc\u00ed epitel','v\u00e4zivo','resorp\u010dn\u00fd epitel','chrupka','zmyslov\u00fd epitel','kutikula','\u017e\u013eazov\u00fd epitel','obrven\u00fd epitel',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(259,'259. Nervov\u00e9 tkanivo tvoria:','gliov\u00e9 bunky','nervov\u00e9 bunky','neur\u00f3ny','zmyslov\u00e9 bunky','bunky zabezpe\u010duj\u00face v\u00fd\u017eivu neur\u00f3nov','mnohojadrov\u00e9 bunky','bezjadrov\u00e9, vysoko \u0161pecializovan\u00e9 bunky','vakuolizovan\u00e9 bunky',TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(260,'260. Vn\u00fatorn\u00e9 oplodnenie:','je charakteristick\u00e9 pre v\u0161etky stavovce','znamen\u00e1 v\u00fdvin jedinca z neoplodnen\u00e9ho vaj\u00ed\u010dka','sa vyskytuje u v\u00e4\u010d\u0161iny r\u00fdb','znamen\u00e1, \u017ee k fertiliz\u00e1cii doch\u00e1dza vo vn\u00fatri tela sami\u010dky, napr. u vt\u00e1kov a cicavcov','znamen\u00e1 oplodnenie vaj\u00ed\u010dka mimo tela matky v chr\u00e1nenom uzavretom prostred\u00ed','nast\u00e1va pri \u017eivo\u010d\u00edchoch spravidla po kopul\u00e1cii','je typick\u00e9 pre v\u00e4\u010d\u0161inu suchozemsk\u00fdch \u017eivo\u010d\u00edchov','je charakteristick\u00e9 pre ryby a oboj\u017eiveln\u00edky',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(261,'261. Vonkaj\u0161ie oplodnenie:','je typick\u00e9 pre v\u00e4\u010d\u0161inu vodn\u00fdch \u017eivo\u010d\u00edchov','je charakteristick\u00e9 pre v\u0161etky stavovce','je charakteristick\u00e9 pre najjednoduch\u0161ie bezstavovce','znamen\u00e1, \u017ee k fertiliz\u00e1cii doch\u00e1dza vo vn\u00fatri tela sami\u010dky, napr. u vt\u00e1kov a cicavcov','znamen\u00e1, \u017ee vaj\u00ed\u010dko sa vyv\u00edja bez \u00fa\u010dasti spermie, napr. u niektor\u00fdch druhov r\u00fdb','je typick\u00e9 pre v\u00e4\u010d\u0161inu suchozemsk\u00fdch \u017eivo\u010d\u00edchov','sa spravidla vyskytuje u \u017eivo\u010d\u00edchov so zn\u00ed\u017eenou pohyblivos\u0165ou','je typick\u00e9 pre dvoch jedincov, ktor\u00ed si navz\u00e1jom vymie\u0148aj\u00fa spermie',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(262,'262. Sam\u010die gam\u00e9ty \u017eivo\u010d\u00edchov:','sa tvoria v pohlavn\u00fdch \u017e\u013eaz\u00e1ch - semenn\u00edkoch','s\u00fa produkovan\u00e9 u\u017e po\u010das embryon\u00e1lneho v\u00fdvinu','maj\u00fa hlavi\u010dku, ktor\u00e1 je temer cel\u00e1 tvoren\u00e1 haploidn\u00fdm jadrom','s\u00fa spravidla pohybliv\u00e9 spermatozoidy','s\u00fa ve\u013ek\u00e9 plazmatick\u00e9 bunky','maj\u00fa spravidla bi\u010d\u00edk a diploidn\u00e9 bunkov\u00e9 jadro','obsahuj\u00fa z\u00e1sobu \u017eiv\u00edn, potrebn\u00fdch pre v\u00fdvin zygoty','pri niektor\u00fdch druhoch s\u00fa schopn\u00e9 partenogenetick\u00e9ho v\u00fdvinu',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(263,'263. Nepohlavn\u00e9 rozmno\u017eovanie:','predstavuje v\u00fdvin nov\u00e9ho jedinca zo somatick\u00fdch buniek rodi\u010dovsk\u00e9ho organizmu','je \u010dast\u00e9 pri gonochoristoch','je typick\u00fd sp\u00f4sob rozmno\u017eovania jednobunkovcov','je typick\u00fd sp\u00f4sob rozmno\u017eovania hermafroditov','je tak\u00e9, pri ktorom je potomstvo geneticky identick\u00e9 s rodi\u010dovsk\u00fdm organizmom','je tak\u00e9, pri ktorom sa potomstvo geneticky l\u00ed\u0161i od rodi\u010dovsk\u00e9ho organizmu','je partenogen\u00e9za','je ozna\u010dovan\u00e9 ako vegetat\u00edvne',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(264,'264. C\u00e9lom je:','prvotn\u00e1 telov\u00e1 dutina','druhotn\u00e1 telov\u00e1 dutina vznikaj\u00faca v \u0161t\u00e1diu blastuly','druhotn\u00e1 telov\u00e1 dutina obklopen\u00e1 endodermom','druhotn\u00e1 telov\u00e1 dutina uzavret\u00e1 mezodermom','prvotn\u00e1 telov\u00e1 dutina uzavret\u00e1 ektodermom','prvotn\u00e1 telov\u00e1 dutina obklopen\u00e1 mezodermom','telov\u00e1 dutina medzi ektodermom a mezodermom','telov\u00e1 dutina medzi mezodermom a endodermom',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(265,'265. Hermafroditizmus:','znamen\u00e1 rozl\u00ed\u0161enos\u0165 pohlavia','znamen\u00e1 diferencovan\u00fa pohlavnos\u0165','znamen\u00e1 obojpohlavnos\u0165','znamen\u00e1 v\u00fdvoj sami\u010d\u00edch i sam\u010d\u00edch gon\u00e1d u toho ist\u00e9ho jedinca','znamen\u00e1 bezpohlavnos\u0165','sa vyskytuje pri paraziticky \u017eij\u00facich organizmoch','sa vyskytuje pri ulitn\u00edkoch','je \u010dast\u00fd pri ploskavcoch',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(266,'266. Gonochorizmus znamen\u00e1:','obojpohlavnos\u0165','odli\u0161nos\u0165 jedincov v prim\u00e1rnych pohlavn\u00fdch znakoch','bezpohlavnos\u0165','\u017ee sam\u010d\u00ed a sami\u010d\u00ed jedinci s\u00fa oddelen\u00e9ho pohlavia','v\u00fdvoj sam\u010d\u00edch i sami\u010d\u00edch gon\u00e1d u toho ist\u00e9ho jedinca','diferencovan\u00fa pohlavnos\u0165','odli\u0161nos\u0165 jedincov v sekund\u00e1rnych pohlavn\u00fdch znakoch','hermafroditizmus',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(267,'267. Sexu\u00e1lny dimorfizmus je:','odl\u00ed\u0161enie jedincov gon\u00e1dami i \u010fal\u0161\u00edmi morfologicko-funk\u010dn\u00fdmi vlastnos\u0165ami','podobnos\u0165 jedincov odl\u00ed\u0161en\u00e9ho pohlavia','rozdielnos\u0165 jedincov v prim\u00e1rnych i sekund\u00e1rnych pohlavn\u00fdch znakoch','tvorba rozdielnych pohlavn\u00fdch buniek','odli\u0161nos\u0165 jedincov len v gon\u00e1dach a gam\u00e9tach','pozorovate\u013en\u00fd pri gonochoristoch','pozorovate\u013en\u00fd pri hermafroditoch','typick\u00fd pre p\u00e1somnice',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(268,'268. Partenogen\u00e9za je:','v\u00fdvin oplodnen\u00e9ho vaj\u00ed\u010dka','v\u00fdvin neoplodnen\u00e9ho vaj\u00ed\u010dka','v\u00fdvin jedinca zo somatickej bunky','vonkaj\u0161ie oplodnenie','v\u00fdvin vaj\u00ed\u010dka bez oplodnenia','v\u00fdvin jedinca zo somatick\u00e9ho tkaniva','oplodnenie vaj\u00ed\u010dka viacer\u00fdmi spermiami','vn\u00fatorn\u00e9 oplodnenie',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(269,'269. Oogamia je:','v\u00fdvin jedinca z neoplodnen\u00e9ho vaj\u00ed\u010dka','typick\u00e1 pre jednobunkov\u00e9 \u017eivo\u010d\u00edchy','spl\u00fdvanie pohybliv\u00e9ho vaj\u00ed\u010dka s nepohyblivou spermiou','prechodn\u00e9 sp\u00e1janie dvoch vaj\u00ed\u010dok','splynutie nepohybliv\u00e9ho vaj\u00ed\u010dka s pohyblivou spermiou','kopul\u00e1cia','konjug\u00e1cia','typick\u00e1 pre mnohobunkov\u00e9 \u017eivo\u010d\u00edchy',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(270,'270. Samooplodnenie hermafroditov:','je z\u00e1kladn\u00fd sp\u00f4sob tvorby zygot','je zriedkav\u00fd jav pri rozmno\u017eovan\u00ed','prebieha len v pr\u00edpade, ke\u010f je zn\u00ed\u017een\u00e1 mo\u017enos\u0165 vz\u00e1jomn\u00e9ho p\u00e1renia','je be\u017en\u00fd sp\u00f4sob rozmno\u017eovania d\u00e1\u017e\u010foviek','je be\u017en\u00fd sp\u00f4sob rozmno\u017eovania p\u00e1somn\u00edc','je pri mnoh\u00fdch druhoch znemo\u017enen\u00e9 t\u00fdm, \u017ee sam\u010die a sami\u010die gam\u00e9ty nedozrievaj\u00fa s\u00fa\u010dasne','je mo\u017en\u00e9 len pri druhoch s pohlavnou dvojtvaros\u0165ou','nie je nikdy mo\u017en\u00e9',FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(271,'271. Vajco\u017eivorod\u00e9 \u017eivo\u010d\u00edchy:','s\u00fa napr. vt\u00e1ky','klad\u00fa oplodnen\u00e9 vaj\u00ed\u010dka','s\u00fa tak\u00e9, ktor\u00fdch oplodnen\u00e9 vaj\u00ed\u010dka sa vyv\u00edjaj\u00fa v tele samice a obaly vaj\u00ed\u010dok pukaj\u00fa pri kladen\u00ed','s\u00fa niektor\u00e9 oboj\u017eivelnficy','klad\u00fa neoplodnen\u00e9 vaj\u00ed\u010dka','s\u00fa gonochoristi','s\u00fa niektor\u00e9 klepietkavce','s\u00fa tak\u00e9, ktor\u00fdch oplodnenie nast\u00e1va mimotelovo',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(272,'272. Ozna\u010dte spr\u00e1vnu odpove\u010f:','sami\u010die gam\u00e9ty maj\u00fa hlavi\u010dku, kr\u010dek a bi\u010d\u00edk','vaj\u00ed\u010dko cicavcov obsahuje m\u00e1lo \u017e\u013atka','vaj\u00ed\u010dka oboj\u017eiveln\u00edkov maj\u00fa r\u00f4solovit\u00e9 obaly','vaj\u00ed\u010dka obr\u00fa\u010dkavcov maj\u00fa r\u00f4solovit\u00e9 obaly','vaj\u00ed\u010dko vt\u00e1kov je obalen\u00e9 bielkom, papierovit\u00fdmi blanami a \u0161krupinou','vaj\u00ed\u010dko cicavcov obsahuje ve\u013ea \u017e\u013atka','vaj\u00ed\u010dka hlavono\u017ecov maj\u00fa mal\u00e9 mno\u017estvo \u017eiv\u00edn','u obr\u00fa\u010dkavcov bielkovinov\u00e9 obaly obklopuj\u00fa viacero vaj\u00ed\u010dok dohromady',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(273,'273. Ontogen\u00e9za mnohobunkov\u00fdch \u017eivo\u010dichov:','za\u010d\u00edna oplodnen\u00edm a kon\u010d\u00ed smr\u0165ou jedinca','za\u010d\u00edna oplodnen\u00edm a kon\u010d\u00ed produkciou vlastn\u00e9ho potomstva','je geneticky riaden\u00fd proces','m\u00e1 embryon\u00e1lnu a postembryon\u00e1lnu etapu','za\u010d\u00edna oplodnen\u00edm a kon\u010d\u00ed naroden\u00edm alebo vyliahnut\u00edm jedinca','za\u010d\u00edna naroden\u00edm alebo vyliahnut\u00edm jedinca a kon\u010d\u00ed smr\u0165ou','zah\u0155\u0148a procesy ryhovania vaj\u00ed\u010dka','zah\u0155\u0148a proces pohlavn\u00e9ho dospievania',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(274,'274. Po\u010das embryon\u00e1lneho v\u00fdvinu prebieha:','gastrul\u00e1cia','br\u00e1zdenie neoplodnen\u00e9ho vaj\u00ed\u010dka pri partenogen\u00e9ze','br\u00e1zdenie oplodnen\u00e9ho vaj\u00ed\u010dka','br\u00e1zdenie spermatozoidu pri partenogen\u00e9ze','organogen\u00e9za','diferenci\u00e1cia z\u00e1rodo\u010dn\u00fdch vrstiev','diferenci\u00e1cia buniek','delenie oplodnen\u00e9ho vaj\u00ed\u010dka pri partenogen\u00e9ze',TRUE,TRUE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(275,'275. Spermatozoidy:','s\u00fa pohybliv\u00e9 sam\u010die gam\u00e9ty','s\u00fa pohybliv\u00e9 sami\u010die gam\u00e9ty','v\u00e4\u010d\u0161iny \u017eivo\u010d\u00edchov maj\u00fa diferencovan\u00fa hlavi\u010dku, kr\u010dek a bi\u010d\u00edk','s\u00fa diploidn\u00e9 bunky','s\u00fa haploidn\u00e9 bunky','maj\u00fa hlavi\u010dku, obsahuj\u00facu genetick\u00fa inform\u00e1ciu','maj\u00fa hlavi\u010dku, obsahuj\u00face z\u00e1sobn\u00e9 l\u00e1tky potrebn\u00e9 pre v\u00fd\u017eivu','s\u00fa nepohybliv\u00e9 sam\u010die gam\u00e9ty hermafroditov',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(276,'276. Sami\u010die gam\u00e9ty \u017eivo\u010d\u00edchov:','sa tvoria v ov\u00e1ri\u00e1ch','s\u00fa haploidn\u00e9','s\u00fa diploidn\u00e9','s\u00fa v\u017edy nepohybliv\u00e9','m\u00f4\u017eu by\u0165 schopn\u00e9 akt\u00edvneho pohybu','sa pri niektor\u00fdch druhoch \u017eivo\u010d\u00edchov m\u00f4\u017eu vyv\u00edja\u0165 aj bez oplodnenia','obsahuj\u00fa \u017eiviny potrebn\u00e9 pre v\u00fdvin po oplodnen\u00ed','vznikaj\u00fa mitotick\u00fdm delen\u00edm somatick\u00fdch buniek',TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(277,'277. Vaj\u00ed\u010dka \u017eivo\u010d\u00edchov:','s\u00fa sami\u010die gam\u00e9ty','s\u00fa pohybliv\u00e9 bunky','s\u00fa haploidn\u00e9','vznikaj\u00fa meiotick\u00fdm delen\u00edm z diploidn\u00fdch buniek','s\u00fa v\u017edy chr\u00e1nen\u00e9 papierovit\u00fdmi blanami','obsahuj\u00fa jadro a z\u00e1sobn\u00e9 l\u00e1tky','sa tvoria v semenn\u00edkoch','pri druhoch s mimotelov\u00fdm v\u00fdvinom obsahuj\u00fa velk\u00e9 mno\u017estvo \u017e\u013atka',TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(278,'278. Biogenetick\u00fd z\u00e1kon formuloval:','Charles Darwin','Karl Linn\u00e9','Jean Baptiste Lamarck','Ernst Haeckel','Aristoteles','Andreas Vesalius','Louis Pasteur','James Watson',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(279,'279. Procesom gastrul\u00e1cie vznikaj\u00fa z\u00e1rodo\u010dn\u00e9 vrstvy ozna\u010dovan\u00e9 ako:','endoderm, ektoderm a mezoderm','uktoderm a endoderm','endoderm, mezench\u00fdm a ektoderm','blastoc\u00e9l a mezench\u00fdm','endoderm, ektoderm a c\u00e9lom','blastoc\u00e9l a c\u00e9lom','mezench\u00fdm a mezoderm','mezench\u00fdm a c\u00e9lom',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(280,'280. Mezodermov\u00e9ho p\u00f4vodu:','je poko\u017eka','je srdce','je tr\u00e1viaca s\u00fastava','je obehov\u00e1 s\u00fastava','s\u00fa zmyslov\u00e9 bunky','je nervov\u00e1 s\u00fastava','je kostra stavovcov','s\u00fa svaly',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(281,'281. Ektodermov\u00e9ho p\u00f4vodu je:','poko\u017eka','nervov\u00e1 s\u00fastava','pe\u010de\u0148','kostra stavovcov','cievna s\u00fastava','vonkaj\u0161ia kostra \u010dl\u00e1nkono\u017ecov','srs\u0165 cicavcov','predn\u00e1 \u010das\u0165 tr\u00e1viacej r\u00fary',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(282,'282. Endodermov\u00e9ho p\u00f4vodu je:','poko\u017eka','nervov\u00e1 s\u00fastava','pe\u010de\u0148','kostra \u010dl\u00e1nkono\u017ecov','cievna s\u00fastava','vonkaj\u0161ia kostra \u010dl\u00e1nkono\u017ecov','tenk\u00e9 \u010drevo','koncov\u00e1 \u010das\u0165 tr\u00e1viacej s\u00fastavy',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(283,'283. Homologick\u00e9 org\u00e1ny s\u00fa:','tak\u00e9, ktor\u00e9 maj\u00fa rovnak\u00fd fylogenetick\u00fd p\u00f4vod, i ke\u010f ich funkcia a tvar m\u00f4\u017eu by\u0165 rozdielne','r\u00f4zneho fylogenetick\u00e9ho p\u00f4vodu, i ke\u010f pri recentn\u00fdch z\u00e1stupcoch m\u00f4\u017eu ma\u0165 podobn\u00fd tvar a plni\u0165 rovnak\u00fa funkciu','napr. modifik\u00e1cie kon\u010dat\u00edn cicavcov','napr. \u0161upiny plazov a perie vt\u00e1kov','napr. kr\u00eddla vt\u00e1kov a hmyzu','napr. kr\u00eddla vt\u00e1kov a \u013eudsk\u00e9 ruky','napr. p\u013e\u00facne vaky ulitn\u00edkov a p\u013e\u00faca stavovcov','r\u00f4zneho fylogenetick\u00e9ho p\u00f4vodu a pri recentn\u00fdch z\u00e1stupcoch plnia rozdielnu funkciu',TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(284,'284. Analogick\u00e9 org\u00e1ny s\u00fa:','tak\u00e9, ktor\u00e9 maj\u00fa rovnak\u00fd fylogenetick\u00fd p\u00f4vod, i ke\u010f ich funkcia a tvar m\u00f4\u017eu by\u0165 rozdielne','napr. modifik\u00e1cie kon\u010dat\u00edn cicavcov','napr. kr\u00eddla vt\u00e1kov a hmyzu','r\u00f4zneho fylogenetick\u00e9ho p\u00f4vodu, i ke\u010f pri recentn\u00fdch z\u00e1stupcoch m\u00f4\u017eu ma\u0165 podobn\u00fd tvar a plni\u0165 rovnak\u00fa funkciu','napr. \u0161upiny plazov a perie vt\u00e1kov','napr. kr\u00eddla vt\u00e1kov a \u013eudsk\u00e9 ruky','napr. p\u013e\u00facne vaky pav\u00fakov a p\u013e\u00faca stavovcov','r\u00f4zneho fylogenetick\u00e9ho p\u00f4vodu a pri recentn\u00fdch z\u00e1stupcoch plnia rozdielnu funkciu',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(285,'285. Poko\u017eka \u017eivo\u010d\u00edchov:','m\u00f4\u017ee by\u0165 tvoren\u00e1 viacvrstvov\u00fdm epitelom','vznikla diferenci\u00e1ciou z mezodermu','m\u00f4\u017ee obsahova\u0165 krycie bunky','je povrchov\u00e1 vrstva tela jednobunkovcov','vznikla diferenci\u00e1ciou z ektodermu','m\u00f4\u017ee by\u0165 tvoren\u00e1 obrven\u00fdm epitelom','\u010dl\u00e1nkono\u017ecov m\u00f4\u017ee by\u0165 chr\u00e1nen\u00e1 nebunkovou vrstvou - pelikulou','hubiek je mezodermov\u00e9ho p\u00f4vodu',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(286,'286. Ko\u017ea cicavcov pozost\u00e1va z:','viacvrstvov\u00e9ho zrohovatel\u00e9ho epitelu','vrstvy zam\u0161e a podko\u017en\u00e9ho v\u00e4ziva','ochrannej povrchovej vrstvy - kutikuly','deriv\u00e1tov poko\u017eky, napr. peria','poko\u017eky, ktor\u00e1 je mezodermov\u00e9ho p\u00f4vodu','vrstvy zam\u0161e, ktor\u00e1 je mezodennov\u00e9ho p\u00f4vodu','poko\u017eky a zam\u0161e, ktor\u00e9 s\u00fa ektodermov\u00e9ho p\u00f4vodu','mnohojadrov\u00fdch zrohovatel\u00fdch buniek',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(287,'287. V ko\u017ei stavovcov sa m\u00f4\u017eu nach\u00e1dza\u0165 \u017e\u013eazy:','potn\u00e9 a pachov\u00e9','mazov\u00e9 a slizov\u00e9','mlie\u010dne a pohlavn\u00e9','jedov\u00e9 a slizov\u00e9','mazov\u00e9 a mlie\u010dne','jedov\u00e9 a slinn\u00e9','pigmentov\u00e9 a tukov\u00e9','potn\u00e9 a pohlavn\u00e9',TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(288,'288. Schr\u00e1nky jednobunkovcov:','plnia funkciu pohybovej organely','obsahuj\u00fa uhli\u010ditan v\u00e1penat\u00fd','obsahuj\u00fa oxid kremi\u010dit\u00fd','obsahuj\u00fa celul\u00f3zu','maj\u00fa ochrann\u00fa funkciu','obsahuj\u00fa bielkoviny a lipidy','vznikaj\u00fa mineraliz\u00e1ciou plazmatickej membr\u00e1ny','nahr\u00e1dzaj\u00fa plazmatick\u00fa bunkov\u00fa membr\u00e1nu',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(289,'289. Na povrchu kosti je:','okostica','v\u00e4zivov\u00e1 blana','epitelov\u00e1 ochrann\u00e1 vrstva','hubov\u00e9 kostn\u00e9 tkanivo','kostn\u00e1 dre\u0148','chrupka','mineralizovan\u00e1 chrupka','glykokalyx',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(290,'290. Z\u00e1kladom vn\u00fatornej kostry stavovcov z fylogenetick\u00e9ho h\u013eadiska je:','chorda dorsalis','chrbtov\u00e1 struna','chrbtica endodermov\u00e9ho p\u00f4vodu','lebka a kostra hrudn\u00edka','spojivov\u00e9 tkanivo ektodermov\u00e9ho p\u00f4vodu','modifikovan\u00e1 vonkaj\u0161ia kostra','povrazec vakuolizovan\u00fdch buniek mezodermu','chorda dorsalis endodermov\u00e9ho p\u00f4vodu',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(291,'291. Kostru trupu stavovcov tvor\u00ed:','chrbtica a kostra hrudn\u00edka s rebrami','columna vertebralis a kostra hrudn\u00edka s rebrami','chrbtica s lebkou','kostra hrudn\u00edka a horn\u00fdch kon\u010dat\u00edn','spojivov\u00e9 tkanivo - kos\u0165, chrupka a v\u00e4zivo','spojivov\u00e9 tkanivo - kos\u0165, v\u00e4zivo a svaly','chorda dorsalis','chrbtica a chrbtov\u00e9 svaly',TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(292,'292. V\u00e4zivo:','je typ podporn\u00e9ho tkaniva','obsahuje mno\u017estvo medzibunkovej hmoty s kolag\u00e9nov\u00fdmi a elastick\u00fdmi vl\u00e1knami','tvor\u00ed napr\u00edklad medzistavcov\u00e9 platni\u010dky','tvor\u00ed napr. \u0161\u013eachy a puzdr\u00e1 org\u00e1nov','vyp\u013a\u0148a priestor medzi org\u00e1nmi','zabezpe\u010duje pevnos\u0165 kosti','je z\u00e1kladom kostry drsnoko\u017ecov','je tvoren\u00e9 bunkami s vysok\u00fdm obsahom miner\u00e1lnych l\u00e1tok',TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(293,'293. Povrch tela \u010dl\u00e1nkono\u017ecov tvor\u00ed:','kutikula','s\u00favisl\u00e1 nebunkov\u00e1 vrstva','kutikula spevnen\u00e1 chit\u00ednom','spong\u00edn, ktor\u00fd m\u00f4\u017ee by\u0165 inkrustovan\u00fd oxidom kremi\u010dit\u00fdm','pelikula','pigmentovan\u00e9 bunky','poko\u017eka tvoren\u00e1 viacvrstvov\u00fdm epitelom','poko\u017eka tvoren\u00e1 jednovrstvov\u00fdm epitelom',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(294,'294. Cel\u00fa kostru stavovcov tvoria tieto funk\u010dn\u00e9 celky:','tv\u00e1rov\u00e1 a mozgov\u00e1 \u010das\u0165 lebky','chrbtica a kostra hrudn\u00edka s rebrami','kostra lebky, kostra trupu, kostra horn\u00fdch a doln\u00fdch kon\u010dat\u00edn','tv\u00e1rov\u00e1 a mozgov\u00e1 \u010das\u0165 lebky, kostra trupu, kostra horn\u00fdch  a doln\u00fdch kon\u010dat\u00edn','kostra lebky, chrbtica, kostra hrudn\u00edka s rebrami, kostra horn\u00fdch a doln\u00fdch kon\u010dat\u00edn','kostra trupu, kostra horn\u00fdch a doln\u00fdch kon\u010dat\u00edn','kostra lebky, kostra horn\u00fdch a doln\u00fdch kon\u010dat\u00edn','kostra lebky, kostra trupu, kosti predn\u00fdch a zadn\u00fdch kon\u010dat\u00edn',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(295,'295. Jednobunkov\u00e9 organizmy sa pohybuj\u00fa pomocou:','pan\u00f4\u017eok','bi\u010d\u00edkov','b\u0155v','svalov','mikrotubul\u00e1rnych \u0161trukt\u00far','intermedi\u00e1rnych filamentov','pohybovej vakuoly','ko\u017enosvalov\u00e9ho vaku',TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(296,'296. Kin\u00e9za je:','zr\u00fdchlenie pohybovej aktivity vzh\u013eadom na ur\u010dit\u00fd podnet','zmena smeru pohybu vzh\u013eadom na ur\u010dit\u00fd podnet','spomalenie pohybovej aktivity vzh\u013eadom na ur\u010dit\u00fd podnet','zmena trvania bunkov\u00e9ho cyklu jednobunkovcov','obdobie zn\u00ed\u017eenej aktivity organizmu v nepriazniv\u00fdch podmienkach','kontrakcia myofibr\u00edl v svalovom vl\u00e1kne','\u0161\u00edrenie vzruchu','sp\u00f4sob pohlavn\u00e9ho rozmno\u017eovania n\u00e1levn\u00edkov',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(297,'297. Pomal\u00e9 svalov\u00e9 vl\u00e1kna kostrov\u00fdch svalov:','s\u00fa viac prisp\u00f4soben\u00e9 na vytrvalostn\u00fa pr\u00e1cu','obsahuj\u00fa menej myoglob\u00ednu','sa unavia r\u00fdchlej\u0161ie','obsahuj\u00fa viac myoglob\u00ednu','sa naz\u00fdvaj\u00fa \u010derven\u00e9 svalov\u00e9 vl\u00e1kna','preva\u017euj\u00fa napr. v lietac\u00edch svaloch vt\u00e1kov','s\u00fa tvoren\u00e9 jednojadrov\u00fdmi bunkami','s\u00fa tvoren\u00e9 bezjadrov\u00fdmi bunkami',TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(298,'298. Hladk\u00e9 svaly:','s\u00fa m\u00e1lo unavite\u013en\u00e9','maj\u00fa men\u0161iu schopnos\u0165 kontrakcie ne\u017e prie\u010dne pruhovan\u00e9 svaly','maj\u00fa v\u00e4\u010d\u0161iu schopnos\u0165 kontrakcie ne\u017e prie\u010dne pruhovan\u00e9 svaly','s\u00fa s\u00fa\u010das\u0165ou stien tr\u00e1viacej r\u00fary stavovcov','s\u00fa s\u00fa\u010das\u0165ou kostrov\u00e9ho svalstva','m\u00f4\u017eu reagova\u0165 na tepeln\u00e9 podnety','podlieh\u00e1K1 priamej regul\u00e1cii \u00fastrednej nervovej s\u00fastavy','s\u00fa regulovan\u00e9 l\u00e1tkovo',TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(299,'299. Hladk\u00e9 svalov\u00e9 tkanivo:','je tvoren\u00e9 mnohojadrov\u00fdmi svalov\u00fdmi vl\u00e1knami','tvor\u00ed pohybov\u00fd apar\u00e1t mnoh\u00fdch ni\u017e\u0161\u00edch bezstavovcov','tvor\u00ed kostrov\u00e9 svaly stavovcov','tvor\u00ed steny vn\u00fatorn\u00fdch org\u00e1nov stavovcov','obsahuje ve\u013ek\u00e9 mno\u017estvo kolag\u00e9nov\u00fdch vl\u00e1ken','je schopn\u00e9 kontrakcie','je pri cicavcoch riaden\u00e9 auton\u00f3mnym nervov\u00fdm syst\u00e9mom','je tvoren\u00e9 jednojadrov\u00fdmi vretenovit\u00fdmi bunkami',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(300,'300. Srdcov\u00fd sval stavovcov:','stavbou pripom\u00edna prie\u010dne pruhovan\u00e9 svaly','funkciou pripom\u00edna hladk\u00e9 svaly','stavbou pripom\u00edna hladk\u00e9 svaly','funkciou pripom\u00edna prie\u010dne pruhovan\u00e9 svaly','m\u00e1 prevahu bielych svalov\u00fdch vl\u00e1kien','m\u00e1 prevahu \u010derven\u00fdch svalov\u00fdch vl\u00e1kien','je regulovan\u00fd auton\u00f3mnym nervov\u00fdm syst\u00e9mom','je ovplyv\u0148ovan\u00fd aktivitami organizmu',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(301,'301. Pod\u013ea chemick\u00e9ho zlo\u017eenia, \u0161trukt\u00fary a sp\u00f4sobu s\u0165ahovania rozli\u0161uje kostrov\u00e9 svalov\u00e9 vl\u00e1kna:','\u010derven\u00e9, r\u00fdchlo vytv\u00e1raj\u00face ve\u013ek\u00fd kysl\u00edkov\u00fd dlh','biele, schopn\u00e9 r\u00fdchlej kontrakcie','biele, schopn\u00e9 pomal\u0161ej kontrakcie','\u010derven\u00e9, schopn\u00e9 pomal\u0161ej kontrakcie','biele, prisp\u00f4soben\u00e9 na vytrval\u00fa pr\u00e1cu','biele, r\u00fdchlo vytv\u00e1raj\u00face velic\u00fd kysl\u00edkov\u00fd dlh','\u010derven\u00e9, schopn\u00e9 r\u00fdchlej kontrakcie','\u010derven\u00e9, prisp\u00f4soben\u00e9 na vytrval\u00fa pr\u00e1cu',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(302,'302. Kontrakcia prie\u010dne pruhovan\u00e9ho svalu:','prebieha pri vysokej koncentr\u00e1cii Ca^2+ v cytoplazme svalovej bunky svalov\u00e9ho vl\u00e1kna','je podmienen\u00e1 reakciou medzi akt\u00ednom a myoz\u00ednom','je podmienen\u00e1 vznikom stabiln\u00e9ho aktinomyoz\u00ednu','prebieha na z\u00e1klade nervov\u00e9ho vzruchu','je riaden\u00e1 \u00fastrednou nervovou s\u00fastavou','je riaden\u00e1 auton\u00f3mnymi nervami','prebieha len ak je inhibovan\u00e1 relaxa\u010dn\u00e1 bielkovina \u2014 tropon\u00edn','je nevratn\u00e1',TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(303,'303. Sval, ktor\u00fd je v pokojovom re\u017eime:','m\u00e1 n\u00edzku koncentr\u00e1ciu Ca^2+ v cytoplazme','m\u00e1 vysok\u00fa koncentr\u00e1ciu Ca^2+ v cytoplazme','m\u00e1 tropon\u00edn naviazan\u00fd na akt\u00edn a myoz\u00edn, \u010d\u00edm zabra\u0148uje vzniku aktinomyoz\u00ednu','m\u00e1 blokovan\u00fa \u010dinnos\u0165 relaxa\u010dn\u00fdch bielkov\u00edn','m\u00e1 Ca^2+ naviazan\u00fd na tropon\u00edn','je schopn\u00fd prija\u0165 podnet na kontrakciu','je nedostato\u010dne z\u00e1sobovan\u00fd kysl\u00edkom','nie je schopn\u00fd meni\u0165 nervov\u00fd sign\u00e1l na podnet pre kontrakciu',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(304,'304. Svalov\u00e1 \u00fanava:','sa objekt\u00edvne prejavuje zn\u00ed\u017een\u00edm v\u00fdkonnosti svalu','je sp\u00f4soben\u00e1 hromaden\u00edm metabolitov, napr. kyseliny mlie\u010dnej','je sp\u00f4soben\u00e1 hromaden\u00edm metabolitov, napr. glykog\u00e9nu','je sp\u00f4soben\u00e1 nedostatkom kysl\u00edka','je sp\u00f4soben\u00e1 nedostatkom kreat\u00ednfosf\u00e1tu','je sp\u00f4soben\u00e1 hromaden\u00edm kreat\u00ednfosf\u00e1tu','sa prejavuje zr\u00fdchlen\u00edm kontrakcie','m\u00f4\u017ee vies\u0165 k zastaveniu \u010dinnosti svalu',TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(305,'305. Vn\u00fatrobunkov\u00e9 tr\u00e1venie sa vyskytuje:','pri obr\u00fa\u010dkavcoch','pri stavovcoch','pri jednobunkov\u00fdch organizmoch','pri \u010dl\u00e1nkono\u017ecoch','pri v\u0161etk\u00fdch m\u00e4kk\u00fd\u0161och','pri jednoduch\u00fdch mnohobunkov\u00fdch organizmoch','pri prvokoch','pri pav\u00fakoch',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(306,'306. Mimobunkov\u00e9 tr\u00e1venie:','sa vyskytuje pri obr\u00fa\u010dkavcoch','sa prv\u00fdkr\u00e1t objavuje pri dvojstranne s\u00famern\u00fdch \u017eivo\u010d\u00edchoch','je enzymatick\u00e9 \u0161tiepenie potravy mimo tr\u00e1viacej bunky','je typick\u00e9 pre stavovce','sa prv\u00fdkr\u00e1t objavuje pri ploskavcoch','je sp\u00f4sob vn\u00fatrotelov\u00e9ho tr\u00e1venia','je enzymatick\u00e9 \u0161tiepenie potravy mimo tela \u017eivo\u010d\u00edcha','je sp\u00f3sob mechanick\u00e9ho spracovania potravy',TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(307,'307. Mimotelov\u00e9 tr\u00e1venie:','je enzymatick\u00e9 \u0161tiepenie potravy mimo tela \u017eivo\u010d\u00edcha','je enzymatick\u00e9 \u0161tiepenie potravy v tr\u00e1viacej vakuole','je enzymatick\u00e9 \u0161tiepenie potravy v tr\u00e1viacej r\u00fare','je mechanick\u00e9 spracovanie potravy mimo tela \u017eivo\u010d\u00edcha','je typick\u00e9 pre obr\u00fa\u010dkavce','sa vyskytuje napr. pri pav\u00fakovcoch','je tr\u00e1venie potravy v gastrovaskul\u00e1rnej s\u00fastave','je enzymatick\u00e9 \u0161tiepenie potravy v bunk\u00e1ch ektodermu',TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(308,'308. Pri mechanickom spracovan\u00ed sa potrava:','v \u00fastnej dutine rozomie\u013ea a navlh\u010duje','sprac\u00fava v \u017e\u013eaznatom \u017eal\u00fadku vt\u00e1kov','rozklad\u00e1 za pomoci bakt\u00e9ri\u00ed v bachore pre\u017e\u00favavcov','rozomie\u013ea v svalnatom \u017eal\u00fadku vt\u00e1kov','enzymaticky rozklad\u00e1 mimo tela \u017eivo\u010d\u00edcha','rozomie\u013ea v hrvoli vt\u00e1kov','emulguje \u017el\u010dou v tenkom \u010dreve cicavcov','zahus\u0165uje v hrubom \u010dreve cicavcov',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(309,'309. Tr\u00e1viaca s\u00fastava p\u0155hlivcov:','m\u00e1 jeden otvor, plniaci funkciu prij\u00edmaciu aj vyvrhovaciu','m\u00e1 samostatn\u00e9 otvory pre pr\u00edjem potravy a vyvrhovanie nestr\u00e1ven\u00fdch zvy\u0161kov','pri niektor\u00fdch druhoch je rozvetven\u00e1 a pln\u00ed aj obehov\u00fa funkciu','m\u00e1 povrch zv\u00e4\u010d\u0161en\u00fd klkmi a mikroklkmi','je \u010dlenen\u00e1 na pa\u017eer\u00e1k a \u017eal\u00fadok','napr. med\u00fazovcov sa naz\u00fdva gastrovaskul\u00e1rna','pos\u00fava potravu peristaltick\u00fdmi pohybmi','mechanicky sprac\u00fava prijat\u00fa potravu',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(310,'310. M\u00e4so\u017erav\u00e9 \u017eivo\u010d\u00edchy:','maj\u00fa dobre vyvinut\u00e9 org\u00e1ny na uchopenie koristi','sa ozna\u010duj\u00fa ako karnivora','sa ozna\u010duj\u00fa ako herbivora','sa ozna\u010duj\u00fa ako omnivora','s\u00fa prim\u00e1rne konzumenty','s\u00fa sekund\u00e1rne konzumenty','s\u00fa dekompozitory','s\u00fa v\u0161e\u017erav\u00e9',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(311,'311. Bylino\u017erav\u00e9 \u017eivo\u010d\u00edchy:','vyu\u017e\u00edvaj\u00fa na spracovanie potravy symbiotick\u00e9 mikroorganizmy','sa ozna\u010duj\u00fa ako karnivora','sa ozna\u010duj\u00fa ako herbivora','sa ozna\u010duj\u00fa ako omnivora','s\u00fa prim\u00e1rne konzumenty','s\u00fa sekund\u00e1rne konzumenty','maj\u00fa tr\u00e1viacu r\u00faru v\u00fdrazne dlh\u0161iu ako m\u00e4so\u017eravce','maj\u00fa \u0161pecifick\u00fa stavbu \u017eal\u00fadka',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(312,'312. Omnivora:','s\u00fa prim\u00e1rne konzumenty','s\u00fa konzumenty vy\u0161\u0161ieho radu','konzumuj\u00fa v\u00fdlu\u010dne rastlinn\u00fa potravu','s\u00fa v\u0161e\u017eravce','konzumuj\u00fa v\u00fdlu\u010dne m\u00e4sit\u00fa potravu','s\u00fa napr. medve\u010f a \u010dlovek','s\u00fa napr. jele\u0148 a vlk','produkuj\u00fa enz\u00fdmy rozkladaj\u00face celul\u00f3zu',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(313,'313. Hepatopankreas je \u017e\u013eaza tr\u00e1viacej s\u00fastavy:','m\u00e4kk\u00fd\u0161ov','stavovcov','obr\u00fa\u010dkavcov','vt\u00e1kov','cicavcov','plazov','krokod\u00edlov','\u010dloveka',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(314,'314. Tr\u00e1venie bielkov\u00edn sa za\u010d\u00edna:','v \u00fastnej dutine','v \u017eal\u00fadku p\u00f4soben\u00edm peps\u00ednu','v \u017eal\u00fadku p\u00f4soben\u00edm tryps\u00ednu','v tenkom \u010dreve','v hrubom \u010dreve','v pod\u017eal\u00fadkovej \u017eraze p\u00f4soben\u00edm tryps\u00ednu','v \u017eal\u00fadku p\u00f4soben\u00edm peps\u00ednog\u00e9nu','v pe\u010deni',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(315,'315. Vt\u00e1ky maj\u00fa \u017eal\u00fadok:','\u0161tvordielny','\u017e\u013eaznaty, v ktorom sa potrava sprac\u00fava mechanicky','svalnat\u00fd, v ktorom sa potrava tr\u00e1vi chemicky','\u017e\u013eaznaty, v ktorom sa potrava tr\u00e1vi chemicky','svalnat\u00fd, v ktorom sa potrava sprac\u00fava mechanicky','dvojdielny','pozost\u00e1vaj\u00faci z hrvo\u013ea a svalnat\u00e9ho \u017eal\u00fadka','rozdelen\u00fd na hrvo\u013e a samotn\u00fd \u017eal\u00fadok',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(316,'316. \u017dal\u00fadok pre\u017e\u00favavcov pozost\u00e1va:','z 3 pred\u017eal\u00fadkov: bachor, \u010depiec, kniha a z vlastn\u00e9ho \u017eal\u00fadka: slez','z 3 pred\u017eal\u00fadkov: bachor, slez, kniha a z vlastn\u00e9ho \u017eal\u00fadka: \u010depiec','z 2 pred\u017eal\u00fadkov: bachor, \u010depiec a 2 \u017eal\u00fadkov: kniha, slez','z 3 pred\u017eal\u00fadkov: slez, kniha, \u010depiec a z vlastn\u00e9ho \u017eal\u00fadka: bachor','zo 4 pred\u017eal\u00fadkov: bachor, kniha, \u010depiec, slez','zo \u017e\u013eaznat\u00e9ho a svalnat\u00e9ho \u017eal\u00fadka','z pred\u017eal\u00fadku a trojdielneho vlastn\u00e9ho \u017eal\u00fadku','z roz\u0161\u00edren\u00e9ho pa\u017eer\u00e1ka a vlastn\u00e9ho \u017eal\u00fadka',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(317,'317. Celul\u00f3za v potrave bylino\u017eravcov je rozkladan\u00e1:','v \u00fastnej dutine enz\u00fdmami obsiahnut\u00fdmi v slin\u00e1ch','mechanicky v \u017eal\u00fadku','v \u017eal\u00fadku \u010dinnos\u0165ou symbiotick\u00fdch mikroorganizmov','v tenkom \u010dreve p\u00f4soben\u00edm \u017el\u010de','v tenkom \u010dreve p\u00f4soben\u00edm amyl\u00e1z','v pankrease p\u00f4soben\u00edm pankreasov\u00fdch enz\u00fdmov','v \u017eal\u00fadku na oligosacharidy, ktor\u00e9 s\u00fa \u010falej nestr\u00e1vite\u013en\u00e9','v hrubom \u010dreve',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(318,'318. \u017d\u013eazy v sliznici \u017eal\u00fadka cicavcov produkuj\u00fa:','\u017eal\u00fado\u010dn\u00fa \u0161\u0165avu, ktor\u00e1 obsahuje enz\u00fdmy potrebn\u00e9 pre tr\u00e1venie bielkov\u00edn','tryps\u00edn, ktor\u00fd \u0161tiepi bielkoviny','kyselinu chlorovod\u00edkov\u00fa','muc\u00edn','ch\u00fdmus','nukle\u00e1zy potrebn\u00e9 pre tr\u00e1venie nukleov\u00fdch kysel\u00edn','\u017el\u010d','bilirub\u00edn',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(319,'319. Najd\u00f4le\u017eitej\u0161\u00edm org\u00e1nom vstreb\u00e1vania l\u00e1tok z tr\u00e1viacej s\u00fastavy je:','\u017eal\u00fadok','hltan','pa\u017eer\u00e1k','tenk\u00e9 \u010drevo','hrub\u00e9 \u010drevo','pankreas','pod\u017eal\u00fadkov\u00e1 \u017e\u013eaza','pe\u010de\u0148',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(320,'320. \u017dl\u010d:','je produktom pe\u010dene','je produktom \u017el\u010dn\u00edka','skladuje sa v \u017el\u010dn\u00edku','vytv\u00e1ra sa nepretr\u017eite','sa vytv\u00e1ra a\u017e po prijat\u00ed potravy','obsahuje enz\u00fdmy potrebn\u00e9 pre tr\u00e1venie tukov','obsahuje l\u00e1tky emulguj\u00face tuky','alkalizuje prostredie v tenkom \u010dreve',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(321,'321. Tr\u00e1venie bielkov\u00edn pri stavovcoch za\u010d\u00edna:','v \u00fastnej dutine p\u00f4soben\u00edm slinn\u00fdch amyl\u00e1z','v \u017eal\u00fadku p\u00f4soben\u00edm tryps\u00ednu','v \u017eal\u00fadku p\u00f4soben\u00edm pepsinog\u00e9nu','v \u017eal\u00fadku p\u00f4soben\u00edm peps\u00ednu','v tenkom \u010dreve p\u00f4soben\u00edm kyseliny chlorovod\u00edkovej','v tenkom \u010dreve p\u00f4soben\u00edm \u017eI\u010de','v \u017eal\u00fadku p\u00f4soben\u00edm muc\u00ednu','v tenkom \u010dreve p\u00f4soben\u00edm bilirub\u00ednu',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(322,'322. Tr\u00e1venie tukov pri stavovcoch prebieha:','v \u017eal\u00fadku p\u00f4soben\u00edm peps\u00ednu','v tenkom \u010dreve p\u00f4soben\u00edm tryps\u00ednu','v tenkom \u010dreve p\u00f4soben\u00edm \u017el\u010de a lip\u00e1zy','v tenkom \u010dreve p\u00f4soben\u00edm \u017el\u010de a amyl\u00e1zy','v pe\u010deni','v \u017eal\u00fadku p\u00f4soben\u00edm lip\u00e1zy','prostredn\u00edctvom pankreasovej \u0161\u0165avy','prostredn\u00edctvom slinn\u00fdch amyl\u00e1z',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(323,'323. Sliny v \u00fastnej dutine cicavcov:','navlh\u010duj\u00fa potravu, \u010d\u00edm u\u013eah\u010duj\u00fa hltanie','obsahuj\u00fa peps\u00edn, ktor\u00fd \u0161tiepi bielkoviny','m\u00f4\u017eu obsahova\u0165 amyl\u00e1zy','pom\u00e1haj\u00fa udr\u017eiava\u0165 \u010distotu \u00fastnej dutiny','neutralizuj\u00fa kyseliny a zabra\u0148uj\u00fa tak vzniku zubn\u00e9ho kazu','obsahuj\u00fa lip\u00e1zu, ktor\u00e1 \u0161tiepi tuky','m\u00f4\u017eu ma\u0165 protibakteri\u00e1lne \u00fa\u010dinky','obsahuj\u00fa enz\u00fdmy \u0161tiepiace celul\u00f3zu',TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(324,'324. Pankreasov\u00e1 \u0161\u0165ava:','p\u00f4sob\u00ed v tr\u00e1viacom syst\u00e9me cicavcov na v\u0161etky \u017eiviny','obsahuje amyl\u00e1zu, ktor\u00e1 rozklad\u00e1 \u0161krob','obsahuje lip\u00e1zu, ktor\u00e1 rozklad\u00e1 tuky','obsahuje tryps\u00edn rozkladaj\u00faci bielkoviny','obsahuje chol\u00edn, ktor\u00fd emulguje tuky','obsahuje amyl\u00e1zu, ktor\u00e1 rozklad\u00e1 tuky','je vylu\u010dovan\u00e1 pankreasom do \u017el\u010dovodu','je vylu\u010dovan\u00e1 pankreasom do tenk\u00e9ho \u010dreva',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(325,'325. Do tenk\u00e9ho \u010dreva vy\u00fas\u0165uje:','pankreas produkuj\u00faci \u017el\u010d','pod\u017eal\u00fadkov\u00e1 \u017e\u013eaza, produkuj\u00faca pankreasov\u00fa \u0161\u0165avu','\u017el\u010dn\u00edk, produkuj\u00faci lip\u00e1zy','pe\u010de\u0148, produkuj\u00faca \u017el\u010d','pankreas','\u017e\u013eazy tenk\u00e9ho \u010dreva produkuj\u00face \u010drevn\u00fa \u0161\u0165avu','pe\u010de\u0148, produkuj\u00faca pepsinog\u00e9n a trypsinog\u00e9n','pod\u017eal\u00fadkov\u00e1 \u017e\u013eaza, produkuj\u00faca bilirub\u00edn a biliverd\u00edn',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(326,'326. Peristaltick\u00e9 pohyby tenk\u00e9ho \u010dreva:','podporuj\u00fa pohyb tr\u00e1veniny','premie\u0161avaj\u00fa tr\u00e1veninu s enz\u00fdmami a podporuj\u00fa vstreb\u00e1vanie','podporuj\u00fa vylu\u010dovanie \u010drevnej \u0161\u0165avy','podporuj\u00fa vylu\u010dovanie \u017el\u010de','napom\u00e1haj\u00fa zahus\u0165ovaniu stolice','s\u00fa riaden\u00e9 motorick\u00fdm\u00ed nervami','s\u00fa regulovan\u00e9 auton\u00f3mnym syst\u00e9mom','s\u00fa v\u00f4\u013eovo ovl\u00e1date\u013en\u00e9',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(327,'327. Nestr\u00e1vite\u013en\u00e9 \u010dasti potravy sa vylu\u010duj\u00fa z tela:','pri prvokoch exocyt\u00f3zou','pri p\u0155hlivcoch otvorom, ktory je z\u00e1rove\u0148 otvorom prij\u00edmaj\u00facim potravu','pri cicavcoch an\u00e1lnym otvorom v podobe stolice','pri prvokoch endocyt\u00f3zou','pri vt\u00e1koch kloakou','pri pre\u017e\u00favavcoch \u00fastnym otvorom','po metabolickom spracovan\u00ed obli\u010dkami','pri mnoho\u0161tetinavcoch nefr\u00eddiami',TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(328,'328. Resorp\u010dn\u00e1 plocha tenk\u00e9ho \u010dreva stavovcov je zv\u00e4\u010d\u0161en\u00e1:','klkmi','mikroklkmi','rozvetven\u00fdmi kapil\u00e1rami','\u017e\u013eazami tenk\u00e9ho \u010dreva','svalov\u00fdmi prstencami','v\u00fdvodmi pankreasu a pe\u010dene','nervov\u00fdmi zakon\u010deniami','prie\u010dnymi prep\u00e1\u017ekami',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(329,'329. Hrub\u00e9 \u010drevo vy\u0161\u0161\u00edch stavovcov zabezpe\u010duje predov\u0161etk\u00fdm:','mechanick\u00e9 spracovanie potravy','vstreb\u00e1vanie rozlo\u017eenej potravy','sp\u00e4tn\u00fa resorpciu vody a miner\u00e1lnych l\u00e1tok','chemick\u00e9 tr\u00e1venie potravy','zahus\u0165ovanie nestr\u00e1ven\u00fdch zvy\u0161kov potravy','emulg\u00e1ciu tukov','vstreb\u00e1vanie z\u00e1kladn\u00fdch \u017eiv\u00edn','tr\u00e1venie celul\u00f3zy pomocou \u010drevn\u00fdch bakt\u00e9ri\u00ed',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(330,'330. \u017divo\u010d\u00edchy m\u00f4\u017eu prij\u00edma\u0165 kysl\u00edk potrebn\u00fd pre d\u00fdchanie:','zo vzduchu, kde je jeho koncentr\u00e1cia priemerne 21 %','z vody, kde je jeho koncentr\u00e1cia 0,6 - 0,85 %','vo forme oxidov uhl\u00edka','zo vzduchu, kde je jeho koncentr\u00e1cia priemerne 78 %','z vody, kde je jeho koncentr\u00e1cia priemerne 0,03 %','vo forme plynn\u00fdch oxidov','\u017eiabrami zo vzduchu','z vody \u017eiabrami alebo poko\u017ekou',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(331,'331. D\u00fdchacie org\u00e1ny v\u00fdvojovo dokonalej\u0161\u00edch \u017eivo\u010d\u00edchov:','sa vyvinuli z mezodermu','m\u00f4\u017eu z\u00e1sobova\u0165 tkaniv\u00e1 kysl\u00edkom priamo, napr. vzdu\u0161nice','m\u00f4\u017eu z\u00e1sobova\u0165 tkaniv\u00e1 kysl\u00edkom nepriamo, napr. p\u013e\u00faca','pracuj\u00fa na princ\u00edpe dif\u00fazie plynov','m\u00f4\u017eu z\u00e1sobova\u0165 tkaniv\u00e1 kyslficom priamo, napr. \u017eiabre','m\u00f4\u017eu z\u00e1sobova\u0165 tkaniv\u00e1 kyslficom priamo, napr. vzdu\u0161nicov\u00e9 \u017eiabre','z\u00edskavaj\u00fa kysl\u00edk z anorganick\u00fdch zl\u00fa\u010den\u00edn','z\u00edskavaj\u00fa kysl\u00edk rozkladom organick\u00fdch zl\u00fa\u010den\u00edn',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(332,'332. Vzdu\u0161nice s\u00fa d\u00fdchacie org\u00e1ny:','pav\u00fakovcov','hmyzu','ktor\u00e9 sa na povrch tela otv\u00e1raj\u00fa stigmami','ktor\u00e9 ved\u00fa kysl\u00edk priamo do tkan\u00edv','ktor\u00e9 pren\u00e1\u0161aj\u00fa kysl\u00edk pomocou dychac\u00edch farb\u00edv','vyu\u017e\u00edvaj\u00face protismerov\u00e9 pr\u00fadenie','rozvetvuj\u00face sa vo vn\u00fatri tkan\u00edv','oboj\u017eiveln\u00edkov',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(333,'333. V srdci r\u00fdb pr\u00fadi krv:','okysli\u010den\u00e1','odkysli\u010den\u00e1','zmie\u0161an\u00e1','smerom k \u017eiabram','obsahuj\u00faca hemoglob\u00edn','neobsahuj\u00faca krvn\u00e9 farbiv\u00e1','obsahuj\u00faca \u010derven\u00e9 krvinky s jadrom','obsahuj\u00faca bezjadrov\u00e9 erytrocyty',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(334,'334. Najefekt\u00edvnej\u0161iu d\u00fdchaciu s\u00fastavu medzi stavovcami maj\u00fa:','ryby','oboj\u017eiveln\u00edky','plazy','vt\u00e1ky','dvojdy\u0161n\u00edky','suchozemsk\u00e9 hady','korytna\u010dky','m\u00e4so\u017eravce',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(335,'335. Ko\u017en\u00e9 d\u00fdchanie:','m\u00e1 rozhoduj\u00faci v\u00fdznam pre cicavce','m\u00e1 ve\u013ek\u00fd v\u00fdznam pre ryby a oboj\u017eiveln\u00edky','m\u00e1 zna\u010dn\u00fd v\u00fdznam pre ve\u013eryby','je jedin\u00fd sp\u00f4sob pr\u00edjmu 0_2 pre vt\u00e1ky','je jediny sp\u00f4sob pr\u00edjmu 0_2 last\u00farnikov','m\u00e1 zna\u010dn\u00fd v\u00fdznam pre delfiny','je jedin\u00fd sp\u00f4sob pr\u00edjmu 0_2 mnoho\u0161tetinavcov','zabezpe\u010duje poko\u017eka tvoren\u00e1 d\u00fdchac\u00edm epitelom',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(336,'336. Vzdu\u0161nice:','pracuj\u00fa bez obehovej s\u00fastavy','spolupracuj\u00fa s obehovou s\u00fastavou','sa otv\u00e1raj\u00fa na povrch tela','s\u00fa pomocn\u00fdm d\u00fdchac\u00edm org\u00e1nom u vt\u00e1kov','s\u00fa hlavn\u00fdm d\u00fdchac\u00edm org\u00e1nom u cicavcov','tvoria jednoduch\u00e9 vaky, ktor\u00fdch steny s\u00fa \u010dlenen\u00e9 priehradkami','sa vyvinuli zo \u017eiaber','ved\u00fa kysl\u00edk do hemolymfy',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(337,'337. Delf\u00edny a ve\u013eryby d\u00fdchaj\u00fa:','vzdu\u0161nicami','postrannou \u010diarou','p\u013e\u00facami','\u017eiabrami','vzdu\u0161n\u00fdmi vakmi','parap\u00f3diami','poko\u017ekou','p\u013e\u00facami a \u017eiabrami',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(338,'338. Vonkaj\u0161ie d\u00fdchanie cicavcov:','je v\u00fdmena d\u00fdchac\u00edch plynov medzi alveol\u00e1rnym vzduchom a krvou','je v\u00fdmena d\u00fdchac\u00edch plynov medzi krvou a tkanivami','prevl\u00e1da nad vn\u00fatorn\u00fdm d\u00fdchan\u00edm','je v\u00fdmena d\u00fdchac\u00edch plynov medzi vonkaj\u0161\u00edm prostred\u00edm a d\u00fdchac\u00edmi cestami','je riaden\u00e9 tak, aby mno\u017estvo vdychovan\u00e9ho 0_2 a vydychovan\u00e9ho CO_2 zodpovedalo intenzite metabolizmu','je pravideln\u00e9 striedanie vdychov a v\u00fddychov','je vnikanie vzduchu do p\u013e\u00fac a unikanie z p\u013e\u00fac','je v\u00fdmena O_2, a CO_2 v tkaniv\u00e1ch a bunk\u00e1ch pracuj\u00facich org\u00e1nov',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(339,'339. V\u00fdmena 0_2 a CO_2 medzi krvou a tkanivami cicavcov sa naz\u00fdva:','vn\u00fatorn\u00e9 d\u00fdchanie','vonkaj\u0161ie d\u00fdchanie','p\u013e\u00facna ventil\u00e1cia','vit\u00e1lna kapacita','vzdu\u0161nicov\u00e9 d\u00fdchanie','celkov\u00e1 kapacita','tkanivov\u00e9 d\u00fdchanie','tkanivov\u00e1 oxid\u00e1cia',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(340,'340. D\u00fdchacie farbiv\u00e1 v krvi stavovcov:','zabezpe\u010duj\u00fa prenos \u017eiv\u00edn','zabezpe\u010duj\u00fa prenos 0_2','s\u00fa uzavret\u00e9 v erytrocytoch','s\u00fa uzavret\u00e9 v trombocytoch','s\u00fa vo\u013ene rozpusten\u00e9 v krvi','s\u00fa uzavret\u00e9 v \u010derven\u00fdch krvink\u00e1ch','s\u00fa uzavret\u00e9 v leukocytoch','s\u00fa schopn\u00e9 viaza\u0165 kysl\u00edk',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(341,'341. D\u00fdchacie pohyby s\u00fa pri stavovcoch riaden\u00e9:','d\u00fdchac\u00edm \u00fastred\u00edm v mieche','d\u00fdchac\u00edm \u00fastred\u00edm v pred\u013a\u017eenej mieche','d\u00fdchac\u00edm \u00fastred\u00edm v strednom mozgu','na z\u00e1klade zmien tenzie 0_2 a CO_2 v krvi','na z\u00e1klade zmien tlaku vzduchu v p\u013e\u00facach','dychac\u00edm \u00fastred\u00edm v medzimozgu','na z\u00e1klade mno\u017estva erytrocytov v krvi','nervovo a chemicky',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(342,'342. Spont\u00e1nne d\u00fdchanie cicavcov riadi centrum v:','mieche','mozo\u010dku','p\u013e\u00facach','pred\u013a\u017eenej mieche','alveol\u00e1ch','prednom mozgu','hypof\u00fdze','hrtane',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(343,'343. Pav\u00fakovce d\u00fdchaj\u00fa:','p\u013e\u00facnymi va\u010dkami','vzdu\u0161nicami','tracheami','cel\u00fdm povrchom tela','\u017eiabrami','p\u013e\u00facami','sliznicou tr\u00e1viacej r\u00fary','tykadlov\u00fdmi \u017e\u013eazami',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(344,'344. D\u00fdchacie farbiv\u00e1 bezstavovcov:','s\u00fa rozpusten\u00e9 v telovej tekutine','pren\u00e1\u0161aj\u00fa kysl\u00edk z d\u00fdchac\u00edch org\u00e1nov do tkan\u00edv','sa z\u00fa\u010dast\u0148uj\u00fa bunkov\u00e9ho d\u00fdchania','oxida\u010dno-reduk\u010dn\u00fdmi reakciami dod\u00e1vaj\u00fa organizmu energiu','s\u00fa anorganick\u00e9 l\u00e1tky','s\u00fa bielkovinovej povahy','sa nach\u00e1dzaj\u00fa v bunk\u00e1ch vzdu\u0161n\u00edc','pren\u00e1\u0161aj\u00fa kysl\u00edk z tkan\u00edv do telov\u00fdch tekut\u00edn',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(345,'345. Typick\u00e9 \u017eiabre ulo\u017een\u00e9 v \u017eiabrovej dutine maj\u00fa:','ryby','mnoho\u0161tetinavce','k\u00f4rovce','\u017eubrienky oboj\u017eiveln\u00edkov','embry\u00e1 cicavcov','korytna\u010dky','m\u00e4kk\u00fd\u0161e','vodn\u00e9 bezstavovce',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(346,'346. P\u013e\u00faca stavovcov:','s\u00fa p\u00e1rov\u00fd org\u00e1n','s\u00fa s vonkaj\u0161\u00edm prostred\u00edm spojen\u00e9 d\u00fdchac\u00edmi cestami','sa vyvinuli z ektodermu','sa vyvinuli z endodermu','sa vyvinuli z mezodermu','zabezpe\u010duj\u00fa nas\u00fdtenie krvi kysl\u00edkom','zabezpe\u010duj\u00fa nas\u00fdtenie krvi oxidom uhli\u010dit\u00fdm','zabezpe\u010duj\u00fa nas\u00fdtenie krvi hemoglob\u00ednom',TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(347,'347. P\u013e\u00facne alveoly:','s\u00fa z\u00e1kladnou \u0161trukt\u00farnou a funk\u010dnou jednotkou pre v\u00fdmenu plynov medzi organizmom a prostred\u00edm','s\u00fa slep\u00e9 zakon\u010denia d\u00fdchac\u00edch ciest','s\u00fa obklopen\u00e9 bohatou sie\u0165ou krvn\u00fdch vl\u00e1so\u010dn\u00edc','mnohon\u00e1sobne zv\u00e4\u010d\u0161uj\u00fa d\u00fdchac\u00ed povrch p\u013e\u00fac','s\u00fa vzdu\u0161n\u00e9 vaky vt\u00e1kov','s\u00fa len minim\u00e1lne prekrven\u00e9','nemaj\u00fa v\u00fdznam pre v\u00fdmenu plynov','sa vyvinuli zo vzdu\u0161n\u00edc',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(348,'348. Nervovosvalov\u00e1 platni\u010dka:','je zakon\u010denie nervov\u00e9ho vl\u00e1kna v svalovej bunke','prij\u00edma sign\u00e1l z \u00fastrednej nervovej s\u00fastavy a men\u00ed ho na podnet','pracuje na princ\u00edpe jednoduchej nervovej synapsie','produkuje medi\u00e1tor - noradrenal\u00edn','je z\u00e1kladnou stavebnou jednotkou myofibr\u00edl','je citliv\u00e1 na niektor\u00e9 jedy','zabezpe\u010duje v\u00fd\u017eivu svalovej bunky','je d\u00f4le\u017eitou s\u00fa\u010das\u0165ou motorickej jednotky',TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(349,'349. Regula\u010dn\u00e9 mechanizmy \u017eivo\u010d\u00edchov:','sa vyvinuli v procese adapt\u00e1cie \u017eivo\u010d\u00edchov na meniace sa podmienky vonkaj\u0161ieho prostredia','zabezpe\u010duj\u00fa st\u00e1los\u0165 vn\u00fatorn\u00e9ho prostredia organizmu','p\u00f4sobia reflexne a l\u00e1tkovo','funguj\u00fa ako obrann\u00e9 mechanizmy','reaguj\u00fa na zmeny vonkaj\u0161\u00edch podmienok','zabezpe\u010duj\u00fa integritu organizmu','zabezpe\u010duj\u00fa riadenie organizmu','zabezpe\u010duj\u00fa st\u00e1los\u0165 vonkaj\u0161ieho prostredia',TRUE,TRUE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(350,'350. Telov\u00e9 tekutiny \u017eivo\u010d\u00edchov:','priv\u00e1dzaj\u00fa ku tkaniv\u00e1m kysl\u00edk a \u017eiviny','zabezpe\u010duj\u00fa homeost\u00e1zu vn\u00fatorn\u00e9ho prostredia','odv\u00e1dzaj\u00fa z tkan\u00edv oxid uhli\u010dit\u00fd a produkty metabolizmu','maj\u00fa r\u00f4zne zlo\u017eenie v z\u00e1vislosti od fylogenetickej \u00farovne organizmov','s\u00fa produkovan\u00e9 vylu\u010dovacou s\u00fastavou','priv\u00e1dzaj\u00fa ku tkaniv\u00e1m horm\u00f3ny a vitam\u00edny','priv\u00e1dzaj\u00fa ku tkaniv\u00e1m nervov\u00e9 vzruchy','produkuj\u00fa enz\u00fdmy',TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(351,'351. Hydrolymfa:','je telov\u00e1 tekutina ploskavcov','obsahuje bunky s transportn\u00fdmi bielkovinami','cirkuluje v uzavretej cievnej s\u00fastave','je telov\u00e1 tekutina obr\u00fa\u010dkavcov','obsahuje bunky bez transportn\u00fdch bielkov\u00edn','pr\u00fadi vo\u013ene v telovej dutine','je telov\u00e1 tekutina stavovcov','pr\u00fadi v kan\u00e1likoch gastrovaskul\u00e1rnej s\u00fastavy',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(352,'352. Krv cicavcov obsahuje:','plazmu a krvn\u00e9 telieska','bielkoviny','anorganick\u00e9 l\u00e1tky','trombocyty s bunkov\u00fdm jadrom','bezjadrov\u00e9 erytrocyty','bezjadrov\u00e9 leukocyty','bezjadrov\u00e9 krvn\u00e9 do\u0161ti\u010dky','horm\u00f3ny',TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(353,'353. Plazmatick\u00e9 bielkovin cicavcov:','s\u00fa produkovan\u00e9 zv\u00e4\u010d\u0161a pe\u010de\u0148ou','pren\u00e1\u0161aj\u00fa l\u00e1tky potrebn\u00e9 pre organizmus','s\u00fa v\u00fdznamn\u00e9 z h\u013eadiska obrany organizmu','s\u00fa produkovan\u00e9 zv\u00e4\u010d\u0161a kostnou dre\u0148ou','sa z\u00fa\u010dast\u0148uj\u00fa procesov zr\u00e1\u017eania krvi','s\u00fa zv\u00e4\u010d\u0161a produktom sleziny','ireverzibilne via\u017eu na svoj povrch pren\u00e1\u0161an\u00e9 l\u00e1tky','reverzibilne via\u017eu na svoj povrch pren\u00e1\u0161an\u00e9 l\u00e1tky',TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(354,'354. Krvn\u00e1 plazma cicavcov:','je slabo z\u00e1sadit\u00e1\u0009-','za fyziologick\u00fdch podmienok m\u00e1 st\u00e1le zlo\u017eenie','obsahuje 90 - 92 % rozpusten\u00fdch l\u00e1tok','obsahuje 8 - 9 % su\u0161iny','za fyziologick\u00fdch podmienok m\u00e1 pH 5,5 - 6,5','obsahuje plazmatick\u00e9 bielkoviny','obsahuje bezjadrov\u00e9 leukocyty','obsahuje splodiny metabolizmu bielkov\u00edn',TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(355,'355. V\u00e4\u010d\u0161ina plazmatick\u00fdch bielkov\u00edn vznik\u00e1 v:','\u010derven\u00fdch krvink\u00e1ch','slezine','kostnej dreni','srdci','pe\u010deni','tkanivovom moku','lymfatick\u00fdch uzlin\u00e1ch','bunk\u00e1ch cievnych stien',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(356,'356. Biele krvinky:','sa vyskytuj\u00fa len v krvi','s\u00fa jadrov\u00e9 bunky','sa naz\u00fdvaj\u00fa erytrocyty','s\u00fa bezjadrov\u00e9 bunky','sa v krvi vyskytuj\u00fa vo v\u00e4\u010d\u0161om po\u010dte ako \u010derven\u00e9 krvinky','sa pod\u013ea pr\u00edtomnosti gran\u00fal v cytoplazme delia na granulocyty a trombocyty','sa pod\u013ea \u0161trukt\u00fary cytoplazmy delia na agranulocyty a granulocyty','sa pod\u013ea pr\u00edtomnosti gran\u00fal v cytoplazme delia na lymfocyty a monocyty',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(357,'357. Ktor\u00e9 organick\u00e9 l\u00e1tky s\u00fa celkovo hmotnostne najviac zast\u00fapen\u00e9 v krvnej plazme \u010dloveka?','mo\u010dovina','nukleov\u00e9 kyseliny','glukoza','monosacharidy','bielkoviny','voda','lipidy','kreat\u00edn',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(358,'358. Jednotliv\u00e9 typy krviniek dospel\u00fdch cicavcov:','vznikaj\u00fa v kostnej dreni','sa vyv\u00edjaj\u00fa z krvotvornej kme\u0148ovej bunky v kostnej dreni','vznikaj\u00fa z krvotvornej kme\u0148ovej bunky v krvnej plazme','s\u00fa produkovan\u00e9 lymfatick\u00fdm syst\u00e9mom','vznikaj\u00fa z totipotentn\u00fdch buniek pe\u010dene','vznikaj\u00fa zo spolo\u010dnej krvotvornej kme\u0148ovej bunky','sa diferencuj\u00fa z r\u00f4znych typov kme\u0148ov\u00fdch buniek','\u017eij\u00fa v krvnom obehu r\u00f4zne dlh\u00fa dobu',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(359,'359. Leukocyty:','s\u00fa nevyhnutn\u00e9 pri zr\u00e1\u017ean\u00ed krvi','sa z\u00fa\u010dast\u0148uj\u00fa na obrann\u00fdch reakci\u00e1ch organizmu','zabezpe\u010duj\u00fa prenos 02','s\u00fa biele krvinky','zabezpe\u010duj\u00fa prenos horm\u00f3nov','sa z\u00fa\u010dast\u0148uj\u00fa na prenose d\u00fdchac\u00edch plynov','zabezpe\u010duj\u00fa udr\u017eiavanie teploty','maj\u00fa povrchov\u00e9 receptory rozli\u0161uj\u00face cudzorod\u00e9 l\u00e1tky',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(360,'360. Erytrocyty bez jadra sa nach\u00e1dzaj\u00fa v krvi:','vt\u00e1kov','r\u00fdb','cicavcov','plazov','jednobunkov\u00fdch organizmov','\u013eud\u00ed','oboj\u017eiveln\u00edkov','v\u0161etk\u00fdch stavovcov',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(361,'361. Celkov\u00fd po\u010det \u010derven\u00fdch krviniek na jednotku objemu krvi stavovcov je podmienen\u00fd:','pohlav\u00edm, samce m\u00e1vaj\u00fa viac erytrocytov','pohlav\u00edm, samice m\u00e1vaj\u00fa viac erytrocytov','vekom, ml\u00e1\u010fat\u00e1 maj\u00fa viac erytrocytov','vekom, dospel\u00ed jedinci m\u00e1vaj\u00fa viac erytrocytov','nadmorskou v\u00fd\u0161kou','pri ni\u017e\u0161\u00edch stavovcoch ro\u010dn\u00fdm obdob\u00edm','mno\u017estvom hemoglob\u00ednu v krvi','celkov\u00fdm po\u010dtom leukocytov v krvi',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(362,'362. Leukocyty sa odli\u0161uj\u00fa od erytrocytov:','d\u013a\u017ekou \u017eivota buniek','funkciami','sp\u00f4sobom vzniku','metabolizmom','pri cicavcoch t\u00fdm, \u017ee s\u00fa bezjadrov\u00e9','pri vt\u00e1koch t\u00fdm, \u017ee s\u00fa bezjadrov\u00e9','po\u010detn\u00fdm zast\u00fapen\u00edm v krvi','rozlo\u017een\u00edm v organizme',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(363,'363. Monocyty:','maj\u00fa schopnos\u0165 diaped\u00e9zy','s\u00fa schopn\u00e9 prenika\u0165 do tkan\u00edv','maj\u00fa schopnos\u0165 fagocytova\u0165 mikroorganizmy','vznikaj\u00fa z kme\u0148ovej bunky v kostnej dreni','pod\u013ea \u0161trukt\u00fary cytoplazmy patria k agranulocytom','pod\u013ea \u0161trukt\u00fary cytoplazmy patria ku granulocytom','pren\u00e1\u0161aj\u00fa kysl\u00edk dovn\u00fatra tkan\u00edv','pren\u00e1\u0161aj\u00fa \u017eiviny z krvi do tkan\u00edv',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(364,'364. Trombocyty cicavcov:','s\u00fa produkovan\u00e9 slezinou','s\u00fa produkovan\u00e9 kostnou dre\u0148ou','s\u00fa produkovan\u00e9 pe\u010de\u0148ou','s\u00fa bezjadrov\u00e9 krvinky','s\u00fa ve\u013ek\u00e9 krvinky s diploidn\u00fdm jadrom','plnia v\u00fdznamn\u00fa \u00falohu pri zastavovan\u00ed krv\u00e1cania','sa v\u00fdznamne podie\u013eaj\u00fa na prenose d\u00fdchac\u00edch plynov','sa uplat\u0148uj\u00fa pri udr\u017eiavan\u00ed celistvosti cievnych stien',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(365,'365. Medzi krvn\u00e9 telieska patria:','erytrocyty','granulocyty','trombocyty','lymfocyty','leukocyty','lyzoz\u00f3my','krvn\u00e9 farbiv\u00e1','antig\u00e9ny',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(366,'366. Podstatou zr\u00e1\u017eania krvi u stavovcov je:','zmena rozpustn\u00e9ho fibrinog\u00e9nu na nerozpustn\u00e9 vl\u00e1kna fibr\u00ednu','s\u0165ahovanie svaloviny v oblasti poranenia','zmena nerozpustn\u00e9ho fibrinog\u00e9nu na rozpustn\u00fd fibr\u00edn','zmena rozpustn\u00e9ho fibr\u00ednu na nerozpustn\u00fd fibrinog\u00e9n','vytvorenie g\u00e9lu, ktor\u00fd uzavrie poranenie','rozpad erytrocytov','protil\u00e1tkov\u00e1 obrana','hemol\u00fdza',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(367,'367. Zastavenie krv\u00e1cania:','je pri \u017eivo\u010d\u00edchoch zabezpe\u010dovan\u00e9 viacer\u00fdmi mechanizmami','sa uskuto\u010d\u0148uje aglutin\u00e1ciou erytrocytov','je zlo\u017eit\u00fd biochemick\u00fd proces','pri stavovcoch sa deje za \u00fa\u010dasti krvn\u00fdch do\u0161ti\u010diek','sa pri \u017eivo\u010d\u00edchoch s otvorenou cievnou s\u00fastavou deje s\u0165ahmi svaloviny okolo poranenia','je pri stavovcoch zabezpe\u010dovan\u00e9 diaped\u00e9zou monocytov','je iniciovan\u00e9 hemol\u00fdzou erytrocytov','je pri cicavcoch ur\u00fdch\u013eovan\u00e9 hepar\u00ednom',TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(368,'368. Fibrinog\u00e9n:','je s\u00fa\u010das\u0165ou krvnej plazmy','sa z\u00fa\u010dast\u0148uje na zr\u00e1\u017ean\u00ed krvi','sa nach\u00e1dza v erytrocytoch','p\u00f4sob\u00ed proti zr\u00e1\u017eaniu krvi','zabezpe\u010duje prenos 0_2','sa z\u00fa\u010dast\u0148uje na obrane organizmu pred infekciou','je rozpusm\u00e1 plazmatick\u00e1 bielkovina','je nerozpustn\u00e1 plazmatick\u00e1 bielkovina',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(369,'369. Biely trombus:','je \u010derven\u00e1 z\u00e1tka','je fibr\u00ednov\u00e1 sie\u0165ka, v ktorej sa zachytili biele krvinky','pri mal\u00fdch poraneniach proviz\u00f3rne upch\u00e1va ranku','vznik\u00e1 aktivitou tkanivov\u00fdch faktorov','je trombokin\u00e1za','sa vytv\u00e1ra aktivitou krvn\u00fdch do\u0161ti\u010diek okolo poranenia','sa vytv\u00e1ra najm\u00e4 pri mal\u00fdch poraneniach','p\u00f4sob\u00ed proti zr\u00e1\u017eaniu krvi',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(370,'370. Aglutinog\u00e9ny:','s\u00fa antig\u00e9ny erytrocytov','s\u00fa protil\u00e1tky erytrocytov','s\u00fa g\u00e9ny k\u00f3dui\u00face antig\u00e9ny','s\u00fa protil\u00e1tky v krvnej plazme','sa tvoria iba v kostnej dreni','s\u00fa geneticky podmienen\u00e9','s\u00fa l\u00e1tky, ktor\u00e9 vznikaj\u00fa pri zr\u00e1\u017ean\u00ed krvi','s\u00fa prirodzen\u00e9 protil\u00e1tky',TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(371,'371. Aglutin\u00edny s\u00fa:','antig\u00e9ny erytrocytov','protil\u00e1tky v krvnej plazme','aglutinog\u00e9ny leukocytov','produkty aglutinog\u00e9nov','zodpovedn\u00e9 za zhlukovanie erytrocytov','antig\u00e9ny na povrchu leukocytov','zodpovedn\u00e9 za zhlukovanie \u010derven\u00fdch krviniek','prirodzen\u00e9 protitil\u00e1tky vo\u010di vlastn\u00fdm krvink\u00e1m',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(372,'372. Hemolymf\u00e1:','sa vyskytuje u viacer\u00fdch bezstavovcov','obsahuje erytrocyty a trombocyty','obsahuje bielkoviny, ktor\u00e9 maj\u00fa charakter krvn\u00fdch farb\u00edv','pri m\u00e4kk\u00fd\u0161och pr\u00fadi pulzuj\u00facimi \u017eilami do telov\u00fdch dut\u00edn','pr\u00fadi v uzavretej cievnej s\u00fastave','sa rozlieva vo\u013ene v telovej dutine','zabezpe\u010duje rozv\u00e1dzanie d\u00fdchac\u00edch plynov','je najd\u00f4le\u017eitej\u0161ou telovou tekutinou stavovcov',TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(373,'373. Tkanivov\u00fd mok:','tvor\u00ed tekut\u00e9 prostredie okolo buniek tkan\u00edv','vznik\u00e1 filtr\u00e1ciou krvnej plazmy','obsahuje plazmatick\u00e9 bielkoviny','pren\u00e1\u0161a n\u00edzkomolekulov\u00e9 rozpustn\u00e9 l\u00e1tky','zabezpe\u010duje v\u00fdmenu plynov medzi krvou a bunkami tkan\u00edv','vznik\u00e1 filtr\u00e1ciou miazgy','obsahuje ve\u013ea enz\u00fdmov','je filtrovan\u00fd v lymfatick\u00fdch uzlin\u00e1ch',TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(374,'374. Miazga:','je filtrovan\u00e1 v lymfatick\u00fdch uzlin\u00e1ch','obsahuje ve\u013ea enz\u00fdmov','obsahuje biele krvinky','pri stavovcoch pr\u00fadi v otvorenom lymfatickom obehu','vznik\u00e1 z tkanivov\u00e9ho moku','pln\u00ed v\u00fdznamn\u00fa \u00falohu v obrann\u00fdch mechanizmoch organizmu','pri stavovcoch pr\u00fadi v uzavretom lymfatickom obehu','obsahuje \u010derven\u00e9 krvinky',TRUE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(375,'375. Lymfatick\u00e9 org\u00e1ny stavovcov s\u00fa:','t\u00fdmus','slezina','\u010dervovit\u00fd pr\u00edvesok slep\u00e9ho \u010dreva','Fabriciov va\u010dok','ch\u00fdlus','statocysta','srdce','pankreas',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(376,'376. Ak\u00fd je v\u00fdznam lymfatickej s\u00fastavy?','z\u00fa\u010dast\u0148uje sa na transporte l\u00e1tok','pren\u00e1\u0161a hormony a medi\u00e1tory k cie\u013eov\u00fdrn bimk\u00e1m','podie\u013ea sa na termoregul\u00e1cii v organizme','je d\u00f4le\u017eit\u00e1 v obrann\u00fdch mechanizmoch','zachyt\u00e1va patog\u00e9nne mikroorganizmy','zne\u0161kod\u0148uje n\u00e1dorov\u00e9 bunky','pren\u00e1\u0161a plyny z krvi k bunk\u00e1m a z buniek do krvi','rozv\u00e1dza teplo z metabolicky akt\u00edvnych org\u00e1nov do chladnej\u0161\u00edch tkan\u00edv',TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(377,'377. Otvoren\u00e1 obehov\u00e1 s\u00fastava sa vyskytuje pri:','ploskavcoch','obr\u00fa\u010dkavcoch','m\u00e4kk\u00fd\u0161och','cicavcoch','plazoch','\u010dl\u00e1nkono\u017ecoch','pl\u00e1\u0161\u0165ovcoch','hlavono\u017ecoch',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(378,'378. Lymfatick\u00fa s\u00fastavu maj\u00fa:','vt\u00e1ky','m\u00e4kk\u00fd\u0161e','obr\u00fa\u010dkavce','hmyz','ploskavce','ryby','oboj\u017eiveln\u00edky','cicavce',TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(379,'379. Srdce r\u00fdb pozost\u00e1va z:','2 predsien\u00ed a 2 kom\u00f4r','predsiene a komory, ktor\u00fdmi prech\u00e1dza odkysli\u010den\u00e1 krv','predsiene a komory, ktor\u00fdmi prech\u00e1dza okysli\u010den\u00e1 krv','2 predsien\u00ed a 1 komory, ktor\u00fdmi prech\u00e1dza okysli\u010den\u00e1 krv','2 predsien\u00ed a 1 komory, ktor\u00fdmi prech\u00e1dza odkysli\u010den\u00e1 krv','2 predsien\u00ed a 2 kom\u00f4r, ktor\u00fdmi prech\u00e1dza odlcysli\u010den\u00e1 krv','predsiene a komory, z ktorej je krv vh\u00e1\u0148an\u00e1 do \u017eilov\u00e9ho splavu','predsiene a komory, z ktorej je krv vh\u00e1\u0148an\u00e1 do \u017eiaber',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(380,'380. \u00dapln\u00fa komorov\u00fa priehradku v srdci maj\u00fa:','ja\u0161terice','\u017eaby','mloky','hady','ryby','vt\u00e1ky','cicavce','drsnoko\u017ece',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(381,'381. Dospel\u00e9 oboj\u017eiveln\u00edky maj\u00fa srdce rozdelen\u00e9 na:','dve predsiene a jednu komoru','dve komory a jednu predsie\u0148','dve predsiene a jednu komoru, pri\u010dom do tela pr\u00fadi zo srdca zmie\u0161an\u00e1 krv','dve predsiene a jednu komoru, pri\u010dom do tela pr\u00fadi zo srdca okysli\u010den\u00e1 krv','dve predsiene a jednu komoru, pri\u010dom do tela pr\u00fadi zo srdca odkysli\u010den\u00e1 krv','jednu predsie\u0148 a jednu komoru, pri\u010dom do tela pr\u00fadi zo srdca zmie\u0161an\u00e1 krv','dve predsiene a dve komory, pri\u010dom do tela pr\u00fadi zo srdca zmie\u0161an\u00e1 krv','jednu predsie\u0148 a jednu komoru, pri\u010dom do tela pr\u00fadi zo srdca okysli\u010den\u00e1 krv',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(382,'382. U cicavcov okysli\u010den\u00e1 krv pr\u00fadi:','z \u013eavej komory do mal\u00e9ho krvn\u00e9ho obehu','z \u013eavej komory do ve\u013ek\u00e9ho krvn\u00e9ho obehu','z pravej predsiene do mal\u00e9ho krvn\u00e9ho obehu','z pravej komory do ve\u013ek\u00e9ho krvn\u00e9ho obehu','z \u013eavej predsiene do mal\u00e9ho krvn\u00e9ho obehu','z pravej komory do p\u013e\u00fac','p\u013e\u00facnymi \u017eilami do \u013eavej predsiene','vr\u00e1tnicou do pe\u010dene',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(383,'383. Podnety na srdcov\u00fa \u010dinnos\u0165 vznikaj\u00fa:','v mozgu','v mieche','v samotnom srdci','v srdcovnici','v mozo\u010dku','v pred\u013a\u017eenej mieche','vo vencovit\u00fdch tepn\u00e1ch','v \u00fastrednej nervovej s\u00fastave',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(384,'384. Z\u00e1znam zmien elektrick\u00e9ho potenci\u00e1lu srdcov\u00e9ho svalu sa ozna\u010duje ako:','EKG','EEG','fonogram','elektrokardiogram','oscilogram','elektroencefalogram','EMG','elektromyelogram',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(385,'385. Meradlom v\u00fdkonnosti srdca je:','frekvencia tepu','min\u00fatov\u00fd objem','mno\u017estvo krvi, ktor\u00e9 srdce vytla\u010d\u00ed za min\u00fatu','mno\u017estvo krvi, ktor\u00e9 srdce vytla\u010d\u00ed pri jednej systole','mno\u017estvo krvi, ktor\u00e9 srdce vytla\u010d\u00ed pri jednej diastole','mno\u017estvo krvi, ktor\u00e9 srdce vytla\u010d\u00ed pri jednom srdcovom cykle','mno\u017estvo krvi, ktor\u00e9 zapin\u00ed cel\u00fd cievny syst\u00e9m tela','celkov\u00fd objem srdca',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(386,'386. Koron\u00e1rne tepny:','zabezpe\u010duj\u00fa v\u00fd\u017eivu srdca','od\u010derp\u00e1vaj\u00fa 10 % krvi vytla\u010denej do aorty pri systole','zabezpe\u010duj\u00fa okysli\u010dovanie srdcov\u00e9ho svalu','s\u00fa rozvetven\u00e9 do rozsiahlej kapil\u00e1rnej siete','vych\u00e1dzaj\u00fa z hornej dutej \u017eily','vych\u00e1dzaj\u00fa z aorty','vych\u00e1dzaj\u00fa z p\u013e\u00facnice','vych\u00e1dzaj\u00fa z pravej komory',TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(387,'387. \u00daplne oddelen\u00fd p\u013e\u00facny a telov\u00fd krvn\u00fd obeh maj\u00fa:','\u017eivo\u010d\u00edchy, ktor\u00e9 maj\u00fa jednokomorov\u00e9 srdce','vt\u00e1ky','\u017eivo\u010d\u00edchy, ktor\u00fdch srdce je tvoren\u00e9 dvoma predsie\u0148ami a jednou komorou','cicavce','oboj\u017eiveln\u00edky','plazy','\u017e\u00edvo\u010d\u00edchy, ktor\u00e9 maj\u00fa \u00faplne vyvinut\u00fa priehradku medzi srdcov\u00fdmi komorami','\u017eivo\u010d\u00edchy, ktor\u00fdch srdce je tvoren\u00e9 jednou predsie\u0148ou a jednou komorou',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(388,'388. Vr\u00e1tnicov\u00fd obeh:','je s\u00fa\u010das\u0165ou p\u013e\u00facneho krvn\u00e9ho obehu','je s\u00fa\u010das\u0165ou telov\u00e9ho krvn\u00e9ho obehu','je svoj\u00edm charakterom \u017eilov\u00fd obeh','priv\u00e1dza \u017eiviny vstreban\u00e9 do krvi v tr\u00e1viacich org\u00e1noch do srdca','priv\u00e1dza \u017eiviny vstreban\u00e9 do krvi v tr\u00e1viacich org\u00e1noch do pe\u010dene','za\u010d\u00edna sie\u0165ou kapil\u00e1r v tr\u00e1viacich org\u00e1noch','vstupuje do pe\u010dene vr\u00e1tnicou','za\u010d\u00edna sie\u0165ou kapil\u00e1r v pe\u010deni',FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(389,'389. K vlastnej v\u00fdmene l\u00e1tok medzi krvou a tkanivami doch\u00e1dza:','v tenkostenn\u00fdch tepn\u00e1ch','v tepn\u00e1ch s pevnou a pru\u017enou stenou','v kapil\u00e1rach','v \u017eil\u00e1ch','vo vr\u00e1tnici','v hornej a dolnej dutej \u017eile','v srdci','v lymfatick\u00fdch uzlin\u00e1ch',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(390,'390. Obehov\u00e1 s\u00fastava stavovcov:','je kardiovaskul\u00e1rna s\u00fastava','je dokonal\u00e1 uzavret\u00e1 srdcovocievna s\u00fastava','je oddelen\u00e1 od lymfatickej s\u00fastavy','nie je oddelen\u00e1 od lymfatickej s\u00fastavy','je pri suchozemsk\u00fdch \u017eivo\u010d\u00edchoch zlo\u017eitej\u0161ia ne\u017e pri vodn\u00fdch','je pri vodn\u00fdch \u017eivo\u010d\u00edchoch zlo\u017eitej\u0161ia ne\u017e pri suchozemsk\u00fdch','zabezpe\u010duje v\u00fdmenu \u017eiv\u00edn,  plynov a odpadov\u00fdch l\u00e1tok medzi krvou a bunkami tkan\u00edv','je dokonal\u00e1 otvoren\u00e1 kardiovaskul\u00e1rna s\u00fastava',TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(391,'391. Protonefr\u00eddie s\u00fa vylu\u010dovacie org\u00e1ny:','cicavcov','ploskavcov','m\u00e4kk\u00fd\u0161ov','r\u00fdb','obr\u00fa\u010dkavcov','prvokov','hlavono\u017ecov','vt\u00e1kopyska',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(392,'392. Do \u010dreva vy\u00fas\u0165uj\u00fa:','protonefr\u00eddie m\u00e4kk\u00fd\u0161ov','nefr\u00eddie k\u00f4rovcov','Malpighiho \u017e\u013eazy hinyzu','tykadlov\u00e9 \u017e\u013eazy ploskavcov','nefr\u00f3ny cicavcov','tykadlov\u00e9 \u017e\u013eazy k\u00f4rovcov','plamienkov\u00e9 bunky ploskavcov','metanefr\u00eddie obr\u00fa\u010dkavcov',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(393,'393. Vylu\u010dovac\u00edmi org\u00e1nmi stavovcov s\u00fa:','protonefr\u00eddie','nefr\u00eddie','malpigick\u00e9 trubice','tykadlov\u00e9 \u017e\u013eazy','obli\u010dky','plamienkov\u00e9 bunky','metanefr\u00eddie','nefr\u00f3ny',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(394,'394. Vylu\u010dovacia s\u00fastava zabezpe\u010duje:','udr\u017eiavanie homeost\u00e1zy vn\u00fatorn\u00e9ho prostredia organizmu','odstra\u0148ovanie splod\u00edn l\u00e1tkovej premeny','odstra\u0148ovanie elektrolytov, vody a v\u0161etk\u00fdch potrebn\u00fdch l\u00e1tok','st\u00e1lu farbu mo\u010du','odstra\u0148ovanie bielkov\u00edn z ultrafiltr\u00e1tu','odstra\u0148ovanie dus\u00edkat\u00fdch produktov metabolizmu bielkov\u00edn','odstra\u0148ovanie star\u00fdch \u010derven\u00fdch krviniek z krvi','odstra\u0148ovanie nadbyto\u010dnej vody z organizmu',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(395,'395. Malpighiho \u017e\u013eazy ako vylu\u010dovacie \u00fastroje maj\u00fa:','obr\u00fa\u010dkavce','pav\u00faky','k\u00f4rovce','stono\u017eky a hmyz','vy\u0161\u0161ie \u010dervy','ploskavce','v\u0161etky \u010dl\u00e1nkono\u017ece','m\u00e4kk\u00fd\u0161e',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(396,'396. S\u00fa\u010das\u0165ou nefr\u00f3nu cicavcov je:','Malpighiho teliesko','Bowmanovo puzdro','Henleho slu\u010dka','mo\u010dovod','mo\u010dov\u00fd mech\u00far','plamienkov\u00e1 bunka','obli\u010dkov\u00e1 panvi\u010dka','Malpighiho \u017e\u013eaza',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(397,'397. Prim\u00e1rny mo\u010d cicavcov:','vznik\u00e1 filtr\u00e1ciou krvi v Malpighiho teliesku','obsahuje bielkoviny','obsahuje rovnak\u00e9 l\u00e1tky ako plazma, okrem bielkov\u00edn','prech\u00e1dza z Bowmanovho puzdra do syst\u00e9mu kan\u00e1likov','je odv\u00e1dzan\u00fd mo\u010dovodmi do mo\u010dov\u00e9ho mech\u00fara','je \u010falej upravovan\u00fd na definit\u00edvny mo\u010d spatn\u00fdm vstreb\u00e1van\u00edm elektrolytov a \u010fal\u0161ich potrebn\u00fdch l\u00e1tok','obsahuje gluk\u00f3zu a aminokyseliny','m\u00e1 rovnak\u00e9 zlo\u017eenie ako plazma',TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(398,'398. K osmoregula\u010dn\u00fdm mechanizmom stavovcov patr\u00ed:','vstreb\u00e1vanie sol\u00ed cez \u017eiabre akt\u00edvnym transportom pri morsk\u00fdch ryb\u00e1ch','vylu\u010dovanie sol\u00ed pomocn\u00fdmi org\u00e1nmi, napr. nosovou \u017e\u013eazou krokod\u00edlov','vylu\u010dovanie sol\u00ed \u017eiabrami pri morsk\u00fdch ryb\u00e1ch','vylu\u010dovanie sol\u00ed \u017eiabrami pri sladkovodn\u00fdch ryb\u00e1ch','vstreb\u00e1vanie soll cez \u017eiabre akt\u00edvnym transportom pri sladkovodn\u00fdch ryb\u00e1ch','exkre\u010dn\u00e9 mechanizmy na vylu\u010dovanie soli nielctor\u00fdch morsk\u00fdch vt\u00e1kov','zakrpaten\u00e9 obli\u010dky sladkovodn\u00fdch r\u00fdb','vstreb\u00e1vanie sol\u00ed pas\u00edvnym transportom',FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(399,'399. Nervov\u00e1 bunka:','je z\u00e1kladn\u00e1 \u0161trukt\u00farna a funk\u010dn\u00e1 jednotka nervovej s\u00fastavy \u017eivo\u010d\u00edchov','sa sklad\u00e1 z tela bunky a v\u00fdbe\u017ekov dvojak\u00e9ho typu','m\u00e1 jeden odstrediv\u00fd v\u00fdbe\u017eok - neurit','m\u00e1 jeden odstrediv\u00fd v\u00fdbe\u017eok - dendrit','m\u00e1 dendrity, ktor\u00e9 priv\u00e1dzaj\u00fa vzruchy do tela bunky','m\u00e1 dendrity, ktor\u00e9 ved\u00fa vzruchy z tela bunky','m\u00e1 dostrediv\u00fd ax\u00f3n','vytv\u00e1ra synapsie s in\u00fdmi nervov\u00fdmi bunkami',TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(400,'400. Nervov\u00e9 vI\u00e1kna miechy tvoria:','bielu hmotu obklopuj\u00facu siv\u00fa hmotu','siv\u00fa hmotu obklopen\u00fa bielou hmotou','bielu hmotu obklopen\u00fa sivou hmotou','siv\u00fa hmotu obklopuj\u00facu bielu hmotu','aj bielu aj siv\u00fa hmotu','z\u00e1klad reflexn\u00e9ho obl\u00faka','miechov\u00fd kan\u00e1lik','mozgov\u00fd kme\u0148',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(401,'401. Miecha:','je centrom zrakov\u00fdch a sluchov\u00fdch reflexov','je fylogeneticky najstar\u0161ou \u010das\u0165ou nervovej s\u00fastavy','je fylogeneticky najmlad\u0161ou \u010das\u0165ou nervovej s\u00fastavy','je reflexn\u00fdm centrom reguluj\u00facim pohyb svalov kon\u010dat\u00edn','sa podie\u013ea na riaden\u00ed niektor\u00fdch auton\u00f3mnych funkci\u00ed','je centrom rovnov\u00e1hy tela','ovplyv\u0148uje fyziologick\u00e9 procesy napr. prij\u00edmanie potravy','sprostredk\u00fava emo\u010dn\u00e9 prejavy',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(402,'402. Auton\u00f3mne nervy:','inervuj\u00fa vn\u00fatorn\u00e9 org\u00e1ny a cievy','vych\u00e1dzaj\u00fa z miechy a mozgu','vych\u00e1dzaj\u00fa len z miechy','vych\u00e1dzaj\u00fa len z mozgu','s\u00fa s\u00fa\u010das\u0165ou vegetat\u00edvnej nervovej s\u00fastavy, ktor\u00e1 je fylogeneticky star\u0161ia ne\u017e s\u00fastava somatick\u00e1','s\u00fa s\u00fa\u010das\u0165ou vegetat\u00edvnej nervovej s\u00fastavy, ktor\u00e1 je fylogeneticky mlad\u0161ia ne\u017e s\u00fastava somatick\u00e1','riadia pohyb kostrov\u00fdch svalov','riadia v\u00f4\u013eou ovl\u00e1dan\u00e9 \u010dinnosti',TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(403,'403. Dostrediv\u00e9 v\u00fdbe\u017eky nervovej bunky:','ved\u00fa podr\u00e1\u017edenie do tela bunky','sa naz\u00fdvaj\u00fa neur\u00f3ny','vytv\u00e1raj\u00fa spoje s in\u00fdmi bunkami','sa naz\u00fdvaj\u00fa neurity','sa naz\u00fdvaj\u00fa dendrity','ved\u00fa podr\u00e1\u017edenie z tela bunky','sa naz\u00fdvaj\u00fa ax\u00f3ny','s\u00fa kr\u00e1tke a rozvetven\u00e9',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(404,'404. D\u00f4le\u017eitou funk\u010dnou vlastnos\u0165ou nervov\u00fdch buniek je:','dr\u00e1\u017edivos\u0165','vodivos\u0165','schopnos\u0165 bunkov\u00e9ho delenia','schopnos\u0165 reagova\u0165 na podnety','pohyblivos\u0165','schopnos\u0165 diaped\u00e9zy','schopnos\u0165 vies\u0165 vzruchy bez zmeny inform\u00e1cie','schopnos\u0165 kontrakcie',TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(405,'405. Stredn\u00fd mozog cicavcov:','je centrom zrakov\u00fdch a sluchov\u00fdch reflexov','podmie\u0148uje vzpriamen\u00fa polohu hlavy a tela','je najvy\u0161\u0161\u00edm \u00fastred\u00edm koordin\u00e1cie pohybov','reguluje teplotu tela','riadi \u010dinnos\u0165 srdca a obeh telov\u00fdch tekut\u00edn','sprostredk\u00fava emo\u010dn\u00e9 prejavy','koordinuje svalov\u00fd tonus','je fylogeneticky najmlad\u0161ou \u010das\u0165ou nervovej s\u00fastavy',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(406,'406. Centrum reguluj\u00face spont\u00e1nne d\u00fdchanie je v:','mozo\u010dku','pred\u013a\u017eenej mieche','moste','mozgovej k\u00f4re','zadnom mozgu','medzimozgu','hypof\u00fdze','prednom mozgu',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(407,'407. Mozgov\u00e1 k\u00f4ra:','m\u00e1 vymedzen\u00e9 oblasti, tzv. k\u00f4rov\u00e9 \u00fastredia','okrem prim\u00e1tov je dobre vyvinut\u00e1 napr. u slona','sa za\u010d\u00edna diferencova\u0165 u\u017e pri plazoch','patr\u00ed medzi fylogeneticky mlad\u0161iu \u010das\u0165 nervovej s\u00fastavy','u vt\u00e1kov tvor\u00ed len tenk\u00fa vrstvu','m\u00e1 povrch zvr\u00e1snen\u00fd len u evolu\u010dne star\u0161\u00edch cicavcov','s\u00fastre\u010fuje inform\u00e1cie prich\u00e1dzaj\u00face len z okolia organizmu','m\u00e1 \u010duchov\u00e9 receptory',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(408,'408. Mozgov\u00fa k\u00f4ru maj\u00fa najviac vyvinut\u00fa:','prim\u00e1ty','plazy','\u017eraloky a delfiny','slony a delf\u00edny','cicavce','vt\u00e1ky','oboj\u017eiveln\u00edky','ryby',TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(409,'409. Mozo\u010dek:','kontroluje rovnov\u00e1hu tela a koordinuje pohyby','je centrom d\u00fdchania','je \u00fastred\u00edm pre k\u00fdchanie','je \u00fastred\u00edm reflexn\u00fdch pohybov','riadi \u010dinnos\u0165 mozgovej k\u00f4ry','sprostredk\u00fava reakcie na boles\u0165 a chlad','je najvy\u0161\u0161\u00edm \u00fastred\u00edm auton\u00f3mnych funkci\u00ed','prij\u00edma sign\u00e1ly z analyz\u00e1torov vn\u00fatorn\u00e9ho ucha a svalov',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(410,'410. Nadradenou \u010das\u0165ou nervov\u00e9ho riadenia vn\u00fatorn\u00fdch org\u00e1nov je:','talamus','hypotalamus','pred\u013a\u017een\u00e1 miecha','predn\u00fd mozog','mozo\u010dek','mozgov\u00e1 k\u00f4ra','podl\u00f4\u017eko','l\u00f4\u017eko',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(411,'411. Prejavom ni\u017e\u0161ej nervovej \u010dinnosti:','s\u00fa vroden\u00e9 reakcie na podnety','s\u00fa nepodmienen\u00e9 reflexy','s\u00fa z\u00edskan\u00e9 reakcie na podnety','s\u00fa podmienen\u00e9 reflexy','je u\u010denie','s\u00fa in\u0161tinkty','s\u00fa em\u00f3cie','je strach',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(412,'412. Podmienen\u00fd reflex:','je vroden\u00fd','je prejav vy\u0161\u0161ej nervovej \u010dinnosti','je z\u00e1kladom v\u00fdvoja in\u0161tinkt\u00edvneho spr\u00e1vania','je z\u00edskan\u00fd po\u010das ontogen\u00e9zy jedinca','je z\u00edskan\u00fd po\u010das fylogen\u00e9zy','vedie k vytv\u00e1raniu nov\u00fdch, do\u010dasn\u00fdch nervov\u00fdch spojen\u00ed','patr\u00ed k z\u00e1kladn\u00fdm mechanizmom u\u010denia','nevy\u017eaduje pre svoj vznik n\u00e1cvik',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(413,'413. Medzi vy\u0161\u0161iu nervov\u00fa \u010dinnos\u0165 patr\u00ed:','podmienen\u00fd reflex','nepodmienen\u00fd reflex','u\u010denie','in\u0161tinkt','strach','myslenie','rodi\u010dovsk\u00fd in\u0161tinkt','pam\u00e4\u0165',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(414,'414. Fagocyt\u00f3za:','patr\u00ed k prejavom bunkovej imunity','je vroden\u00e1 a ne\u0161pecifick\u00e1','patr\u00ed k prejavom protil\u00e1tkovej \u0161pecifickej imunity','patr\u00ed k prejavom celul\u00e1rnej \u0161pecifickej imunity','je v\u00fdvojovo mlad\u0161\u00edm typom imunitnej reakcie','je v\u00fdvojovo star\u0161\u00edm typom imunitnej reakcie','sa vyvinula z p\u00f4vodn\u00e9ho mechanizmu v\u00fd\u017eivy jednobunkov\u00fdch organizmov','je obrann\u00fd mechanizmus spolo\u010dn\u00fd pre v\u0161etky skupiny \u017eivo\u010d\u00edchov',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(415,'415. Bunkov\u00fa \u0161pecifick\u00fa imunitn\u00fa reakciu zabezpe\u010duj\u00fa:','B lymfocyty','neutrofiln\u00e9 granulocyty','T lymfocyty','plazmatick\u00e9 bunky','monocyty','trombocyty','lymfocyty','lymfocyty, ktor\u00e9 dozrievaj\u00fa v t\u00fdmuse',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(416,'416. 0bjavite\u013e fagocyt\u00f3zy bol:','P. Ehrlich','I. I. Me\u010dnikov','I. M. Se\u010denov','I. P. Pavlov','N. Tinbergen','L. Pasteur','W. Harvey','S. W. Fox',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(417,'417. Najlep\u0161ie vyvinut\u00fd sluch so schopnos\u0165ou vn\u00edma\u0165 aj ultrazvukov\u00e9 sign\u00e1ly maj\u00fa:','ma\u010dkovit\u00e9 m\u00e4so\u017eravce','netopiere','ve\u013eryby','psovit\u00e9 m\u00e4so\u017eravce','ryby','larvy oboj\u017eiveln\u00edkov','hmyz','obr\u00fa\u010dkavce',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(418,'418. Protil\u00e1tky:','sa vytv\u00e1raj\u00fa v plazmatick\u00fdch bunk\u00e1ch po styku B lymfocytov s antig\u00e9nom','s\u00fa bielkoviny a reaguj\u00fa len s ur\u010dit\u00fdm antig\u00e9nom','reaguj\u00fa len s antig\u00e9nom produkovan\u00fdm B lymfocytmi','sa vytv\u00e1raj\u00fa v T lymfocytoch','sa vytv\u00e1raj\u00fa v T lymfocytoch po styku s antig\u00e9nom','s\u00fa imunoglobul\u00edny a reaguj\u00fa len s ur\u010dit\u00fdm antig\u00e9nom, ktor\u00fd vyvol\u00e1va ich tvorbu','s\u00fa antig\u00e9ny \u0161pecifick\u00e9 pre dan\u00fd organizmus','s\u00fa polysacharidy produkovan\u00e9 B lymfocytmi',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(419,'419. Protil\u00e1tkov\u00e1 \u0161pecifick\u00e1 imunitn\u00e1 reakcia:','je zabezpe\u010dovan\u00e1 tvorbou protil\u00e1tok v T lymfocytoch','je v\u00fdvojovo mlad\u0161ia ako ne\u0161pecifick\u00e1 imunita','je sprostredkovan\u00e1 B lymfocytmi','je zabezpe\u010dovan\u00e1 tvorbou antig\u00e9nov','je zabezpe\u010dovan\u00e1 tvorbou protil\u00e1tok plazmatick\u00fdmi bunkami','je zabezpe\u010dovan\u00e1 tvorbou aglutinog\u00e9nov','je zabezpe\u010den\u00e1 protil\u00e1tkami, ktor\u00e9 produkuj\u00fa monocyty','je zabezpe\u010den\u00e1 protil\u00e1tkami, ktor\u00e9 produkuj\u00fa lymfoblasty vznikaj\u00face z T lymfocytov',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(420,'420. Ozna\u010dte spr\u00e1vne odpovede:','funkcia protil\u00e1tok je zne\u0161kod\u0148ova\u0165 v\u00edrusy a bakteri\u00e1lne tox\u00edny','bunkov\u00fa \u0161pecifick\u00fa imunitu zabezpe\u010duj\u00fa T lymfocyty','fagocyt\u00f3za patr\u00ed k prejavom ne\u0161pecifickej bunkovej imunity','\u0161pecifick\u00e1 imunita sa vyskytuje u\u017e u \u010dl\u00e1nkono\u017ecov','pas\u00edvna imuniz\u00e1cia mci\u017ee by\u0165 zabezpe\u010dovan\u00e1 aj matersk\u00fdm mliekom','akt\u00edvna imuniz\u00e1cia organizmu vznik\u00e1 po prekonan\u00ed infek\u010dn\u00e9ho ochorenia','fagocyt\u00f3za patr\u00ed k prejavom protil\u00e1tkovej imunity','bunkov\u00fa \u0161pecifick\u00fa imunitu zabezpe\u010duj\u00fa B lymfocyty',TRUE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(421,'421. Proprioreceptory:','s\u00fa receptory nach\u00e1dzaj\u00face sa vo svaloch a \u0161\u013each\u00e1ch','s\u00fa exteroreceptory','s\u00fa interoreceptory','informuj\u00fa o polohe \u010dast\u00ed tela','informuj\u00fa o zmen\u00e1ch vonkaj\u0161ieho prostredia','s\u00fa pomocn\u00e9 org\u00e1ny interoreceptorov','s\u00fa v princ\u00edpe r\u00e1dioreceptory','pom\u00e1haj\u00fa \u017eivo\u010d\u00edchom orientova\u0165 sa',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(422,'422. Medzi chemoreceptory patria:','sluchov\u00e9 receptory','chu\u0165ov\u00e9 receptory','\u010duchov\u00e9 receptory','receptory rovnov\u00e1hy','fotoreceptory','receptory ko\u017ee','receptory citliv\u00e9 na vidite\u013en\u00e9 svetlo','tepeln\u00e9 receptory',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(423,'423. R\u00e1dioreceptory:','del\u00edme na fotoreceptory, termoreceptory a vlastn\u00e9 r\u00e1dioreceptory','niektor\u00e9 typy s\u00fa vyvinut\u00e9 u\u017e u jednobunkovcov','umo\u017e\u0148uj\u00fa niektor\u00fdm \u017eivo\u010d\u00edchom reagova\u0165 na magnetizmus','m\u00f4\u017eu registrova\u0165 ionizuj\u00face \u017eiarenie','v\u0161etk\u00fdch typov m\u00e1 dokonale vyvinut\u00e9 \u010dlovek','m\u00f4\u017eu reagova\u0165 na zmenu chemick\u00e9ho zlo\u017eenia prostredia','umo\u017e\u0148uj\u00fa \u017eivo\u010d\u00edchom orientova\u0165 sa v priestore napr. pri sez\u00f3nnej migr\u00e1cii','reaguj\u00fa na zvukov\u00e9 podnety',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(424,'424. Statocysta je \u00fastrojom rovnov\u00e1hy:','k\u00f4rovcov','hmyzu','stavovcov','r\u00fdb','cicavcov','m\u00e4kk\u00fd\u0161ov','plazov','vt\u00e1kov',TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(425,'425. Statokinetick\u00fd org\u00e1n:','poskytuje inform\u00e1cie o polohe a pohybe tela','cicavcov je ulo\u017een\u00fd vo vn\u00fatornom uchu','maj\u00fa raky ulo\u017een\u00fd na b\u00e1ze tykadiel','obsahuje receptorov\u00e9 bunky s citliv\u00fdmi brvami, reaguj\u00facimi na mechanick\u00e9 dr\u00e1\u017edenie','obsahuje Cortiho bunky','obsahuje receptorov\u00e9 bunky reaguj\u00face na zmeny teploty','je tympan\u00e1lny org\u00e1n kobyliek','je dr\u00e1\u017eden\u00fd sveteln\u00fdmi podnetmi',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(426,'426. Receptorov\u00e9 bunky, meniace zvukov\u00fd podnet na nervov\u00fd sign\u00e1l sa nach\u00e1dzaj\u00fa:','pri ryb\u00e1ch v bo\u010dnej \u010diare','pri cicavcoch v Cortiho org\u00e1ne vn\u00fatorn\u00e9ho ucha','pri hmyze v tympan\u00e1lnych org\u00e1noch','pri cicavcoch v Cortiho org\u00e1ne vonkaj\u0161ieho ucha','pri vt\u00e1koch v u\u0161nom bubienku','pri cicavcoch v strednom uchu','pri bezstavovcoch v statocyste','pri vt\u00e1koch a cicavcoch v spolo\u010dnom sluchovopolohovom org\u00e1ne',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(427,'427. Hmyz m\u00e1 o\u010di:','komorov\u00e9','zlo\u017een\u00e9','facetov\u00e9','ploch\u00e9','miskovit\u00e9','poh\u00e1rikovit\u00e9','teleskopick\u00e9','zlo\u017een\u00e9 z omat\u00eddi\u00ed',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(428,'428. Chemoreceptory:','riadia chemick\u00e9 reakcie','\u010duchov\u00e9 ve\u013eryb\u00e1m ch\u00fdbaj\u00fa','\u010duchov\u00e9 maj\u00fa dobre vyvinut\u00e9 psy a kone','chu\u0165ov\u00e9 s\u00fa ulo\u017een\u00e9 v chu\u0165ov\u00fdch poh\u00e1rikoch','nie s\u00fa vyvinut\u00e9 pri hmyze','chu\u0165ov\u00e9 s\u00fa tzv. dia\u013ekov\u00e9 chemoreceptory','analyzuj\u00fa pr\u00edtomnos\u0165 chemick\u00fdch l\u00e1tok v ovzdu\u0161\u00ed a vode','s\u00fa naj\u010dastej\u0161ie umiestnen\u00e9 vo vn\u00fatorn\u00fdch org\u00e1noch',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(429,'429. Epif\u00fdza:','produkuje horm\u00f3n, ktor\u00fd brzd\u00ed tvorbu pohlavn\u00fdch horm\u00f3nov','produkuje horm\u00f3n, ktor\u00fd aktivuje tvorbu pohlavn\u00fdch horm\u00f3nov','je funk\u010dnou s\u00fa\u010das\u0165ou hypof\u00fdzy','produkuje rastov\u00fd horm\u00f3n','reaguje na intenzitu svetla','produkuje melaton\u00edn','produkuje melan\u00edn','produkuje melanotropn\u00fd horm\u00f3n',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(430,'430. Horm\u00f3ny \u0161t\u00edtnej \u017e\u013eazy:','ovplyv\u0148uj\u00fa intenzitu metabolizmu','s\u00fa tyrox\u00edn a tyron\u00edn','s\u00fa stavebn\u00fdmi l\u00e1tkami','patria medzi gestag\u00e9ny','sa vytv\u00e1raj\u00fa v hypof\u00fdze','udr\u017eiavaj\u00fa vhodn\u00fa koncentr\u00e1ciu i\u00f3nov sod\u00edka v krvnej plazme','podmie\u0148uj\u00fa vytvorenie dokonalej obrany organizmu proti infekcii','reguluj\u00fa napr. metamorf\u00f3zu oboj\u017eiveln\u00edkov',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(431,'431. Horm\u00f3ny hmyzu:','s\u00fa produkovan\u00e9 neurosekre\u010dn\u00fdmi bunkami hlavov\u00e9ho ganglia','s\u00fa produkovan\u00e9 \u017e\u013eazami nach\u00e1dzaj\u00facimi sa v hrudi','m\u00f4\u017eu by\u0165 uvo\u013e\u0148ovan\u00e9 do vonkaj\u0161ieho prostredia ako ferom\u00f3ny','riadia procesy spojen\u00e9 s dokonalou alebo nedokonalou premenou','m\u00f4\u017eu ma\u0165 vn\u00fatrodruhov\u00fa komunika\u010dn\u00fa funkciu','s\u00fa produkovan\u00e9 Malpighiho \u017e\u013eazami','s\u00fa distribuovan\u00e9 do tkan\u00edv krvou','s\u00fa vylu\u010dovan\u00e9 do uzavret\u00e9ho lymfatick\u00e9ho cievneho syst\u00e9mu',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(432,'432. Pri\u0161t\u00edtne telieska sa prv\u00fdkr\u00e1t objavuj\u00fa pri:','cicavcoch','hmyze','oboj\u017eiveln\u00edkoch','vt\u00e1koch','ryb\u00e1ch','bezstavovcoch','m\u00e4kk\u00fd\u0161och','plazoch',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(433,'433. Inzul\u00edn:','zni\u017euje hladinu gluk\u00f3zy v krvi','zvy\u0161uje ukladanie glykog\u00e9nu v pe\u010deni a svaloch','zvy\u0161uje hladinu gluk\u00f3zy v krvi','ur\u00fdch\u013euje rozklad glykog\u00e9nu v pe\u010deni','zvy\u0161uje tvorbu tukov v pankrease','zni\u017euje ukladanie glykog\u00e9nu v pe\u010deni a svaloch','ovplyv\u0148uje metabolizmus sacharidov opa\u010dne ako glukag\u00f3n','podporuje zv\u00fd\u0161en\u00e9 zu\u017eitkovanie gluk\u00f3zy v bunk\u00e1ch',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(434,'434. Ektotermn\u00e9 \u017eivo\u010d\u00edchy s\u00fa:','\u017eivo\u010d\u00edchy s premenlivou teplotou tela','\u017eivo\u010d\u00edchy, ktor\u00fdch telesn\u00e1 teplota sa men\u00ed v z\u00e1vislosti na teplote prostredia','\u017eivo\u010d\u00edchy, ktor\u00fdch telesn\u00e1 teplota je st\u00e1la','\u017eivo\u010d\u00edchy, ktorych telesn\u00e1 teplota nez\u00e1vis\u00ed od teploty okolia','niektor\u00e9 bezstavovce a v\u0161etky stavovce','napr. drsnoko\u017ece','v\u0161etky bezstavovce','plazy',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(435,'435. Endotermia znamen\u00e1:','st\u00e1lu telesn\u00fa teplotu nez\u00e1visl\u00fa od teploty prostredia','st\u00e1lu telesn\u00fa teplotu z\u00e1visl\u00fa od teploty prostredia','premenliv\u00fa telesn\u00fa teplotu nez\u00e1visl\u00fa od teploty prostredia','prehriatie organizmu','zn\u00ed\u017eenie produkcie tepla organizmom','podchladenie organizmu','\u017ee \u017eivo\u010d\u00edchy s\u00fa schopn\u00e9 regulova\u0165 tvorbu tepla v tepelnom jadre organizmu','\u017ee \u017eivo\u010d\u00edchy s\u00fa schopn\u00e9 regulova\u0165 tvorbu tepla v tepelnom obale organizmu',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(436,'436. Medzi endotermn\u00e9 \u017eivo\u010d\u00edchy patria:','hmyz','obr\u00fa\u010dkavce','cicavce a vt\u00e1ky','plazy','oboj\u017eiveln\u00edky','drsnoko\u017ece','ryby','v\u0161etky bezstavovce',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(437,'437. Fyzik\u00e1lna termoregul\u00e1cia sa uskuto\u010d\u0148uje:','roz\u0161\u00edren\u00edm ko\u017en\u00fdch ciev a odparovan\u00edm','zv\u00fd\u0161en\u00fdm pr\u00edjmom potravy','v\u00fddajom tepla vy\u017earovan\u00edm a pr\u00faden\u00edm','p\u00f4soben\u00edm horm\u00f3nov \u0161t\u00edtnej \u017e\u013eazy','zv\u00fd\u0161en\u00edm intenzity metabolizmu','p\u00f4soben\u00edm horm\u00f3nov drene nadobli\u010diek','aktiviz\u00e1ciou svalov\u00fdch pohybov','zmenou prietoku krvi v ko\u017ei',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(438,'438. Heterotermia znamen\u00e1:','prisp\u00f4sobovanie sa teplote prostredia \u017eivo\u010d\u00edchov so st\u00e1lou telesnou teplotou','prisp\u00f4sobovanie sa teplote prostredia \u017eivo\u010d\u00edchov ektotermn\u00fdch','zv\u00fd\u0161en\u00fd baz\u00e1lny metabolizmus za nepriazniv\u00fdch podmienok','\u017ee v pr\u00edpade nepriaznivej teploty prostredia sa endotermn\u00e9 \u017eivo\u010d\u00edchy st\u00e1vaj\u00fa do\u010dasne ektotermn\u00fdmi','\u017ee nastupuje fyzik\u00e1lna termoregul\u00e1cia','\u017ee nastupuje chemick\u00e1 termoregul\u00e1cia','prisp\u00f4sobovanie sa teplote prostredia \u017eivo\u010d\u00edchov s premenlivou telesnou teplotou','\u017ee organizmus m\u00e1 premenliv\u00fa teplotu tela nez\u00e1visle na teplote prostredia',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(439,'439. Horm\u00f3ny:','p\u00f4sobia ne\u0161pecificky na bunky cie\u013eovych tkan\u00edv','p\u00f4sobia \u0161pecificky na bunky cie\u013eov\u00fdch tkan\u00edv','s\u00fa stavebn\u00fdmi l\u00e1tkami','s\u00fa zdrojom ATP','sa z\u00fa\u010dast\u0148uj\u00fa proteosynt\u00e9zy','vyl\u00fa\u010den\u00e9 do telov\u00fdch tekut\u00edn s\u00fa akt\u00edvne nieko\u013eko min\u00fat alebo dn\u00ed','s\u00fa vysoko \u00fa\u010dinn\u00e9 organick\u00e9 l\u00e1tky, ktor\u00e9 ovplyv\u0148uj\u00fa \u010dinnos\u0165 telesn\u00fdch org\u00e1nov','p\u00f4sobia ako regul\u00e1tory l\u00e1tkovej premeny',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(440,'440. Hormon\u00e1lne regula\u010dn\u00e9 mechanizmy:','sa uplat\u0148uj\u00fa u\u017e pri bezstavovcoch','sa najsk\u00f4r objavuj\u00fa pri stavovcoch','pri stavovcoch sa odli\u0161uj\u00fa od hormon\u00e1lnych regul\u00e1ci\u00ed pri bezstavovcoch','pri stavovcoch sa nel\u00ed\u0161ia od hormon\u00e1lnych regul\u00e1ci\u00ed pri bezstavovcoch','s\u00fa pri bezstavovcoch nahraden\u00e9 nervovou regul\u00e1ciou','s\u00fa r\u00fdchlej\u0161ie ako nervov\u00e9 regul\u00e1cie','s\u00fa pomal\u0161ie ako nervov\u00e9 regul\u00e1cie','s\u00fa priamo ovplyv\u0148ovan\u00e9 aj nervov\u00fdm syst\u00e9mom',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(441,'441. Ferom\u00f3ny hmyzu:','vyvol\u00e1vaj\u00fa agresivitu','vyvol\u00e1vaj\u00fa \u00fanikov\u00e9 reakcie','sp\u00f4sobuj\u00fa ospanlivos\u0165','vyvol\u00e1vaj\u00fa obrann\u00e9 reakcie','sp\u00f4sobuj\u00fa pri hmyze spomalenie rastu','zabra\u0148uj\u00fa premene hmyzu','maj\u00fa vplyv na metabolizmus hmyzu','vyvol\u00e1vaj\u00fa pohlavn\u00fa aktivitu',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(442,'442. Predn\u00fd lalok hypof\u00fdzy stavovcov:','produkuje horm\u00f3n prolakt\u00edn','produkuje melanotropn\u00fd horm\u00f3n','sa naz\u00fdva adenohypof\u00fdza','sa naz\u00fdv\u00e1 neurohypof\u00fdza','produkuje horm\u00f3ny riadiace \u010dinnos\u0165 nadobli\u010diek','produkuje rastov\u00fd horm\u00f3n','produkuje horm\u00f3n vazopres\u00edn','produkuje horm\u00f3n oxytoc\u00edn',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(443,'443. Hibernanty:','patria medzi heterotermn\u00e9 organizmy','s\u00fa napr. je\u017e, plch, syse\u013e','s\u00fa napr. krt, medve\u010f, netopier','upadaj\u00fa do zimn\u00e9ho sp\u00e1nku pri trvalom poklese teploty prostredia pod 0\u00b0C','po\u010das hibern\u00e1cie \u010derpaj\u00fa energiu zo z\u00e1soby tuku','upadaj\u00fa do zimn\u00e9ho sp\u00e1nku pri trvalom poklese teploty prostredia pod 10\u00b0C','po\u010das zimn\u00e9ho sp\u00e1nku zvy\u0161uj\u00fa teplotu tepeln\u00e9ho jadra organizmu','si zabezpe\u010duj\u00fa pre\u017eitie za nepriazniv\u00fdch podmienok zv\u00fd\u0161en\u00edm aktivity metabolizmu',TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(444,'444. Hlavn\u00fdm producentom tepla pri endotermn\u00fdch \u017eivo\u010d\u00edchoch:','s\u00fa kostrov\u00e9 svaly','je mozog','je pe\u010de\u0148','je podko\u017en\u00fd tuk','s\u00fa endokrinn\u00e9 \u017erazy','s\u00fa obli\u010dky','je d\u00fdchacia s\u00fastava','je tenk\u00e9 \u010drevo',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(445,'445. Estiv\u00e1cia je:','prebytok potravy a nedostatok vody','letn\u00fd sp\u00e1nok','zv\u00fd\u0161enie telesnej teploty \u017eivo\u010d\u00edcha','prehriatie organizmu pri v\u00e4\u010d\u0161\u00edch zmen\u00e1ch teploty prostredia','typick\u00e1 pre niektor\u00e9 p\u00fa\u0161tne a stepn\u00e9 \u017eivo\u010d\u00edchy','typick\u00e1 pre \u017eivo\u010d\u00edchy pol\u00e1rnych oblast\u00ed','vyvol\u00e1van\u00e1 nedostatkom vody a potravy vplyvom vysok\u00fdch tepl\u00f4t prostredia','zimn\u00fd sp\u00e1nok',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(446,'446. K v\u00fdskumn\u00fdm oblastiam zaoberaj\u00facim sa pozn\u00e1van\u00edm spr\u00e1vania zvierat patr\u00ed:','v\u00fdskum fyziol\u00f3gie podmienen\u00fdch reflexov','sk\u00famanie procesu u\u010denia u zvierat v laborat\u00f3rnych podmienkach','behaviorizmus','etol\u00f3gia','v\u00fdskum spr\u00e1vania \u017eivo\u010d\u00edchov v prirodzenom prostred\u00ed','anat\u00f3mia','cytol\u00f3gia','sk\u00famanie fyziol\u00f3gie prenosu nervov\u00e9ho sign\u00e1lu',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(447,'447. In\u0161tinkt\u00edvne spr\u00e1vanie \u017eivo\u010d\u00edchov:','je predmetom sk\u00famania etol\u00f3gie','je predmetom sk\u00famania exobiol\u00f3gie','je vroden\u00e9 spr\u00e1vanie vyvol\u00e1van\u00e9 endog\u00e9nnymi alebo exog\u00e9nnymi podnetmi','je spr\u00e1vanie, ktor\u00e9ho z\u00e1kladom s\u00fa nepodmienen\u00e9 refleu','je spr\u00e1vanie, ktor\u00e9ho z\u00e1ldadom s\u00fa podmienen\u00e9 reflexy','je napr. obrann\u00e9 a \u00fatekov\u00e9 spr\u00e1vanie','zah\u0155\u0148a schopnos\u0165 generaliz\u00e1cie','je napr. komfortn\u00e9 spr\u00e1vanie',TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(448,'448. Teritori\u00e1lne spr\u00e1vanie \u017eivo\u010d\u00edchov:','je vroden\u00e9 in\u0161tinkt\u00edvne spr\u00e1vanie','umo\u017e\u0148uje \u017eivo\u010d\u00edchom zabezpe\u010di\u0165 si dostato\u010dn\u00e9 mno\u017estvo potravy','zabezpe\u010duje rozptyl \u017eivo\u010d\u00edchov v biotope','zni\u017euje mo\u017enos\u0165 vn\u00fatrodruhov\u00fdch konfliktov','je spojen\u00e9 predov\u0161etk\u00fdm s obranou terit\u00f3ria pred pr\u00edslu\u0161n\u00edkmi in\u00fdch \u017eivo\u010d\u00ed\u0161nych druhov','je spojen\u00e9 predov\u0161etk\u00fdm s obranou terit\u00f3ria pred pr\u00edslu\u0161n\u00edkmi vlastn\u00e9ho druhu','zah\u0155\u0148a ozna\u010dovanie a obranu vlastn\u00e9ho terit\u00f3ria','je rozmno\u017eovacie spr\u00e1vanie',TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(449,'449. Fakultat\u00edvne u\u010denie:','prebieha len v ranom veku \u017eivo\u010dhov','nie je vekovo obmedzen\u00e9','prebieha prakticky po cel\u00fd \u017eivot jedinca','vedie k zdokona\u013eovaniu \u010dinnosti \u017eivo\u010d\u00edcha','je napr. vpe\u010datenie','je prisp\u00f4sobenie sa endog\u00e9nnym biologick\u00fdm rytmom','je z\u00e1kladom obrann\u00fdch reakci\u00ed ml\u00e1\u010fat','prebieha na z\u00e1klade nepodmienen\u00fdch reflexov',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(450,'450. Komunika\u010dn\u00e9 sign\u00e1ly \u017eivo\u010d\u00edchov:','m\u00f4\u017eu by\u0165 akustick\u00e9 alebo optick\u00e9','maj\u00fa identifika\u010dn\u00fa alebo v\u00fdstra\u017en\u00fa funkciu','m\u00f4\u017eu by\u0165 dotykov\u00e9 alebo elektrick\u00e9','sa uplat\u0148uj\u00fa napr. pri obrane terit\u00f3ria','neovplyv\u0148uj\u00fa spr\u00e1vanie in\u00fdch pr\u00edslu\u0161n\u00edkov druhu','nie s\u00fa s\u00fa\u010das\u0165ou soci\u00e1lneho spr\u00e1vania','sl\u00fa\u017eia na prenos inform\u00e1cie','nemaj\u00fa informa\u010dn\u00fa funkciu',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(451,'451. Cel\u00e1 tr\u00e1viaca s\u00fastava \u010dloveka m\u00e1 tieto \u010dasti:','\u00fastna dutina, hltan, pa\u017eer\u00e1k, \u017eal\u00fadok, tenk\u00e9 a hrub\u00e9 \u010drevo, kone\u010dn\u00edk a an\u00e1lny otvor, tr\u00e1viace \u017e\u013eazy','\u00fastna dutina, hrtan, pa\u017eer\u00e1k, \u017eal\u00fadok, tenk\u00e9 a hrub\u00e9 \u010drevo, tr\u00e1viace \u017e\u013eazy','\u00fastna dutina, pa\u017eer\u00e1k, \u017eal\u00fadok, tenk\u00e9 a hrub\u00e9 \u010drevo, pe\u010de\u0148 a slezina','\u00fastna dutina, hltan, pa\u017eer\u00e1k, \u017eal\u00fadok, tenk\u00e9 a hrub\u00e9 \u010drevo, kone\u010dn\u00edk, an\u00e1lny otvor, samostatn\u00e9 mal\u00e9 tr\u00e1viace \u017e\u013eazy a ve\u013ek\u00e9 tr\u00e1viace \u017e\u013eazy ulo\u017een\u00e9 v stene tr\u00e1viacej r\u00fary','\u00fastna dutina, hltan, \u017eal\u00fadok, tenk\u00e9, hrub\u00e9 a slep\u00e9 \u010drevo, pe\u010de\u0148 a slezina','\u00fastna dutina, hltan, pa\u017eer\u00e1k, \u017eal\u00fadok, hrub\u00e9 \u010drevo','\u00fastna dutina, hltan, pa\u017eer\u00e1k, \u017eal\u00fadok, tenk\u00e9 a hrub\u00e9 \u010drevo, kone\u010dn\u00edk, an\u00e1lny otvor, mal\u00e9 a ve\u013ek\u00e9 tr\u00e1viace \u017e\u013eazy','\u00fastna dutina, hltan, hrtan, pa\u017eer\u00e1k, \u017eal\u00fadok, tenk\u00e9 a hrub\u00e9 \u010drevo, tr\u00e1viace \u017e\u013eazy',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(452,'452. Ktor\u00e9 zuby ch\u00fdbaj\u00fa v detskom chrupe:','rez\u00e1ky','o\u010dn\u00e9 zuby','\u010drenov\u00e9 zuby','mlie\u010dne zuby','v\u0161etky stoli\u010dky','8 \u010drenov\u00fdch zubov','4 stoli\u010dky','2 rez\u00e1ky',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(453,'453. Do \u00fastnej dutiny \u00fastia:','3 p\u00e1ry ve\u013ek\u00fdch slinn\u00fdch \u017eliaz','mal\u00e9 a ve\u013ek\u00e9 tr\u00e1viace \u017e\u013eazy','3 ve\u013ek\u00e9 slinn\u00e9 \u017e\u013eazy, ktor\u00e9 produkuj\u00fa sliny iba pri vyvolan\u00ed podmienen\u00e9ho reflexu','pr\u00edu\u0161n\u00e9 \u017e\u013eazy produkuj\u00face sliny s obsahom ptyal\u00ednu','podjazykov\u00e9 \u017e\u013eazy produkuj\u00face sliny s obsahom amyl\u00e1z','pods\u00e1nkov\u00e9 \u017e\u013eazy produkuj\u00face sliny s obsahom ptyal\u00ednu','3 p\u00e1ry vel k\u00fdch slinn\u00fdch \u017eliaz produkuj\u00face sliny, ktor\u00e9 obsahuj\u00fa bakteric\u00eddne l\u00e1tky a amyl\u00e1zy','3 p\u00e1ry ve\u013ekych slinn\u00fdch \u017eliaz \u2014 podjazykov\u00e9, pods\u00e1nkov\u00e9, pr\u00edu\u0161n\u00e9',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(454,'454. V \u00fastnej dutine \u010dloveka:','kon\u010d\u00ed tr\u00e1venie \u0161krobov','\u017eij\u00fa bakt\u00e9tie','za\u010d\u00edna vstreb\u00e1vanie niektor\u00fdch l\u00e1tok','nach\u00e1dzame ptyal\u00edn','za\u010d\u00edna tr\u00e1venie polysacharidov','nach\u00e1dzame amyl\u00e1zu','nach\u00e1dzame v slin\u00e1ch bakteric\u00eddne l\u00e1tky, napr. lyzoz\u00f3m','doch\u00e1dza k mechanick\u00e9mu rozdrobovaniu potravy',FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(455,'455. P\u00f4soben\u00edm \u017eal\u00fadkovej \u0161\u0165avy sa u \u013eud\u00ed \u0161tiepia:','pepsinog\u00e9n na peps\u00edn','bielkoviny p\u00f4soben\u00edm chymoz\u00ednu','a detoxikuj\u00fa prijat\u00e9 l\u00e1tky','sacharidy p\u00f4soben\u00edm peps\u00ednu','aminokyseliny p\u00f4soben\u00edm peps\u00ednu','bielkoviny','niektor\u00e9 tuky p\u00f4soben\u00edm \u017eal\u00fado\u010dnej lip\u00e1zy','niektor\u00e9 tuky p\u00f4soben\u00edm pankreatickej lip\u00e1zy',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(456,'456. Ktor\u00e1 zlo\u017eka \u017eal\u00fadkovej \u0161\u0165avy stabilizuje vitam\u00edny skupiny B a vitam\u00edn C ?','HCl','HCl, ktor\u00e1 vytv\u00e1ra v \u017eal\u00fadku silne alkalick\u00e9 prostredie potrebn\u00e9 na aktivovanie peps\u00ednu','peps\u00edn vylu\u010dovan\u00fd vo forme neakt\u00edvneho pepsinog\u00e9nu','hlien','muc\u00edn, ktor\u00fd vylu\u010duj\u00fa hlienov\u00e9 \u017eliazky pri vchode do \u017eal\u00fadka a v jeho koncovej \u010dasti','kyselina chlorovod\u00edkov\u00e1','kyselina chlorovod\u00edkov\u00e1, ktor\u00e1 vytv\u00e1ra v \u017eal\u00fadku silne alkalick\u00e9 prostredie a t\u00fdm ni\u010d\u00ed aj choroboplodn\u00e9 z\u00e1rodky','kyselina chlorovod\u00edkov\u00e1, ktor\u00e1 vytv\u00e1ra v \u017eal\u00fadku silne kysl\u00e9 prostredie a t\u00fdm ni\u010d\u00ed aj choroboplodn\u00e9 z\u00e1rodky',TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(457,'457. \u017dal\u00fado\u010dn\u00e1 \u0161\u0165ava obsahuje:','peps\u00edn, ktor\u00fd je vylu\u010dovan\u00fd vo forme neakt\u00edvneho pepsinog\u00e9nu','HCl, ktor\u00e1 vytv\u00e1ra v \u017eal\u00fadku silne kysl\u00e9 prostredie','muc\u00edn, ktor\u00fd vylu\u010duj\u00fa hlienov\u00e9 \u017eliazky pri vchode do \u017eal\u00fadka','peps\u00edn, ktor\u00fd je vylu\u010dovan\u00fd vo forme neakt\u00edvneho trypsinog\u00e9nu','alkalick\u00fd hlien, ktor\u00fd zabra\u0148uje znehodnoteniu vitam\u00ednov skupiny B a vitam\u00ednu C','vodu','kysl\u00fd hlien, ktor\u00fd pokr\u00fdva steny \u017eal\u00fadka a t\u00fdm ich chr\u00e1ni pre \u00fa\u010dinkami HCl','gastr\u00edn, \u00falohou ktor\u00e9ho je aktivova\u0165 pepsinog\u00e9n na peps\u00edn',TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(458,'458. \u00dapln\u00fd rozklad \u017eiv\u00edn nast\u00e1va:','v tenkom \u010dreve','v \u017eal\u00fadku p\u00f4soben\u00edm peps\u00ednu','v hrubom \u010dreve p\u00f4soben\u00edm hnilobn\u00fdch bakt\u00e9ri\u00ed','v pe\u010deni','v tenkom \u010dreve p\u00f3soben\u00edm pankreatickej \u0161\u0165avy, \u017el\u010de a \u010drevnej \u0161\u0165avy','v tenkom \u010dreve p\u00f4soben\u00edm peps\u00ednu, lip\u00e1zy a amyl\u00e1zy','v hrubom \u010dreve p\u00f4soben\u00edm tryps\u00ednu, lip\u00e1zy, amyl\u00e1zy a enz\u00fdmov \u010drevnej \u0161\u0165avy','v \u017eal\u00fadku p\u00f4soben\u00edm peps\u00ednu, amyl\u00e1zy a \u017eal\u00fado\u010dnej lip\u00e1zy',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(459,'459. Ptyal\u00edn je enz\u00fdm:','pr\u00edtomn\u00fd v \u017eal\u00fado\u010dnej \u0161\u0165ave','vylu\u010dovan\u00fd pr\u00edu\u0161n\u00fdmi \u017e\u013eazami','pr\u00edtomn\u00fd v slin\u00e1ch','natr\u00e1vuj\u00faci polysacharidy v \u00fastnej dutine','ktor\u00fd \u0161tiepi bielkoviny v \u00fastnej dutine','pr\u00edtomn\u00fd v \u010drevnej \u0161\u0165ave','ktor\u00fd m\u00e1 bakteric\u00eddne \u00fa\u010dinky','ktor\u00fd za\u010d\u00edna rozklada\u0165 bielkoviny v \u00fastnej dutine',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(460,'460. Chymoz\u00edn je:','enz\u00fdm \u017eal\u00fadka doj\u010diat zr\u00e1\u017eaj\u00faci mlieko','kysl\u00e1 tr\u00e1venina','natr\u00e1ven\u00e1 potrava','enz\u00fdm ch\u00fdmusu','enz\u00fdm \u017eal\u00fado\u010dnej \u0161\u0165avy','\u017eal\u00fado\u010dn\u00e1 lip\u00e1za','enz\u00fdm tenk\u00e9ho \u010dreva doj\u010diat zr\u00e1\u017eaj\u00faci mlieko','alkalick\u00fd hlien, ktor\u00fd pokr\u00fdva steny \u017eal\u00fadka',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(461,'461. Hrub\u00e9 \u010drevo \u010dloveka sa za\u010d\u00edna:','zostupnou \u010das\u0165ou','slep\u00fdm \u010drevom a pokra\u010duje v\u00fdstupnou \u010das\u0165ou','ako slep\u00e9 \u010drevo','ako slep\u00e9 \u010drevo v \u013eavej bedrovej jame','ako slep\u00e9 \u010drevo v pravej bedrovej jame','ako dvan\u00e1stnik v pravej bedrovej jame','ako slep\u00e9 \u010drevo a kon\u010d\u00ed sa bedrovn\u00edkom','ako slep\u00e9 \u010drevo a kon\u010d\u00ed sa kone\u010dn\u00edkom',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(462,'462. Jemn\u00e9 v\u00fdbe\u017eky sliznice tenk\u00e9ho \u010dreva \u010dloveka umo\u017e\u0148uj\u00face lep\u0161ie vstreb\u00e1vanie \u017eiv\u00edn s\u00fa:','drobn\u00e9 \u017eliazky produkuj\u00face \u010drevn\u00fa \u0161\u0165avu','riasy','klky','kruhovit\u00e9 zvr\u00e1snenia - mikroklky','lal\u00f4\u010diky','riasinky','ostrov\u010deky','brvy',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(463,'463. Hlavn\u00fdm miestom vstreb\u00e1vania \u017eiv\u00edn je:','\u017eal\u00fadok','hrub\u00e9 \u010drevo, ktor\u00e9 m\u00e1 tri \u010dasti \u2014 dvan\u00e1stnik, la\u010dn\u00edk a bedrovn\u00edk','intestinum tenue','tenk\u00e9 \u010drevo, ktor\u00e9 m\u00e1 tri \u010dasti \u2014 dvan\u00e1stnik, la\u010dn\u00edk a bedrovn\u00edk','pe\u010de\u0148','svalov\u00e1 r\u00fara, ktor\u00e1 sp\u00e1ja \u017eal\u00fadok s dvan\u00e1stnikom','\u00fasek tr\u00e1viaceho traktu medzi \u017eal\u00fadkom a hrub\u00fdm \u010drevom','svalov\u00e1 r\u00fara, ktor\u00e1 sp\u00e1ja \u017eal\u00fadok s hrub\u00fdm \u010drevom',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(464,'464. Miestom tr\u00e1venia bielkov\u00edn je:','\u00fastna dutina','\u017eal\u00fadok','tenk\u00e9 \u010drevo','hrub\u00e9 \u010drevo','pe\u010de\u0148','pankreas','slep\u00e9 \u010drevo','\u017el\u010dn\u00edk',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(465,'465. Miestom tr\u00e1venia tukov je:','\u00fastna dutina','hlavne \u017eal\u00fadok','tenk\u00e9 \u010drevo','hrub\u00e9 \u010drevo','pe\u010de\u0148','\u017el\u010dn\u00edk','\u017eal\u00fadok  - v obmedzenej miere','pod\u017eal\u00fadkov\u00e1 \u017e\u013eaza',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(466,'466. Na tr\u00e1ven\u00ed v tenkom \u010dreve \u010dloveka sa v plnej miere z\u00fa\u010dast\u0148uj\u00fa enz\u00fdmy:','lip\u00e1za','tryps\u00edn, ktor\u00fd prich\u00e1dza do dvan\u00e1stnika vo forme neakt\u00edvneho pepsinog\u00e9nu','peps\u00edn, ktor\u00fd \u0161tiepi bielkoviny na peptidy a\u017e aminokyseliny','tryps\u00edn, Ictor\u00fd \u0161tiepi bielkoviny na peptidy a\u017e aminokyseliny','ptyal\u00edn, ktor\u00fd \u0161tiepi polysacharidy a\u017e na jednoduch\u00e9 cukry','chymoz\u00edn, ktor\u00fd zr\u00e1\u017ea mlieko','amyl\u00e1za, ktor\u00e1 \u0161tiepi polysacharidy a\u017e na jednoduch\u00e9 cukry','pankreatick\u00e9 \u0161\u0165avy',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(467,'467. Tryps\u00edn u \u010dloveka:','sa tvor\u00ed v pankrease, v \u017eal\u00fadku \u0161tiepi bielkoviny na peptidy a\u017e aminokyseliny','poch\u00e1dza z pankreasu, v tenkom \u010dreve \u0161tiepi polypeptidy na peptidy a aminokyseliny','sa tvor\u00ed v \u017e\u013eaz\u00e1ch \u017eal\u00fadka, kde \u0161tiepi bielkoviny na polypeptidy','sa tvor\u00ed v pankrease, v tenkom \u010dreve \u0161tiepi tuky na glycerol a mastn\u00e9 kyseliny','je proteolytick\u00fdm enz\u00fdmom','prich\u00e1dza do dvan\u00e1stnika v podobe neakt\u00edvneho pepsinog\u00e9nu','prich\u00e1dza do dvan\u00e1stnika v podobe neakt\u00edvneho trypsinog\u00e9nu','je pankreatick\u00fdm horm\u00f3nom',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(468,'468. Pankreatick\u00e1 \u0161\u0165ava obsahuje:','soli neutralizuj\u00face kysl\u00fa tr\u00e1veninu','akt\u00edvny trypsinog\u00e9n, ktor\u00fd sa inaktivuje enterokin\u00e1zou','neakt\u00edvny trypsinog\u00e9n, ktor\u00fd sa aktivuje enterokin\u00e1zou','neakt\u00edvny pepsinog\u00e9n, ktor\u00fd sa aktivuje enterokin\u00e1zou','peps\u00edn, lip\u00e1zu, chymoz\u00edn','aj horm\u00f3ny \u2014 glukag\u00f3n a inzul\u00edn','enz\u00fdmy \u0161tiepiace bielkoviny, sacharidy a tuky','neakt\u00edvny trypsinog\u00e9n, ktor\u00fd sa v \u017eal\u00fadku aktivuje enterokin\u00e1zou',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(469,'469. \u013dudsk\u00fd pankreas:','produkuje pankreatick\u00fa \u0161\u0165avu a jeho v\u00fdvody vy\u00fas\u0165uj\u00fa do bedrovn\u00edka','produkuje pankreatick\u00fa \u0161\u0165avu a jeho v\u00fdvody vy\u00fas\u0165uj\u00fa do dvan\u00e1stnika','produkuje prote\u00e1zu, lip\u00e1zu, amyl\u00e1zu','produkuje tryps\u00edn, amyl\u00e1zu, lip\u00e1zu','le\u017e\u00ed pod pe\u010de\u0148ou v ohybe dvan\u00e1stnika','le\u017e\u00ed pod \u017eal\u00fadkom v ohybe dvan\u00e1stnika','produkuje enz\u00fdmy \u0161tiepiace bielkoviny, sacharidy a tuky','produkuje chymoz\u00edn, ptyal\u00edn, lip\u00e1zu',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(470,'470. Soli \u017el\u010dov\u00fdch kysel\u00edn v tenkom \u010dreve \u010dloveka:','rozkladaj\u00fa tuky na glycerol a mastn\u00e9 kyseliny','s\u00fa sterkobil\u00edn a urobil\u00edn','rozpty\u013euj\u00fa tuky na drobn\u00e9 kvap\u00f4\u010dky s ve\u013ek\u00fdm povrchom','u\u013eah\u010duj\u00fa p\u00f4sobenie enz\u00fdmov, ktor\u00e9 tr\u00e1via tuky','u\u013eah\u010duj\u00fa p\u00f4sobenie amyl\u00e1z','u\u013eah\u010duj\u00fa p\u00f4sobenie lip\u00e1zy','s\u00fa bilirub\u00edn a biliverd\u00edn \u2014 vznikaj\u00fa ako produkt rozpadu hemoglob\u00ednu','pom\u00e1haj\u00fa emulgova\u0165 tuky',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(471,'471. \u017dl\u010d sa u \u010dloveka tvor\u00ed v:','\u017el\u010dn\u00edku a emulguje tuky na drobn\u00e9 kvap\u00f4\u010dky','pe\u010deni a zhroma\u017e\u010fuje sa v tenkom \u010dreve','pe\u010deni a zhroma\u017e\u010fuje sa v \u017el\u010dn\u00edku','pe\u010deni a u\u013eah\u010duje p\u00f4sobenie lip\u00e1zy','pe\u010deni a u\u013eah\u010duje p\u00f4sobenie tryps\u00ednu','pe\u010deni a emulguje tuky na drobn\u00e9 kvap\u00f4\u010dky','\u017el\u010dn\u00edku a u\u013eah\u010duje p\u00f4sobenie lip\u00e1zy','pe\u010deni a rozklad\u00e1 tuky na glycerol a mastn\u00e9 kyseliny',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(472,'472. Hrub\u00e9 \u010drevo zdrav\u00e9ho \u010dloveka:','je dlh\u00e9 asi 1,5 m, m\u00e1 klky a prie\u010dne krkvy','obsahuje ve\u013ea poh\u00e1rikovit\u00fdch buniek, ktor\u00e9 produkuj\u00fa \u010drevn\u00fa \u0161\u0165avu','obsahuje symbiotick\u00e9 kvasn\u00e9 a hnilobn\u00e9 bakt\u00e9rie','\u010do sa t\u00fdka \u010dinnosti je ovplyvnen\u00e9 pr\u00edtomnos\u0165ou bakt\u00e9ri\u00ed a ich produktov','je miestom, kde nestr\u00e1ven\u00e9 zvy\u0161ky potravy podliehaj\u00fa bakteri\u00e1lnemu rozkladu a kvaseniu','obsahuje ve\u013ea poh\u00e1rikovit\u00fdch buniek, ktor\u00e9 produkuj\u00fa hlien','je vyplnen\u00e9 lymfatick\u00fdm tkanivom, ktor\u00e9 m\u00f4\u017ee reagova\u0165 na podr\u00e1\u017edenie z\u00e1palom','nevylu\u010duje \u0161\u0165avy, ktor\u00e9 maj\u00fa tr\u00e1viacu schopnos\u0165',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(473,'473. K naj\u010dastej\u0161\u00edm funk\u010dn\u00fdm poruch\u00e1m tr\u00e1viacej s\u00fastavy patr\u00ed:','hypertenzia','apendicit\u00edda','ateroskler\u00f3za','an\u00e9mia','laryngit\u00edda','obezita','gastrit\u00edda','tvorba vredov',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(474,'474. Hrub\u00e9 \u010drevo dospel\u00e9ho \u010dloveka je dlh\u00e9 asi:','12 m','3 m','0,5 m','1,5 m','20 - 25 m','40 - 50 m','4 - 5 m','3 m',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(475,'475. V hrubom \u010dreve \u010dloveka sa vstreb\u00e1vaj\u00fa:','voda','niektor\u00e9 soli','niektor\u00e9 lieky','mal\u00e9 mno\u017estvo bielkov\u00edn, tukov a cukrov','hlien, ktor\u00fd produkuj\u00fa poh\u00e1rikovit\u00e9 bunky','symbiotick\u00e9 hnilobn\u00e9 a kvasn\u00e9 bakt\u00e9rie','nestr\u00e1vite\u013en\u00e9 zvy\u0161ky potravy','iba voda',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(476,'476. Ak\u00e9 tr\u00e1viace enz\u00fdmy vylu\u010duje \u010dlovek do hrub\u00e9ho \u010dreva:','kvasn\u00e9','lip\u00e1zu','amyl\u00e1zu','tryps\u00edn','\u017eiadne','hnilobn\u00e9','prote\u00e1zy','disacharid\u00e1zy',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(477,'477. V hrubom \u010dreve dospel\u00e9ho \u010dloveka norm\u00e1lne trvalo \u017eij\u00fa:','nepatog\u00e9nne bakt\u00e9rie','kvasn\u00e9 bakt\u00e9rie','patog\u00e9nne bakt\u00e9rie','nitrifika\u010dn\u00e9 bakt\u00e9rie','symbiotick\u00e9 bakt\u00e9rie','hnilobn\u00e9 bakt\u00e9rie','Salmonelly a Shigelly','drobn\u00e9 ne\u0161kodn\u00e9 helminty',TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(478,'478. Zvracanie:','je obrann\u00fd reflex','nast\u00e1va podr\u00e1\u017eden\u00edm \u017eal\u00fadka','je obrann\u00fd reflex, ktor\u00fdm sa \u010drevo zbavuje dr\u00e1\u017ediv\u00fdch l\u00e1tok','je obrann\u00fd refiex, ktor\u00fdm sa \u017eal\u00fadok zbavuje dr\u00e1\u017ediv\u00fdch l\u00e1tok','je obrann\u00fd reflex, ktor\u00fdm sa \u017eal\u00fadok zbavuje dr\u00e1\u017ediv\u00fdch l\u00e1tok cez \u010drevo','nast\u00e1va v d\u00f4sledku peristaltick\u00fdch pohybov svaloviny \u017eal\u00fadka','nast\u00e1va v d\u00f4sledku antiperistaltick\u00fdch pohybov svaloviny \u017eal\u00fadka','nast\u00e1va podr\u00e1\u017eden\u00edm \u00fastnej dutiny alebo \u010dreva',TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(479,'479. Hrub\u00e1 vl\u00e1knina \u013eudskej potravy:','je d\u00f4le\u017eit\u00e1 pre dobr\u00e9 vypr\u00e1zd\u0148ovanie hrub\u00e9ho \u010dreva','obmedzuje nadmern\u00e9 rozmno\u017eovanie bakt\u00e9ri\u00ed','nepodnecuje pohyb \u010driev','sa nach\u00e1dza najm\u00e4 v bielej m\u00fake','nepoch\u00e1dza obvykle zo \u017eivo\u010d\u00ed\u0161nej potravy bohatej na bielkoviny','je najm\u00e4 rastlinn\u00e9ho p\u00f4vodu','tvoria ju hlavne celul\u00f3za, lign\u00edn a pekt\u00edny','je str\u00e1ven\u00e1 v tenkom \u010dreve \u00fa\u010dinkom enz\u00fdmov pankreasu',TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(480,'480. Vyberte spr\u00e1vne tvrdenia o tr\u00e1ven\u00ed \u010dloveka:','chymoz\u00edn je kysl\u00e1 tr\u00e1venina \u017eal\u00fadka, ktor\u00e1 prich\u00e1dza do dvan\u00e1stnika','zo \u017eal\u00fadka potrava prech\u00e1dza do dvan\u00e1stnika','sacharidy a bielkoviny potravy s\u00fa bohat\u00fdm zdrojom vitam\u00ednov a miner\u00e1lov','niektor\u00e9 \u017eiviny sa m\u00f4\u017eu vstreb\u00e1va\u0165 u\u017e v \u017eal\u00fadku','\u017el\u010d sa tvor\u00ed v \u017el\u010dn\u00edku a pritek\u00e1 do tenk\u00e9ho \u010dreva','v hrubom \u010dreve sa dokon\u010duje tr\u00e1venie potravy a vstreb\u00e1vanie \u017eiv\u00edn','pre tr\u00e1viace procesy s\u00fa d\u00f4le\u017eit\u00e9 aj v\u00fdlu\u010dky \u017eal\u00fadka, pankreasu i pe\u010dene','bakt\u00e9rie v \u010dreve m\u00f4\u017eu tvori\u0165 l\u00e1tky telu prospe\u0161n\u00e9 i \u0161kodliv\u00e9',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(481,'481. Nedostatok \u017eeleza u \u010dloveka m\u00f4\u017ee ma\u0165 za n\u00e1sledok:','m\u00e1lokrvnos\u0165','strumu','nedostatok hemoglob\u00ednu','zv\u00fd\u0161en\u00fa sediment\u00e1ciu \u010derven\u00fdch krviniek','bul\u00edmiu','hypovitamin\u00f3zu','an\u00e9miu','pernici\u00f3znu an\u00e9miu',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(482,'482. Energeticky najbohat\u0161ou zlo\u017ekou \u013eudskej potravy na jednotku hmotnosti s\u00fa:','sacharidy','tuky','bielkoviny','esenci\u00e1lne aminokyseliny','miner\u00e1lne l\u00e1tky','vl\u00e1knina a gluk\u00f3za','peptidy','lipidy',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(483,'483. Najbe\u017enej\u0161\u00edm zdrojom energie z potravy pre organizmus \u010dloveka s\u00fa:','sacharidy','bielkoviny','aminokyseliny','mikro\u017eiviny','tuky','nukleov\u00e9 kyseliny','ATP a ADP','lipidy',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(484,'484. Optim\u00e1lny denn\u00fd pr\u00edvod bielkov\u00edn vo v\u00fd\u017eive det\u00ed je:','1g na 1kg hmotnosti tela','2 g na 1kg hmotnosti tela','4 g na 1kg hmotnosti tela','5 g na 1kg hmotnosti tela','5 - 6 g na 1kg hmotnosti tela','rovnak\u00fd ako u dospel\u00fdch','vy\u0161\u0161\u00ed ako u dospel\u00fdch','ni\u017e\u0161\u00ed ako u dospel\u00fdch',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(485,'485. Pre \u010dloveka nepostr\u00e1date\u013en\u00e9 (esenci\u00e1lne) aminokyseliny:','si organizmus vytv\u00e1ra pod\u013ea potreby pod pr\u00edsnou kontrolou horm\u00f3nov','tvoria va\u010d\u0161inu z prijat\u00fdch aminokysel\u00edn be\u017enej potravy','s\u00fa pr\u00edtomn\u00e9 len v rastlinn\u00fdch bielkovin\u00e1ch','si organizmus nedok\u00e1\u017ee s\u00e1m vytvori\u0165','s\u00fa zdrojom energie pre \u0161pecifick\u00e9 bunkov\u00e9 reakcie','s\u00fa v potrave zast\u00fapen\u00e9 v po\u010dte 20','s\u00fa v potrave zast\u00fapen\u00e9 v po\u010dte 8','m\u00f4\u017eu by\u0165 nahraden\u00e9 in\u00fdmi \u017eivinami',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(486,'486. 0 prote\u00ednoch vo v\u00fd\u017eive \u010dloveka plat\u00ed, \u017ee:','s\u00fa zdrojom esenci\u00e1lnych aminokysel\u00edn','s\u00fa zdrojom neesenci\u00e1lnych aminokysel\u00edn','s\u00fa energeticky najbohat\u0161ou zlo\u017ekou potravy','ich zv\u00fd\u0161en\u00fa potrebu maj\u00fa deti a uzdravuj\u00faci sa','s\u00fa po natr\u00e1ven\u00ed zdrojom te\u00ednu','s\u00fa v tr\u00e1viacom trakte \u0161tiepen\u00e9 enz\u00fdmami na jednotliv\u00e9 at\u00f3my','biologicky najvhodnej\u0161ie s\u00fa bielkoviny \u017eivo\u010d\u00ed\u0161neho p\u00f4vodu','s\u00fa bohat\u00fdm zdrojom vitam\u00ednov a miner\u00e1lov',TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(487,'487. Procesy, pri ktor\u00fdch sa uvo\u013e\u0148uje energia:','s\u00fa anabolick\u00e9','s\u00fa katabolick\u00e9','sa ozna\u010duj\u00fa ako baz\u00e1lny metabolizmus','prebiehaj\u00fa pri \u0161tiepen\u00ed zlo\u017eit\u00fdch l\u00e1tok na jednoduch\u0161ie','sa ozna\u010duj\u00fa ako katalyz\u00e1tory','s\u00fa katabolick\u00e9, pri\u010dom organizmus m\u00f4\u017ee energiu priamo zu\u017eitkova\u0165','s\u00fa katabolick\u00e9, pri\u010dom organizmus nem\u00f4\u017ee energiu priamo z\u00fa\u017eitkova\u0165','s\u00fa katabolick\u00e9, pri\u010dom uvo\u013enen\u00e1 energia sa uklad\u00e1 do makroergick\u00fdch fosf\u00e1tov\u00fdch v\u00e4zieb',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(488,'488. Tuky z potravy \u010dloveka s\u00fa:','tr\u00e1ven\u00e9 najm\u00e4 v \u017eal\u00fadku','najv\u00fddatnej\u0161\u00edm zdrojom energie','slab\u00fdm zdrojom energie','najstr\u00e1vite\u013enej\u0161ou \u017eivinou','tr\u00e1ven\u00e9 v tenkom \u010dreve','d\u00f4le\u017eit\u00e9 pre pr\u00edjem niektor\u00fdch vitam\u00ednov','postr\u00e1date\u013en\u00e9, tj. nemusia by\u0165 v strave nikdy pr\u00edtomn\u00e9','zaraden\u00e9 k makro\u017eivin\u00e1m',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(489,'489. Tuky s\u00fa:','\u0161tiepen\u00e9 lip\u00e1zou na glycerol a mastn\u00e9 kyseliny','emulgovan\u00e9 lip\u00e1zou na drobn\u00e9 kvap\u00f3\u010dky','\u0161tiepen\u00e9 amyl\u00e1zou na glycerol a mastn\u00e9 kyseliny','vstreb\u00e1van\u00e9 v tenkom \u010dreve ako glycerol a mastn\u00e9 kyseliny','vstreb\u00e1van\u00e9 v hrubom \u010dreve ako glycerol a mastn\u00e9 kyseliny','\u0161tiepen\u00e9 lip\u00e1zou v pod\u017eal\u00fadkovej \u017e\u013eaze','\u0161tiepen\u00e9 tryps\u00ednom v tenkom \u010dreve','najd\u00f4le\u017eitej\u0161\u00edm okam\u017eit\u00fdm zdrojom energie pre \u010dloveka',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(490,'490. Optim\u00e1lny denn\u00fd pr\u00edvod bielkov\u00edn vo v\u00fd\u017eive zdrav\u00e9ho dospel\u00e9ho \u010dloveka je pribli\u017ene:','5 g/kg telesnej hmotnosti','0,5g/kg telesnej hmotnosti','1 g/kg telesnej hmotnosti','15 g/kg telesnej hmotnosti','8 g/kg telesnej hmotnosti','100 g/kg telesnej hmotnosti','10 g/10kg telesnej hmotnosti','7 g/kg telesnej hmotnosti',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(491,'491. Pri \u00faplnom hladovan\u00ed u \u010dloveka:','sa vy\u010derp\u00e1vaj\u00fa telov\u00e9 z\u00e1soby glykog\u00e9nu','sa vy\u010derp\u00e1vaj\u00fa telov\u00e9 z\u00e1soby \u0161krobu a tukov','sa najprv vy\u010derp\u00e1va pohotovostn\u00e1 z\u00e1soba glykog\u00e9nu','po spotrebovan\u00ed glykog\u00e9nu organizmus siaha na tuky a bielkoviny','ub\u00fada aj z hmotnosti niektor\u00fdch org\u00e1nov','s\u00fa v tele metabolicky najviac \u0161etren\u00e9 bielkoviny','sa energetick\u00e1 spotreba plne hrad\u00ed z bielkov\u00edn a\u017e pred smr\u0165ou','sa m\u00f4\u017ee spotrebova\u0165 takmer v\u0161etok telesn\u00fd tuk',TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(492,'492. Nedostatok ktor\u00e9ho vitam\u00ednu nebol doposia\u013e op\u00edsan\u00fd u \u010dloveka:','B 5','ergokalciferolu','kyseliny folovej','B9','kyseliny pantot\u00e9novej','K','fylochinonu','kyseliny nikot\u00ednovej',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(493,'493. Medzi nevyhnutn\u00e9 stopov\u00e9 prvky patr\u00ed:','kadmium','kobalt','sel\u00e9n','v\u00e1pnik','chl\u00f3r','fosfor','kobalam\u00edn','hor\u010d\u00edk',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(494,'494. Ktor\u00e9 tvrdenie z oblasti biol\u00f3gie \u010dloveka je spr\u00e1vne:','pri zhubnej m\u00e1lokrvnosti b\u00fdva nedostatok vitam\u00ednu B_12','riboflav\u00edn je najd\u00f4le\u017eitej\u0161\u00ed pre metabolizmus cukrov','vitam\u00edn PP zabra\u0148uje vzniku pernici\u00f3znej an\u00e9mie','retinol je nevyhnutn\u00fd pre tvorbu rodops\u00ednu','kobalam\u00edn podporuje vstreb\u00e1vanie v\u00e1pnika v tenkom \u010dreve','vitam\u00edn A m\u00f4\u017ee vznika\u0165 aj v tele \u010dloveka z provitam\u00ednov','vitam\u00edn D m\u00f4\u017ee vznika\u0165 aj v tele \u010dloveka z provitam\u00ednu','vitam\u00edn K si organizmus \u010dloveka nedok\u00e1\u017ee s\u00e1m syntetizova\u0165',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(495,'495. Stav pri nedostato\u010dnom pr\u00edvode vitam\u00ednov sa v\u0161eobecne naz\u00fdva:','avitamin\u00f3za','hypervitamin\u00f3za','hypovitamin\u00f3za','skorbut','pernici\u00f3zna an\u00e9mia','hypotenzia','anorexia','an\u00e9mia',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(496,'496. Ak\u00fa funkciu m\u00e1 kyselina askorbov\u00e1 v \u013eudskom tele:','je d\u00f4le\u017eit\u00e1 pre celkov\u00fd metabolizmus aminolcysel\u00edn','je potrebn\u00e1 na vstreb\u00e1vanie \u017eeleza','je d\u00f4le\u017eit\u00e1 na tvorbu horm\u00f3nov obli\u010diek','pom\u00e1ha regenerova\u0165 spojivov\u00e9 tkaniv\u00e1','je d\u00f4le\u017eit\u00e1 na tvorbu horm\u00f3nov nadobli\u010diek','zabra\u0148uje vzniku skorbutu','podiera sa na hojen\u00ed r\u00e1n','prispieva k celkovej odolnosti vo\u010di infek\u010dn\u00fdm ochoreniam',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(497,'497. Vitam\u00edn A u \u010dloveka je:','organizmus schopn\u00fd z\u00edska\u0165 z betakarot\u00e9nu','nevyhnutn\u00fd pre metabolizmus sacharidov','nevyhnutn\u00fd pre tvorbu \u010derven\u00fdch krviniek','nevyhnutn\u00fd pre norm\u00e1lny stav a funkciu epitelov\u00fdch buniek','nevyhnutn\u00fd pre tvorbu a udr\u017eiavanie v\u00e4ziva','nevyhnutn\u00fd pre synt\u00e9zu o\u010dn\u00e9ho purpuru (rodops\u00ednu)','postr\u00e1date\u013en\u00fd, \u010dlovek si ho m\u00f4\u017ee tvori\u0165 aj s\u00e1m, ak m\u00e1 vhodn\u00fa stravu','z\u00edskavan\u00fd z rastlinnej i \u017eivo\u010d\u00ed\u0161nej stravy',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(498,'498. Vitam\u00edn B_2 je:','aneur\u00edn','rozpustn\u00fd v tukoch','riboflav\u00edn','s\u00fa\u010das\u0165ou enz\u00fdmov podmie\u0148uj\u00facich bunkov\u00e9 oxid\u00e1cie','nevyhnutn\u00fd pre tvorbu v\u00e4ziva a regener\u00e1ciu kost\u00ed','tiam\u00edn','potrebn\u00fd pre tvorbu faktorov krvn\u00e9ho zr\u00e1\u017eania','pr\u00edtomn\u00fd v \u010dervenom ovoc\u00ed a zelenine',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(499,'499. Smrte\u013en\u00e9 ochorenie skorbut je sp\u00f4soben\u00e9 avitamin\u00f3zou vitam\u00ednu:','A','C','D','E','C, ktor\u00fd sa m\u00f4\u017ee uklada\u0165 do z\u00e1soby v organizme \u010dloveka','C a prejavuje sa krv\u00e1can\u00edm pod ko\u017eou, do k\u013abov, krv\u00e1can\u00edm \u010fasien','A a prejavuje sa \u0161eroslepotou a rohovaten\u00edm epitelov','C a prejavuje sa poruchami ko\u017ee, tr\u00e1viaceho a nervov\u00e9ho syst\u00e9mu',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(500,'500. Avitamin\u00f3za:','vitam\u00ednu PP sp\u00f3sobuje pelagru','vitam\u00ednu B_2 sp\u00f4sobuje chorobu beri-beri','kyseliny askorbovej sp\u00f4sobuje skorbut','B_9 sp\u00f4sobuje pernici\u00f3znu an\u00e9miu','vitam\u00ednu B_12 sp\u00f4sobuje zhubn\u00fa m\u00e1lokrvnos\u0165','tokoferolu sp\u00f4sobuje pelagru','Vitam\u00ednu B_1 sp\u00f4sobuje chorobu beri-beri','znamen\u00e1 prechodne zn\u00ed\u017een\u00fd prijem vitam\u00ednov',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(501,'501. Ktor\u00fd z komplexu vitam\u00ednov B je d\u00f4le\u017eit\u00fd pre metabolizmus bielkov\u00edn:','thiam\u00edn','B_2','B_5','B_9','aneur\u00edn','B_6','pyridox\u00edn','kyselina folov\u00e1',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(502,'502. Rohovatenie epitelov, \u0161eroslepotu a\u017e slepotu sp\u00f4sobuje nedostatok vitam\u00ednu:','A','retinolu','laktoflavinu','pyridox\u00ednu','C','D','aneur\u00ednu','niac\u00ednu',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(503,'503. Skupina vitam\u00ednov D_1\u2014 D_7 u \u010dloveka:','podporuje vstreb\u00e1vanie v\u00e1pnika a fosforu v tenkom \u010dreve','podporuje najm\u00e4 tvorbu v\u00e4ziva a regener\u00e1ciu kostn\u00e9ho tkaniva','ovplyv\u0148uje ukladanie v\u00e1pnika v kostnom tkanive','priamo sp\u00f4sobuje zr\u00e1\u017eanie krvi i ukladanie v\u00e1pnika v kostnom tkanive','je rozpustn\u00e1 vo vode','je prij\u00edman\u00e1 najm\u00e4 zo \u017eivo\u010d\u00ed\u0161nej potravy','za ist\u00fdch okolnost\u00ed sa m\u00f4\u017ee v tele prirodzene tvori\u0165','vo vysok\u00fdch d\u00e1vkach m\u00f4\u017ee sp\u00f4sobi\u0165 hypovitamin\u00f3zu',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(504,'504. M\u00e4knutie a rednutie kost\u00ed dospel\u00fdch, krivica u det\u00ed a celkov\u00fd nedostatok v\u00e1pnika s\u00fa n\u00e1sledkom nedostatku vitam\u00ednu:','biot\u00ednu','C','D','ktor\u00fd sa m\u00f4\u017ee v tele \u010dloveka vytv\u00e1ra\u0165 z betakarot\u00e9nu \u00fa\u010dinkom UV l\u00fa\u010dov','E','A, D, E, K','ktor\u00fd sa m\u00f4\u017ee v tele \u010dloveka vytv\u00e1ra\u0165 z provitam\u00ednu \u00fa\u010dinkom UV l\u00fa\u010dov','ergokalciferolu',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(505,'505. Pri nedostatku ktor\u00e9ho vitam\u00ednu vznik\u00e1 u \u010dloveka zhubn\u00e1 m\u00e1lokrynos\u0165:','B_12','niac\u00ednu','K','B_6','kobalam\u00ednu','C','Fe','pyridox\u00ednu',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(506,'506. Nedostatok vitam\u00ednu K sp\u00f4sobuje u \u013eud\u00ed:','krv\u00e1cavos\u0165','m\u00e1lokrvnos\u0165 a nervov\u00e9 poruchy','poruchy zr\u00e1\u017eania krvi','zn\u00ed\u017een\u00fa odolnos\u0165 proti infekci\u00e1m','zr\u00e1\u017eanie krvi pri z\u00e1stave krv\u00e1cania','zn\u00ed\u017een\u00fa odolnos\u0165 proti stresu','krv\u00e1canie z \u010fasien','osteomal\u00e1ciu',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(507,'507. Medzi vitam\u00edny rozpustn\u00e9 vo vode patria:','B, C, A, D','B_1, B_2, PP, B_6, C','A, D, E ,K','niac\u00edn a pyridox\u00edn','retinol, fylochinon a tokoferol','v\u0161etk\u00fd okrem A, D, E a K','v\u0161etky vitam\u00edny skupiny B','retinol, kyselina askorbov\u00e1 a v\u0161etky vitam\u00edny skupiny B',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(508,'508. Medzi vitam\u00edny rozpustn\u00e9 v tukoch patr\u00ed:','kobalam\u00edn','retinol','tokoferol','ergokalciferol','kyselina folov\u00e1','biot\u00edn','kyselina askorbov\u00e1','aneur\u00edn',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(509,'509. Nedostatok ktor\u00e9ho vitam\u00ednu zapr\u00ed\u010di\u0148uje u \u010dloveka smrte\u013en\u00fa chorobu beri-beri:','B_1','B_2','riboflav\u00ednu','thiam\u00ednu','biot\u00ednu','B_6','pyridox\u00ednu','aneur\u00ednu',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(510,'510. Typick\u00fdm pr\u00edznakom nedostatku vitam\u00ednu B_2 u \u010dloveka:','je chudokrvnos\u0165','je skorbut','s\u00fa zmeny na ko\u017ei a slizniciach','s\u00fa nervov\u00e9 poruchy','nie je beri-beri','s\u00fa z\u00e1paly spojoviek','je osteomal\u00e1cia','je pernici\u00f3zna an\u00e9mia',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(511,'511. Ktor\u00fd vitam\u00edn podporuje \u010dinnos\u0165 pohlavn\u00fdch \u017eliaz:','A','C','D','E','tokoferol','kalciferol','betakarot\u00e9n','H',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(512,'512. Z\u00e1sobn\u00fdm pohotovostn\u00fdm zdrojom gluk\u00f3zy v organizme \u010dloveka je:','sachar\u00f3za','galalct\u00f3za','frukt\u00f3za','glykog\u00e9n','glycerol','lakt\u00f3za','\u0161krob','glyc\u00edn',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(513,'513. Pri cukrovke mo\u017eno v mo\u010di zisti\u0165 najm\u00e4 nadmern\u00fa pr\u00edtomnos\u0165:','inzul\u00ednu','sachar\u00f3zy','frukt\u00f3zy','glykog\u00e9nu','bielkov\u00edn','gluk\u00f3zy','krvi','acetaldehydu',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(514,'514. Pe\u010de\u0148 \u010dloveka:','je miestom produkcie \u017el\u010de','je miestom synt\u00e9zy glykog\u00e9nu','je ulo\u017een\u00e1 pod br\u00e1nicou','m\u00e1 aj endokrinn\u00fa funkciu','le\u017e\u00ed pod \u017eal\u00fadkom v ohybe dvan\u00e1stnika','je miesto tr\u00e1venia a vstreb\u00e1vanie \u017eiv\u00edn','m\u00e1 v\u00fdznam pri detoxik\u00e1cii \u0161kodliv\u00fdch l\u00e1tok z potravy','m\u00e1 v\u00fdznam pri spracovan\u00ed l\u00e1tok vstreban\u00fdch v tenkom \u010dreve',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(515,'515. Pe\u010de\u0148 \u010dloveka:','je najv\u00e4\u010d\u0161ou endokrinnou \u017e\u013eazou v tele','je najv\u00e4\u010d\u0161ou exokrinnou \u017e\u013eazou v tele','m\u00e1 priemern\u00fa hmotnos\u0165 3 kg','je ulo\u017een\u00e1 pod br\u00e1nicou na \u013eavej strane bru\u0161nej dutiny','je miestom tvorby mo\u010doviny','je ulo\u017een\u00e1 pod br\u00e1nicou na pravej strane bru\u0161nej dutiny','je ulo\u017een\u00e1 pod \u017eal\u00fadkom na pravej strane bru\u0161nej dutiny','je z\u00e1sob\u00e1r\u0148ou cukrov vo forme glykog\u00e9nu',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(516,'516. Vr\u00e1tnikov\u00fd zviera\u010d u \u010dloveka sa nach\u00e1dza:','medzi pa\u017eer\u00e1kom a \u017eal\u00fadkom','medzi \u017eal\u00fadkom a dvan\u00e1stnikom','medzi \u017eal\u00fadkom a tenk\u00fdm \u010drevom','medzi \u017eal\u00fadkom a hrub\u00fdm \u010drevom','na vy\u00fasten\u00ed \u017el\u010dovodu','na vy\u00fasten\u00ed v\u00fdvodu pankreasu','medzi \u017eal\u00fadkom a bedrovn\u00edkom','na vy\u00fasten\u00ed hrub\u00e9ho \u010dreva',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(517,'517. Obli\u010dky \u010dloveka s\u00fa umiestnen\u00e9:','v oblasti driekovej \u010dasti chrbtice','ako nep\u00e1rov\u00fd org\u00e1n fazu\u013eovit\u00e9ho tvaru po stran\u00e1ch driekovej chrbtice','na zadnej stene bru\u0161nej dutiny vo v\u00fd\u0161ke driekov\u00fdch stavcov','v oblasti ve\u013ekej panvy','v oblasti malej panvy','ako p\u00e1rov\u00fd org\u00e1n fazu\u013eovit\u00e9ho tvaru po stran\u00e1ch driekovej chrbtice','ako p\u00e1rov\u00fd org\u00e1n fazu\u013eovit\u00e9ho tvaru po stran\u00e1ch kr\u00ed\u017eovej chrbtice','po stran\u00e1ch hrudn\u00edkovej chrbtice na zadnej stene bru\u0161nej dutiny',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(518,'518. Obli\u010dky \u010dloveka produkuj\u00fa:','mo\u010d','mo\u010dovinu','adiuret\u00edn','ren\u00edn','erytropoet\u00edn','mo\u010d, ktor\u00fd obsahuje vodu a v\u0161etky rozpustn\u00e9 zlo\u017eky v rovnakej koncentr\u00e1cii ako s\u00fa v krvnej plazme','mo\u010d, ktor\u00fd obsahuje vodu, mo\u010dovinu, mal\u00e9 mno\u017estvo organick\u00fdch l\u00e1tok a anorganick\u00fdch l\u00e1tok','ren\u00edn, ktor\u00fd ovplyv\u0148uje tvorbu krvi',TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(519,'519. Ktor\u00e9 z uveden\u00fdch l\u00e1tok nem\u00e1 obsahova\u0165 mo\u010d zdrav\u00e9ho \u010dloveka:','gluk\u00f3zu vo vy\u0161\u0161ej koncentr\u00e1cii','hnis','kreatin\u00edn','kyselinu mo\u010dov\u00fa','erytrocyty','leukocyty','mo\u010dovinu','NaCl, K^+, Na^+, Mg^2+, Ca^2+',TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(520,'520. S\u00fa\u010dast ou nefr\u00f3nu je:','pr\u00edvodn\u00e1 tepni\u010dka','odvodn\u00e1 tepni\u010dka','glomerulus','Bowmanovo puzdro','zbern\u00fd kan\u00e1lik','obli\u010dkov\u00e1 panvi\u010dka','obli\u010dkov\u00fd kalich','sto\u010den\u00fd kan\u00e1lik II.r\u00e1du',TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(521,'521. Nefr\u00f3n vy\u00fas\u0165uje do:','Malpighiho telieska','zbern\u00e9ho kan\u00e1lika','mo\u010dovodu','mo\u010dov\u00e9ho mech\u00fara','sto\u010den\u00e9ho kan\u00e1lika I.r\u00e1du','sto\u010den\u00e9ho kan\u00e1lika II.r\u00e1du','zbern\u00e9ho kan\u00e1lika I.r\u00e1du','zbern\u00e9ho kan\u00e1lika II.r\u00e1du',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(522,'522. Mo\u010dovina sa v \u013eudskom organizme tvor\u00ed v:','obli\u010dk\u00e1ch a vylu\u010duje sa z tela von','pe\u010deni','obli\u010dk\u00e1ch a pe\u010deni','obli\u010dk\u00e1ch filtr\u00e1ciou krvi','nefr\u00f3noch a vylu\u010duje sa obli\u010dkami z tela von','pe\u010deni a vylu\u010duje sa v podobe \u017el\u010de','glomeruloch','pe\u010deni pri odb\u00faravan\u00ed star\u00fdch erytrocytov',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(523,'523. Filtr\u00e1t vznikaj\u00faci v glomerule \u013eudskej obli\u010dky:','sa naz\u00fdva prim\u00e1rny mo\u010d','sa naz\u00fdva definit\u00edvny mo\u010d','je identick\u00fd s \u013eudskou plazmou','je identick\u00fd s \u013eudskou plazmou bez krvn\u00fdch aminokysel\u00edn','je identick\u00fd s \u013eudskou plazmou bez krvn\u00fdch bielkov\u00edn','obsahuje e\u0161te krv a stopy bielkov\u00edn','sa prechodom cez obli\u010dkov\u00e9 kan\u00e1liky men\u00ed na prim\u00e1rny mo\u010d','sa prechodom cez obli\u010dkov\u00e9 kan\u00e1liky men\u00ed na definit\u00edvny mo\u010d',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(524,'524. Prim\u00e1rny mo\u010d u \u010dloveka:','vznik\u00e1 filtr\u00e1ciou v zbern\u00fdch kan\u00e1likoch','vznik\u00e1 filtr\u00e1ciou v Malpighiho teliesku','vznik\u00e1 v procese sp\u00e4tnej resorpcie a exkr\u00e9cie v obli\u010dkov\u00fdch kan\u00e1likoch','sa vytv\u00e1ra v mno\u017estve 1,5 litra za 24 hod\u00edn','sa vytv\u00e1ra v mno\u017estve 170 l za hodinu','sa vytv\u00e1ra v mno\u017estve 170 l za 24 hod\u00edn','vznik\u00e1 filtr\u00e1ciou v mo\u010dov\u00fdch cest\u00e1ch','je filtr\u00e1t krvi pretekaj\u00facej vl\u00e1so\u010dnicami glomerulu',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(525,'525. S\u00fa\u010das\u0165ou Malpighiho telieska je:','obli\u010dkov\u00e9 teliesko','glomerulus','Bowmanovo puzdro','sto\u010den\u00fd kan\u00e1lik I. r\u00e1du','sto\u010den\u00fd kan\u00e1lik II. r\u00e1du','Hesova slu\u010dka','Henleho slu\u010dka','klbko krvn\u00fdch vl\u00e1so\u010dn\u00edc',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(526,'526. Hnacou silou filtr\u00e1cie v obli\u010dk\u00e1ch \u010dloveka je:','osmotick\u00fd tlak','tlak krvi','aktivita steny glomerulov','vinenie pr\u00edvodn\u00fdch tepien','peristaltick\u00e9 vlny','gravit\u00e1cia','podtlak vyv\u00edjan\u00fd obli\u010dkou','atmosf\u00e9rick\u00fd tlak',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(527,'527. Pri prechode kan\u00e1likmi v obli\u010dk\u00e1ch \u010dloveka:','sa filtruje do prim\u00e1rneho mo\u010du voda, soli, gluk\u00f3za, aminokyseliny a vitam\u00edn C','sa vstreb\u00e1vaj\u00fa z prim\u00e1rneho mo\u010du sp\u00e4\u0165 do krvi bielkoviny, \u010derven\u00e9 a biele krvinky','sa filtruj\u00fa do prim\u00e1rneho mo\u010du odpadov\u00e9 l\u00e1tky','sa vstreb\u00e1va z prim\u00e1rneho mo\u010du zna\u010dn\u00e9 mno\u017estvo vody, sol\u00ed, gluk\u00f3zy, aminokysel\u00edn sp\u00e4\u0165 do krvi','z mo\u010du norm\u00e1lne celkom vymizne gluk\u00f3za a odpadov\u00e9 l\u00e1tky sa koncentruj\u00fa','sa mno\u017estvo prech\u00e1dzaj\u00facej tekutiny mnohon\u00e1sobne zmen\u0161\u00ed','st\u00fapa v prech\u00e1dzaj\u00facej tekutine koncentr\u00e1cia mo\u010doviny','st\u00fapa v prech\u00e1dzaj\u00facej tekutine koncentr\u00e1cia kyseliny mo\u010dovej, kreatin\u00ednu a amoniaku',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(528,'528. Tekutina, ktor\u00e1 sa do Bowmanovho puzdra prefiltruje cez steny vl\u00e1so\u010dn\u00edc, sa naz\u00fdva:','prim\u00e1rny mo\u010d','sekund\u00e1rny mo\u010d','glomerul\u00e1rny filtr\u00e1t','prim\u00e1rny mo\u010d, ktor\u00fd obsahuje soli, kyselinu mo\u010dov\u00fa, mo\u010dovinu, gluk\u00f3zu','prim\u00e1rny mo\u010d, ktor\u00fd obsahuje vysokomolekulov\u00e9 l\u00e1tky','definit\u00edvny mo\u010d','prim\u00e1rny mo\u010d, ktor\u00fd obsahuje soli, kyselinu mo\u010dov\u00fa, mo\u010dovinu, bielkoviny, krvn\u00e9 bunky','kan\u00e1likov\u00fd exkr\u00e9t',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(529,'529. Pri prechode prim\u00e1rneho mo\u010du cez sto\u010den\u00e9 kan\u00e1liky a Henleho slu\u010dku:','sa vylu\u010duj\u00fa do mo\u010du niektor\u00e9 odpadov\u00e9 l\u00e1tky','sa sp\u00e4tne vstreb\u00e1va \u010das\u0165 vody','sa sp\u00e4tne vstreb\u00e1vaj\u00fa odpadov\u00e9 l\u00e1tky','sa sp\u00e4tne vstreb\u00e1va \u010das\u0165 sol\u00ed a l\u00e1tok d\u00f4le\u017eit\u00fdch pre telo','prech\u00e1dzaj\u00fa do krvi odpadov\u00e9 l\u00e1tky cez steny kan\u00e1likov','sa filtruje krv','sa filtruje prim\u00e1rny mo\u010d','sa v\u00e4\u010d\u0161ina l\u00e1tok ako aj vody vstreb\u00e1va v sto\u010denom kan\u00e1liku II. r\u00e1du',TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(530,'530. Vylu\u010dovacia s\u00fastava \u010dloveka m\u00e1 za \u00falohu odstr\u00e1ni\u0165 z tela:','nadbyto\u010dn\u00fa vodu','nestr\u00e1vite\u013en\u00e9 zvy\u0161ky potravy o','odpadov\u00e9 l\u00e1tky metabolizmu','prebyto\u010dn\u00e9 teplo','nadbyto\u010dn\u00e9 mno\u017estvo sol\u00ed','star\u00e9 a po\u0161koden\u00e9 krvn\u00e9 bunky','nadbyto\u010dn\u00e9 vysokomolekulov\u00e9 l\u00e1tky','dus\u00edkat\u00e9 splodiny metabolizmu bielkov\u00edn',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(531,'531. Obli\u010dky \u010dloveka:','maj\u00fa aj endokrinn\u00fa funkciu','maj\u00fa aj exokrinn\u00fa funkciu','produkuj\u00fa mo\u010dovinu','s\u00fa miestom tvorby mo\u010du','produkuj\u00fa a uvo\u013e\u0148uj\u00fa do krvi','produkuj\u00fa a uvo\u013e\u0148uj\u00fa do krvi erytropoet\u00edn','produkuj\u00fa a uvo\u013e\u0148uj\u00fa do krvi ren\u00edn, ktor\u00fd ovplyv\u0148uje tvorbu krvi','produkuj\u00fa a uvo\u013e\u0148uj\u00fa do krvi erytropoet\u00edn, ktor\u00fd ovplyv\u0148uje krvn\u00fd tlak',TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(532,'532. \u010cinnos\u0165 \u013eudsk\u00fdch obli\u010diek je riaden\u00e1:','horm\u00f3nom zadn\u00e9ho laloku hypof\u00fdzy ren\u00ednom','hormon\u00e1lne vazopres\u00ednom, ktor\u00fd reguluje sp\u00e4tn\u00fa resorpciu vody v obli\u010dkov\u00fdch kan\u00e1likoch','horm\u00f3nom adiuret\u00ednom vylu\u010dovan\u00fdm do krvi zo zadn\u00e9ho laloku hypof\u00fdzy','neurohumor\u00e1lne','nervovo prostredn\u00edctvom antidiuretick\u00e9ho horm\u00f3nu','nervovo a l\u00e1tkovo','hormon\u00e1lne erytropoet\u00ednom, ktor\u00fd reguluje sp\u00e4tn\u00fa resorpciu vody v obli\u010dkov\u00fdch kan\u00e1likoch','hormon\u00e1lne vazopres\u00ednom, ktor\u00fd reguluje mno\u017estvo prefiltrovanej krvi',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(533,'533. Z\u00e1kladnou stavebnou a funk\u010dnou jednotkou obli\u010dky je:','glomerulus','obli\u010dkov\u00e9 teliesko','glomerulus, Bowmanovo puzdro, pr\u00edvodn\u00e1 a odvodn\u00e1 tepni\u010dka','Malpighiho teliesko','k\u00f4ra na povrchu a dre\u0148 vo vn\u00fatri','nefr\u00f3n','Malpighiho \u017e\u013eaza','ureter',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(534,'534. Sto\u010den\u00e9 kan\u00e1liky I. a II. r\u00e1du s\u00fa spojen\u00e9:','glomerulom','Bowmanov\u00fdm puzdrom','Henleho slu\u010dkou v tvare p\u00edsmena U','Henleho slu\u010dkou v tvare klbka','pr\u00edvodnou tepni\u010dkou','zbern\u00fdm kan\u00e1likom','odvodnou tepni\u010dkou','obli\u010dkov\u00fdm kan\u00e1likom v tvare p\u00edsmena U',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(535,'535. V nefr\u00f3ne sa tvor\u00ed mo\u010d v procese:','filtr\u00e1cie prim\u00e1rneho mo\u010du','glomerul\u00e1rnej filtr\u00e1cie krvi','kan\u00e1likovej resorpcie','dial\u00fdzy','kan\u00e1likovej exkr\u00e9cie','exocyt\u00f3zy','transl\u00e1cie','diaped\u00e9zy',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(536,'536. V \u013eudskom mo\u010di sa koncentruj\u00fa:','bielkoviny','krvn\u00e9 bunky','gluk\u00f3za','kyselina mo\u010dov\u00e1','mo\u010dovina','kyselina mlie\u010dna','kreatin\u00edn','aminokyseliny',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(537,'537. Umel\u00e1 obli\u010dka je pr\u00edstroj:','ktor\u00fdm sa zr\u00fdch\u013euje proces filtr\u00e1cie krvi','na podporu \u010dinnosti obli\u010dky','na tvorbu umel\u00e9ho mo\u010da','ktor\u00fdm sa pre\u010dis\u0165uje krv pri obli\u010dkovej nedostato\u010dnosti','dialyzuj\u00faci krv','potrebn\u00fd na pre\u010distenie krvi pri tvorbe mo\u010dov\u00fdch kame\u0148ov','ktor\u00fdm sa zv\u00e4\u010d\u0161uje objem prefiltrovanej krvi','potrebn\u00fd pri zn\u00ed\u017eenej funkcii obli\u010diek',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(538,'538. Chondrocyty s\u00fa:','v\u00e4zivov\u00e9 bunky','chrupkov\u00e9 bunky','kostn\u00e9 bunky','bunky spojivov\u00e9ho tkaniva','kolag\u00e9nov\u00e9 bunky','v medzistavcov\u00fdch platni\u010dk\u00e1ch','v \u0161\u013each\u00e1ch','v klbov\u00fdch puzdr\u00e1ch',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(539,'539. Pribli\u017ene ko\u013eko kost\u00ed m\u00e1 kostra \u013eudsk\u00e9ho tela:','400','600','200','180','100','230','660','300',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(540,'540. Svaly, ktor\u00e9 sp\u00f4sobuj\u00fa odtiahnutie, sa naz\u00fdvaj\u00fa:','synergisty','antagonisty','vystiera\u010de','od\u0165ahova\u010de','abduktory','adduktory','extenzory','flexory',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(541,'541. Pod\u013ea tvaru rozozn\u00e1vame kosti:','hubovit\u00e9','riedke','ploch\u00e9','kr\u00e1tke','dlh\u00e9','zlo\u017een\u00e9','hust\u00e9','jednoduch\u00e9',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(542,'542. Synergisty s\u00fa:','svaly, ktor\u00e9 p\u00f4sobia antagonisticky','svaly, ktor\u00e9 spolupracuj\u00fa na jednom pohybe','napr. pri\u0165ahova\u010de a od\u0165ahova\u010de','napr. oh\u00fdba\u010de a vystiera\u010de','napr. vretenn\u00fd a lak\u0165ov\u00fd oh\u00fdba\u010d z\u00e1p\u00e4stia','napr. \u0161tvorhlav\u00fd sval stehna a dvojhlav\u00fd sval stehna','napr. dvojhlav\u00fd sval ramena a trojhlav\u00fd sval ramena','svaly, ktor\u00e9 p\u00f4sobia napr. ako oh\u00fdba\u010de v k\u013abe kolena \u2014 dvojhlav\u00fd sval stehna a trojhlav\u00fd sval l\u00fdtka',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(543,'543. Najmohutnej\u0161ia a najdlh\u0161ia kos\u0165 v \u013eudskom tele je:','panvov\u00e1','stehnov\u00e1','ramenn\u00e1','chrbtica','hrudn\u00e1','mozgov\u00e1','p\u00ed\u0161\u0165ala','lopatka',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(544,'544. Najv\u00e4\u010d\u0161\u00ed a najzlo\u017eitej\u0161\u00ed k\u013ab v kostre \u010dloveka je:','ramenn\u00fd','bedrov\u00fd','\u010dlenok','spojenie stehnovej kosti, ihlice a p\u013e\u0161\u0165aly','spojenie stehnovej kosti a panvy','spojenie stehnovej kosti, ihlice a jab\u013a\u010dka','k\u013ab kolena','spojenie stehnovej kosti, p\u00ed\u0161\u0165aly a jab\u013a\u010dka',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(545,'545. Vyber spr\u00e1vne tvrdenia o kostiach \u010dloveka:','prv\u00fd kr\u010dn\u00fd stavec sa naz\u00fdva \u010dapovec','mozgov\u00e1 \u010das\u0165 lebky je jedna kos\u0165','po vytvoren\u00ed kosti u\u017e kostn\u00e9 tkanivo ne\u017eije','kosti rast\u00fa do d\u013a\u017eky po cel\u00fd \u017eivot','povrch kost\u00ed tvor\u00ed spongi\u00f3za pokryt\u00e1 periostom','ihlica je s\u00fa\u010das\u0165ou lak\u0165ov\u00e9ho k\u013abu','do vn\u00fatra kosti cievy neprenikaj\u00fa','epif\u00fdza a hypof\u00fdza s\u00fa \u010das\u0165ami dlh\u00fdch kost\u00ed',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(546,'546. K\u013abov\u00e9 plochy bedrov\u00e9ho k\u013abu pokr\u00fdva chrupka:','v\u00e4zivov\u00e1','elastick\u00e1','sklovit\u00e1','hyal\u00ednna','kolag\u00e9nov\u00e1','tvoren\u00e1 osteocytmi','tvoren\u00e1 chondrocytmi','hyal\u00ednna, ktor\u00e1 tvor\u00ed aj lonov\u00fa sponu',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(547,'547. Term\u00ednom epif\u00fdza ozna\u010dujeme:','jednu zo \u017eliaz s vn\u00fatornou sekr\u00e9ciou','hlavicu dlhej kosti','hlavicu hrudnej kosti','roz\u0161\u00edren\u00e9 konce dlh\u00fdch kost\u00ed','\u0161u\u0161kovit\u00e9 teliesko','stredn\u00fa r\u00farkovit\u00fa \u010das\u0165 dlh\u00fdch kost\u00ed','\u017e\u013eazu, ktor\u00e1 produkuje melan\u00edn','stredn\u00fd lalok hypof\u00fdzy',TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(548,'548. Okostica u \u013eud\u00ed pokr\u00fdva:','vn\u00fatraj\u0161ok k\u013abov','povrch hubovit\u00fdch kost\u00ed','vn\u00fatorn\u00fd povrch dut\u00fdch kost\u00ed','cel\u00fa kos\u0165 okrem k\u013abov\u00fdch koncov','vonkaj\u0161\u00ed povrch kost\u00ed','cel\u00fa kos\u0165 aj k\u013abov\u00e9 plochy','vn\u00fatorn\u00fa plochu hrudnej steny','periost',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(549,'549. Tenk\u00e1 v\u00e4zivov\u00e1 blana na povrchu kosti:','vy\u017eivuje a inervuje kos\u0165','obsahuje kos\u0165otvorn\u00e9 bunky','sa naz\u00fdva periost','umo\u017e\u0148uje rast kosti do hr\u00fabky','umo\u017e\u0148uje rast kosti do d\u013a\u017eky','sa naz\u00fdva okostica','kos\u0165ou prenik\u00e1 a\u017e do kostnej drene','sa naz\u00fdva ose\u00edn',TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(550,'550. Organick\u00e1 hmota \u013eudsk\u00fdch kost\u00ed:','sa naz\u00fdva ose\u00edn','s vekom \u010dloveka ub\u00fada','predstavuje asi 90 % hmoty kost\u00ed','je s\u00fastreden\u00e1 v kostnej dreni','zabezpe\u010duje pru\u017enos\u0165 kost\u00ed','predstavuje 25 \u2014 30 % medzibunkovej hmoty kosti v dospelosti','je ni\u017e\u0161ia u novorodenca v porovnan\u00ed s dospel\u00fdm \u010dlovekom','predstavuje asi 50 - 55% medzibunkovej hmoty kosti v dospelosti',TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(551,'551. Rastov\u00e1 chrupka \u013eudsk\u00fdch kost\u00ed:','umo\u017e\u0148uje hrubnutie kost\u00ed','sp\u00e1ja prav\u00e9 rebr\u00e1 navz\u00e1jom','umo\u017e\u0148uje pred\u013a\u017eovanie kost\u00ed','v dospelosti zanik\u00e1','je ulo\u017een\u00e1 medzi diaf\u00fdzou a epif\u00fdzou','je v\u00e4zivo vmedzeren\u00e9 medzi kostn\u00e9 tkanivo','je medzi hlavicami a telom dlh\u00fdch kost\u00ed v obdob\u00ed rastu jedinca','zabezpe\u010duje rast kosti do d\u013a\u017eky',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(552,'552. Chrupkou s\u00fa v tele \u010dloveka spojen\u00e9:','kr\u00ed\u017eov\u00e9 stavce','temenn\u00e1 a z\u00e1hlavn\u00e1 kos\u0165','ihlica a stehnov\u00e1 kos\u0165','z\u00e1hlavn\u00e1 kos\u0165 a chrbtica','lonov\u00e9 kosti','z\u00e1hlavn\u00e1 a klinov\u00e1 kos\u0165 do 18. roku','kostr\u010d','\u010dapovec s nosi\u010dom',FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(553,'553. Dutiny dlh\u00fdch \u013eudsk\u00fdch kost\u00ed:','s\u00fa vyplnen\u00e9 okosticou','s\u00fa vyplnen\u00e9 kostnou dre\u0148ou','m\u00f4\u017eu by\u0165 vyplnen\u00e9 tukom','s\u00fa vyplnen\u00e9 tr\u00e1m\u010dekmi','s\u00fa pr\u00e1zdne','s\u00fa vypinen\u00e9 kostnou tekutinou','s\u00fa obvykl\u00fdm miestom tvorby krvn\u00fdch buniek','s\u00fa vyplnen\u00e9 kostnou dre\u0148ou, ktorej konzistencia sa po\u010das ontogen\u00e9zy men\u00ed',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(554,'554. Hubovit\u00e9 kostn\u00e9 tkanivo sa u \u013eud\u00ed nach\u00e1dza:','tesne pod periostom ploch\u00fdch a kr\u00e1tkych kost\u00ed','v diaf\u00fdzach dlh\u00fdch kost\u00ed','najm\u00e4 v koncov\u00fdch \u010dastiach dlh\u00fdch kost\u00ed, ale i v ploch\u00fdch a kr\u00e1tkych kostiach','len v koncov\u00fdch \u010dastiach dlh\u00fdch kost\u00ed','hlavne v dlh\u00fdch a drobn\u00fdch kostiach','v dre\u0148ovej dutine dlh\u00fdch kost\u00ed','vo vn\u00fatri ploch\u00fdch a kr\u00e1tkych kost\u00ed a v hlaviciach dlh\u00fdch kost\u00ed','len v hrudnej kosti',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(555,'555. Na chrbtici \u010dloveka m\u00f4\u017eeme rozozna\u0165:','kr\u010dn\u00fa lord\u00f3zu, hrudn\u00edkov\u00fa kyf\u00f3zu, driekov\u00fa lord\u00f3zu a kr\u00ed\u017eov\u00fa kyf\u00f3zu','kr\u010dn\u00fa kyf\u00f3zu, hrudn\u00edkov\u00fa lord\u00f3zu, driekov\u00fa kyf\u00f3zu a kr\u00ed\u017eov\u00fa lord\u00f3zu','zakrivenia, ktor\u00e9 dod\u00e1vaj\u00fa chrbtici pru\u017enos\u0165','dvakr\u00e1t vyklenutie dopredu a dvalcr\u00e1t vyklenutie dozadu','zakrivenia dopredu a dozadu, ktor\u00e9 tlmia n\u00e1razy pri ch\u00f4dzi','kr\u010dn\u00fa a hrudn\u00edkov\u00fa lord\u00f3zu, driekov\u00fa a kr\u00ed\u017eov\u00fa kyf\u00f3zu','kr\u010dn\u00fa lord\u00f3zu, hrudn\u00edkov\u00fa kyf\u00f3zu, driekov\u00fa kyf\u00f3zu a kr\u00ed\u017eov\u00fa lord\u00f3zu','skoli\u00f3zu \u2014 mierne vybo\u010denie chrbtice do strany',TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(556,'556. Polohu lopatky pri r\u00f4znych pohyboch hornej kon\u010datiny zais\u0165uje:','k\u00fdva\u010d hlavy','trojhlav\u00fd sval ramena','lichobe\u017en\u00edkov\u00fd sval','naj\u0161ir\u0161\u00ed sval chrbta','deltov\u00fd sval','dvojhlav\u00fd sval ramena','obl\u00fd privraca\u010d','vretenn\u00fd sval',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(557,'557. Spojenie kost\u00ed v kostre \u010dloveka m\u00f4\u017ee by\u0165:','jednoduch\u00e9','naj\u010dastej\u0161ie pomocou okostice','pevn\u00e9 aj k\u013abov\u00e9','ploch\u00e9','aj pomocou v\u00e4ziva, napr. spojenie lonov\u00fdch kost\u00ed','pomocou kostn\u00e9ho tkaniva, napr. kr\u00ed\u017eov\u00e1 kos\u0165','klinov\u00e9','chrupkov\u00e9',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(558,'558. Pr\u00edkladom chrupkov\u00e9ho spojenia kost\u00ed je:','lonov\u00e1 spona','vz\u00e1jomn\u00e9 spojenie kr\u00ed\u017eov\u00fdch stavcov po 20. roku','spojenie z\u00e1hlavnej a klinovej kosti do 18. roku','kostr\u010d','spojenie medzistavcov\u00fdmi platni\u010dkami medzi nosi\u010dom a \u010dapovcom','spojenie panvovej kosti s kr\u00ed\u017eovou','spojenie ploch\u00fdch lebe\u010dn\u00fdch kost\u00ed','jednoduch\u00fd k\u013ab',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
