INSERT INTO BIOLOGIA VALUES(774,'774. Ozna\u010dte spr\u00e1vne tvrdenie t\u00fdkaj\u00face sa v\u00fdvinu spermi\u00ed \u010dloveka:','v\u00fdsledkom spermiogen\u00e9zy s\u00fa spermie s diploidn\u00fdm po\u010dtom chromoz\u00f3mov','v\u00fdsledkom spermiogen\u00e9zy s\u00fa spermie s haploidn\u00fdm po\u010dtom chromoz\u00f3mov','spr\u00e1vny priebeh spermiogen\u00e9zy je z\u00e1visl\u00fd od teploty v semenn\u00edkoch','spermie s\u00fa dlh\u00e9 90 um','spermatog\u00f3nie s\u00fa schopn\u00e9 akt\u00edvne sa pohybova\u0165','spermie sa tvoria nepretr\u017eite od narodenia a\u017e do konca \u017eivota mu\u017ea','spermie sa skladaj\u00fa zo \u017ealu\u010fa, strednej \u010dasti a bi\u010d\u00edka','spermatog\u00f3nie vznikaj\u00fa v semenotvornom epiteli semenn\u00e9ho mech\u00farika',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(775,'775. Vyberte spr\u00e1vne tvrdenie o mu\u017esk\u00fdch pohlavn\u00fdch org\u00e1noch:','nadsemen\u00edk je \u017eiaza produkuj\u00faca alkalick\u00fd sekr\u00e9t, ktor\u00fd ovplyv\u0148uje pohyb spermi\u00ed','semenovod je trubica dlh\u00e1 pribli\u017ene 50 cm','v stene semenovodu s\u00fa ulo\u017een\u00e9 z\u00e1rodo\u010dn\u00e9 pohlavn\u00e9 bunky','predstojnica je p\u00e1rov\u00fd org\u00e1n, ktor\u00fd v starobe \u010dasto hypertrofuje','erekcia nast\u00e1va nahromaden\u00edm krvi v dutink\u00e1ch dutinkat\u00fdch telies','\u00fastredie pre erekciu sa nach\u00e1dza v hrudn\u00edkovej \u010dasti miechy','vy\u017eivovaciu zlo\u017eku semennej tekutiny produkuje mie\u0161ok','semenn\u00fd mech\u00farik je dlh\u00fd 4 - 5 cm',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
