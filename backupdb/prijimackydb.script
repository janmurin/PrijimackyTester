INSERT INTO BIOLOGIA VALUES(615,'615. Lichobe\u017en\u00edkov\u00fd sval:','patr\u00ed k svalom hrudn\u00edka','dv\u00edha rebr\u00e1 a umo\u017e\u0148uje vdych','\u0165ah\u00e1 rebr\u00e1 a umo\u017e\u0148uje v\u00fddych','vytv\u00e1ra stenu bru\u0161nej dutiny','patr\u00ed k svalom chrbta','zais\u0165uje polohu lopatky pri pohyboch homej kon\u010datiny','patr\u00ed k povrchov\u00fdm svalom chrbta','patr\u00ed k hlbok\u00fdm \u2014 vlastn\u00fdm svalom chrbta',FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(616,'616. Medzi svaly chrbta u \u010dloveka zara\u010fujeme:','br\u00e1nicu','vonkaj\u0161ie medzirebrov\u00e9 svaly','najv\u00e4\u010d\u0161\u00ed sedac\u00ed sval','hlbok\u00e9 svaly, ktor\u00e9 udr\u017euj\u00fa vzpriamen\u00fa chrbticu','k\u00fdva\u010d hlavy','lichobe\u017en\u00edkov\u00fd sval','naj\u0161ir\u0161\u00ed sval chrbta, ktor\u00fd umo\u017e\u0148uje \u00faklon a rot\u00e1ciu chrbtice i hlavy','predn\u00fd p\u00edlovit\u00fd sval',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(617,'617. K svalom hrudn\u00edka patria:','vonkaj\u0161ie medzirebrov\u00e9 svaly, ktor\u00e9 umo\u017e\u0148uj\u00fa v\u00fddych','vn\u00fatorn\u00e9 medzirebrov\u00e9 svaly, ktor\u00e9 umo\u017e\u0148uj\u00fa vdych','vonkaj\u0161ie medzirebrov\u00e9 svaly, ktor\u00e9 umo\u017e\u0148uj\u00fa vdych','vn\u00fatorn\u00e9 medzirebrov\u00e9 svaly, ktor\u00e9 umo\u017e\u0148uj\u00fa v\u00fddych','br\u00e1nica','vonkaj\u0161\u00ed prsn\u00fd sval','deltov\u00fd sval','priamy sval brucha',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(618,'618. V krvi zdrav\u00e9ho \u010dloveka je:','pH 7,2 \u2014 7,8','u mu\u017ea v jednom litri priemerne menej hemoglob\u00ednu ako u \u017eeny','4,3 \u2014 5,3 x 10^6 erytrocytov v 1 ml krvi','viac erytrocytov ako leukocytov','4 \u2014 9 tis\u00edc leukocytov v jednom mikrolitri','150 \u2014 300 x 10^12 trombocytov v jednom litri','150 \u2014 300 x 10^9 trombocytov v jednom litri','menej erytrocytov ako leukocytov',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(619,'619. Krv v tele \u010dloveka rozv\u00e1dza:','lymfu','tkanivov\u00fd mok','ATP','HLA antig\u00e9ny','teplo','odpadov\u00e9 l\u00e1tky napr. mo\u010dovinu','horm\u00f3ny','d\u00fdchacie plyny',FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(620,'620. Krv \u010dloveka :','sa z\u00fa\u010dast\u0148uje termoregula\u010dn\u00fdch dejov','obsahuje \u017eiv\u00e9 bunky','pln\u00ed transportn\u00fa \u00falohu','sprostredk\u00fava prenos niektor\u00fdch inform\u00e1ci\u00ed a sign\u00e1lov','z\u00fa\u010dast\u0148uje sa imunitn\u00fdch pochodov','je \u010derven\u00e1 prieh\u013eadn\u00e1 hust\u00e1 tekutina','je nevyhnutn\u00e1 pre priebeh vylu\u010dovac\u00edch procesov','m\u00e1 ve\u013emi nest\u00e1le pH a osmotick\u00fd tlak',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(621,'621. Celkov\u00e9 mno\u017estvo krvi dospel\u00e9ho \u010dloveka je asi:','3 l, \u010do predstavuje 7 - 10 % celkovej hmotnosti tela','5 l, \u010do predstavuje 60 % celkov\u00e9ho mno\u017estva vody v organizme','5 1, \u010do predstavuje 76 % mimobunkovej vody','7 - 10 % celkovej hmotnosti tela','7 - 10 % celkovej hmotnosti tela, t. j. asi 5 l (pri hmotnosti cca 70 kg)','5 l, \u010do predstavuje 20 - 30 % celkovej hmotnosti tela','5 litrov (pri hmotnosti cca 70 kg)','2,8 \u2014 3,5 litra',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(622,'622. Vyberte spr\u00e1vne tvrdenia:','krvn\u00e1 plazma \u010dloveka obsahuje 91 - 92% vody','schopnos\u0165 diaped\u00e9zy a fagocyt\u00f3zy maj\u00fa neutrofiln\u00e9 granulocyty a monocyty','erytrocyty \u010dloveka maj\u00fa \u017eivotnos\u0165 120 dn\u00ed, obmenia sa asi trikr\u00e1t do roka','biele krvinky zanikaj\u00fa v pe\u010deni','krv a miazga sa u \u013eud\u00ed zmie\u0161avaj\u00fa v krvnom obehu','erytrocyty \u010dloveka akt\u00edvne prestupuj\u00fa cez stenu ciev a okysli\u010duj\u00fa tkaniv\u00e1','schopnos\u0165 diaped\u00e9zy a fagocyt\u00f3zy maj\u00fa eozinofiln\u00e9 granulocyty a lymfocyty T a B','erytrocyty sa v krvi u\u017e nedelia, dop\u013a\u0148aj\u00fa sa z kostnej drene',TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(623,'623. S\u00fa\u010das\u0165ou krvnej plazmy:','je mo\u010dovina','je hemoglob\u00edn','s\u00fa album\u00edny','je gluk\u00f3za','s\u00fa tuky','je voda','je methemoglob\u00edn','s\u00fa kati\u00f3ny Mg^2+, Ca^2+, K^+, Na^+',TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(624,'624. O album\u00ednoch v krvnej plazme \u010dloveka plat\u00ed, \u017ee:','s\u00fa pren\u00e1\u0161a\u010dmi horm\u00f3nov','via\u017eu vodu a t\u00fdm udr\u017eiavaj\u00fa st\u00e1ly objem krvi','udr\u017eiavaj\u00fa st\u00e1le pH krvi','podie\u013eaj\u00fa sa na \u0161pecifickej obrane organizmu','z\u00fa\u010dast\u0148uj\u00fa sa mechanizmu zr\u00e1\u017eania krvi','s\u00fa to imunoglobul\u00edny','s\u00fa produktom lymfocytov','vznikaj\u00fa v pe\u010deni',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(625,'625. O imunoglobul\u00ednoch v krvnej plazme \u010dloveka plat\u00ed, \u017ee:','s\u00fa produktom lymfocytov','vznikaj\u00fa v pe\u010deni','s\u00fa pren\u00e1\u0161a\u010dmi horm\u00f3nov','podie\u013eaj\u00fa sa na \u0161pecifickej obrane organinnu','podie\u013eaj\u00fa sa na ne\u0161pecifickej obrane organizmu','via\u017eu vodu a t\u00fdm udr\u017eiavaj\u00fa st\u00e1ly objem krvi','maj\u00fa schopnos\u0165 zne\u0161kod\u0148ova\u0165 antig\u00e9ny, ktor\u00e9 sa dostan\u00fa do organizmu','z\u00fa\u010dast\u0148uj\u00fa sa mechanizmu zr\u00e1\u017eania krvi',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(626,'626. Mechanizmu zr\u00e1\u017eania krvi sa z plazmatick\u00fdch bielkov\u00edn z\u00fa\u010dast\u0148uj\u00fa:','antig\u00e9ny','imunoglobul\u00edny','hemoglob\u00edn','fibrinog\u00e9n','vitam\u00edn K','i\u00f3ny Ca^2+','aglutin\u00edny','gama globul\u00edny',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(627,'627. Koncentr\u00e1cia gluk\u00f3zy v krvi \u010dloveka sa naz\u00fdva:','glyk\u00e9mia','sediment\u00e1c\u00eda','hematokrit','hemokoagul\u00e1cia','hemost\u00e1za','diabetes mellitus','an\u00e9mia','homeost\u00e1za',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(628,'628. Trval\u00e9 chorobn\u00e9 zv\u00fd\u0161enie hladiny gluk\u00f3zy v krvi:','naz\u00fdvame glyk\u00e9mia','naz\u00fdvame sediment\u00e1cia','je charakteristick\u00e9 pre cukrovku','je charakteristick\u00e9 pre diabetes mellitus','naz\u00fdvame an\u00e9mia','naz\u00fdvame hypertenzia','naz\u00fdvame aliment\u00e1rna hyperglyk\u00e9mia','naz\u00fdvame chronick\u00e1 hypoglyk\u00e9mia',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(629,'629. Podiel \u010derven\u00fdch krviniek v celkovom objeme krvi sa naz\u00fdva:','sediment\u00e1cia','glyk\u00e9mia','hematokrit','aglutin\u00e1cia','hemokoagul\u00e1cia','vazokonstrikcia','leuk\u00f3za','hypertenzia',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(630,'630. Priemern\u00e9 mo\u017estvo hemoglob\u00ednu v jednom litri krvi \u010dloveka je:','135 \u2014 170 g u mu\u017ea','120 \u2014 160 g u \u017eeny','4,3 \u20145,3 x 10^12 u mu\u017ea','3,8 \u2014 4,8 x 10^12 u \u017eeny','60 \u2014 80 g','13,5 \u2014 17,0 g','0,135 \u2014 0,170 kg umu\u017ea','0,120 - 0,160 kg u \u017eeny',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(631,'631. Priemerny po\u010det \u010derven\u00fdch krviniek dospel\u00e9ho mu\u017ea je:','4,3 \u2014 5,3 mil\u00f3nov v 1 mikrolitri krvi','4,3 \u2014 5,3 x 10^12/l krvi','3,8 \u2014 4,8 x 10^12/l krvi','4 \u2014 9 x 10^9/l krvi','135 \u2014 170 g/l krvi','150 \u2014 300 x 10^9/l krvi','4,3 \u2014 5,3 x 10^9/l krvi','18 \u2014 20 x 10^9/l krvi',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(632,'632. Zn\u00ed\u017een\u00fd po\u010det erytrocytov alebo krvn\u00e9ho farbiva v krvi \u010dloveka sa naz\u00fdva:','leukocyt\u00f3za','an\u00e9mia','leuk\u00f3za','m\u00e1lokrvnos\u0165','hemof\u00edlia','cyan\u00f3za','hypertenzia','leuk\u00e9mia',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(633,'633. M\u00e1lokrvnos\u0165 u \u010dloveka:','m\u00f4\u017ee znamena\u0165 zn\u00ed\u017eenie mno\u017estva hemoglob\u00ednu v krvi','m\u00f4\u017ee znamena\u0165 zn\u00ed\u017eenie po\u010dtu \u010derven\u00fdch krviniek v krvi','m\u00f4\u017ee by\u0165 d\u00f4sledkom nedostatku kobalam\u00ednu','m\u00f4\u017ee by\u0165 d\u00f4sledkom nedostatku kyseliny listovej','m\u00f4\u017ee by\u0165 d\u00f4sledkom nedostatku \u017eeleza v organizme','znamen\u00e1 zn\u00ed\u017eenie sediment\u00e1cie erytrocytov','znamen\u00e1 zv\u00fd\u0161enie sediment\u00e1cie erytrocytov','m\u00f4\u017ee by\u0165 d\u00f4sledkom nespr\u00e1vnej v\u00fd\u017eivy',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(634,'634. \u010cerven\u00e9 krvinky zdrav\u00e9ho dospel\u00e9ho \u010dloveka:','sa tvoria v slezine','opotrebovan\u00e9 a naru\u0161en\u00e9 s\u00fa fagocytovan\u00e9 leukocytmi v slezine','sa tvoria v kostnej dreni','opotrebovan\u00e9 a naru\u0161en\u00e9 s\u00fa fagocytovan\u00e9 leukocytmi v kostnej dreni','zabezpe\u010duj\u00fa vroden\u00e9 a z\u00edskan\u00e9 imunitn\u00e9 reakcie','maj\u00fa na povrchu nieko\u013eko desiatok aglutin\u00ednov','maj\u00fa na povrchu nieko\u013eko desiatok antig\u00e9nov','del\u00edme na granulocyty a agranulocyty',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(635,'635. Pre tvorbu \u013eudsk\u00fdch erytrocytov s\u00fa nevyhnutne a najviac potrebn\u00e9 tieto vitam\u00edny:','kyselina askorbov\u00e1','album\u00edny','B_12','kyselina folov\u00e1','kyselina listov\u00e1','\u017eelezo','aminokyseliny','kobalam\u00edn',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(636,'636. Ktor\u00fd prvok je s\u00fa\u010das\u0165ou molekuly hemoglob\u00ednu u \u010dloveka:','\u017eelezo','me\u010f','uhl\u00edk','vod\u00edk','dus\u00edk','kobalt','kysl\u00edk','s\u00edra',TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(637,'637. V\u00e4\u010d\u0161ina molekulov\u00e9ho kysl\u00edka v \u013eudskej krvi je:','fyzik\u00e1lne rozpusten\u00e1','viazan\u00e1 na at\u00f3my \u017eeleza krvn\u00e9ho farbiva','viazan\u00e1 na at\u00f3my \u017eeleza plazmatickej bielkoviny','viazan\u00e1 na glob\u00ednov\u00fa \u010das\u0165 hemoglob\u00ednu','viazan\u00e1 na dvojmocn\u00e9 \u017eelezo hemu','viazan\u00e1 na hemov\u00fa \u010das\u0165 hemoglob\u00ednu','viazan\u00e1 na trojmocn\u00e9 \u017eelezo hemu','viazan\u00e1 na plazmatick\u00e9 bielkoviny',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(638,'638. Po\u010det \u010derven\u00fdch krviniek \u010dloveka sa zvy\u0161uje pri:','z\u00e1palov\u00fdch ochoreniach','dlh\u0161om pobyte vo vysok\u00fdch nadmorsk\u00fdch v\u00fd\u0161kach','alergick\u00fdch a parazit\u00e1rnych ochoreniach','zv\u00fd\u0161enej diaped\u00e9ze \u010derven\u00fdch krviniek','dlhodobej\u0161om nedostatku kysl\u00edka','celkovo zv\u00fd\u0161enej \u010dinnosti kostnej drene','ni\u017e\u0161ej koncentr\u00e1cii 0_2 vo vzduchu','zv\u00fd\u0161enej sediment\u00e1cii \u010derven\u00fdch krviniek',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(639,'639. Sediment\u00e1cia erytrocytov v odobratej \u013eudskej krvi:','vyjadruje mno\u017estvo usaden\u00fdch erytrocytov','je ukazovate\u013eom r\u00fdchlosti usadenia leukocytov','je ukazovate\u013eom r\u00fdchlosti usadenia eryrocytov','sa zvy\u0161uje pri z\u00e1palov\u00fdch ochoreniach','je ukazovate\u013eom r\u00fdchlosti aglutin\u00e1cie erytrocytov','je pomerne st\u00e1la','u \u017eien je ni\u017e\u0161ia ako u mu\u017eov','je ukazovate\u013eom rozpadu \u010derven\u00fdch krviniek',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(640,'640. U \u010dloveka v\u00e4zbou kysl\u00edka na hemoglob\u00ednu vznik\u00e1:','oxyhemoglob\u00edn, pri\u010dom sa kysl\u00edk via\u017ee na trojmocn\u00e9 \u017eelezo hemu','oxytoc\u00edn','karboxyhemoglob\u00edn','karbonylhemoglob\u00edn','oxyhemoglob\u00edn, pri\u010dom sa kysl\u00edk via\u017ee na dvojmocn\u00e9 \u017eelezo hemu','methemoglob\u00edn','oxyhemoglob\u00edn, pri\u010dom sa kysl\u00edk via\u017ee na dvojmocn\u00e9 \u017eelezo glob\u00ednu','karbaminohemoglob\u00edn',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(641,'641. Hematokrit:','predstavuje podiel bielych krviniek v celkovom objeme krvi','predstavuje podiel \u010derven\u00fdch krviniek v celkovom objeme krvi','sa v\u00fdrazne zvy\u0161uje pri pobyte vo vysok\u00fdch nadmorsk\u00fdch v\u00fd\u0161kach','sa v\u00fdrazne zni\u017euje pri pobyte vo vysok\u00fdch nadmorsk\u00fdch v\u00fd\u0161kach','je kritick\u00e9 minim\u00e1lne mno\u017estvo krvi v obehu','vyjadruje pomer medzi objemom \u010derven\u00fdch krviniek a krvnej plazmy','je mno\u017estvo hemoglob\u00ednu v celkovom objeme krvi','sa m\u00f4\u017ee u \u0161portovcov zv\u00fd\u0161i\u0165 v d\u00f4sledku dopingu',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(642,'642. Geneticky podmienen\u00e1 porucha zr\u00e1\u017eania krvi sa naz\u00fdva:','an\u00e9mia','glyk\u00e9mia','leuk\u00e9mia','leuk\u00f3za','hypertenzia','aglutin\u00e1cia','transf\u00fazia','hemof\u00edlia',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(643,'643. Biele krvinky maj\u00fa d\u00f4le\u017eit\u00fa \u00falohu pri:','zastaven\u00ed krv\u00e1cania','prenose kysl\u00edka','obrane proti infekcii','transporte odpadov\u00fdch l\u00e1tok','transporte \u017eiv\u00edn','fagocyt\u00f3ze cudzorod\u00e9ho materi\u00e1lu','produkcii imunoglobul\u00ednov proti antig\u00e9nom','produkcii imunoglobul\u00ednov proti aglutin\u00ednom',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(644,'644. Leukocyty \u010dloveka:','maj\u00fa bikonk\u00e1vny tvar','sa rozli\u0161uj\u00fa pod\u013ea pr\u00edtomnosti gran\u00fal v cytoplazme na granulocyty a agranulocyty','zanikaj\u00fa v pe\u010deni','zanikaj\u00fa v slezine','zabezpe\u010duj\u00fa prenos horm\u00f3nov do cel\u00e9ho tela','sa rozli\u0161uj\u00fa pod\u013ea pritomnosti gran\u00fal v jadre na granulocyty a agranulocyty','tvoria d\u00f4le\u017eit\u00fa \u010das\u0165 krvnej zrazeniny','s\u00fa produkovan\u00e9 \u010dervenou kostnou dre\u0148ou',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(645,'645. Krvn\u00e9 do\u0161ti\u010dky \u010dloveka:','vznikaj\u00fa rozpadom bielych krviniek','vznikaj\u00fa rozpadom \u010derven\u00fdch krviniek','patria k bunkov\u00fdm krvn\u00fdm telieskam','patria k nebunkov\u00fdm krvn\u00fdm telieskam','maj\u00fa d\u00f4le\u017eit\u00fa \u00falohu pri hemost\u00e1ze','vznikaj\u00fa mitotick\u00fdm delen\u00edm v slezine a kostnej dreni','maj\u00fa d\u00f4le\u017eit\u00fa \u00falohu pri udr\u017eiavan\u00ed homeost\u00e1zy','produkciou rastov\u00e9ho faktora sa z\u00fa\u010dast\u0148uj\u00fa regener\u00e1cie v\u00fdstelky ciev',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(646,'646. \u010cerven\u00e9 krvinky pr\u00edtomn\u00e9 v krvi \u010dloveka:','s\u00fa schopn\u00e9 fagocyt\u00f3zy','s\u00fa bezjadrov\u00e9 krvn\u00e9 elementy','s\u00fa schopn\u00e9 akt\u00edvne prestupova\u0165 cez medzibunkov\u00e9 priestory v sten\u00e1ch kapil\u00e1r','pren\u00e1\u0161aj\u00fa kysl\u00edk a oxid uhli\u010dit\u00fd','obsahuj\u00fa hemoglob\u00edn, ktor\u00fd je schopn\u00fd ireverzibilne viaza\u0165 a uvo\u013e\u0148ova\u0165 kysl\u00edk a oxid uhli\u010dit\u00fd','s\u00fa jadrov\u00e9 krvn\u00e9 elementy','obsahuj\u00fa hemoglob\u00edn, ktor\u00fd je schopn\u00fd reverzibilne viaza\u0165 a uvo\u013e\u0148ova\u0165 kysl\u00edk a oxid uhli\u010dit\u00fd','s\u00fa nebunkov\u00e9 krvn\u00e9 telieska',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(647,'647. V\u00fdmena l\u00e1tok medzi krvou a tkanivov\u00fdm mokom u \u013eud\u00ed prebieha:','na za\u010diatku aj na konci \u017eilky','tak, \u017ee na konci vl\u00e1so\u010dnice prestupuje kysl\u00edk a \u017eiviny z krvi do medzibunkov\u00fdch priestorov a na za\u010diatku sa vracia do krvi tekutina obsahuj\u00faca splodiny l\u00e1tkovej premeny','migr\u00e1ciou bielych krviniek cez steny vl\u00e1so\u010dn\u00edc','migr\u00e1ciou \u010derven\u00fdch krviniek cez steny vl\u00e1so\u010dn\u00edc','na za\u010diatku aj na konci kapil\u00e1r','tak, \u017ee na za\u010diatku vl\u00e1so\u010dnice prestupuj\u00fa kysl\u00edk a \u017eiviny z krvi do medzibunkov\u00fdch priestorov a na konci sa vracia do krvi tekutina obsahuj\u00faca splodiny l\u00e1tkovej premeny','oboma smermi vzh\u013eadom na stenu pr\u00edslu\u0161nej cievy','diaped\u00e9zou',FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(648,'648. Zastavenie krv\u00e1cania sa ozna\u010duje term\u00ednom:','hemost\u00e1za','homeost\u00e1za','hemokoagul\u00e1cia','diaped\u00e9za','tromb\u00f3za','aglutin\u00e1cia \u010derven\u00fdch krviniek','vazokonstrikcia','aglutin\u00e1cia bielych krviniek',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(649,'649. Ktor\u00e9 organick\u00e9 l\u00e1tky s\u00fa celkovo hmotnostne najviac zast\u00fapen\u00e9 v krvnej plazme \u010dloveka?','gluk\u00f3za','bielkoviny','mo\u010dovina','cukry','aminokyseliny','voda','dus\u00edkat\u00e9 l\u00e1tky nebielkovinovej povahy','tuky',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(650,'650. Plazmatick\u00e9 bielkoviny \u010dloveka s\u00fa d\u00f4le\u017eit\u00e9 pre:','zr\u00e1\u017eanie krvi \u2014 album\u00edny','prenos kysl\u00edka a oxidu uhli\u010dit\u00e9ho','imunitn\u00e9 deje - fibrinog\u00e9n','regener\u00e1ciu v\u00fdstelky ciev','zr\u00e1\u017eanie krvi','transport niektor\u00fdch l\u00e1tok, napr. horm\u00f3nov','imunitn\u00e9 deje','regula\u010dn\u00e9 deje',FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE)
