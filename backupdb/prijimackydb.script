INSERT INTO BIOLOGIA VALUES(586,'586. O lak\u0165ovom k\u013abe plat\u00ed, \u017ee:','je najpohyblivej\u0161\u00edm k\u013abom v tele','sa v \u0148om sp\u00e1jaj\u00fa tri kosti','patr\u00ed k jednoduch\u00fdm Idbom','ramenn\u00e1 kos\u0165 sa v lak\u0165ovom k\u013abe kon\u010d\u00ed hlavicou','je to pohybliv\u00e9 spojenie','sa v \u0148om sp\u00e1jaj\u00fa ramenn\u00e1 kos\u0165, lak\u0165ov\u00e1 na strane mal\u00ed\u010dka a vretenn\u00e1 na strane palca','sa v \u0148om sp\u00e1jaj\u00fa ramenn\u00e1 kos\u0165, lak\u0165ov\u00e1 na strane palca a vretenn\u00e1 na strane mal\u00ed\u010dka','je najmenej pohybliv\u00fdm k\u013abom v tele \u010dloveka',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(587,'587. Kostra hornej kon\u010datiny \u010dloveka:','nesie hlavne hmotnos\u0165 tela, preto s\u00fa jej kosti ve\u013emi siln\u00e9','m\u00e1 in\u00fa z\u00e1kladn\u00fa stavbu ako kostra dolnej kon\u010datiny','je toto\u017en\u00e1 s kostrou dolnej kon\u010datiny','m\u00e1 ve\u013emi pohybliv\u00e9 k\u013aby','je prisp\u00f4soben\u00e1 na uchopovanie a pr\u00e1cu','sa sklad\u00e1 z pletenca a kostry vo\u013enej kon\u010datiny','m\u00e1 rovnak\u00fa z\u00e1kladn\u00fa stavbu ako kostra dolnej kon\u010datiny','zah\u0155\u0148a najmohutnej\u0161iu a najdlh\u0161iu kos\u0165 v \u013eudskom tele',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(588,'588. Skoli\u00f3za je ochorenie:','so zv\u00fd\u0161en\u00fdm rizikom vzniku zlomen\u00edn kost\u00ed','chrbtice','doln\u00fdch kon\u010dat\u00edn','horn\u00fdch kon\u010dat\u00edn','sp\u00f4soben\u00e9 n\u00edzkou hustotou kost\u00ed','ktor\u00e9 sa vyskytuje u \u017eien naj\u010dastej\u0161ie v menopauze','pri ktorom doch\u00e1dza k vybo\u010deniu chrbtice do strany','sp\u00f4soben\u00e9 morfologickou a funk\u010dnou asymetriou tela',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(589,'589. Kostrov\u00e9 svaly:','s\u00fa ovl\u00e1dan\u00e9 v\u00f4\u013eou','s\u00fa riaden\u00e9 mozgov\u00fdmi a miechov\u00fdmi nervami','nie s\u00fa ovl\u00e1dan\u00e9 v\u00f4\u013eou','s\u00fa ovl\u00e1dan\u00e9 auton\u00f3mnymi nervov\u00fdmi vl\u00e1knami','umo\u017e\u0148uj\u00fa peristaltick\u00e9 pohyby \u017eal\u00fadka','umo\u017e\u0148uj\u00fa peristaltick\u00e9 pohyby tenk\u00e9ho \u010dreva','s\u00fa tvoren\u00e9 mnohojadrov\u00fdmi svalov\u00fdmi vl\u00e1knami','sa vyzna\u010duj\u00fa dr\u00e1\u017edivos\u0165ou, kontraktilitou a pru\u017enos\u0165ou',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
