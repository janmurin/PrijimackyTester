INSERT INTO BIOLOGIA VALUES(929,'929. Pri synt\u00e9ze RNA v bunke prebiehaj\u00fa tieto deje:','RNA-polymer\u00e1za katalyzuje sp\u00e1janie nukleotidov obsahuj\u00facich A,C,G,T pod\u013ea matrice DNA','DNA-polymer\u00e1za katalyzuje sp\u00e1janie nukleotidov obsahuj\u00facich A, U, G, C pod\u013ea matrice DNA','RNA-polymer\u00e1za katalyzuje sp\u00e1janie nukleotidov obsahuj\u00facich A, C, G, U pod\u013ea matrice DNA','RNA-polymer\u00e1za katalyzuje sp\u00e1janie nukleotidov obsahuj\u00facich A, C, G, U pod\u013ea matrice DNA, potrebn\u00fa energiu dod\u00e1va ADP','RNA-polymer\u00e1za katalyzuje sp\u00e1janie nukleotidov obsahuj\u00facich A, C, G, U pod\u013ea matrice DNA, potrebn\u00fa energiu dod\u00e1va AMP','RNA-polymer\u00e1za katalyzuje synt\u00e9zu RNA v M f\u00e1ze bunkov\u00e9ho cyklu','RNA-polymer\u00e1za katalyzuje sp\u00e1janie nukleotidov obsahuj\u00facich A,C,G,U pod\u013ea matrice RNA','RNA-polymer\u00e1za katalyzuje sp\u00e1janie nukleotidov obsahuj\u00facich A, C, G, U pod\u013ea matrice DNA, potrebn\u00fa energiu dod\u00e1va ATP',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(930,'930. Prim\u00e1rna \u0161trukt\u00fara DNA je ur\u010den\u00e1:','porad\u00edm nuklidov v re\u0165azci','sp\u00f4sobom jej \u0161piraliz\u00e1cie','porad\u00edm ribonukleotidov v oboch vl\u00e1knach','porad\u00edm nukleotidov s b\u00e1zami A, G, C, T v polynukleotidovom re\u0165azci','rovnak\u00fdm princ\u00edpom pri v\u0161etk\u00fdch bunk\u00e1ch','porad\u00edm ribonukleotidov s b\u00e1zami A, G, C, T v re\u0165azci','komplementaritou nukleotidov s nukleotidmi mRNA','komplementaritou medzi kod\u00f3nmi a antikod\u00f3nmi',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(931,'931. Za\u010diatok g\u00e9nu v jadrovej DNA je vymedzen\u00fd tripletom:','ATC alebo TAC','TAC alebo TCA','TAC','ATT','TAG','ktor\u00fd k\u00f3duje metion\u00edn','ACT','ATC',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
