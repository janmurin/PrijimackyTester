INSERT INTO BIOLOGIA VALUES(1396,'1396. Trval\u00e1 bipedia je pozorovan\u00e1 u:','Homo sapiens','\u0161impanza','Australopitheca','Homo neanderthalensis','\u010dloveka zru\u010dn\u00e9ho','Paranthropus','Homo erectus','v\u0161etk\u00fdch z\u00e1stupcov rodu Homo',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1397,'1397. Homo erectus \u017eil najpravdepodobnej\u0161ie v obdob\u00ed pred:','150 000 a\u017e 120 000 rokmi','20 000 a\u017e 35 000 rokmi','40 000 a\u017e 10 000 rokmi','1 800 000 a\u017e 230 000 rokmi','3 000 000 a\u017e 2 500 000 rokmi','200 000 a\u017e 100 000 rokmi','1,9 mili\u00f3nmi rokov','14 mili\u00f3nmi rokov',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1398,'1398. Prv\u00fd z\u00e1stupca rodu Homo sa objavil pred:','5 mili\u00f3nmi rokov','2 mili\u00f3nmi rokov','500 000 rokmi','1,5 mili\u00f3nmi rokov na \u00fazem\u00ed \u00c1zie','500 000 - 1 000 000 rokov','1,6 - 2,4 mili\u00f3nmi rokov na \u00fazem\u00ed Afriky','100 000 rokmi','2 mili\u00f3nmi rokov na \u00fazem\u00ed Ameriky',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1399,'1399. Peri\u00f3da sil\u00far je \u010das\u0165ou:','druhoh\u00f4r','prvoh\u00f4r','trefoh\u00f4r','\u0161tvrtoh\u00f4r','prekambria','paleozoika','keozoika','fanerozoika',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1400,'1400. Peri\u00f3da trias je \u010das\u0165ou:','druhoh\u00f4r','tre\u0165oh\u00f4r','prvoh\u00f4r','prekambria','\u0161tvrtoh\u00f4r','mezozoika','paleozoika','kenozoika',TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1401,'1401. Epocha oligoc\u00e9n je \u010das\u0165ou:','prvoh\u00f4r','prekambria','\u0161tvrtoh\u00f4r','tre\u0165oh\u00f4r','druhoh\u00f4r','kenozoika','prechodn\u00e9ho obdobia medzi mezozoikom a kenozoikom','mezozoika',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1402,'1402. Za\u010diatok fanerozoika sa datuje do obdobia:','asi pred 3,5 miliardami rokov','vzniku \u017eivota na Zemi','asi pred 550 mili\u00f3nmi rokov','vzniku sin\u00edc','kedy sa na Zemi objavili \u017eivo\u010d\u00edchy s pevnou schr\u00e1nkou alebo kostrou','vzniku Zeme','asi pred 4,5 miliardami rokov','asi pred 50 mili\u00f3nmi rokov',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1403,'1403. K prechodu oboj\u017eiveln\u00edkov z vody na s\u00fa\u0161 do\u0161lo v:','sil\u00fare','\u0161tvrtohor\u00e1ch','dev\u00f3ne','perme','jure','paleoc\u00e9ne','v paleozoiku','v kenozoiku',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1404,'1404. Pr\u00edrodn\u00fd v\u00fdber:','je pr\u00ed\u010dinou, \u017ee z novovzniknut\u00fdch jedincov niekedy pre\u017e\u00edva len m\u00e1lo jedincov','p\u00f4sob\u00ed na z\u00e1klade nemennosti organizmov','je pova\u017eovan\u00fd za hybn\u00fa silu ontogen\u00e9zy','znamen\u00e1, \u017ee pre\u017e\u00edvaj\u00fa najlep\u0161ie prisp\u00f4soben\u00ed jedinci','p\u00f4sob\u00ed na z\u00e1klade premenlivosti organizmov','nep\u00f4sob\u00ed nikdy proti heterozygotom','zabezpe\u010duje zmeny genofondu popul\u00e1cie','nem\u00f4\u017ee nikdy ovplyv\u0148ova\u0165 genofond popul\u00e1cie',TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1405,'1405. Spr\u00e1vne tvrdenie je:','v\u00fdznam pr\u00edrodn\u00e9ho v\u00fdberu vysvetlil u\u017e Lamarck','premenlivos\u0165 znakov organizmov je podmienen\u00e1 len geneticky','geografick\u00e1 izol\u00e1cia prispieva k vzniku nov\u00fdch foriem v r\u00e1mci druhu','pr\u00edrodn\u00fd v\u00fdber p\u00f4sobil menej v\u00fdrazne v rastlinnej r\u00ed\u0161i','premenlivos\u0165 znakov organizmov je podmienen\u00e1 aj geneticky','pr\u00edrodn\u00fd v\u00fdber je hybnou silou evol\u00facie','v\u00fdchodiskov\u00fdm predpokladom pre pr\u00edrodn\u00fd v\u00fdber je nemennos\u0165 znakov','pr\u00edrodn\u00fd v\u00fdber je hybnou silou ontogen\u00e9zy',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1406,'1406. V\u0161etky \u017eivo\u010d\u00edchy \u017eij\u00face na ur\u010ditom \u00fazem\u00ed a vz\u0165ahy medzi nimi tvoria:','fytocen\u00f3zu','zoocen\u00f3zu','ekosyst\u00e9m','biocen\u00f3zu','biosf\u00e9ru','popul\u00e1ciu','biotop','ekologick\u00fa niku',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1407,'1407. Biocen\u00f3za spolu s ne\u017eiv\u00fdm prostred\u00edm tvor\u00ed:','stanovi\u0161te','biosf\u00e9ru','ekosyst\u00e9m','ekologick\u00fa niku','ekologick\u00fa s\u00fastavu','biocen\u00f3zu','biotop','ekotop',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1408,'1408. Rozmedzie podmienok prostredia, ktor\u00fdm sa organizmus m\u00f4\u017ee prisp\u00f4sobi\u0165, ozna\u010dujeme ako:','ekologick\u00e1 intolerancia','minim\u00e1lne podmienky','ekologick\u00e1 valencia','optim\u00e1lne podmienky','abiotick\u00e9 podmienky','biotick\u00e9 podmienky','varia\u010dn\u00fd rozptyl','ekologick\u00fd status',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1409,'1409. Organizmus zahynie, ak:','v\u0161etky podmienky prostredia pre \u017eivot organizmu s\u00fa nevhodn\u00e9','minim\u00e1lne jedna podmienka prostredia je pre \u017eivot organizmu nevhodn\u00e1','v\u0161etky podmienky prostredia s\u00fa v ekologickej valencii','iba jedna podmienka prostredia je pre \u017eivot organizmu nevhodn\u00e1','aspo\u0148 jedna podmienka prostredia klesne pod minim\u00e1lnu (doln\u00fa) hranicu','aspo\u0148 jedna podmienka prostredia presialme maxim\u00e1lnu (horn\u00fa) hranicu','v\u0161etky podmienky prostredia s\u00fa optim\u00e1lne','m\u00e1 naru\u0161en\u00fd biotick\u00fd potenci\u00e1l',TRUE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1410,'1410. Medzi abiotick\u00e9 faktory \u017eivota zah\u0155\u0148ame:','antropologick\u00fd faktor','fytocen\u00f3zu','\u010dloveka','slne\u010dn\u00e9 \u017eiarenie','teplo, vlhkos\u0165','s\u00fahrn vplyvov \u017eiv\u00fdch organizmov na \u010dloveka','vodu','CO_2 a 0_2',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1411,'1411. Organizmy, pod\u013ea ktor\u00fdch usudzujeme ak\u00e9 s\u00fa podmienky prostredia, s\u00fa:','kozmopolitn\u00e9','organizmy s \u00fazkou ekologickou valenciou k niektorej podmienke prostredia','napr. stenotermn\u00e9 organizmy','bioindik\u00e1tory','mikroindik\u00e1tory','adaptabiln\u00e9','ekoindik\u00e1tory','napr. fytoindik\u00e1tory',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1412,'1412. Kozmopolitn\u00e9 organizmy:','maj\u00fa najv\u00e4\u010d\u0161\u00ed are\u00e1l v\u00fdskytu','s\u00fa napr. kengury','s\u00fa najmenej adaptabiln\u00e9','maj\u00fa najmen\u0161\u00ed are\u00e1l v\u00fdskytu','s\u00fa roz\u0161\u00edren\u00e9 na celom svete','maj\u00fa \u00fazku ekologick\u00fa valenciu k niektorej podmienke prostredia','s\u00fa napr. muchy domov\u00e9','s\u00fa napr. pandy',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1413,'1413. Bioindik\u00e1tory:','s\u00fa roz\u0161\u00edren\u00e9 len vo vy\u0161\u0161\u00edch nadmorsk\u00fdch v\u00fd\u0161kach','s\u00fa napr. li\u0161ajn\u00edky','maj\u00fa najv\u00e4\u010d\u0161\u00ed are\u00e1l v\u00fdskytu','s\u00fa napr. muchy domov\u00e9','s\u00fa napr. \u010du\u010doriedky','s\u00fa stenotermn\u00e9 k niektor\u00fdm podmienkam prostredia','maj\u00fa \u00fazku ekologick\u00fa valenciu k niektorej podmienke prostredia','s\u00fa napr. kamz\u00edk a \u0161afr\u00e1n',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1414,'1414. Oxid uhli\u010dit\u00fd v atmosf\u00e9re Zeme:','je v\u00fdhradne produktom \u013eudskej \u010dinnosti','je biologicky v\u00fdznamn\u00e1 zlo\u017eka vzduchu','je st\u00e1lou zlo\u017ekou vzduchu','je biologicky nev\u00fdznamn\u00e1 zlo\u017eka vzduchu','nach\u00e1dza sa len vo vzduchu','je v st\u00e1lom obehu','je nevyhnutn\u00fd pre fotosynt\u00e9zu','ovplyv\u0148uje produkciu biomasy',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1415,'1415. Popul\u00e1cia:','je skupina organizmov r\u00f4znych druhov, zauj\u00edmaj\u00faca v ur\u010ditom \u010dase ur\u010dit\u00fd priestor','je napr. s\u00fabor v\u0161etk\u00fdch organizmov v rybn\u00edku','predstavuje skupinu organizmov rovnak\u00e9ho druhu \u017eij\u00facich v ur\u010ditom priestore a \u010dase','je napr. skupina organizmov \u017eij\u00faca v lese','ako celok sa vyzna\u010duje minim\u00e1lnou prisp\u00f4sobivos\u0165ou k prostrediu','je charakterizovan\u00e1 aj ve\u013ekos\u0165ou','je nemenn\u00e1','sa vyzna\u010duje ur\u010dit\u00fdm popula\u010dn\u00fdm rastom',FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1416,'1416. Ekologick\u00e1 nika:','je dan\u00e1 len priestorom','charakterizuje funkciu druhu v danom prostred\u00ed','je dan\u00e1 len biotick\u00fdmi zlo\u017ekami prostredia','m\u00f4\u017ee by\u0165 spravidla obsaden\u00e1 i nieko\u013ek\u00fdmi druhmi spolu','je dan\u00e1 terit\u00f3riom','ur\u010duje \u0161pecifick\u00e9 postavenie a jedine\u010dn\u00e9 funkcie druhu v danom prostred\u00ed','je dan\u00e1 len abiotick\u00fdmi zlo\u017ekami prostredia','m\u00f4\u017ee by\u0165 ovplyv\u0148ovan\u00e1 \u010dlovekom',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1417,'1417. Parazity:','likviduj\u00fa popul\u00e1ciu in\u00e9ho druhu','zvy\u0161uj\u00fa ve\u013ekos\u0165 popul\u00e1cie in\u00e9ho druhu','s\u00fa zvy\u010dajne men\u0161ie ako hostite\u013e','zni\u017euj\u00fa ve\u013ekos\u0165 popul\u00e1cie in\u00e9ho druhu','sa rozmno\u017euj\u00fa pomal\u0161ie ako pred\u00e1tory','zabra\u0148uj\u00fa premno\u017eeniu druhov','sa svojou funkciou v pr\u00edrode ve\u013emi odli\u0161uj\u00fa od pred\u00e1torov','sa m\u00f4\u017eu vyu\u017e\u00edva\u0165 v cielenej regul\u00e1cii premno\u017eenej popul\u00e1cie',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1418,'1418. Ekosyst\u00e9m:','je tvoren\u00fd spolo\u010denstvom organizmov a abiotick\u00fdm prostred\u00edm','obsahuje aj ne\u017eiv\u00e9 prostredie','je statick\u00fd','obsahuje \u017eiv\u00fa hmotu, tzv. biosf\u00e9ru','je dynamick\u00fd','je druh spolo\u010denstva','je uzatvoren\u00fd','je otvoren\u00fd',TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1419,'1419. Potravov\u00e9 re\u0165azce s\u00fa:','pastevno-koristn\u00edcky a detritov\u00fd','pastevno-parazitick\u00fd a saprofytick\u00fd','pastevno-parazitick\u00fd, rozkladn\u00fd a detritick\u00fd','pastevn\u00fd, koristn\u00edcky a priamy','pastevno-koristn\u00edcky a rozkladn\u00fd','na seba nadv\u00e4zuj\u00face','na seba nenadv\u00e4zuj\u00face','pastevno-koristn\u00edcky a dekompozi\u010dn\u00fd',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1420,'1420. Pojem ekologick\u00e1 sukcesia znamen\u00e1:','druhov\u00fa z\u00e1vislos\u0165','ust\u00e1len\u00fd kone\u010dn\u00fd ekosyst\u00e9m','zastavenie rastu popul\u00e1cie','naru\u0161enie potravov\u00fdch re\u0165azcov','v\u00fdvoj biocen\u00f3zy a vytvorenie ust\u00e1len\u00e9ho ekosyst\u00e9mu','z\u00e1sah do ekosyst\u00e9mu','rovnov\u00e1hu hustoty popul\u00e1cie','v\u00fdvoj ekosyst\u00e9mu v z\u00e1vislosti od meniacich sa podmienok prostredia',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1421,'1421. Klimax:','s\u00fa najoptim\u00e1lnej\u0161ie klimatick\u00e9 podmienky','je ust\u00e1len\u00fd kone\u010dn\u00fd ekosyst\u00e9m','je nez\u00e1visl\u00fd od podnebia','je v\u00fdsledkom ekologickej sukcesie','s\u00fa maxim\u00e1lne \u00fanosn\u00e9 klimatick\u00e9 podmienky pre \u017eivot organizmov','je d\u00f4sledkom hierarchie v spolo\u010denstv\u00e1ch','je vrcholov\u00e9 \u0161t\u00e1dium v\u00fdvoja biocen\u00f3z','je napr. les alebo step',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1422,'1422. St\u00fapaj\u00face mno\u017estvo CO_2 v ovzdu\u0161\u00ed:','ak je nepatrn\u00e9, m\u00f4\u017ee sp\u00f4sobi\u0165 zv\u00fd\u0161enie intenzity fotosynt\u00e9zy','neovplyv\u0148uje podnebie','ovplyv\u0148uje vznik sklen\u00edkov\u00e9ho efektu','je sp\u00f4soben\u00e9 najm\u00e4 vulkanickou \u010dinnos\u0165ou','vyvol\u00e1va oteplenie','m\u00f4\u017ee sp\u00f4sobi\u0165 roz\u0161irovanie tropick\u00e9ho podnebia','m\u00f4\u017ee sp\u00f4sobi\u0165 roz\u0161irovanie arktick\u00e9ho podnebia','m\u00f4\u017ee ovplyvni\u0165 odvod tepla vy\u017earovan\u00e9ho z povrchu Zeme',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1423,'1423. Ozna\u010dte spr\u00e1vne odpovede:','rozp\u00e4tie hodn\u00f4t ekologick\u00e9ho faktora sa ozna\u010duje ako tolerancia organizmu','schopnos\u0165 zn\u00e1\u0161a\u0165 r\u00f4zne podmienky prostredia je ekologick\u00e1 valencia organizmu','organizmom sa najlep\u0161ie dar\u00ed v oblasti optima ekologick\u00e9ho faktora','hrani\u010dne podmienky ekologick\u00e9ho faktora s\u00fa pesimum','kozmopolitn\u00e9 organizmy maj\u00fa \u0161irok\u00fa ekologick\u00fa valenciu','bioindik\u00e1tory maj\u00fa \u00fazku ekologick\u00fa valenciu','medzidruhov\u00e9 vz\u0165ahy m\u00f4\u017eu by\u0165 neutr\u00e1lne','symbi\u00f3za patr\u00ed k z\u00e1pom\u00fdm medzidruhov\u00fdm vz\u0165ahom',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1424,'1424. Pre jednotliv\u00e9 organizmy vr\u00e1mci ekosyst\u00e9mu plat\u00ed:','mikroorganizmy zavr\u0161uj\u00fa detritov\u00e9 potravov\u00e9 re\u0165azce','prim\u00e1rne konzumenty sa \u017eivia in\u00fdmi \u017eivo\u010d\u00edchmi','zdochlin\u00e1re patria medzi reducenty','reducenty redukuj\u00fa po\u010detnos\u0165 svojich hostiterov','\u010dlovek patr\u00ed v potravovej pyram\u00edde medzi producenty','prim\u00e1rne konzumenty sa \u017eivia rastlinami','reducenty sa \u017eivia napr. exkrementami konzumentov','sekund\u00e1rne konzumenty sa \u017eivia in\u00fdmi \u017eivo\u010d\u00edchmi alebo parazituj\u00fa',TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1425,'1425. Popul\u00e1ciu charakterizuj\u00fa vlastnosti:','ve\u013ekos\u0165','priestorov\u00e1 \u0161trukt\u00fara','hustota','vekov\u00e1 \u0161trukt\u00fara','pohlavn\u00e1 \u0161trukt\u00fara','popula\u010dn\u00fd rast','druhov\u00e1 rozmanitos\u0165','druhov\u00e1 d\u00edverzita',TRUE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1426,'1426. Kysl\u00edk obsiahnut\u00fd vo vodnom i vzdu\u0161nom prostred\u00ed :','je produkovan\u00fd zelen\u00fdmi rastlinami pri fotosynt\u00e9ze','nie je v rovnov\u00e1he s CO_2','je produkovan\u00fd konzumentmi','sa spotreb\u00fava pri d\u00fdchan\u00ed','nie je v st\u00e1lom obehu','je v neust\u00e1lom obehu','je produkovan\u00fd zelen\u00fdmi rastlinami pri d\u00fdchan\u00ed','vznik\u00e1 pri biologickej oxid\u00e1cii l\u00e1tok',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1427,'1427. Pr\u00ed\u010dinou boja o \u017eivot je:','nadprodukcia potomstva','rozvetvovanie v\u00fdvojov\u00fdch l\u00edni\u00ed','kozmopolitn\u00e9 roz\u0161\u00edrenie \u017eivo\u010d\u00edchov','premenlivos\u0165 druhov','premenlivos\u0165 znakov','n\u00edzky po\u010det jedincov v popul\u00e1cii','vz\u0165ah pred\u00e1tora a koristi','premno\u017eenie druhu',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1428,'1428. Vz\u0165ahy medzi r\u00f4znymi popul\u00e1ciami:','m\u00f4\u017eu by\u0165 len pozit\u00edvne alebo negat\u00edvne','v pr\u00edrode s\u00fa v\u017edy negat\u00edvne','m\u00f4\u017eu by\u0165 neutr\u00e1lne','m\u00f4\u017eu by\u0165 obojstranne negat\u00edvne','m\u00f4\u017eu by\u0165 jednostranne negat\u00edvne','m\u00f4\u017eu by\u0165 aj konkuren\u010dn\u00e9','s\u00fa konkuren\u010dn\u00e9, ak sa druhy navz\u00e1jom obmedzuj\u00fa','nie s\u00fa ovplyv\u0148ovan\u00e9 n\u00e1rokmi na potravu',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1429,'1429. Kozmopolitn\u00e9 organizmy:','s\u00fa \u00fazko \u0161pecializovan\u00e9 na prostredie','s\u00fa tzv. perzistenty','s\u00fa roz\u0161\u00edren\u00e9 prakticky po celom svete','s\u00fa endemity','maj\u00fa ve\u013ek\u00fa schopnos\u0165 adapt\u00e1cie','m\u00f4\u017eu by\u0165 roz\u0161irovan\u00e9 \u010dinnos\u0165ou \u010dloveka','maj\u00fa n\u00edzku schopnos\u0165 adapt\u00e1cie','naz\u00fdvame modelov\u00e9 objekty pre lety do vesm\u00edru',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1430,'1430. Spr\u00e1vne tvrdenie je:','evol\u00facia je historick\u00fd v\u00fdvoj \u017eiv\u00fdch organizmov','roz\u0161\u00edrenie rastl\u00edn a \u017eivo\u010d\u00edchov na Zemi nie je n\u00e1hodn\u00e9','pod\u013ea darwinizmu je jedinec rozhoduj\u00facim \u010dinite\u013eom v evol\u00facii organizmov','v\u0161etky druhy vznikli dlh\u00fdm v\u00fdvojom zo spolo\u010dn\u00fdch predkov','roz\u0161\u00edrenie rastl\u00edn a \u017eivo\u010d\u00edchov na Zemi je n\u00e1hodn\u00e9','roz\u0161\u00edrenie rastl\u00edn a \u017e\u00edvo\u010d\u00edchov na Zemi je z\u00e1konit\u00e9','pr\u00edrodn\u00fd v\u00fdber je proces, v ktorom pre\u017e\u00edvaj\u00fa jedinci najlep\u0161ie adaptovan\u00ed na podmienky prostredia','fos\u00edlie s\u00fa primit\u00edvne kamenn\u00e9 n\u00e1stroje hominidov',TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1431,'1431. Ozna\u010dte spr\u00e1vnu odpove\u010f:','slne\u010dn\u00e9 \u017eiarenie je z\u00e1kladn\u00fdm zdrojom energie pre \u017eivot na Zemi','teplo ovplyv\u0148uje r\u00fdchlos\u0165 a priebeh \u017eivotn\u00fdch procesov','lokom\u00f3cia je pas\u00edvny pohyb pomocou ur\u010dit\u00e9ho prostriedku','0_2 (78%) vo vzduchu je podmienkou d\u00fdchania','chorobnos\u0165 sa in\u00e1\u010d naz\u00fdva aj mortalita','vidite\u013en\u00e9 svetlo je d\u00f4le\u017eit\u00e9 pre fotosynt\u00e9zu','r\u00fdchlos\u0165 rastu popul\u00e1cie z\u00e1vis\u00ed najm\u00e4 od pomeru natality a mortality organizmov','li\u0161ajn\u00edky s\u00fa tvoren\u00e9 autotrofn\u00fdmi riasami alebo sinicami a hubov\u00fdmi vl\u00e1knami',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1432,'1432. Ozna\u010dte spr\u00e1vne odpovede:','li\u0161ajn\u00edk je pr\u00edkladom symbi\u00f3zy','parazit svojho hostite\u013ea zvy\u010dajne zab\u00edja','parazit od\u010derp\u00e1va hostite\u013eovi \u017eiviny','pr\u00edkladom komenzalizmu je epifyt rast\u00faci na strome','koris\u0165 sa \u017eiv\u00ed pred\u00e1torom','konkurencia je zvy\u010dajne vz\u0165ah medzi druhmi s rovnakou ekologickou nikou','pred\u00e1tor lov\u00ed koris\u0165','pri neutralizme s\u00fa organizmy od seba nez\u00e1visl\u00e9',TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1433,'1433. Najvhodnej\u0161\u00ed s\u00fahrn ur\u010dit\u00fdch podmienok prostredia ozna\u010dujeme:','minim\u00e1lne podmienky','limituj\u00face podmienky','maxim\u00e1lne podmienky','nevyhnutn\u00e9 podmienky','optim\u00e1lne podmienky','existen\u010dn\u00e9 podmienky','ekologick\u00e1 valencia','ekologick\u00e1 nika',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1434,'1434. Mno\u017estvo ultrafialov\u00e9ho \u017eiarenia, ktor\u00e9 dopad\u00e1 na povrch Zeme:','je st\u00e1le rovnak\u00e9','nem\u00e1 vplyv na \u017eivot na Zemi','je ve\u013ek\u00e9, preto\u017ee je len nepatrne zachyt\u00e1van\u00e9 vrstvou oz\u00f3nu','vyvol\u00e1va taxie','je mal\u00e9, preto\u017ee je va\u010d\u0161inou zachyt\u00e1van\u00e9 oz\u00f3nosf\u00e9rou','je nulov\u00e9','sa nepriamo zvy\u0161uje v d\u00f4sledku narastaj\u00faceho zne\u010distenia \u017eivotn\u00e9ho prostredia','m\u00f4\u017ee pri zv\u00fd\u0161enej miere negat\u00edvne vpl\u00fdva\u0165 na zdravotn\u00fd stav organi\u0139mov',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1435,'1435. Kysl\u00e9 da\u017ede:','ni\u010dia \u017eivot v riekach, jazer\u00e1ch i lesoch','vznikaj\u00fa zmie\u0161avan\u00edm niektor\u00fdch plynov so vzdu\u0161nou vlhkos\u0165ou','okys\u013euj\u00fa p\u00f4du a\u017e na pH 7,0','neutralizuj\u00fa p\u00f4du','sp\u00f4sobuj\u00fa n\u00e1rast biomasy','zni\u017euj\u00fa pH p\u00f4dy','zyy\u0161uj\u00fa pH p\u00f4dy','po\u0161kodzuj\u00fa pr\u00edrodn\u00fa vrstvu oz\u00f3nu v atmosf\u00e9re',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1436,'1436. Symbi\u00f3za znamen\u00e1:','parazitizmus','ust\u00e1len\u00fd ekosyst\u00e9m','komenzalizmus','spolu\u017eitie organizmov r\u00f4znych druhov','napr. spolu\u017eitie dravca a koristi','v\u00fdvoj ekosyst\u00e9mu','pozit\u00edvne spolu\u017eitie organicniov','vz\u00e1jomn\u00e9 v\u00fdhodn\u00e9 spolu\u017eitie medzi organizmami',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1437,'1437. Rastliny so \u0161irokou ekologickou valenciou vo\u010di faktorom \u017eivotn\u00e9ho prostredia sa naz\u00fdvaj\u00fa:','euryekn\u00e9','stenoekn\u00e9','bioindik\u00e1tory','fytoindik\u00e1tory','ekotonov\u00e9','synantropn\u00e9','alelopatick\u00e9','planktonick\u00e9',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1438,'1438. Vekov\u00e1 \u0161trukt\u00fara popul\u00e1cie rastl\u00edn:','vyjadruje romiestnenie jedincov v priestore','vyjadruje vekov\u00e9 zast\u00fapenie jedincov v jednotliv\u00fdch vekov\u00fdch triedach','sa graficky zn\u00e1zor\u0148uje vekov\u00fdmi pyram\u00eddami','zah\u0155\u0148a juveniln\u00e9 \u0161t\u00e1dium','zah\u0155\u0148a inv\u00e1zne \u0161t\u00e1dium','zah\u0155\u0148a virgin\u00e1lne \u0161t\u00e1dium','zah\u0155\u0148a senn\u00e9 \u0161t\u00e1dium','zah\u0155\u0148a seniln\u00e9 \u0161t\u00e1dium',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1439,'1439. Spolo\u010denstvo tvoren\u00e9 jedincami jednej popul\u00e1cie:','naz\u00fdvame fytocen\u00f3za','naz\u00fdvame polycen\u00f3za','naz\u00fdvame monocen\u00f3za','je naj\u010dastej\u0161ie divo rast\u00face','je naj\u010dastej\u0161ie kult\u00farne, umelo vysaden\u00e9 spolo\u010denstvo','naz\u00fdvame dominantn\u00e9','naz\u00fdvame ekotonov\u00e9','je ustupuj\u00faci typ popul\u00e1cie',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1440,'1440. Term\u00edn ekosyst\u00e9m zaviedol :','A. G. Tansley','D. Watson','C. Woese','F. H. Crick','J. G. Mendel','Galenos','E. Haeckel','Ch. Darwin',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1441,'1441. N\u00e1rodn\u00fd park je:','\u00fazemie s ekosyst\u00e9mami v\u00fdrazne upraven\u00fdmi \u013eudskou \u010dinnos\u0165ou','\u00fazemie s osobitnou ochranou na vymedzenom \u00fazem\u00ed','\u00fazemie, v ktorom je zoh\u013eadnen\u00e9 pohodlie \u013eud\u00ed pri rekre\u00e1cii','napr. N\u00e1rodn\u00fd park Slovensk\u00fd raj','napr. N\u00e1rodn\u00fd park Zempl\u00ednska \u0160\u00edrava','\u00fazemie, kde zvykne plati\u0165 aj druhov\u00e1 ochrana \u017eivo\u010d\u00edchov, rastl\u00edn, nerastov a skamenel\u00edn','napr. N\u00e1rodn\u00fd park Poloniny','napr. Pieninsk\u00fd n\u00e1rodn\u00fd park',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1442,'1442. Ktor\u00e9 tvrdenie je nespr\u00e1vne:','v \u00fastiach riek do mora s\u00fa brakick\u00e9 vody','rastliny pri d\u00fdchan\u00ed spotreb\u00favaj\u00fa CO_2 a uvo\u013e\u0148uj\u00fa 0_2','svetlo vo vode prenik\u00e1 len do h\u013abky asi 200 cm','fytoplankt\u00f3n tvoria aj drobn\u00e9 k\u00f4rovce a larvy m\u00e4kk\u00fd\u0161ov','zooplankt\u00f3n tvoria aj sinice','plankt\u00f3nom sa \u017eivia napr. hubky a last\u00farniky','bakt\u00e9rie zavr\u0161uj\u00fa kolobeh l\u00e1tok v pr\u00edrode','voda m\u00e1 najv\u00e4\u010d\u0161iu hustotu pri teplote 14\u00b0C',FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1443,'1443. Habitus:','je s\u00fabor genetick\u00fdch znakov vlastn\u00fdch v\u0161etk\u00fdm \u010dlenom ur\u010ditej popul\u00e1cie','je s\u00fabor morfologick\u00fdch znakov vlastn\u00fdch niektor\u00fdm jedincom popul\u00e1cie','s\u00fabor morfologick\u00fdch znakov vlastn\u00fdch v\u0161etk\u00fdm \u010dlenom ur\u010ditej popul\u00e1cie','predstavuje napr. tvar tela a sfarbenie','predstavuje napr. ve\u013ekos\u0165, hmotnos\u0165','z\u00e1vis\u00ed od natality','z\u00e1vis\u00ed od hustoty popul\u00e1cie','je dan\u00fd podielom hynutia jedincov v popul\u00e1cii za \u010dasov\u00fa jednotku',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1444,'1444. Ekoton:','je prechodn\u00e9 p\u00e1smo medzi ekosyst\u00e9mami','je s\u00fabor ekologick\u00fdch po\u017eiadaviek jednotliv\u00fdch rastlinn\u00fdch druhov','je napr. spolo\u010denstvo vzniknut\u00e9 medzi l\u00fakou a susedn\u00fdm lesom','je spolo\u010denstvo l\u00fak, pasienkov a vresov\u00edsk','naz\u00fdvame tie\u017e lemov\u00e9 spolo\u010denstvo','naz\u00fdvame tie\u017e krajov\u00e9 spolo\u010denstvo','s\u00fa kr\u00e1tkodob\u00e9 zmeny vyvolan\u00e9 meniacimi sa podmienkami prostredia','je spolo\u010denstvo rastl\u00edn s vodoochran\u00e1rskou funkciou',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1445,'1445. Morbidita:','znamen\u00e1 chorobnos\u0165','znamen\u00e1 \u00famrtnos\u0165','je s\u00fahrn rozmanit\u00fdch konan\u00ed a reakci9 \u017eivo\u010d\u00edchov na rozli\u010dn\u00e9 podnety','je schopnos\u0165 \u017eivo\u010d\u00edchov produkova\u0165 potomstvo','je teoreticky mo\u017en\u00e1 produkcia potomstva','vyjadruje pomer zast\u00fapenia sam\u010d\u00edch a sami\u010dich jedincov v popul\u00e1cii','predstavuje podiel hynutia jedincov v popul\u00e1cii za \u010dasov\u00fa jednotku','vyjadruje zdravotn\u00fd stav popul\u00e1cie v % chor\u00fdch a nakazen\u00fdch jedincov',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1446,'1446. Natalita:','znamen\u00e1 schopnos\u0165 jedinca pod\u00e1va\u0165 fyziologick\u00e9 v\u00fdkony','vyjadruje produkciu potomstva za obmedzen\u00fdch vonkaj\u0161\u00edch podmienok','sa prejavuje v odohiosti popul\u00e1cie vo\u010di patog\u00e9nom a parazitom','je \u00fabytok popul\u00e1cie sp\u00f4soben\u00fd \u00famrtnos\u0165ou','vyjadruje po\u010det oplodnenia schopn\u00fdch sam\u00edc v popul\u00e1cii','ozna\u010duje r\u00fdchlos\u0165 individu\u00e1lneho v\u00fdvinu','je schopnos\u0165 \u017eivo\u010d\u00edchov produkova\u0165 potomstvo','ovplyv\u0148uje po\u010detnos\u0165 organizmov v danom spolo\u010denstve',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1447,'1447. Mortalita:','ozna\u010duje popul\u00e1ciu r\u00fdb \u017eij\u00facu v mori','je s\u00fahrn rozmanit\u00fdch konan\u00ed \u017eivo\u010d\u00edchov na rozli\u010dn\u00e9 podnety','vyjadruje podiel hynutia jedincov v popul\u00e1cii za \u010dasov\u00fa jednotku','je v dlh\u0161om \u010dasovom \u00faseku vyrovn\u00e1van\u00e1 pr\u00edrastkom v popul\u00e1cii, \u010d\u00edm sa udr\u017eiava pribli\u017ene rovnak\u00e1 hustota','prirodzen\u00fdm sp\u00f4sobom, je pre ve\u013ek\u00e9 cicavce pomerne n\u00edzka','vz\u0165ahuj\u00faca sa na ist\u00fa vekov\u00fa skupinu sa ozna\u010duje ako \u0161pecifick\u00e1','ozna\u010duje taktiku rozmno\u017eovania rozli\u010dn\u00fdch tax\u00f3nov','je mno\u017estvo jedincov na ur\u010dit\u00fa plo\u0161n\u00fa jednotku',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1448,'1448. Hustota popul\u00e1cie:','sa naz\u00fdva tie\u017e stenovalencia','sa ozna\u010duje tie\u017e ako abundancia','je po\u010det jedincov jedn\u00e9ho druhu na ur\u010dit\u00fa plo\u0161n\u00fa alebo objemov\u00fa jednotku','m\u00f4\u017ee by\u0165 vyjadren\u00e1 v g/m2','m\u00f4\u017ee by\u0165 vyjadren\u00e1 v kg/ha','vyjadruje rozdelenie jedincov danej popul\u00e1cie v priestore','je s\u00fabor jedincov rozli\u010dn\u00fdch druhov \u017eij\u00facich v spolo\u010dnom prostred\u00ed','je mno\u017eina v\u0161etk\u00fdch \u017eivo\u010d\u00edchov nach\u00e1dzaj\u00faca sa na ur\u010ditej plo\u0161nej jednotke',FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1449,'1449. Ozna\u010dte spr\u00e1vnu odpove\u010f:','vysokohorsk\u00e9 spolo\u010denstv\u00e1 sa vytv\u00e1raj\u00fa nad hornou hranicou lesa','horn\u00e1 hranica lesa m\u00e1 charakter prechodn\u00e9ho spolo\u010denstva','s rast\u00facou nadmorskou v\u00fd\u0161kou sa teplota zvy\u0161uje','vysokohorsk\u00e9 spolo\u010denstv\u00e1 s\u00fa limitovan\u00e9 kr\u00e1tkym vegeta\u010dn\u00fdm obdob\u00edm','s rast\u00facou nadmorskou v\u00fd\u0161kou sa zvy\u0161uje intenzita UV \u017eiarenia','glaci\u00e1lne relikty sa zachovali z obdobia za\u013eadnenia','medzi tatransk\u00e9 glaci\u00e1lne relikty patr\u00ed dry\u00e1dka osemlupienkov\u00e1','endemity s\u00fa druhy vyskytuj\u00face sa v\u00fdhradne na ur\u010ditom \u00fazem\u00ed',TRUE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1450,'1450. Sklen\u00edkov\u00fd efekt:','je sp\u00f4soben\u00fd \u0165a\u017ek\u00fdmi kovmi','je sp\u00f4soben\u00fd oz\u00f3nom','je sp\u00f4soben\u00fd oxidom uhli\u010dit\u00fdm','napom\u00e1ha oteprovaniu Zeme','zapr\u00ed\u010dinil \u00fastup \u013eadovcov v Alp\u00e1ch','je pr\u00ed\u010dinou poklesu hlad\u00edn oce\u00e1nov','chr\u00e1ni \u013eud\u00ed pred \u0161kodliv\u00fdmi zlo\u017ekami slne\u010dn\u00fdch l\u00fa\u010dov','vznik\u00e1 bez pri\u010dinenia \u010dloveka',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1451,'1451. Kysl\u00fd d\u00e1\u017e\u010f:','vznik\u00e1 zmie\u0161an\u00edm v\u00fdfukov\u00fdch plynov a plynov z tov\u00e1rn\u00ed s oz\u00f3nom','vznik\u00e1 zmie\u0161an\u00edm plynov z tov\u00e1rn\u00ed a v\u00fdfukov\u00fdch plynov s kysl\u00edkom a vzdu\u0161nou vlhkos\u0165ou','je pr\u00ed\u010dinou glob\u00e1lneho otep\u013eovania','je sp\u00f4soben\u00fd chloroflourokarbon\u00e1tmi','je prospe\u0161n\u00fd pre v\u0161etky druhy rastl\u00edn','po\u0161kodzuje len oblas\u0165, nad ktorou sa vytvoril','sa vyu\u017e\u00edva ako hnojivo pr\u00edrodn\u00e9ho p\u00f4vodu','sp\u00f4sobuje oz\u00f3nov\u00e9 diery',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1452,'1452. Medzi z\u00e1kladn\u00e9 faktory ovplyv\u0148uj\u00face zlo\u017eenie genofondu popul\u00e1cie patria:','mut\u00e1cie','slne\u010dn\u00e9 \u017eiarenie','sp\u00f4sob kr\u00ed\u017eenia','pr\u00edrodn\u00fd v\u00fdber','tok g\u00e9nov','migr\u00e1cia','selekcia','zlo\u017eenie p\u00f4dy',TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1453,'1453. Evolu\u010dn\u00e1 biol\u00f3gia:','vych\u00e1dza z poznatkov v\u0161etk\u00fdch biologick\u00fdch discipl\u00edn','dokumentuje, \u017ee jednotliv\u00e9 druhy organizmov sa nemenia','objas\u0148uje mechanizmy historick\u00e9ho v\u00fdvoja organizmov','dokumentuje, \u017ee jednotliv\u00e9 druhy organizmov s\u00fa kon\u0161tantn\u00e9','zaober\u00e1 sa hlavn\u00fdmi etapami evol\u00facie organizmov','vych\u00e1dza hlavne z poznatkov kvantovej fyziky','dokumentuje, \u017ee jednotliv\u00e9 druhy sa v priebehu \u010dasu menia','nepatr\u00ed do skupiny biologick\u00fdch vied',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1454,'1454. Z\u00e1kladn\u00e9 my\u0161lienky darwinizmu hovoria, \u017ee:','organizmy sa od svojho stvorenia u\u017e nemenia','medzi organizmami existuje boj o \u017eivot','organizmy lep\u0161ie adaptovan\u00e9 bud\u00fa tvori\u0165 menej potomstva','vo v\u00e4\u010d\u0161ine pr\u00edrodn\u00fdch popul\u00e1cii existuje individu\u00e1lna premenlivos\u0165 znakov a vlastnost\u00ed','organizmy s\u00fa pod tlakom pr\u00edrodn\u00e9ho v\u00fdberu','organizmy vznikaj\u00fa v procese postupn\u00fdch adapt\u00edvnych zmien','k zmen\u00e1m organizmov doch\u00e1dza akt\u00edvnym \u00fasil\u00edm dan\u00e9ho indiv\u00eddua o lep\u0161ie prisp\u00f4sobenie sa prostrediu','pr\u00edrodn\u00fd v\u00fdber nehr\u00e1 \u00falohu v evol\u00facii organizmov',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1455,'1455. Sp\u00f4sob kr\u00ed\u017eenia jedincov:','je napr\u00edklad panmixia','je napr\u00edklad mit\u00f3za a mei\u00f3za','je napr\u00edklad pr\u00edbuzensk\u00e9 kr\u00ed\u017eenie','v \u013eudskej popul\u00e1cii je v\u017edy panmixia','typu autogamia je krajnou formou pr\u00edbuzensk\u00e9ho kr\u00ed\u017eenia','je napr\u00edklad samooplodnenie','typu inbr\u00edding sa odohr\u00e1va najm\u00e4 v mal\u00fdch popul\u00e1ci\u00e1ch','typu panmixia je n\u00e1hodn\u00e9 kr\u00ed\u017eenie',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1456,'1456. Pod\u013ea vplyvu na reproduk\u010dn\u00fa zdatnos\u0165 jedinca mut\u00e1cie m\u00f4\u017eu by\u0165:','iba nededi\u010dn\u00e9','pozit\u00edvne','\u0161kodliv\u00e9','neutrofiln\u00e9','neurotick\u00e9','neutr\u00e1lne','negat\u00edvne','pas\u00edvne',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1457,'1457. Ozna\u010dte spr\u00e1vnu odpove\u010f:','panmixia je pr\u00edbuzensk\u00e9 kr\u00ed\u017eenie','g\u00e9nov\u00fd drift je nen\u00e1hodn\u00fd proces','p\u00f4sobenie prostredia na fenotyp vy\u00fas\u0165uj\u00face do rozdielov v prenose g\u00e9nov sa naz\u00fdva selek\u010dn\u00fd tlak','pr\u00edrodn\u00fd v\u00fdber je rozhoduj\u00facim \u010dinite\u013eom evol\u00facie organizmov','\u0161\u013eachten\u00edm sa vyv\u00edja umel\u00fd selek\u010dn\u00fd tlak na organizmy','p\u00f4soben\u00edm pr\u00edrodn\u00e9ho v\u00fdberu vznikaj\u00fa nov\u00e9 vlastnosti organizmov','makroevol\u00facia sa odohr\u00e1va na \u00farovni jedinca','n\u00e1hodn\u00e9 kol\u00edsanie po\u010detnosti alel je t\u00fdm v\u00e4\u010d\u0161ie, \u010d\u00edm je popul\u00e1cia v\u00e4\u010d\u0161ia',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1458,'1458. Vymieranie druhov po\u010das evol\u00facie:','nebolo doteraz nikdy zdokumentovan\u00e9','neexistuje, existuje len premena jedn\u00e9ho druhu na in\u00fd','nikdy nemalo masov\u00fd charakter','najv\u00e4\u010d\u0161ie prebiehalo najm\u00e4 koncom permu','vytv\u00e1ra priestor na v\u00fdvoj in\u00fdch druhov','najv\u00e4\u010d\u0161ie bolo na konci paleozoika','m\u00f4\u017ee by\u0165 zapr\u00ed\u010dinen\u00e9 aj dopadom meteoritov a erupciou vulk\u00e1nov','najv\u00e4\u010d\u0161ie bolo po\u010das permu a vyhynulo pri \u0148om 80 % v\u0161etk\u00fdch vtedy \u017eij\u00facich druhov',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1459,'1459. Medzi kladn\u00e9 vz\u0165ahy v spolo\u010denstv\u00e1ch \u017eiv\u00fdch organizmov patr\u00ed:','konkurencia','parazitizmus','superparazitizmus','symbi\u00f3za','alelopatia','lichenizmus','mykor\u00edza','epifytizmus',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1460,'1460. Medzi z\u00e1porn\u00e9 vz\u0165ahy v spolo\u010denstv\u00e1ch \u017eiv\u00fdch organizmov patr\u00ed:','alelopatia','epifytizmus','darwinizmus','lichenizmus','parazitizmus','konkurencia','symbi\u00f3za','mykor\u00edza',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1461,'1461. Rudiment\u00e1rne org\u00e1ny:','maj\u00fa pre dan\u00fd organizmus podstatn\u00fd v\u00fdznam','maj\u00fa pre organizmus minim\u00e1lny alebo \u017eiadny v\u00fdznam','s\u00fa historick\u00fdm pozostatkom \u0161trukt\u00far, zohr\u00e1vaj\u00facich v\u00fdznamn\u00fa \u00falohu v \u017eivote d\u00e1vnych predkov dan\u00e9ho organizmu','s\u00fa napr\u00edklad kr\u00eddla vt\u00e1kov','s\u00fa napr\u00edklad zadn\u00e9 kon\u010datiny a panva u ve\u013er\u00fdb','s\u00fa v\u017edy novovytvoren\u00e9 org\u00e1ny u dan\u00e9ho jedinca','s\u00fa v\u0161etky org\u00e1ny jedinca','s\u00fa org\u00e1ny, ktor\u00e9 umo\u017enuj\u00fa dan\u00e9mu jedincovi pre\u017ei\u0165, napr\u00edklad srdce a p\u013e\u00faca u \u010dloveka',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1462,'1462. Ktor\u00e9 z druhohorn\u00fdch plazov pre\u017eili a\u017e do s\u00fa\u010dasnosti:','krokod\u00edly','dinosaury','pterosaury','\u017eaby','mloky','tyranosaury','\u017eraloky','plesiosaury',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1463,'1463. Vyberte spr\u00e1vne tvrdenia:','prek\u00e1\u017eka zabra\u0148uj\u00faca dvom indiv\u00eddu\u00e1m ma\u0165 plodn\u00e9 potomstvo sa naz\u00fdva reproduk\u010dn\u00e1 bari\u00e9ra','mulica a tap\u00edr s\u00fa pr\u00edkladom neplodn\u00fdch hybridov','liger a tiglon s\u00fa schopn\u00ed \u010fal\u0161ieho rozmno\u017eovania','evol\u00facia jedn\u00e9ho druhu m\u00f4\u017ee ovplyv\u0148ova\u0165 aj ostatn\u00e9 druhy','geografick\u00e1 bari\u00e9ra je najzn\u00e1mej\u0161\u00edm prficladom reproduk\u010dnej bari\u00e9ry','mut\u00e1cie s\u00fa zdrojom evolu\u010dn\u00fdch noviniek','p\u00f4soben\u00edm pr\u00edrodnej selekcie nevznikaj\u00fa nov\u00e9 g\u00e9ny','reproduk\u010dnou bari\u00e9rou m\u00f4\u017eu by\u0165 aj odli\u0161nosti v organiz\u00e1cii genetick\u00e9ho materi\u00e1lu',TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1464,'1464. Doklady dokumentuj\u00face existenciu evol\u00facie s\u00fa zalo\u017een\u00e9 aj na poznatkoch:','biogeografie','molekulovej biol\u00f3gie','paleontol\u00f3gie','porovn\u00e1vacej embryol\u00f3gie','porovn\u00e1vacej anat\u00f3mie','porovn\u00e1vac\u00edch anal\u00fdz tzv. informa\u010dn\u00fdch mikromolek\u00fal','o geografickej distrib\u00facii druhov','hydrometeorol\u00f3gie',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1465,'1465. Ozna\u010dte nespr\u00e1vne odpovede:','pod\u013ea kreacionizmu sa druhy v priebehu \u010dasu nemenia','pod\u013ea transformizmu sa druhy v priebehu \u010dasu nemenia','organizmy maj\u00fa tendenciu produkova\u0165 viac potomstva ako je prostredie schopn\u00e9 u\u017eivi\u0165','selekcia pr\u00edrodou je podra darwinizmu hybnou silou evol\u00facie','zlo\u017eenie genofondu popul\u00e1cie sa z gener\u00e1cie na gener\u00e1ciu nem\u00f4\u017ee meni\u0165','makroevol\u00facia sa odohr\u00e1va na \u00farovni druhu a vy\u0161\u0161fch tax\u00f3nov v priebehu dlh\u00e9ho \u010dasov\u00e9ho obdobia','koevol\u00facia je, ke\u010f v\u00fdvoj jedn\u00e9ho druhu je ovplyv\u0148ovan\u00fd evol\u00faciou in\u00e9ho druhu','Archeopteryx tvor\u00ed prechodn\u00fa formu medzi rybami a vt\u00e1kmi',FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1466,'1466. Kreacionizmus:','je jedin\u00fd sp\u00f4sob vysvetlenia vz\u0165ahov medzi r\u00f4znymi druhmi organizmov v \u010dase','hl\u00e1sa, \u017ee v\u0161etky druhy maj\u00fa spolo\u010dn\u00e9ho predka','vych\u00e1dza z predstavy stvorenia \u017eivota na Zemi nadprirodzenou bytos\u0165ou','hovor\u00ed, \u017ee druhy sa v priebehu \u010dasu nemenia','je jedn\u00fdm z my\u0161lienkov\u00fdch smerov vo v\u00fdvoji evolu\u010dn\u00e9ho myslenia','rovnako ako darwinizmus predpoklad\u00e1, \u017ee druhy sa v priebehu \u010dasu nemenia','hovor\u00ed, \u017ee odli\u0161n\u00e9 druhy maj\u00fa samostatn\u00fd p\u00f4vod','rovnako ako transformizmus predpoklad\u00e1, \u017ee druhy sa v priebehu \u010dasu menia',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1467,'1467. Riboz\u00fdm je:','katalyticky akt\u00edvna molekula DNA','katalyticky akt\u00edvna molekula prote\u00ednu','katalyticky akt\u00edvna molekula lipidu','katalyticky akt\u00edvna molekula RNA','katalyticky akt\u00edvna molekula sacharidu','molekula RNA, ktor\u00e1 je schopn\u00e1 enz\u00fdmovej katalyzy','katalyticky akt\u00edvna molekula lyzoz\u00fdmu','molekula DNA, ktor\u00e1 je schopn\u00e1 enz\u00fdmovej katal\u00fdzy',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1468,'1468. Pod\u013ea s\u00fa\u010dasn\u00fdch predst\u00e1v v prvej etape existencie \u017eivota na Zemi v\u0161etky pre \u017eivot nevyhnutn\u00e9 funkcie vykon\u00e1vali molekuly:','DNA','bielkov\u00edn','hemoglob\u00ednu','RNA','deoxyribonukleov\u00fdch kysel\u00edn','ribonukleov\u00fdch kysel\u00edn','riboz\u00fdmov','fosfolipidov',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1469,'1469. N\u00e1lezy najstar\u0161Ich mikrofos\u00edfil dokumentuj\u00face pr\u00edtomnos\u0165 bunkov\u00e9ho \u017eivota na Zemi, s\u00fa datovan\u00e9 do obdobia:','pred 3,5 a\u017e 3,8 miliardami rokov','pred 5 miliardami rokov','mezozoika','druhoh\u00f4r','pred 6 miliardami rokov','poslednej doby \u013eadovej','vrchnej jury','eoc\u00e9nu',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1470,'1470. Hypotetick\u00fd spolo\u010dn\u00fd predok v\u0161etk\u00fdch organizmov sa naz\u00fdva:','eukaryot','protogenot','progenot','arche\u00f3n','prabunka','praeukaryot','prot\u00f3n','probiotikum',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1471,'1471. Progenot:','je hypotetick\u00fd spolo\u010dn\u00fd predok v\u0161etk\u00fdch organizmov','je in\u00e9 ozna\u010denie pre sinice','je nefunk\u010dn\u00fd g\u00e9n','je prabunka','je predchodca bakt\u00e9ri\u00ed, arche\u00f3nov a eukaryotov','mal v jednoduchej podobe rozvinut\u00e9 v\u0161etky vlastnosti \u017eivota','je prabunka, v ktorej pravdepodobne prebiehali procesy transl\u00e1cie DNA do RNA','je prabunka, v ktorej pravdepodobne prebiehali procesy replik\u00e1cie DNA',TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1472,'1472. Nevyhnutnou podmienkou na zabezpe\u010denie stability prvotn\u00fdch foriem \u017eivota:','bola fotosynt\u00e9za','bol vznik prv\u00fdch membr\u00e1n, ktor\u00e9 ohrani\u010dovali prvotn\u00e9 formy \u017eivota','bola produkcia vitam\u00ednov','bol vznik bi\u010d\u00edkov','bol vznik jadra','bolo ich ohrani\u010denie vo\u010di okoliu','bolo UV \u017eiarenie','bol vznik pohybov\u00fdch \u0161trukt\u00far',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1473,'1473. Hypot\u00e9za panspermie:','nerie\u0161i ot\u00e1zku vzniku \u017eivota v kozme','hovor\u00ed, \u017ee ka\u017ed\u00fd organizmus vznik\u00e1 z pohlavnej bunky','uva\u017euje o mimozemskom p\u00f4vode \u017eivota','hovor\u00ed o \u0161\u00edren\u00ed \u017eivota kozmick\u00fdm priestorom','je zavrhnut\u00e1 najnov\u0161\u00edmi vedeck\u00fdmi poznatkami','je st\u00e1le \u017eiv\u00e1 aj v\u010faka objavu mikroorganizmov, tzv. extr\u00e9mofilov','odpoved\u00e1 na ot\u00e1zku ako vznikol \u017eivot v kozme','uva\u017euje o pozemskom p\u00f4vode vzniku \u017eivota',TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1474,'1474. Ozna\u010dte spr\u00e1vnu odpove\u010f:','po\u010das evol\u00facie sa najprv vyvinuli eukaryotick\u00e9 organizmy','v\u0161etky dnes \u017eij\u00face organizmy maj\u00fa jedn\u00e9ho spolo\u010dn\u00e9ho predchodcu','d\u00f4sledkom fotosynt\u00e9zy cyanobakt\u00e9ri\u00ed bolo hromadenie s\u00edry v atmosf\u00e9re','prv\u00e9 mnohobunkov\u00e9 organizmy sa na Zemi objavili asi pred 700 mili\u00f3nmi rokov','evol\u00facia \u017eivota na bunkovej \u00farovni trvala na Zemi viac ako 3 miliardy rokov','arche\u00f3ny s\u00fa schopn\u00e9 \u017eivota pri teplot\u00e1ch a\u017e 113\u00b0C','riboz\u00fdm je katalyticky akt\u00edvna molekula DNA','prokaryotick\u00e9 bunky nikdy nevytv\u00e1raj\u00fa mnohobunkov\u00e9 organizmy',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1475,'1475. Eukaryotick\u00e9 bunky:','s\u00fa evolu\u010dne star\u0161ie ako prokaryotick\u00e9 bunky','vytv\u00e1raj\u00fa mnohobunkov\u00e9 organizmy','maj\u00fa jednoduch\u0161iu \u0161trukt\u00faru ako prokaryotick\u00e9 bunky','vznikli asi pred 1,7 a\u017e 2 miliardami rokov','nemaj\u00fa jadro ohrani\u010den\u00e9 od cytoplazmy','sa pova\u017euj\u00fa za chim\u00e9ry vzniknut\u00e9 splynut\u00edm viacer\u00fdch p\u00f4vodne samostatn\u00fdch prokaryotick\u00fdch buniek','obsahuj\u00fa okrem jadra aj \u010fal\u0161ie membr\u00e1nov\u00e9 organely','s\u00fa z evolu\u010dn\u00e9ho h\u013eadiska mlad\u0161ie ako prokaryotick\u00e9 bunky',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1476,'1476. Prokaryotick\u00e9 bunky:','nemaj\u00fa spolo\u010dn\u00e9ho v\u00fdvojov\u00e9ho predka s eukaryotick\u00fdmi bunkami','s\u00fa v\u00fdvojovo mlad\u0161ie ako eukaryotick\u00e9 bunky','maj\u00fa spolo\u010dn\u00e9ho v\u00fdvojov\u00e9ho predka s arche\u00f3nmi','sa vyvinuli z progenota','s\u00fa v\u00fdvojovo star\u0161ie ako eukaryotick\u00e9 bunky','obsahuj\u00fa okrem jadra aj \u010fal\u0161ie membr\u00e1nov\u00e9 organely','vytv\u00e1raj\u00fa mnohobunkov\u00e9 organizmy','nikdy nevytv\u00e1raj\u00fa mnohobunkov\u00e9 organizmy',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1477,'1477. V \u017eivo\u010d\u00ed\u0161nej r\u00ed\u0161i zara\u010fujeme \u010dloveka:','do triedy prim\u00e1tov','do \u010de\u013eade Hominidae','do radu Primates','do nad\u00e9e\u013eade hominidov','do nad\u010de\u013eade Hominoidea','do rodu Homo','do pod\u010de\u013eade hominidov','do pod\u010de\u013eade Homininae',FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1478,'1478. Proces antroposociogen\u00e9zy zah\u0155\u0148a tieto skupiny:','sapon\u00e1cia','sapient\u00e1cia','hominiz\u00e1cia','hominidiz\u00e1cia','homologiz\u00e1cia','homeopatia','logop\u00e9dia','antropol\u00f3gia',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1479,'1479. Pod\u010de\u013ea\u010f homininov charakterizuj\u00fa znaky:','mal\u00e1 mozgov\u00e1 \u010das\u0165 lebky','parabolick\u00e9 zubn\u00e9 obl\u00faky','va\u010d\u0161ia hlavica stehnovej kosti','ch\u00f4dza po v\u0161etk\u00fdch \u0161tyroch kon\u010datin\u00e1ch','ve\u013ek\u00e1 mozgov\u00e1 \u010das\u0165 lebky presahuj\u00faca 500 cm3','men\u0161ia a ten\u0161ia s\u00e1nka','kolmej\u0161ia tv\u00e1r so slab\u0161\u00edm vy\u010dnievan\u00edm \u010de\u013euste','mohutne vyvinut\u00e9 o\u010dn\u00e9 zuby',FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1480,'1480. Ozna\u010dte spr\u00e1vnu odpove\u010f:','proces z\u00e1vere\u010dnej f\u00e1zy v\u00fdvoja \u010dloveka sa naz\u00fdva antropogenetika','Homo sapiens sa vyvinul na americkom kontinente','odli\u0161n\u00e1 farba poko\u017eky u \u013eud\u00ed je adapt\u00e1ciou na podmienky prostredia','Homo sapiens sa vyvinul v Afrike','Homo habilis sa in\u00e1\u010d naz\u00fdva aj ako \u010dlovek vzpriamen\u00fd','\u010dlovek zru\u010dn\u00fd sa in\u00e1\u010d naz\u00fdva aj ako Homo erectus','\u010dlovek rozumn\u00fd sa vyvinul pred asi 200 a\u017e 170 tis\u00edc rokmi v Afrike','pri sapient\u00e1cii ide najm\u00e4 o zmeny s\u00favisiace s bip\u00e9diou',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1481,'1481. U hominidov ve\u013ek\u00fd mozog, mal\u00e9 zuby a trval\u00e1 bipedia s\u00fa charakteristick\u00e9 pre:','\u0161impanza','Australopithecus africanus','Australopithecus afarensis','Homo sapiens','Homo neanderthalensis','\u010dloveka zru\u010dn\u00e9ho','\u010dloveka rozumn\u00e9ho','Homo erectus',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1482,'1482. Ozna\u010dte spr\u00e1vnu odpove\u010f:','v \u00e1zijsk\u00fdch popul\u00e1ci\u00e1ch je \u017elt\u00fd odtie\u0148 poko\u017eky sp\u00f4soben\u00fd adapt\u00e1ciou na chladnej\u0161iu kl\u00edmu','v skupin\u00e1ch \u013eud\u00ed, ktor\u00e9 sa formovali v oblastiach s vysokou intenzitou slne\u010dn\u00e9ho \u017eiarenia, sa zn\u00ed\u017eil obsah pigmentu v ko\u017ei','v skupin\u00e1ch \u013eud\u00ed, ktor\u00e9 sa formovali v oblastiach s vysokou intenzitou slne\u010dn\u00e9ho \u017eiarenia, sa zv\u00fd\u0161il obsah pigmentu v ko\u017ei','v skupin\u00e1ch \u013eud\u00ed, ktor\u00e9 maj\u00fa p\u00f4vod v Eur\u00f3pe, sa m\u00f4\u017ee vyskytova\u0165 ru\u017eov\u00e1 ko\u017ea, blond vlasy a modr\u00e9 o\u010di','u obyvate\u013eov Eur\u00f3py do\u0161lo po\u010das v\u00fdvoja k \u010diasto\u010dnej strate melan\u00ednu v d\u00f4sledku men\u0161ieho mno\u017estva slne\u010dn\u00e9ho \u017eiarenia v tejto oblasti','u obyvate\u013eov Eur\u00f3py do\u0161lo po\u010das v\u00fdvoja k zv\u00fd\u0161eniu mno\u017estva melan\u00ednu v d\u00f4sledku men\u0161ieho mno\u017estva slne\u010dn\u00e9ho \u017eiarenia v tejto oblasti','v skupin\u00e1ch Fud\u00ed, ktor\u00e9 sa formovali v oblastiach s ni\u017e\u0161ou intenzitou slne\u010dn\u00e9ho \u017eiarenia, sa zn\u00ed\u017eil obsah pigmentu v ko\u017ei','pred vysok\u00fdmi d\u00e1vkami UV \u017eiarenia s\u00fa hlbok\u00e9 vrstvy ko\u017ee chr\u00e1nen\u00e9 zv\u00fd\u0161en\u00fdm obsahom melan\u00ednu',TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1483,'1483. U hominidov mal\u00fd mozog, ve\u013ek\u00e9 zuby a ob\u010dasn\u00e1 dvojnohos\u0165 s\u00fa charakteristick\u00e9 pre:','\u0161impanza','Australopithecus africanus','Australopithecus afarensis','Homo sapiens','Homo neanderthalensis','Homo habilis','Homo erectus','\u010dloveka zru\u010dn\u00e9ho',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1484,'1484. Term\u00edn ekol\u00f3gia zaviedol do biol\u00f3gie:','Pythagoras','E. Haeckel','J. D. Watson','F. H. C. Crick','F. H. Wilkins','L. D. Landau','L. C. Pauling','J. C. Kendrew',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1485,'1485. Ekologick\u00e9 optimum rastl\u00edn:','je maxim\u00e1lna hodnota faktora, ktorej sa rastlina dok\u00e1\u017ee prisp\u00f4sobi\u0165','je minim\u00e1lna hodnota faktora, ktorej sa rastlina dok\u00e1\u017ee prisp\u00f4sobi\u0165','s\u00fa hodnoty p\u00f4sobenia faktora, ke\u010f sa rastline najlep\u0161ie dar\u00ed','s\u00fa optim\u00e1lne podmienky pre rast a rozmno\u017eovanie rastl\u00edn','s\u00fa podmienky, kedy rastlina zastavuje rast a za\u010d\u00edna vysycha\u0165','s\u00fa podmienky, kedy rastliny r\u00fdchlo rast\u00fa a rozmno\u017euj\u00fa sa','m\u00f4\u017eu dosahova\u0165 len synantropn\u00e9 druhy rastl\u00edn','je pre v\u0161etky rastliny \u00fapine rovnak\u00e9',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1486,'1486. Ekologick\u00e1 valencia:','popisuje hustotu popul\u00e1cie','je rozsah tolerancie druhu','je pre v\u0161etky organizmy rovnak\u00e1','je ur\u010den\u00e1 m\u00ednimom a maximom intenzity faktora','je napr. euryvalencia, ak m\u00e1 amplit\u00fada a optimum faktora \u00fazke rozp\u00e4tie','je napr. stenovalencia, ak m\u00e1 amplit\u00fada a optimum faktora \u0161irok\u00e9 rozp\u00e4tie','je napr. euryvalencia, ak m\u00e1 amplit\u00fada a optimum faktora \u0161irok\u00e9 rozp\u00e4tie','je napr. stenovalencia, ak m\u00e1 amplit\u00fada a optimum faktora \u00fazke rozp\u00e4tie',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1487,'1487. Stenotopn\u00e9 \u017eivo\u010d\u00edchy:','\u017eij\u00fa na rozmanit\u00fdch stanovi\u0161tiach','s\u00fa napr. jaskynn\u00e9 druhy','s\u00fa napr. vysokohorsk\u00e9 druhy','maj\u00fa vyhranen\u00e9 n\u00e1roky na stanovi\u0161te','s\u00fa v\u017edy \u0161pecializovan\u00e9 len na ur\u010dit\u00fd druh potravy','maj\u00fa \u00fazku amplit\u00fadu tolerancie pre stanovi\u0161te','s\u00fa napr. v\u00fdlu\u010dne pr\u00fadomiln\u00e9 druhy','s\u00fa z\u00e1rove\u0148 aj eurypotentn\u00e9',FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1488,'1488. Ozna\u010dte spr\u00e1vnu odpove\u010f:','kozmopolitn\u00e9 druhy s\u00fa roz\u0161\u00edren\u00e9 najm\u00e4 v kozme','bioindik\u00e1tory s\u00fa viazan\u00e9 na ur\u010dit\u00fd typ biotopu','rastliny os\u00edd\u013euj\u00fa na Zemi dva podstatne odli\u0161n\u00e9 typy prostredia: suchozemsk\u00e9 a vodn\u00e9','biotop tvoria dve z\u00e1kladn\u00e9 zlo\u017eky: ne\u017eiv\u00e1 \u2014 biotick\u00e1 a \u017eiv\u00e1 \u2014 abiotick\u00e1','synantropn\u00e9 druhy rastl\u00edn s\u00fa \u010dasto roz\u0161irovan\u00e9 \u010dinnos\u0165ou \u010dloveka','symbi\u00f3za je vz\u0165ah, ke\u010f jeden organizmus rastie na \u00fakor toho druh\u00e9ho','rastlinn\u00e9 spolo\u010denstvo sa in\u00e1\u010d naz\u00fdva aj zoocen\u00f3za','naj\u010dastej\u0161\u00ed typ konkurencie u rastl\u00edn je mykor\u00edza',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1489,'1489. Priestorov\u00e9 rozmiestnenie jedincov v rastlinnej popul\u00e1cii:','sa tie\u017e naz\u00fdva aj priestorov\u00e1 \u0161trukt\u00fara popul\u00e1cie','vo vo\u013enej pr\u00edrode je zv\u00e4\u010d\u0161a pravideln\u00e9','sa tie\u017e naz\u00fdva aj abundancia','je disperzia popul\u00e1cie','m\u00f4\u017ee by\u0165 pravideln\u00e1, n\u00e1hodn\u00e1 a zhlukovit\u00e1','m\u00f4\u017ee by\u0165 rovnomern\u00e1, n\u00e1hodn\u00e1 a skupinov\u00e1','sa vyjadruje po\u010dtom jedincov na jednotku plochy','sa vyjadruje po\u010dtom jedincov na jednotku objemu',TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1490,'1490. Pri rastlin\u00e1ch rozli\u0161ujeme nasledovn\u00e9 typy interakci\u00ed:','epifytizmus','epif\u00fdza','alelopatia','homeopatia','symbi\u00f3za','parazitizmus','lichenizmus','darwinizmus',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1491,'1491. Rozli\u0161ujeme nasledovn\u00e9 typy vekov\u00fdch pyram\u00edd:','bojov\u00fd','inv\u00e1zny','synantropn\u00fd','zmie\u0161an\u00fd','norm\u00e1lny','ustupuj\u00faci','konkuren\u010dn\u00fd','terci\u00e1rny',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1492,'1492. Medzi hiavn\u00e9 typy rastlinn\u00fdch spolo\u010denstiev na \u00fazem\u00ed Slovenska patria:','spolo\u010denstv\u00e1 p\u00fa\u0161tnych porastov','spolo\u010denstv\u00e1 tropick\u00fdch pralesov','spolo\u010denstv\u00e1 opadav\u00fdch listnat\u00fdch a zmie\u0161an\u00fdch lesov','spolo\u010denstv\u00e1 ihli\u010dnat\u00fdch lesov','spolo\u010denstv\u00e1 l\u00fak, pasienkov a vresov\u00edsk','lemov\u00e9 \u2014 ekotonov\u00e9 spolo\u010denstv\u00e1','spolo\u010denstv\u00e1 porastov sav\u00e1n','spolo\u010denstv\u00e1 pr\u00e9riov\u00e9',FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1493,'1493. Medzi n\u00e1rodn\u00e9 parky na Slovensku patria:','N\u00e1rodn\u00fd park Tur\u010dianska planina','N\u00e1rodn\u00fd park Liptovsk\u00e1 Mara','Pieninsk\u00fd n\u00e1rodn\u00fd park','N\u00e1rodn\u00fd park Domica','N\u00e1rodn\u00fd park Poloniny','N\u00e1rodn\u00fd park Mal\u00e1 Fatra','N\u00e1rodn\u00fd park Mur\u00e1nska planina','N\u00e1rodn\u00fd park Slovensk\u00fd raj',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1494,'1494. Prekambrium:','je obdobie za\u010d\u00ednaj\u00face vznikom \u017eivo\u010d\u00edchov s pevnou telesnou schr\u00e1nkou','je obdobie za\u010d\u00ednaj\u00face vznikom \u017eivo\u010d\u00edchov s kostrou','je obdobie, ke\u010f nejestvovali \u017eivo\u010d\u00edchy s pevnou schr\u00e1nkou alebo kostrou','je obdob\u00edm najva\u010d\u0161ieho rozmachu trilobitov','trvalo dlh\u0161ie ako fanerozoikum','zah\u0155\u0148a tri \u00e9ry: paleozoikum, mezozoikum a kenozoikum','zah\u0155\u0148a prvohory, druhohory, tre\u0165ohory a \u0161tvrtohory','sa skon\u010dilo pribli\u017ene pred 600 mili\u00f3nmi rokov',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1495,'1495. Peri\u00f3da karb\u00f3n:','je s\u00fa\u010das\u0165ou paleozoika','je s\u00fa\u010das\u0165ou prvoh\u00f4r','dostala n\u00e1zov pod\u013ea toho, \u017ee vo vtedaj\u0161ej atmosf\u00e9re sa nahromadilo ve\u013ek\u00e9 mno\u017estvo uhl\u00edka \u2014 karb\u00f3nu','dostala svoj n\u00e1zov podFa toho, \u017ee sa po\u010das nej z mo\u010diarnych lesov vytvorili obrovsk\u00e9 mno\u017estv\u00e1 uhlia','je dobou, ke\u010f sa v mo\u010diaroch roz\u0161\u00edrili oboj\u017eiveln\u00edky, ktor\u00e9 sa stali z\u00e1kladom pre vznik prv\u00fdch r\u00fdb','sa in\u00e1\u010d naz\u00fdva aj vekom plazov','je obdob\u00edm, ke\u010f sa na s\u00fa\u0161i objavil aj hmyz','je obdob\u00edm rozvoja kreodontov',TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1496,'1496. Kreodonty:','\u017eili najm\u00e4 v druhohor\u00e1ch','\u017eili v kenozoiku','patria medzi plazy','patria medzi cicavce','s\u00fa m\u00e4so\u017erav\u00e9 dinosaury','sa \u017eivili v\u00fdlu\u010dne rastlinnou potravou','lovili bylino\u017erav\u00e9 cicavce','podobali sa na dne\u0161n\u00e9 ma\u010dkovit\u00e9, medve\u010fovit\u00e9 a vlkovit\u00e9 \u0161elmy',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1497,'1497. Prv\u00e9 prim\u00e1ty sa vyvinuli:','v \u010dase vzniku \u010dloveka','u\u017e v paleozoiku','v kenozoiku','v tre\u0165ohor\u00e1ch','v paleoc\u00e9ne','vo \u0161tvrtohor\u00e1ch','v plioc\u00e9ne','vo fanerozoiku',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1498,'1498. Ozna\u010dte nespr\u00e1vnu odpove\u010f:','proces antroposociogen\u00e9zy del\u00edme na 2 skupiny: hominiz\u00e1ciu a sapient\u00e1ciu','z\u00e1vere\u010dn\u00e1 f\u00e1za v\u00fdvoja \u010dloveka sa naz\u00fdva antroposociogen\u00e9za','pri sapient\u00e1cii ide o zmeny s\u00favisiace so vznikom rodu Homo','hominidi vznikli asi pred 80 mili\u00f3nmi rokov','pohyb po \u0161tyroch kon\u010datin\u00e1ch sa naz\u00fdva bipedia','v\u00fdvoj \u010dloveka sa riadil a riadi z\u00e1konitos\u0165ami evol\u00facie','pri sapient\u00e1cii ide o zv\u00e4\u010d\u0161ovanie mozgovej a zmen\u0161ovanie tv\u00e1rovej \u010dasti lebky \u010dloveka','ch\u00f4dza na dvoch kon\u010datin\u00e1ch sa ozna\u010duje ako bipedia',TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1499,'1499. Homeostatick\u00e9 mechanizmy v ekosyst\u00e9moch:','s\u00fa autoregula\u010dn\u00e9 mechanizmy','nikdy neovplyv\u0148uj\u00fa popul\u00e1cie','sp\u00f4sobuj\u00fa naru\u0161enie a rozvrat syst\u00e9mu','zabezpe\u010duj\u00fa vyv\u00e1\u017eenos\u0165 ekosyst\u00e9mu','maj\u00fa ur\u010dit\u00fa kapacitu','maj\u00fa neobmedzen\u00fa kapacitu','v\u00f4bec nep\u00f4sobia','ke\u010f sa ve\u013emi za\u0165a\u017eia, m\u00f4\u017ee d\u00f4js\u0165 a\u017e k rozvratu syst\u00e9mu',TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1500,'1500. Ekotop:','je spolo\u010denstvo organizmov, ktor\u00e9 tvoria biocen\u00f3zu','pozost\u00e1va aj z fyzik\u00e1lnych faktorov prostredia','pozost\u00e1va aj z klimatick\u00e9ho re\u017eimu','nie je stavebnou s\u00fa\u010das\u0165ou ekosyst\u00e9mu','pozost\u00e1va aj z geologick\u00e9ho podkladu','m\u00e1 ur\u010dit\u00fa etilitu','m\u00e1 ur\u010dit\u00fa sexilitu','sklad\u00e1 sa z producentov, konzumentov a reducentov',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
