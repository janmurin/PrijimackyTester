INSERT INTO BIOLOGIA VALUES(951,'951. Molekuly DNA, ktor\u00e9 nes\u00fa genetick\u00fa inform\u00e1ciu, s\u00fa usporiadan\u00e9:','do jadrov\u00fdch a jadierkov\u00fdch chromoz\u00f3mov a do mimojadrov\u00fdch chromoz\u00f3mov','do jadrov\u00fdch a do mimojadrov\u00fdch chromoz\u00f3mov','do jadrov\u00fdch chromoz\u00f3mov a do mitochondri\u00e1lneho chromoz\u00f3mu v prokaryotick\u00fdch bunk\u00e1ch','len do jadrov\u00fdch chromoz\u00f3mov','v line\u00e1rnom alebo kruhovom tvare','v eukaryotick\u00fdch bunk\u00e1ch do chromat\u00ednov\u00e9ho vl\u00e1kna','v mitochondri\u00e1ch v kruhovom tvare','v Golgiho apar\u00e1te v kruhovom tvare',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
