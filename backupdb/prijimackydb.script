INSERT INTO BIOLOGIA VALUES(653,'653. Glyk\u00e9mia je:','zhlukovanie \u010derven\u00fdch krviniek','porucha metabolizmu sacharidov','koncentr\u00e1cia gluk\u00f3zy v krvi','nedostatok gluk\u00f3zy v organizme','nadmern\u00e9 vylu\u010dovanie gluk\u00f3zy obli\u010dkami','krvn\u00e1 gluk\u00f3za','celkov\u00fd obsah sacharidov v erytrocytoch','jedn\u00fdm z parametrov pri diagnostike cukrovky',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
