INSERT INTO BIOLOGIA VALUES(107,'107. V jadrovom obale s\u00fa:','mikrotubuly','otvory','plazmodezmy','protoplasty','peptidoglykan','pekt\u00edny','lign\u00edn','jadrov\u00e9 p\u00f3ry',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(108,'108. Rastlinn\u00e1 bunka sa odli\u0161uje od \u017eivo\u010d\u00ed\u0161nej t\u00fdm, \u017ee m\u00e1:','mitochondrie','plastidy','riboz\u00f3my','vakuoly','zrnit\u00e9 endoplazmatick\u00e9 retikulum','bunkov\u00fa stenu','lyzoz\u00f3my','mikrotubuly',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(109,'109. Tylakoidy s\u00fa:','s\u00fa\u010das\u0165ou mitochondri\u00ed','v str\u00f3me chloroplastov','v cytoplazme v\u0161etk\u00fdch bakt\u00e9ri\u00ed','membr\u00e1nov\u00e9 organely eukaryotickej bunky','mal\u00e9 mech\u00fariky Golgiho apar\u00e1tu','s\u00fa\u010das\u0165ou chloroplastov','splo\u0161ten\u00e9 mech\u00fariky via\u017euce molekuly chlorofylu','v cytoplazme cyanobakt\u00e9ri\u00ed',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(110,'110. Bi\u010d\u00edky:','sa skladaj\u00fa z dev\u00e4\u0165 troj\u00edc mikrotubulov','maj\u00fa na povrchu plazmatick\u00fa membr\u00e1nu','s\u00fa zakotven\u00e9 v bunkovej stene','sa skladaj\u00fa z mikrotubulov','obsahuj\u00fa kolag\u00e9n','s\u00fa zakotven\u00e9 v baz\u00e1lnom teliesku','maj\u00fa na povrchu v\u017edy pru\u017en\u00fa pelikulu','s\u00fa pohybov\u00e9 organely niektor\u00fdch bakt\u00e9ri\u00ed a prvokov',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(111,'111. Bunky v\u0161etk\u00fdch \u017eivo\u010d\u00edchov:','s\u00fa eukaryotick\u00e9','v niektor\u00fdch pr\u00edpadoch prokaryotick\u00e9','maj\u00fa membr\u00e1nov\u00e9 organely','maj\u00fa lyzoz\u00f3my','maj\u00fa pelikulu','s\u00fa posp\u00e1jan\u00e9 plazmodezmami','obsahuj\u00fa melan\u00edn','sa rozmno\u017euj\u00fa priamym delen\u00edm',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
