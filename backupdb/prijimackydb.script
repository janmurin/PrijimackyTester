INSERT INTO BIOLOGIA VALUES(655,'655. Nedostato\u010dn\u00e1 zr\u00e1\u017eanlivos\u0165 \u013eudskej krvi m\u00f4\u017ee by\u0165 sp\u00f4soben\u00e1:','reakciou aglutin\u00ednu s cudz\u00edm aglutinog\u00e9nom','nedostatkom vitam\u00ednu B_12','nedostatkom vitam\u00ednu K','dedi\u010dn\u00fdm ochoren\u00edm - hemofiliou','nedostatkom \u017eeleza v organizme','dedi\u010dn\u00fdm ochoren\u00edm - albinizmom','chorobou s gonoz\u00f3mov\u00fdm typom dedi\u010dnosti','nedostato\u010dnou vazokonstrikciou ciev v mieste poranenia',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(656,'656. Pre udr\u017eanie zr\u00e1\u017eanlivosti \u013eudskej krvi je zo v\u0161etk\u00fdch vitam\u00ednov najviac potrebn\u00fd:','vitam\u00edn K, ktor\u00fd vznik\u00e1 aj \u010dinnos\u0165ou bakt\u00e9ri\u00ed v hrubom \u010dreve','vitam\u00edn E','vitam\u00edn B_1','vitam\u00edn K','fylochinon','vitam\u00edn B_12, ktor\u00fd vznik\u00e1 v tele z betakarot\u00e9nu','vitam\u00edn D, ktor\u00fd vznik\u00e1 aj \u010dinnos\u0165ou bakt\u00e9ri\u00ed v hrubom \u010dreve','tzv. antihepar\u00ednov\u00fd vitam\u00edn',TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(657,'657. Pri zr\u00e1\u017ean\u00ed krvi u \u010dloveka doch\u00e1dza k:','premene fibrinog\u00e9nu na fibrin','premene rozpustnej plazmatickej bielkoviny na nerozpustn\u00fd vl\u00e1knit\u00fd fibr\u00edn','premene fibr\u00ednu na fibrinog\u00e9n','premene tromb\u00ednu na fibr\u00edn za \u00fa\u010dasti fibrinog\u00e9nu','imunitnej reakcii medzi fibrinog\u00e9nom a fibr\u00ednom','aglutin\u00e1cii erytrocytov','sediment\u00e1cii erytrocytov','diaped\u00e9ze erytrocytov',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
