INSERT INTO BIOLOGIA VALUES(1194,'1194. Pri \u00faplnej dominancii odhadujeme relat\u00edvnu po\u010detnos\u0165 alel v rovnov\u00e1\u017enej popul\u00e1cii na z\u00e1klade:','odmocniny z hodnoty relat\u00edvnej po\u010detnosti jedincov s dominantn\u00fdm fenotypom','odmocniny z hodnoty relat\u00edvnej po\u010detnosti reces\u00edvnych homozygotov','odmocniny z hodnoty relat\u00edvnej po\u010detnosti heterozygotov','odmocniny z hodnoty relat\u00edvnej po\u010detnosti homozygotov a heterozygotov','s\u00fa\u010dinu po\u010detnosti dominantn\u00fdch alel','rozdielu po\u010detnost\u00ed jedincov s dominantn\u00fdm a reces\u00edvnym fenotypom','Hardyho-Weinbergovho z\u00e1lcona','zast\u00fapenia jedincov s heterozygotn\u00fdm genotypom',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1195,'1195. Ak\u00e9 bude genotypov\u00e9 zlo\u017eenie popul\u00e1cie po dvoch gener\u00e1ci\u00e1ch panmixie, ak v\u0161etci jedinci v\u00fdchodzej popul\u00e1cie mali genotyp Aa ?','v\u0161etci jedinci bud\u00fa heterozygoti Aa','50 % popul\u00e1cie bud\u00fa tvori\u0165 dominantn\u00ed homozygoti a 50 % reces\u00edvni homozygoti','50 % popul\u00e1cie bud\u00fa tvori\u0165 heterozygoti a 50 % homozygoti','\u0161tvrtinu popul\u00e1cie bud\u00fa tvori\u0165 reces\u00edvni homozygoti','AA : Aa : aa v pomere 1 : 2 :1','v popul\u00e1cii bud\u00fa prevl\u00e1da\u0165 dominantn\u00ed homozygoti','25% popul\u00e1cie bud\u00fa tvori\u0165 dominantn\u00ed homozygoti','25% popul\u00e1cie bud\u00fa tvori\u0165 heterozygoti',FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
