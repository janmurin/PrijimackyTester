INSERT INTO BIOLOGIA VALUES(116,'116. Tonoplast:','je na povrchu leukoplastov','je priepustn\u00e1 membr\u00e1na na povrchu vakuol','je polopriepustn\u00e1 membr\u00e1na na povrchu vakuol','je semipermeabiln\u00fd','je na povrchu lyzoz\u00f3mov','oba\u013euje vakuoly','je na povrchu transportn\u00fdch vezikulov','odde\u013euje obsah vakuol od cytoplazmy',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE)
