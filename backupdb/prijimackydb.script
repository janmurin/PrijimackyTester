INSERT INTO BIOLOGIA VALUES(770,'770. Po\u010das pohlavnej zrelosti dozrie v organizme \u017eeny pribli\u017ene:','6000 folikulov','1400 folikulov','1460 folikulov','5000 folikulov','2000 folikulov','1200 folikulov','450 folikulov','40 folikulov',FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(771,'771. Uvo\u013enenie vaj\u00ed\u010dka (ovul\u00e1cia) nast\u00e1va u \u017eeny spravidla:','po skon\u010den\u00ed uterinn\u00e9ho cyklu','medzi 14. \u2014 16. d\u0148om ovari\u00e1lneho cyklu','28. de\u0148 po za\u010diatku ovari\u00e1lneho cyklu','24. de\u0148 po za\u010diatku men\u0161trua\u010dn\u00e9ho cyklu','raz za lun\u00e1rny mesiac','po\u010das menopauzy','pravidelne','po\u010das men\u0161tru\u00e1cie',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(772,'772. \u013dudsk\u00fd Graafov folikul za norm\u00e1lnych okolnost\u00ed v \u010dase po uvo\u013enen\u00ed vaj\u00ed\u010dka:','sa odlupuje a nadob\u00fada hubovit\u00fa konzistenciu','sa m\u00f4\u017ee zmeni\u0165 na corpus luteum menstruationis','sa resorbuje','sa m\u00f4\u017ee zmeni\u0165 na corpus luteum graviditatis','prech\u00e1dza do menopuazy','produkuje horm\u00f3ny','m\u00e1 funkciu endokrinnej \u017e\u013eazy','okam\u017eite podlieha autolytick\u00fdm zmen\u00e1m',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE)
