INSERT INTO BIOLOGIA VALUES(1041,'1041. Pri monohybridizme s \u00faplnou dominanciou:','sa potomstvo \u0161tiepi v F_2 gener\u00e1cii na dominantn\u00fa formu znaku a reces\u00edvnu formu znaku vo fenotypovom \u0161tiepnom pomere 1 : 2 : 1','sa potomstvo \u0161tiepi v F_2 gener\u00e1cii na dominantn\u00fa formu znaku a reces\u00edvnu formu znaku vo fenotypovom \u0161tiepnom pomere 3 : 1','sa fenotypov\u00fd \u0161tiepny pomer v potomstve dvoch heterozygotov zhoduje s genotypov\u00fdm \u0161tiepnym pomerom 1 : 1','je potomstvo v F_2 gener\u00e1cii uniformn\u00e9','v F_2 gener\u00e1cii vznikaj\u00fa dve fenotypov\u00e9 skupiny','je potomstvo v F_1 gener\u00e1cii uniformn\u00e9 fenotypovo, ale nie genotypovo','v F_2 gener\u00e1cii prevl\u00e1daj\u00fa jedinci s dominantn\u00fdm fenotypom','sa potomstvo \u0161tiepi v F_1 gener\u00e1cii na dominantn\u00fa formu znaku a reces\u00edvnu formu znaku vo fenotypovom \u0161tiepnom pomere 3 : 1',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
