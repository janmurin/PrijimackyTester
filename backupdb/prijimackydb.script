INSERT INTO BIOLOGIA VALUES(562,'562. Prv\u00e9 dva kr\u010dn\u00e9 stavce \u010dloveka s\u00fa:','nosi\u010d, ktor\u00fd m\u00e1 telo a \u010dapovec, ktor\u00fd nem\u00e1 telo','nosi\u010d, ktor\u00e9ho telo vybieha smerom hore do zuba a \u010dapovec','atlas, ktory nem\u00e1 telo a \u010dapovec','nosi\u010d, ktor\u00fd nem\u00e1 telo a \u010dapovec','nosi\u010d a \u010dapovec, ktor\u00e9 maj\u00fa telo, obl\u00fak a v\u00fdbe\u017eky','tvarovo v\u00fdrazne odli\u0161n\u00e9 od ostatn\u00fdch stavcov','nosi\u010d a \u010dapovec','nosi\u010d a \u010dapovec, medzi nimi je chrupkov\u00e1 platni\u010dka',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(563,'563. Po\u010det kr\u010dn\u00fdch stavcov u \u010dloveka je:','7, na bo\u010dn\u00fdch v\u00fdbe\u017ekoch maj\u00fa k\u013abov\u00e9 jamky na k\u013abov\u00e9 spojenie s rebrami','7','7, prv\u00e9 dva umo\u017e\u0148uj\u00fa ot\u00e1\u010danie hlavy do str\u00e1n a k\u00fdvav\u00e9 pohyby hore a dolu','7, prv\u00fd a druh\u00fd maj\u00fa netypick\u00fa stavbu','7, v\u0161etky maj\u00fa telo, obl\u00fak a v\u00fdbe\u017eky','7, medzi v\u0161etk\u00fdmi s\u00fa medzistavcov\u00e9 platni\u010dky','7, \u010dapovec sa od ostatn\u00fdch odli\u0161uje t\u00fdm, \u017ee nem\u00e1 telo','7, nosi\u010d sa od ostatn\u00fdch odli\u0161uje t\u00fdm, \u017ee m\u00e1 len telo a predn\u00fd a zadn\u00fd obl\u00fak',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(564,'564. Ktor\u00e9 stavce v chrbtici \u010dloveka s\u00fa zrasten\u00e9:','nosi\u010d a \u010dapovec','kostr\u010dov\u00e9','kr\u00ed\u017eov\u00e9','4. a 5. hrudn\u00edkov\u00fd','driekov\u00e9 po 20. roku','kr\u00ed\u017eov\u00e9, 2. a\u017e 5. zrastaj\u00fa medzi 14. a\u017e 20. rokom, prv\u00fd prirast\u00e1 medzi 20. a\u017e 30. rokom','5 kr\u00ed\u017eov\u00fdch do Icr\u00ed\u017eovej kosti a 4 - 5 kostr\u010dov\u00fdch do kostr\u010de','v starobe v d\u00f4sledku osteopor\u00f3zy aj hrudn\u00edkov\u00e9',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE)
