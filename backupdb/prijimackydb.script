INSERT INTO BIOLOGIA VALUES(592,'592. Z\u00e1kladnou jednotkou kostrov\u00e9ho svalu je:','prie\u010dne pruhovan\u00e9 svalov\u00e9 vl\u00e1kno','svalov\u00fd snop\u010dek','jednojadrov\u00e9 svalov\u00e9 vl\u00e1kno','mnohojadrov\u00e9 svalov\u00e9 vl\u00e1kno','myofibrila','hlava, bru\u0161ko a chvost','akt\u00edn a myoz\u00edn','sarcolemma',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(593,'593. Prie\u010dne pruhovan\u00e9 svaly sa up\u00ednaj\u00fa:','na hladk\u00e9 svaly','na kosti','na \u0161\u013eachy','na kostru tak, \u017ee sval premos\u0165uje jeden alebo viac k\u013abov','ku kostiam priamo','ku kostiam prostredn\u00edctvom v\u00e4zivov\u00fdch pruhov','na kostru tak, \u017ee sval premos\u0165uje iba jeden k\u013ab','na chrupky',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(594,'594. Ktor\u00fd sval v tele \u010dloveka m\u00e1 osobitne zvl\u00e1\u0161tne morfologick\u00e9 a funk\u010dn\u00e9 postavenie:','myokard','myokard, preto\u017ee funk\u010dne aj morfologicky sp\u00e1ja v sebe vlastnosti hladkej aj prie\u010dne pruhovanej svaloviny','myokard, preto\u017ee je dut\u00fd','srdcov\u00fd','epikard \u2014 \u0161pecifick\u00e1 svalovina, ktor\u00e1 zabezpe\u010duje \u010dinnos\u0165 srdca','osrdcovn\u00edk \u2014 zvl\u00e1\u0161tny prie\u010dne pruhovan\u00fd sval','myokard, preto\u017ee je \u0161trukt\u00farou podobn\u00fd kostrov\u00e9mu svalu a funkciou hladk\u00e9mu svalu','srdcov\u00fd, preto\u017ee je \u0161trukt\u00farou podobn\u00fd hladk\u00e9mu svalu a funkciou kostrov\u00e9mu svalu',TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(595,'595. Svalov\u00fd snop\u010dek \u010dloveka pozost\u00e1va:','z 10 a\u017e 100 jednojadrov\u00fdch svalov\u00fdch vl\u00e1kien','z 10 a\u017e 100 mnohojadrov\u00fdch svalov\u00fdch vl\u00e1kien','z 10 a\u017e 100 myofibr\u00edl','z hlavy, bru\u0161ka a chvosta','z 10 - 100 vl\u00e1kien','z jednolomn\u00fdch akt\u00ednov\u00fdch a dvojlomn\u00fdch myoz\u00ednov\u00fdch myofibr\u00edl','z 10 a\u017e 100 snopcov','z 10 a\u017e 100 mnohojadrov\u00fdch svalov\u00fdch vl\u00e1kien, ktor\u00e9 pracuj\u00fa antagonisticky alebo synergisticky',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(596,'596. Ak\u00fa \u010das\u0165 hmotnosti tela tvor\u00ed hmotnos\u0165 svalov \u010dloveka:','u mu\u017eov priemerne 36 % hmotnosti tela','u \u017eien priemerne 32 % hmotnosti tela','u \u017eien priemerne 36 % hmotnosti tela','u mu\u017eov priemerne 32 % hmotnosti','u oboch pohlav\u00ed je podiel rovnak\u00fd','premenlivo pod\u013ea sp\u00f4sobu \u017eivota','individu\u00e1lne pod\u013ea danost\u00ed jedinca','u mu\u017eov \u010dasto v\u00e4\u010d\u0161iu \u010das\u0165 ako u \u017eien',TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
