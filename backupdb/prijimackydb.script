INSERT INTO BIOLOGIA VALUES(1227,'1227. Dedi\u010dnos\u0165 znakov u \u010dloveka m\u00f4\u017eeme sk\u00fama\u0165:','cestou pokusn\u00e9ho kr\u00ed\u017eenia','pou\u017eit\u00edm genealogickej met\u00f3dy, tj. \u0161t\u00fadiom rodokme\u0148ov','u oboch \u010dlenov man\u017eelsk\u00e9ho p\u00e1ru, ide o tzv. gemelologick\u00fd v\u00fdskum','aj pomocou \u0161t\u00fadia dvoj\u010diat','met\u00f3dou anal\u00fdzy DNA','uplatnen\u00edm selek\u010dn\u00fdch a eugenick\u00fdch pr\u00edstupov','s vyu\u017eit\u00edm pozorovac\u00edch met\u00f3d namiesto experiment\u00e1lnych','met\u00f3dami popula\u010dn\u00e9ho v\u00fdskumu',FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1228,'1228. Ozna\u010dte spr\u00e1vne priraden\u00e9 genotypy ku fenotypom pri krvnoskupinovom syst\u00e9me AB0 u \u010dloveka (geno - fenotyp):','ii \u2014 A alebo B','ii \u2014 0','I^AI^B \u2014 A alebo B','I^Ai,I^AI^A \u2014 A','I^AI \u2014 AB','I^Bi \u2014 B','I^Ai \u2014 A alebo 0','I^BI^B, I^Bi - B',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1229,'1229. Ak maj\u00fa traja s\u00farodenci krvn\u00fa skupinu A, B, AB, ich rodi\u010dia m\u00f4\u017eu ma\u0165 krvn\u00fa skupinu:','A a AB','B a AB','0 a B','AB a AB','A a B','0 a AB','A a 0','0 a 0',TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1230,'1230. Ak maj\u00fa obaja rodi\u010dia krvn\u00fa skupinu AB, ich deti m\u00f4\u017eu ma\u0165 krvn\u00e9 skupiny:','A, B, AB s rovnakou pravdepodobnos\u0165ou','A s pravdepodobnos\u0165ou 25 %','AB s pravdepodobnos\u0165ou 50 %','A, B, 0, AB','A aj B s pravdepodobnos\u0165ou 50 %','A alebo B s pravdepodobnos\u0165ou 50 %','0 s pravdepodobnos\u0165ou 25 %','len AB',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1231,'1231. Ak s\u00fa rodi\u010dia homozygotn\u00ed pre krvn\u00e9 skupiny A a B, ich deti m\u00f4\u017eu ma\u0165 krvn\u00fa skupinu:','B s pravdepodobnos\u0165ou 50 %','A s pravdepodobnos\u0165ou 50 %','A, B alebo AB s rovnakou pravdepodobnos\u0165ou','len AB','ur\u010den\u00fa reces\u00edvnymi alelami','AB s pravdepodobnos\u0165ou 50 %,','B s pravdepodobnos\u0165ou 25 %','0 s pravdepodobnos\u0165ou 25 %',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1232,'1232. Pr\u00edkladom autoz\u00f3movo reces\u00edvnej choroby u \u010dloveka je:','neur\u00f3za','polydaktylia','r\u00e1z\u0161tepov\u00e9 ochorenie','porucha \u0161trukt\u00fary enz\u00fdmu \u0161tiepiaceho cukor galakt\u00f3zu - galaktoz\u00e9mia','porucha tvorby enz\u00fdmov','Downov syndr\u00f3m','hemof\u00edlia','alergia',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1233,'1233. Dedi\u010dnos\u0165 krvnoskupinov\u00e9ho syst\u00e9mu AB0 je pr\u00edkladom:','polyg\u00e9nnej dedi\u010dnosti','mendelovskej (monog\u00e9nnej) dedi\u010dnosti','heterochromoz\u00f3movej dedi\u010dnosti','famili\u00e1rnej dispoz\u00edcie','dedi\u010dnosti kvalitat\u00edvnych znakov','kvantitat\u00edvnej dedi\u010dnosti','gonoz\u00f3movej dedi\u010dnosti','autoz\u00f3movej dedi\u010dnosti',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1234,'1234. Rodi\u010dom s krvnou skupinou AB sa m\u00f4\u017ee narodi\u0165 die\u0165a s krvou skupinou:','A','A, B, AB alebo 0','B','AB','0','AB alebo 0','A, AB alebo B','iba takou ako maj\u00fa rodi\u010dia',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1235,'1235. Matka m\u00e1 krvn\u00fa skupinu A a otec m\u00e1 krvn\u00fa skupinu B. Ich potomkovia:','m\u00f4\u017eu ma\u0165 v\u0161etky typy krvn\u00fdch skup\u00edn','nem\u00f4\u017eu ma\u0165 krvn\u00fa skupinu 0','nem\u00f4\u017eu ma\u0165 krvn\u00fa skupinu AB','dc\u00e9ry m\u00f4\u017eu ma\u0165 len krvn\u00fa skupinu B ako otec, lebo ide o dedi\u010dnos\u0165 kr\u00ed\u017eom','synovia m\u00f4\u017eu ma\u0165 len krvn\u00fa skupinu A ako matka, lebo ide o dedi\u010dnos\u0165 kr\u00ed\u017eom','m\u00f4\u017eu ma\u0165 krvn\u00fa skupinu A, B, AB alebo 0','m\u00f4\u017eu ma\u0165 krvn\u00fa skupinu A','m\u00f4\u017eu ma\u0165 krvn\u00fa skupinu AB',TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1236,'1236. Ak m\u00e1 jeden z rodi\u010dov krvn\u00fa skupinu 0 a druh\u00fd A, m\u00f4\u017ee sa im narodi\u0165 potomok s krvnou skupinou:','B','AB','A','0','A alebo B','A alebo AB','A alebo 0','B alebo 0',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1237,'1237. Ak maj\u00fa obaja rodi\u010dia krvn\u00fa skupinu 0, ich potomkovia:','m\u00f4\u017eu ma\u0165 krvn\u00fa skupinu A','m\u00f4\u017eu ma\u0165 krvn\u00e9 skupiny A, B, AB alebo 0','m\u00f4\u017eu ma\u0165 len tak\u00fa krvn\u00fa skupinu ako maj\u00fa ich rodi\u010d\u00eda','nem\u00f4\u017eu ma\u0165 krvn\u00fa skupinu 0','m\u00f4\u017eu ma\u0165 len krvn\u00fa skupinu 0','m\u00f4\u017eu ma\u0165 krvn\u00fa skup\u00ednu A, ak aspon jeden zo star\u00fdch rodi\u010dov mal krvn\u00fa skupinu A','m\u00f4\u017eu ma\u0165 krvn\u00fa skupinu B','nem\u00f4\u017eu ma\u0165 krvn\u00e9 skupiny A, B alebo AB',FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1238,'1238. Ozna\u010dte nespr\u00e1vne priraden\u00e9 genotypy ku fenotypom pri krvnoskupinovom syst\u00e9me AB0 u \u010dloveka (genotyp - fenotyp):','I \u2014 A alebo 0','I^AI^B \u2014 A alebo B','ii \u2014 A,B','I^Bi \u2014 B','I^BI^B, I^Bi \u2014 B','ii \u2014 0','I^AI^B \u2014 AB','I^Ai, I^AI^A \u2014 A alebo 0',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1239,'1239. Otec a syn s\u00fa hemofilici. Pravdepodobnos\u0165, \u017ee syn zdedil reces\u00edvnu alelu pre tento znak od otca, je:','25%','100%','75%','50%','0%','rovnak\u00e1 ako pre dc\u00e9ry tohto otca','odli\u0161n\u00e1 ako pre dc\u00e9ry tohto otca','rovnak\u00e1 ako pre v\u0161etk\u00fdch potomkov tohto otca bez oh\u013eadu na pohlavie potomka',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1240,'1240. Predpokladajme, \u017ee farba o\u010d\u00ed je podmienen\u00e1 jedn\u00fdm g\u00e9nom s dvoma alelami, pri\u010dom hned\u00e1 farba o\u010d\u00ed je dominantn\u00e1 nad modrou. Pravdepodobnos\u0165, \u017ee sa dvom hnedook\u00fdm rodi\u010dom narod\u00ed modrook\u00e9 die\u0165a, je:','0%, ak s\u00fa obaja rodi\u010dia homozygoti','100%','50%, ak s\u00fa obaja rodi\u010dia heterozygoti','25%, ak s\u00fa obaja rodi\u010dia heterozygoti','75%, ak jeden z rodi\u010dov je dominantn\u00fd homozygot a druh\u00fd heterozygot','25%, ak jeden z rodi\u010dov matky a jeden z rodi\u010dov otca mal modr\u00e9 o\u010di','50%, ak jeden z rodi\u010dov matky a jeden z rodi\u010dov otca mal modr\u00e9 o\u010di','0%, za \u017eiadnych okolnost\u00ed sa im nem\u00f4\u017ee narodi\u0165 modrook\u00e9 die\u0165a',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1241,'1241. Pri dedi\u010dn\u00fdch ochoreniach:','sa uplat\u0148uje len dedi\u010dn\u00e1 premenlivos\u0165 g\u00e9nov ve\u013ek\u00e9ho \u00fa\u010dinku','sa uplat\u0148uje len dedi\u010dn\u00e1 premenlivos\u0165 g\u00e9nov mal\u00e9ho \u00fa\u010dinku','sa uplat\u0148uje v\u017edy dedi\u010dn\u00e1 a nededi\u010dn\u00e1 premenlivos\u0165','sa neuplat\u0148uje nededi\u010dn\u00e1 premenlivos\u0165','uplatnenie dedi\u010dnej a nededi\u010dnej premenlivosti z\u00e1vis\u00ed od genetickej determin\u00e1cie ochorenia','sa uplat\u0148uj\u00fa mut\u00e1cie r\u00f4zneho typu','v\u017edy platia Mendelove z\u00e1kony dedi\u010dnosti','sa uplat\u0148uj\u00fa aj mut\u00e1cie mitochondri\u00e1lnej DNA',FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1242,'1242. \u0160t\u00fadium dedi\u010dnosti \u010dloveka vyu\u017e\u00edva:','genetick\u00e9 a biochemick\u00e9 met\u00f3dy','anal\u00fdzu kr\u00ed\u017een\u00ed \u010dist\u00fdch l\u00edni\u00ed','rodokme\u0148ov\u00fa met\u00f3du','testovacie kr\u00ed\u017eenie','\u0161t\u00fadium karyotypu','met\u00f3dy popula\u010dnej genetiky','genetick\u00fa selekciu','gemelologick\u00fd v\u00fdskum',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1243,'1243. \u0160t\u00fadium \u013eudsk\u00e9ho karyotypu:','je s\u00fa\u010das\u0165ou lek\u00e1rskej cytogenetiky','je vhodn\u00e1 met\u00f3da diagnostilcy g\u00e9nov\u00fdch mut\u00e1ci\u00ed','vyu\u017e\u00edva \u0161peci\u00e1lne farbiace met\u00f3dy','umo\u017e\u0148uje diagnostiku viacer\u00fdch dedi\u010dn\u00fdch ochoren\u00ed','je mo\u017en\u00e9 aj v prenat\u00e1lnom \u0161t\u00e1diu ontogen\u00e9zy','sa vyu\u017e\u00edva v genetickom skr\u00edningu niektor\u00fdch ochoren\u00ed','sa vyu\u017e\u00edva pri diagnostike polyg\u00e9nne podmienen\u00fdch ochoren\u00ed','je mo\u017en\u00e9 len u dospel\u00fdch jedincov',TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1244,'1244. Molekulov\u00e1 DNA diagnostika:','sa vyu\u017e\u00edva najm\u00e4 pri diagnostike polyg\u00e9nnych ochoren\u00ed','sa vyu\u017e\u00edva najm\u00e4 pri diagnostike monog\u00e9nnych ochoren\u00ed','m\u00f4\u017ee by\u0165 vykonan\u00e1 u\u017e v prenat\u00e1lnom \u0161t\u00e1diu v\u00fdvinu','sa vyu\u017e\u00edva pri diagnostike n\u00e1dorov\u00fdch ochoren\u00ed','sa vyu\u017e\u00edva najm\u00e4 pri diagnostike nededi\u010dn\u00fdch ochoren\u00ed','nie je pou\u017eite\u013en\u00e1 pre diagnostiku dedi\u010dn\u00fdch ochoren\u00ed viazan\u00fdch na pohlavn\u00e9 chromoz\u00f3my','umo\u017e\u0148uje ur\u010di\u0165 dedi\u010dn\u00e9 vlohy v ktoromko\u013evek \u0161t\u00e1diu ontogen\u00e9zy','je synonymum pre g\u00e9nov\u00fa terapiu',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1245,'1245. \u013dudsky gen\u00f3m obsahuje pribli\u017ene:','150 000 \u2014 200 000 g\u00e9nov','10^12 p\u00e1rov nukleotidov','300 000 \u2014 400 000 g\u00e9nov','3 x 10^9 p\u00e1rov nukleotidov','34 mili\u00f3nov nukleotidov','1 500 g\u00e9nov','viac ne\u017e 20 000 g\u00e9nov','3 x 10^6 p\u00e1rov nukleotidov',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1246,'1246. Genetick\u00e9 riziko pr\u00edbuzenskych sob\u00e1\u0161ov spo\u010d\u00edva:','vo zv\u00fd\u0161en\u00ed po\u010dtu mutantov a mut\u00e1ci\u00ed','vo zv\u00fd\u0161en\u00ed pravdepodobnosti narodenia reces\u00edvnych homozygotov s dedi\u010dn\u00fdm ochoren\u00edm','vo zv\u00fd\u0161en\u00ed por\u00fach meiotick\u00e9ho delenia','v zhor\u0161ovan\u00ed karyotypu','vo zv\u00fd\u0161en\u00ed rizika v\u00fdskytu reces\u00edvnych ochoren\u00ed v potomstve','vo zv\u00fd\u0161en\u00ed rizika v\u00fdskytu spont\u00e1nnych potratov','vo zv\u00fd\u0161en\u00ed v\u00fdskytu dominantne podmienen\u00fdch ochoren\u00ed v potomstve','vo zv\u00fd\u0161enej pravdepodobnosti genetick\u00e9ho po\u0161kodenia die\u0165a\u0165a',FALSE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1247,'1247. vo zv\u00fd\u0161enej pravdepodobnosti genetick\u00e9ho po\u0161kodenia die\u0165a\u0165a','m\u00e1 za cie\u013e lie\u010di\u0165 dedi\u010dn\u00e9 ochorenia','vyu\u017e\u00edva met\u00f3dy rodokme\u0148ovej anal\u00fdzy','m\u00e1 za cier doporu\u010di\u0165 postupy, ktor\u00e9 vylu\u010duj\u00fa alebo zni\u017euj\u00fa riziko prenosu dedi\u010dn\u00fdch ochoren\u00ed','m\u00e1 za cie\u013e stanovi\u0165 progn\u00f3zu rizika pre potomstvo','m\u00e1 za cie\u013e diagnostikova\u0165 genetick\u00e9 ochorenie','vyu\u017e\u00edva met\u00f3dy kr\u00ed\u017eenia','m\u00f4\u017ee zah\u0155\u0148a\u0165 aj cytogenetick\u00fa diagnostiku','m\u00e1 v\u00fdznam pri prevencii pred infek\u010dn\u00fdmi chorobami',FALSE,TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1248,'1248. G\u00e9nov\u00e9 in\u017einierstvo sa zaober\u00e1:','identifik\u00e1ciou os\u00f4b','anal\u00fdzou karyotypov','\u0161\u013eachten\u00edm zvierat a rastl\u00edn','tvorbou hybridn\u00fdch molek\u00fal DNA a ich klonovan\u00edm','technol\u00f3giou vytv\u00e1rania rekombinantn\u00fdch molek\u00fal DNA','\u0161tiepen\u00edm izolovan\u00fdch molek\u00fal DNA na kr\u00e1tke \u00faseky pomocou restrik\u010dn\u00fdch endonukle\u00e1z','\u0161tiepen\u00edm izolovan\u00fdch molek\u00fal DNA na kr\u00e1tke \u00faseky pomocou restrik\u010dn\u00fdch polymer\u00e1z','za\u010dle\u0148ovan\u00edm izolovan\u00fdch g\u00e9nov do vhodn\u00fdch molek\u00fal DNA',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1249,'1249. Biotechnol\u00f3gie:','sa zaoberaj\u00fa pestovan\u00edm buniek in vitro','sa zaoberaj\u00fa pestovan\u00edm buniek in vivo','s\u00fa aplik\u00e1ciou g\u00e9nov\u00e9ho in\u017einierstva v praxi','vyu\u017e\u00edvaj\u00fa g\u00e9nov\u00e9 manipul\u00e1cie na v\u00fdrobu nebiologick\u00fdch prepar\u00e1tov','vyu\u017e\u00edvaj\u00fa bakteri\u00e1lne bunky na produkciu biologick\u00fdch prepar\u00e1tov','vyu\u017e\u00edvaj\u00fa met\u00f3dy kon\u0161trukcie hybridn\u00fdch molek\u00fal DNA','sa zaoberaj\u00fa terapiou monog\u00e9nne podmienen\u00fdch ochoren\u00ed','vyu\u017e\u00edvaj\u00fa umelo vytvoren\u00e9 organizmy na produkciu napr. horm\u00f3nov, vakc\u00edn, protil\u00e1tok',FALSE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1250,'1250. Rekombinantn\u00e1 DNA vznik\u00e1:','za\u010dlenen\u00edm izolovanej DNA do cudzej molekuly DNA','pri rekombina\u010dnom kr\u00ed\u017een\u00ed','pri mitotickom delen\u00ed','v d\u00f4sledku rekombin\u00e1ci\u00ed v priebehu mei\u00f3zy','u bakt\u00e9ri\u00ed pri pohlavnom rozmno\u017eovan\u00ed','g\u00e9nov\u00fdmi manipul\u00e1ciami','v S f\u00e1ze bunkov\u00e9ho cyklu','ako v\u00fdsledok crossing-overu',TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1251,'1251. Sinice:','nemaj\u00fa jadro obalen\u00e9 cytoplazmatickou membr\u00e1nou','s\u00fa autotrofn\u00e9 organizmy','rozmno\u017euj\u00fa sa len pohlavne','maj\u00fa line\u00e1rnu DNA','maj\u00fa kruhov\u00fa DNA','rozmno\u017euj\u00fa sa len nepohlavne','s\u00fa heterotrofn.\u00e9 organizmy','obsahuj\u00fa modrozelen\u00e9 farbivo fykocyan\u00edn',TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1252,'1252. Ozna\u010dte spr\u00e1vne zoraden\u00e9 (od najvy\u0161\u0161ej po najni\u017e\u0161iu) systematick\u00e9 jednotky:','trieda, r\u00ed\u0161a, \u010de\u013ea\u010f, druh,rod','rad, \u010de\u013ea\u010f, rod, druh','rad, oddelenie, trieda,druh','r\u00ed\u0161a, podr\u00ed\u0161a, oddelenie, trieda','oddelenie, trieda, rod, rad','rod, druh, \u010de\u013ea\u010f, rad','oddelenie, trieda, rad, \u010de\u013ea\u010f','\u010de\u013ea\u010f, rad, trieda, oddelenie',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1253,'1253. Riasy sa rozmno\u017euj\u00fa:','nepohlavne','r\u00f4znymi \u010das\u0165ami stielky','reduk\u010dn\u00fdm delen\u00edm','pu\u010dan\u00edm','prie\u010dnym delen\u00edm','mei\u00f3zou','mit\u00f3zou','vegetat\u00edvne',TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1254,'1254. \u010cerven\u00e9 riasy (Rhodophyta):','charakterizuje \u010derven\u00e9 farbivo - fukoxant\u00edn','sa vyu\u017e\u00edvaj\u00fa na produkciu antibiot\u00edk','sa vyu\u017e\u00edvaj\u00fa na produkciu agaru','sa vyu\u017e\u00edvaj\u00fa na produkciu l\u00e1tok vyu\u017e\u00edvan\u00fdch v mikrobiol\u00f3gii','patria medzi vy\u0161\u0161ie rastliny','patria medzi ni\u017e\u0161ie rastliny','charakterizuje \u010derven\u00e9 farbivo - fykoerytr\u00edn','s\u00fa zast\u00fapen\u00e9 predov\u0161etk\u00fdm chaluhami',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1255,'1255. Gametofyt:','prevl\u00e1da nad sporofytom u semenn\u00fdch rastl\u00edn','v bunk\u00e1ch obsahuje 2n chromoz\u00f3mov','stratil u semenn\u00fdch rastl\u00edn svoju fyziologick\u00fa samostatnos\u0165','sa od sporofytu l\u00ed\u0161i po\u010dtom chromoz\u00f3mov','sa od sporofytu nel\u00ed\u0161i po\u010dtom chromoz\u00f3mov','sa od sporofytu l\u00ed\u0161i len morfologicky','prevl\u00e1da nad sporofytom u machorastov','sa od sporofytu l\u00ed\u0161i len fyziologicky',FALSE,FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1256,'1256. Ozna\u010dte spr\u00e1vnu odpove\u010f:','pri izogamii spl\u00fdvaj\u00fa dve gam\u00e9ty rozdielneho tvaru a ve\u013ekosti','pri anizogamii spl\u00fdvaj\u00fa dve gam\u00e9ty rozdielneho tvaru a ve\u013ekosti','pri oogamii spl\u00fdvaj\u00fa dve pohybliv\u00e9 bunky rozdielneho tvaru a ve\u013ekosti','pri oogamii spl\u00fdva v\u00e4\u010d\u0161ia nepohybliv\u00e1 gam\u00e9ta s men\u0161ou pohyblivou gam\u00e9tou','gametofyt zabezpe\u010duje tvorbu sp\u00f3r','rodozmena je striedanie pohlavnej a nepohlavnej gener\u00e1cie','gametofyt zabezpe\u010duje tvorbu pohlavn\u00fdch buniek','pri izogamii spl\u00fdvaj\u00fa dve gam\u00e9ty rovnak\u00e9ho tvaru a ve\u013ekosti',FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1257,'1257. \u010cerven\u00e1 o\u010dn\u00e1 \u0161kvrna:','je charakteristick\u00e1 pre eugl\u00e9ny','obsahuje \u0161krob','reaguje na smer a intenzitu dopadaj\u00faceho svetla','je charakteristick\u00e1 pre sp\u00e1javky','je charakteristick\u00e1 pre chaluhy','le\u017e\u00ed v jadre','je charakteristick\u00e1 pre rozsievky','le\u017e\u00ed v cytoplazme',TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1258,'1258. Zelen\u00e9 riasy sa rozde\u013euj\u00fa na triedy:','zelen\u00e9 bi\u010d\u00edkovce, vl\u00e1knit\u00e9 zelen\u00e9 riasy, bunkov\u00e9 zelen\u00e9 riasy','vlastn\u00e9 zelen\u00e9 riasy a zelen\u00e9 bi\u010d\u00edkovce','chaluhy a chary','Chlorophyceae, Conjugatophyceae, Charophyceae','rozsievky a hned\u00e9 riasy','vlastn\u00e9 zelen\u00e9 riasy, sp\u00e1javky a chary','kryptomon\u00e1dy a panciernatky','chary a sp\u00e1javky',FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1259,'1259. Riasy:','s\u00fa autotrofn\u00e9 organizmy','patria medzi eukaryotick\u00e9 organizmy','patria medzi prokaryotick\u00e9 organizmy','zara\u010fujeme medzi Cormobionta','zara\u010fujeme medzi Thallobionta','zara\u010fujeme medzi ni\u017e\u0161ie rastl\u00edny','neobsahuj\u00fa \u017eiadne membr\u00e1nov\u00e9 organely','s\u00fa v\u00fdznamnou s\u00fa\u010das\u0165ou zooplankt\u00f3nu',TRUE,TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1260,'1260. Chary:','maj\u00fa stielku rozl\u00ed\u0161en\u00fa na pakorienky, paby\u013eku a pal\u00edstky','predstavuj\u00fa najdokonalej\u0161\u00ed v\u00fdvojov\u00fd stupe\u0148 oddelenia zelen\u00fdch rias','rast\u00fa v moriach','maj\u00fa mikroskopick\u00e9 rozmery','s\u00fa makroskopick\u00e9 riasy','maj\u00fa jednoduch\u00fa vl\u00e1knit\u00fa stielku','maj\u00fa bunkov\u00fa stenu rozdelen\u00fa na viac \u010dast\u00ed','maj\u00fa pletivov\u00fa stielku',TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1261,'1261. P\u00f4vod vy\u0161\u0161\u00edch rastl\u00edn sa odvodzuje od oddelenia:','Dinophyta','Cryptophyta','Euglenophyta','Chlorophyceae','\u010dervenoo\u010dk\u00e1','zelen\u00e9 riasy','Chlorophyta','Heterokontophyta',FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1262,'1262. \u010co zabezpe\u010duje siniciam modrozelen\u00e9 sfarbenie:','fykocyan\u00edn','fykoerytr\u00edn','karotenoid','fukoxant\u00edn','xantofyl','beta karot\u00e9n','sinicov\u00fd \u0161krob','bakt\u00e9riochlorofyl',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1263,'1263. Pre sinice plat\u00ed, \u017ee:','\u010dasto tvoria aj slizom obalen\u00e9 kol\u00f3nie','patria medzi eukaryotick\u00e9 organizmy','patria medzi prokaryotick\u00e9 organizmy','ich telo m\u00f4\u017ee by\u0165 jednobunkov\u00e9','obsahuj\u00fa chloroplasty so zelen\u00fdm chlorofylom','nach\u00e1dzame ich v sladk\u00fdch aj slan\u00fdch vod\u00e1ch','niektor\u00e9 patria k p\u00f4dnym organizmom','s\u00fa najstar\u0161ie a najjednoduch\u0161ie autotrofm\u00e9 organizmy na Zemi',TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1264,'1264. Rozsievky:','maj\u00fa bunkov\u00fa stenu inkrustovan\u00fa oxidom kremi\u010dit\u00fdm','maj\u00fa jednobunkov\u00fa stielku obsahuj\u00facu v plastidoch hned\u00e9 farbiv\u00e1','vytv\u00e1raj\u00fa sedimenta\u010dn\u00fa horninu zn\u00e1mu ako biolit','s\u00fa mikroskopick\u00e9 organizmy','s\u00fa jednobunkov\u00e9 riasy','\u017eij\u00fa len v kol\u00f3ni\u00e1ch','patria do triedy hned\u00e9 riasy','maj\u00fa v bunkovej stene oxid v\u00e1penat\u00fd',TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1265,'1265. Pre jednobunkov\u00e9 riasy je typick\u00fd typ stielky:','plazm\u00f3dium','vl\u00e1knit\u00fd','bunkov\u00fd','bi\u010d\u00edkat\u00fd','r\u00farkovit\u00fd','me\u0148avkovit\u00fd','pletivov\u00fd','amorfn\u00fd',FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1266,'1266. Pre huby plat\u00ed, \u017ee:','v bunkov\u00fdch sten\u00e1ch prevl\u00e1da chit\u00edn','patria medzi heterotrofn\u00e9 organizmy','ich telo tvor\u00ed stielka','s\u00fa preva\u017ene saprofyty','niektor\u00e9 druhy m\u00f4\u017eu ma\u0165 chloroplasty','nie s\u00fa schopn\u00e9 fotosynt\u00e9zy','produktom ich metabolizmu je \u0161krob','patria medzi prokaryotick\u00e9 organizmy',TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1267,'1267. Sple\u0165 hubov\u00fdch vl\u00e1kien tvor\u00ed:','myc\u00e9lium','h\u00fdfy','schizog\u00f3nium','gametangium','plazm\u00f3dium','plektench\u00fdm','podhubie','prvorast',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1268,'1268. Plazm\u00f3dium je stielka:','rozsievok','sp\u00e1javiek','kvasiniek','Myxomycota','v\u00e1\u013ea\u010da','kryptomon\u00e1d','Sargassum','slizoviek',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1269,'1269. Bunka h\u00fab:','obsahuje vakuoly','nikdy neobsahuje plastidy','nie je schopn\u00e1 fotosynt\u00e9zy','obsahuje lyzoz\u00f3my','obsahuje plastidy','obsahuje v bunkovej stene glykog\u00e9n','m\u00e1 bunkov\u00fa stenu ve\u013em\u00ed zriedkavo','m\u00f4\u017ee by\u0165 s\u00fa\u010das\u0165ou dlh\u00fdch vl\u00e1kien \u2014 h\u00fdf',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1270,'1270. Medzi najzn\u00e1mej\u0161ie jedl\u00e9 huby patria:','koz\u00e1k brezov\u00fd','hr\u00edb vysok\u00fd','hr\u00edb dubov\u00fd','pe\u010diarka po\u013en\u00e1','hr\u00edb satansk\u00fd','muchotr\u00e1vka zelen\u00e1','muchotr\u00e1vka tigrovan\u00e1','bed\u013ea vysok\u00e1',TRUE,FALSE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1271,'1271. Penicillium notatum:','sa pou\u017e\u00edva pri v\u00fdrobe pen\u00edcil\u00ednu','je plese\u0148 hlavi\u010dkat\u00e1','patr\u00ed medzi paples\u0148otvar\u00e9 huby','vytv\u00e1ra mycel\u00e1rne povlaky na potravin\u00e1ch','je huba vreckat\u00e1','je huba baz\u00eddiova','produkuje antibiotik\u00e1','patr\u00ed medzi sp\u00e1jav\u00e9 plesne',TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1272,'1272. Hrdze patria medzi:','baz\u00eddiov\u00e9 huby','sp\u00e1jav\u00e9 plesne','paples\u0148otvar\u00e9 huby','parazitick\u00e9 huby, ktor\u00e9 nevytv\u00e1raj\u00fa plodnice','cudzopasn\u00e9 vreckat\u00e9 huby','zn\u00e1me parazity hospod\u00e1rskych rastl\u00edn','kvasinkotvar\u00e9 huby','autotrofn\u00e9 huby',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1273,'1273. N\u00e1me\u013e je:','drevokazovit\u00e1 huba','\u00fatvar, ktor\u00fd vznik\u00e1 v d\u00f4sledku p\u00f4sobenia parazituj\u00facej huby u r\u00f4znych druhov lipnicovit\u00fdch','pestovan\u00fd umelo najm\u00e4 na ra\u017ei siatej','infikovan\u00fd semenn\u00edk ra\u017ei premenen\u00fd na tvrd\u00fd, \u010dernast\u00fd \u00fatvar','tmav\u00fd, ro\u017ekovit\u00fd \u00fatvar obsahuj\u00faci alkaloidy','ra\u017en\u00e1 hubka','va\u010dkovit\u00fd n\u00e1dor na kukurici','surovina na v\u00fdrobu liekov na \u00fapravu krvn\u00e9ho tlaku',FALSE,TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1274,'1274. Kyjani\u010dka purpurov\u00e1 patr\u00ed k:','baz\u00eddiov\u00fdm hub\u00e1m','kvasinkotvar\u00fdm hub\u00e1m','paples\u0148otvar\u00fdm hub\u00e1m','parazitick\u00fdm hub\u00e1m','cudzopasn\u00fdm hub\u00e1m','vreckat\u00fdm hub\u00e1m','sp\u00e1jav\u00fdm ples\u0148am','hub\u00e1m, ktor\u00e9 sa pestuj\u00fa umelo najm\u00e4 na ra\u017ei siatej',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1275,'1275. Penicil\u00edn bol vyroben\u00fd:','z paplesne \u0161tetkovitej','z kyjani\u010dky purpurovej','z plesne \u0161tetkovitej','z Claviceps purpurea','z druhu Penicillium notatum','z druhu Aspergillus niger','z plesne hlavi\u010dkatej','z druhu Candida albicans',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1276,'1276. Ko\u017en\u00e9 alebo p\u013e\u00facne ochorenia \u010dloveka sp\u00f4sobuje:','plese\u0148 hlavi\u010dkat\u00e1','Penicillium notatum','Candida albicans','Candida niger','Mucor mucedo','kyjani\u010dka purpurov\u00e1','paplese\u0148 \u0161tetkovit\u00e1','Aspergillus niger',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1277,'1277. Ozna\u010dte spr\u00e1vnu odpove\u010f:','po\u010detn\u00e9 druhy rodu Candida vyvol\u00e1vaj\u00fa kvasinkov\u00e9 ochorenia','paples\u0148otvar\u00e9 huby s\u00fa preva\u017ene saprofytick\u00e9','z kvasiniek sa vyr\u00e1baj\u00fa vitam\u00ednov\u00e9 lie\u010div\u00e1','niektor\u00e9 paplesne produkuj\u00fa mykotox\u00edny','kvasinky sa rozmno\u017euj\u00fa pu\u010dan\u00edm','sp\u00e1jav\u00e9 plesne s\u00fa v\u00e4\u010d\u0161inou parazitick\u00e9 huby','kvasinky patria do triedy vreckat\u00fdch h\u00fab','kvasinky patria do triedy kvasinkotvar\u00fdch h\u00fab',TRUE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1278,'1278. N\u00e1dorovky:','parazituj\u00fa na kapuste','\u017eij\u00fa saprofyticky na r\u00f4znych vodn\u00fdch rastlin\u00e1ch','parazituj\u00fa na \u010dloveku','infikuj\u00fa listy zemiakov a raj\u010diakov','sp\u00f4sobuj\u00fa \u0161kody v po\u013enohospod\u00e1rstve','sp\u00f4sobuj\u00fa \u0161kody v zdravotn\u00edctve','vyvol\u00e1vaj\u00fa n\u00e1dorov\u00e9 ochorenia rastl\u00edn','vyvol\u00e1vaj\u00fa n\u00e1dorov\u00e9 ochorenia \u017eivo\u010d\u00edchov',TRUE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1279,'1279. Ako huby sp\u00e1jav\u00e9 sa ozna\u010duj\u00fa:','napr. peronosp\u00f3ra','napr. plese\u0148 hlavi\u010dkat\u00e1','paples\u0148otvar\u00e9 huby','mycel\u00e1rne huby','endomyc\u00e9ty','baz\u00eddiov\u00e9 huby','sp\u00e1jav\u00e9 plesne','Zygomycetes',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1280,'1280. Kvasinky rodu Candida:','sa vyu\u017e\u00edvaj\u00fa pri v\u00fdrobe piva','m\u00f4\u017eu sp\u00f4sobova\u0165 p\u013e\u00facne ochorenia','sa vyu\u017e\u00edvaj\u00fa pri v\u00fdrobe v\u00edna','patria medzi sp\u00e1jav\u00e9 plesne','sa vyu\u017e\u00edvaj\u00fa pri v\u00fdrobe kef\u00edru','m\u00f4\u017eu sp\u00f4sobova\u0165 ko\u017en\u00e9 ochorenia','m\u00f4\u017eu sp\u00f4sobova\u0165 gynekologick\u00e9 ochorenia','patria medzi Micromyc\u00e9ty',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1281,'1281. Kyjani\u010dka purpurov\u00e1:','cudzopas\u00ed na ryb\u00e1ch','cudzopas\u00ed na ra\u017ei','vytv\u00e1ra \u00fatvar naz\u00fdvan\u00fd n\u00e1me\u013e','patr\u00ed medzi baz\u00eddiov\u00e9 huby','sa vyu\u017e\u00edva pri v\u00fdrobe lie\u010d\u00edv','m\u00f4\u017ee sp\u00f4sobova\u0165 hromadn\u00e9 otravy po\u013enohospod\u00e1rskych zvierat','patr\u00ed medzi vreckat\u00e9 huby','m\u00f4\u017ee produkova\u0165 toxick\u00e9 l\u00e1tky',FALSE,TRUE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1282,'1282. Na hnilobe ovocia sa podie\u013eaj\u00fa:','sneti','r\u00f4znotvar\u00e9 huby','saprofytick\u00e9 huby','kyjani\u010dka purpurov\u00e1','baz\u00eddiov\u00e9 huby','kyjani\u010dkotvar\u00e9 huby','kvasinkotvar\u00e9 huby','paples\u0148otvar\u00e9 huby',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1283,'1283. Telo vy\u0161\u0161\u00edch rastl\u00edn:','je kormus','je roz\u010dlenen\u00e9 na z\u00e1kladn\u00e9 vegetat\u00edvne org\u00e1ny a reproduk\u010dn\u00e9 org\u00e1ny','je rozdelen\u00e9 na kore\u0148, stonku, listy, v\u00fdtrusnice, kvety','reprezentuje pohlavn\u00e1 gener\u00e1cia','reprezentuje gametofyt','reprezentuje sporofyt','je stielka','je thallus',TRUE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1284,'1284. Borovicorasty:','sa rozde\u013euj\u00fa na triedy ginkg\u00e1, ihli\u010dnany a borovicotvar\u00e9','s\u00fa nahosemenn\u00e9 rastliny','tvoria kvety naz\u00fdvan\u00e9 \u0161i\u0161ti\u010dky','zara\u010fujeme medzi Sporophyta','s\u00fa vetroopeliv\u00e9 rastliny','s\u00fa krytosemenn\u00e9 rastliny','sa rozde\u013euj\u00fa na triedy ginkg\u00e1 a ihli\u010dnany','s\u00fa semenn\u00e9 rastliny',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1285,'1285. Ozna\u010dte nespr\u00e1vne odpovede:','oplodnenie pri semenn\u00fdch rastlin\u00e1ch je viazan\u00e9 na pr\u00edtomnos\u0165 vody','ihli\u010dnany s\u00fa preva\u017ene jednodom\u00e9 stromy','semenn\u00e9 rastliny s\u00fa r\u00f4znov\u00fdtrusn\u00e9','\u0161i\u0161ky smreka oby\u010dajn\u00e9ho visia smerom nadol a opad\u00e1vaj\u00fa vcelku','ihli\u010dnany s\u00fa naj\u010dastej\u0161ie dvojdom\u00e9 rastliny','magn\u00f3liorasty patria medzi nahosemenn\u00e9 rastliny','ginkgo dvojlalo\u010dn\u00e9 patr\u00ed do radu tisotvar\u00e9','ihli\u010dnany s\u00fa triedou oddelenia borovicorasty',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1286,'1286. Ozna\u010dte spr\u00e1vne odpovede:','sporofyt vytv\u00e1ra v\u00fdtrusy, gametofyt pohlavn\u00e9 bunky','gametofyt vytv\u00e1ra v\u00fdtrusy, sporofyt pohlavn\u00e9 bunky','riasy patria medzi semenn\u00e9 rastliny','telo semenn\u00fdch rastl\u00edn tvor\u00ed diferencovan\u00e1 stielka','u machorastov je dominantn\u00e9 \u0161t\u00e1dium gametofytu','slad\u00ed\u010dorasty s\u00fa v\u00fdtrusn\u00e9 rastliny','porastnica mnohotvar\u00e1 je dvojdom\u00e1 rastlina','jasene, bresty a duby s\u00fa dreviny m\u00e4kk\u00e9ho lu\u017en\u00e9ho lesa',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1287,'1287. Pre \u010de\u013ea\u010f astrovit\u00e9 je charakteristick\u00e9 s\u00fakvetie:','\u00fabor','\u0161\u00fa\u013eok','pavidlica','chochol\u00edk','hl\u00e1vka','strapec','okol\u00edk','metlina',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1288,'1288. V ktorej \u010de\u013eadi rastl\u00edn je v pletiv\u00e1eh mnoh\u00fdch druhov ve\u013ea alkaloidov:','hluchavkovit\u00e9','makovit\u00e9','p\u0155h\u013eavovit\u00e9','palmovit\u00e9','ru\u017eovit\u00e9','mrkvovit\u00e9','kapustovit\u00e9','\u013eu\u013ekovit\u00e9',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1289,'1289. V ktorej \u010de\u013eadi rastl\u00edn je v pletiv\u00e1ch mnoh\u00fdch druhov ve\u013ea sil\u00edc:','\u013eu\u013ekovit\u00e9','hluchavkovit\u00e9','p\u0155h\u013eavovit\u00e9','mrkvovit\u00e9','makovit\u00e9','konopovit\u00e9','kapustovit\u00e9','palmovit\u00e9',FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1290,'1290. Pre ktor\u00fa \u010de\u013ea\u010f je najtypickej\u0161ie s\u00fakvetie okol\u00edk:','mrkvovit\u00e9','lipnicovit\u00e9','hluchavkovit\u00e9','b\u00f4bovit\u00e9','silenkovit\u00e9','astrovit\u00e9','ru\u017eovit\u00e9','\u013ealiovit\u00e9',TRUE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1291,'1291. Dvojkl\u00ed\u010dnolistov\u00e9 rastliny maj\u00fa:','kore\u0148ov\u00fa s\u00fastavu pozost\u00e1vaj\u00facu z hlavn\u00e9ho a bo\u010dn\u00fdch kore\u0148ov','ved\u013eaj\u0161iu kore\u0148ov\u00fa s\u00fastavu','v stonke roztr\u00fasen\u00e9 cievne zvazky','v stonke cievne zv\u00e4zky usporiadan\u00e9 do kruhu','listy zvy\u010dajne jednoduch\u00e9','\u017eilnatinu listov naj\u010dastej\u0161ie rovnobe\u017en\u00fa','kvety naj\u010dastej\u0161ie trojpo\u010detn\u00e9','kvety zvy\u010dajne \u0161tvor- a p\u00e4\u0165po\u010detn\u00e9',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1292,'1292. Do \u010de\u013eade ru\u017eovit\u00e9 patria:','slivka dom\u00e1ca','mrkva oby\u010dajn\u00e1','ag\u00e1t biely','\u010dere\u0161\u0148a vi\u0161\u0148ov\u00e1','raj\u010diak jedl\u00fd','jahoda oby\u010dajn\u00e1','margar\u00e9ta biela','ostru\u017eina malinov\u00e1',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1293,'1293. Do \u010de\u013eade Pu\u013ekovit\u00e9 patria:','paprika ro\u010dn\u00e1','\u0161alvia l\u00fa\u010dna','m\u00e4ta pieporn\u00e1','tabak virg\u00ednsky','ruman\u010dek kamilkov\u00fd','raj\u010diak jedl\u00fd','p\u00fapava lek\u00e1rska','mak siaty',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1294,'1294. Plod malvicu m\u00e1:','slivka dom\u00e1ca','jablo\u0148 dom\u00e1ca','slivka trnkov\u00e1','marhu\u013ea oby\u010dajn\u00e1','\u010dere\u0161\u0148a vi\u0161\u0148ov\u00e1','hru\u0161ka oby\u010dajn\u00e1','hru\u0161ka plan\u00e1','ostru\u017eina malinov\u00e1',FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1295,'1295. Medzi jednokl\u00ed\u010dnolistov\u00e9 rastliny patria:','cibu\u013ea','lop\u00fach','s\u00f3ja','narcis','zeler','ry\u017ea','kapusta','palmy',TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1296,'1296. Li\u0161ajn\u00edky:','vznikli symbi\u00f3zou h\u00fab a rias','vznikli symbi\u00f3zou prvokov a rias','nie s\u00fa schopn\u00e9 autotrofie','nie s\u00fa schopn\u00e9 autotrofie','vznikli symbi\u00f3zou h\u00fab a sin\u00edc','nezn\u00e1\u0161aj\u00fa v porovnan\u00ed s in\u00fdmi rastlinami vysok\u00e9 a n\u00edzke teploty','m\u00f4\u017eu ma\u0165 cytostatick\u00e9 \u00fa\u010dinky','rast\u00fa len na organicky bohat\u00fdch p\u00f4dach',TRUE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1297,'1297. Biogenetick\u00fd z\u00e1kon:','tvrd\u00ed, \u017ee v\u0161etky vlastnosti organizmov sa pren\u00e1\u0161aj\u00fa z rodi\u010dov na potomkov','tvrd\u00ed, \u017ee organizmy prekon\u00e1vaj\u00fa vo svojom embryon\u00e1lnom v\u00fdvine skr\u00e1ten\u00fd v\u00fdvoj \u2014 fylogen\u00e9zu','charakterizuje podstatu prenosu g\u00e9nov z gener\u00e1cie na gener\u00e1ciu','vyjadruje, \u017ee ka\u017ed\u00fd organizmus uskuto\u010d\u0148uje vo svojej fylogen\u00e9ze skr\u00e1ten\u00fa ontogen\u00e9zu','sa naz\u00fdva tie\u017e z\u00e1kon rekapitul\u00e1cie','tvrd\u00ed, \u017ee v\u0161etky organizmy prekon\u00e1vaj\u00fa v svojom embryon\u00e1lnom v\u00fdvine skr\u00e1ten\u00fd v\u00fdvoj \u2014 ontogen\u00e9zu','sa naz\u00fdva tie\u017e z\u00e1kon retrospekt\u00edvy','tvrd\u00ed, \u017ee v\u0161etky biologick\u00e9 vlastnosti organizmu s\u00fa podmienen\u00e9 geneticky',FALSE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1298,'1298. Ozna\u010dte spr\u00e1vne usporiadanie systematick\u00fdch jednotiek od najvy\u0161\u0161ej po najni\u017e\u0161iu:','r\u00ed\u0161a, kme\u0148, trieda, rad, \u010de\u013ea\u010f, rod, druh','rad, \u010de\u013ea\u010f, rod, druh, r\u00ed\u0161a, kme\u0148, trieda','kme\u0148, r\u00ed\u0161a, trieda, \u010de\u013ea\u010f, rad, rod, druh','r\u00ed\u0161a, kme\u0148, podkme\u0148, trieda, rad, \u010de\u013ea\u010f, rod, druh','kme\u0148, trieda, r\u00ed\u0161a, rad, \u010de\u013ea\u010f, rod, druh','r\u00ed\u0161a, trieda, kme\u0148, rad, \u010de\u013ea\u010f, druh, rod','r\u00ed\u0161a, kme\u0148, trieda, podtrieda, rad, \u010de\u013ea\u010f, rod, druh','druh, rod, \u010de\u013ea\u010f, rad, trieda, kme\u0148, ri\u0161a',TRUE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1299,'1299. \u017divo\u010d\u00edchy s\u00fa:','heterotrofne sa vy\u017eivuj\u00face organizmy','Prokaryota','v\u0161etky bez v\u00fdnimky zlo\u017een\u00e9 z eukaryotick\u00fdch buniek','zlo\u017een\u00e9 z prokaryotick\u00fdch i eukaryotick\u00fdch buniek','mnohobunkov\u00e9 organizmy','v\u0161etky heterotrofn\u00e9 organizmy \u2014 jednobunkov\u00e9 i mnohobunkov\u00e9','konzumenty','preva\u017ene heterotrofn\u00e9 organizmy, autotrofia je zriedkav\u00e1',TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1300,'1300. Pre prvoky plat\u00ed, \u017ee:','s\u00fa mikroskopick\u00e9 organizmy','sa vy\u017eivuj\u00fa len autotrofne','m\u00f4\u017eu by\u0165 parazity','maj\u00fa na povrchu tenk\u00fa, elastick\u00fa blanu - kutikulu','niektor\u00e9 vytv\u00e1raj\u00fa schr\u00e1nky','mnoh\u00e9 maj\u00fa organelu zvan\u00fa bunkov\u00e9 \u00fasta','v\u0161etky s\u00fa horninotvorn\u00e9','sa pohybuj\u00fa pomocou bi\u010dficov, b\u0155v a pseudop\u00f3di\u00ed',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1301,'1301. Medzi parazituj\u00face prvoky sp\u00f4sobuj\u00face r\u00f3zne ochorenia u \u010dloveka patria:','Negleria eliptica','Trypanosoma gambiense','kokc\u00eddia pe\u010de\u0148ov\u00e1','Toxoplazma gondii','dierkavce','me\u0148avka po\u0161vov\u00e1','Trichomonas histolytica','slzovi\u010dkovka zhubn\u00e1',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1302,'1302. Me\u0148avka \u010dervienkov\u00e1:','v agres\u00edvnej forme magna sp\u00f4sobuje krv\u00e1canie do \u010dreva a hna\u010dky','v ne\u0161kodnej forme minuta sa \u017eiv\u00ed \u010drevn\u00fdmi bakt\u00e9riami','sp\u00f4sobuje v niektor\u00fdch oblastiach epid\u00e9mie','patr\u00ed medzi cudzopasn\u00e9 v\u00fdtrusovce','patr\u00ed medzi cudzopasn\u00e9 kore\u0148ono\u017ece','\u017eije v \u010dreve \u010dloveka v troch form\u00e1ch: minuta, magna a sporog\u00f3nium','v agres\u00edvnej forme m\u00ednuta nap\u00e1da \u010drevn\u00fd epitel','v ne\u0161kodnej forme magna sa \u017eiv\u00ed \u010drevn\u00fdmi bakt\u00e9riami',TRUE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,FALSE)
INSERT INTO BIOLOGIA VALUES(1303,'1303. Toxoplazma gondii:','patr\u00ed medzi v\u00fdtrusovce','vyvol\u00e1va z\u00e1pal mozgov\u00fdch bl\u00e1n','patr\u00ed medzi n\u00e1levn\u00edky','vyvol\u00e1va spav\u00fa nemoc','m\u00f4\u017ee sp\u00f4sobi\u0165 potratenie plodu','m\u00f4\u017ee sp\u00f4sobi\u0165 po\u0161kodenie plodu','pren\u00e1\u0161a sa pohlavn\u00fdm stykom','pren\u00e1\u0161a sa cystami pri nedostato\u010dnej hygiene',TRUE,FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1304,'1304. Plasmodium malariae:','patr\u00ed medzi bi\u010d\u00edkovce','sp\u00f4sobuje spav\u00fa nemoc','patr\u00ed medzi kore\u0148ono\u017ece','sp\u00f4sobuje ochorenie, ktor\u00e9 sa vyskytuje hlavne v seversk\u00fdch krajin\u00e1ch','sa pren\u00e1\u0161a na \u010dloveka kom\u00e1rom rodu Anopheles','sp\u00f4sobuje u \u010dloveka hna\u010dkovit\u00e9 ochorenie','vyskytuje sa hlavne v tropick\u00fdch krajin\u00e1ch s mo\u010diarmi','sa pren\u00e1\u0161a na \u010dloveka muchou tse tse',FALSE,FALSE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1305,'1305. Trypanoz\u00f3ma spavi\u010dn\u00e1:','sp\u00f4sobuje mal\u00e1riu','nap\u00e1da nervov\u00fa s\u00fastavu \u010dloveka','\u017eije v pohlavn\u00fdch org\u00e1noch \u017eivo\u010d\u00edchov','sp\u00f4sobuje spav\u00fa nemoc','\u017eije v krvi \u017eivo\u010d\u00edchov','patr\u00ed medzi bi\u010d\u00edkovce','je na \u010dloveka pren\u00e1\u0161an\u00e1 muchou tse tse','je na \u010dloveka pren\u00e1\u0161an\u00e1 kom\u00e1rom rodu Anopheles',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1306,'1306. Trichomonas po\u0161vov\u00fd:','\u017eije v pohlavn\u00fdch org\u00e1noch','patr\u00ed medzi kore\u0148ono\u017ece','patr\u00ed medzi bi\u010d\u00edkovce','sp\u00f4sobuje spav\u00fa nemoc','sa pren\u00e1\u0161a pohlavn\u00fdm stykom','m\u00f4\u017ee sp\u00f4sobi\u0165 a\u017e neplodnos\u0165','patr\u00ed medzi parazity \u010dloveka','sp\u00f4sobuje hna\u010dkovit\u00e9 ochorenie',TRUE,FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1307,'1307. Slzovi\u010dkovka zhubn\u00e1:','sp\u00f4sobuje u \u010dloveka nadmern\u00e9 slzenie','patr\u00ed medzi bi\u010d\u00edkovce','sp\u00f4sobuje u \u010dloveka mal\u00e1riu','\u017eije v tepl\u00fdch vod\u00e1ch','sp\u00f4sobuje u \u010dloveka atypick\u00fd z\u00e1pal mozgov\u00fdch bl\u00e1n','pren\u00e1\u0161a sa pohlavn\u00fdm stykom','patr\u00ed medzi kore\u0148ono\u017ece','nosom sa dost\u00e1va k mozgov\u00fdm nervom',FALSE,FALSE,FALSE,TRUE,TRUE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1308,'1308. P\u00f4vodcom mal\u00e1rie je:','kom\u00e1r pisk\u013eav\u00fd','z\u00e1stupca triedy Apicomplexa','z\u00e1stupca kme\u0148a v\u00fdtrusovce','prvok z triedy v\u00fdtrusovcov','z\u00e1stupca triedy kokc\u00eddie','z\u00e1stupca rodu Culex','z\u00e1stupca rodu Plasmodium','Anopheles maculipenis',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1309,'1309. Ozna\u010dte spr\u00e1vne odpovede:','vo\u0161 \u0161atov\u00e1 m\u00f4\u017ee pren\u00e1\u0161a\u0165 p\u00f4vodcu \u0161kvrnit\u00e9ho t\u00fdfusu','Negleria fowleri sp\u00f4sobuje u \u010dloveka atypick\u00fd z\u00e1pal mozgov\u00fdch bl\u00e1n','spav\u00fa nemoc sp\u00f4sobuje trypanoz\u00f3ma spavi\u010dn\u00e1','mal\u00e1riu sp\u00f4sobuj\u00fa bakt\u00e9rie pren\u00e1\u0161an\u00e9 kom\u00e1rmi na \u010dloveka','charakteristick\u00e9 pre v\u00fdtrusovce s\u00fa makronukleus a mikronukleus','ve\u013ek\u00e9 jadro n\u00e1levn\u00edkov sa po reduk\u010dnom delen\u00ed st\u00e1va pohlavn\u00fdm jadrom','toxoplazm\u00f3za je nebezpe\u010dn\u00e9 ochorenie predov\u0161etk\u00fdm pre tehotn\u00e9 \u017eeny','p\u00f4vodcom morovej n\u00e1kazy je blcha \u013eudsk\u00e1',TRUE,TRUE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1310,'1310. Ozna\u010dte nespr\u00e1vne odpovede:','Animalia sa delia na jednolistovce (monoblastica) a dvojlistovce (diblastica)','do oddelenia dvojlistovcov patria len \u017eivo\u010d\u00edchy s dvojstrannou s\u00famernos\u0165ou tela','u mnohobunkov\u00fdch organizmov sa prudko zni\u017euje schopnos\u0165 jednotliv\u00fdch buniek pre\u017e\u00edva\u0165 samostatne','\u017eivo\u010d\u00edchy (Animalia) sa delia na jednobunkovce (Protozoa) a mnohobunkovce (Anthozoa)','mnohobunkov\u00e9 \u017e\u00edvo\u010d\u00edchy maj\u00fa telo tvoren\u00e9 dvoma z\u00e1kladn\u00fdmi typmi buniek, s\u00fa to pohlavn\u00e9 a generat\u00edvne bunky','pre v\u0161etky skupiny mnohobunkov\u00fdch \u017eivo\u010dichov s\u00fa spolo\u010dn\u00e9 jednotn\u00e9 \u0161t\u00e1dia embryogen\u00e9zy \u2014 morula, blastula a gastrula','prvotn\u00e1 telov\u00e1 dutina sa vytv\u00e1ra medzi endodermom a mezodermom','prvotn\u00e1 telov\u00e1 dutina sa naz\u00fdva blastoc\u00e9l',TRUE,TRUE,FALSE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1311,'1311. Z\u00e1stupcovia kme\u0148a hubky (Porifera):','\u017eij\u00fa prisadnuto','tr\u00e1via svoju potravu intracelul\u00e1rne','maj\u00fa telo l\u00fa\u010dovito s\u00famern\u00e9','maj\u00fa telo nepravideln\u00e9ho tvaru','s\u00fa predov\u0161etk\u00fdm vodn\u00e9 \u017eivo\u010d\u00edchy, ale patria tu i suchozemsk\u00e9 \u017eivo\u010d\u00edchy','dr\u00e1\u017ediv\u00fa \u010dinnos\u0165 prejavuj\u00fa a\u017e v dospelosti','sa rozmno\u017euj\u00fa pohlavne i nepohlavne','v nepriazniv\u00fdch podmienkach vytv\u00e1raj\u00fa vonkaj\u0161ie puky \u2014 gemule',TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1312,'1312. P\u0155hlivce:','u\u017e maj\u00fa pohlavn\u00e9 \u017e\u013eazy a oplodnenie je vn\u00fatorn\u00e9','maj\u00fa vysok\u00fa schopnos\u0165 regener\u00e1cie','sa delia na 4 triedy: polypovce, med\u00fazovce, \u0161tvorbranovce a koraly','s\u00fa v\u017edy drav\u00e9 \u017eivo\u010d\u00edchy','maj\u00fa telo l\u00fa\u010dovito s\u00famern\u00e9','maj\u00fa telo dvojstranne s\u00famern\u00e9','sa vyskytuj\u00fa bu\u010f v \u0161t\u00e1diu polypa alebo med\u00fazy','sa delia na tri triedy: \u0161tvorhranovce, polypovce a med\u00fazovce',FALSE,TRUE,TRUE,TRUE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1313,'1313. Trojlistovce \u2014 Triblastica:','maj\u00fa telo tvoren\u00e9 3 z\u00e1rodo\u010dn\u00fdmi vrstvami \u2014 ektodermom, endodermom a mezodermom','s\u00fa oproti dvojlistovcom na vy\u0161\u0161om fylogenetickom stupni','s\u00fa oproti dvojstranovcom na vy\u0161\u0161om fylogenetickom stupni','maj\u00fa telo dvojstranne i l\u00fa\u010dovito s\u00famern\u00e9','s\u00fa v\u0161etky dvojstranne s\u00famern\u00e9 a preto sa naz\u00fdvaj\u00fa tie\u017e dvojstranovce \u2014 Bilateralia','maj\u00fa telo v\u017edy tvoren\u00e9 troma z\u00e1rodo\u010dn\u00fdmi vrstvami \u2014 ektodermom, endodermom a mezench\u00fdmom','sa delia na prvo\u00faste a druho\u00faste','sa delia na prvo\u00faste, kde z prvotn\u00e9ho otvoru \u2014 prvo\u00fast vznik\u00e1 an\u00e1lny otvor a \u00fastny sa znovu otvor\u00ed a na druho\u00faste, kde na mieste prvo\u00fast vznikaj\u00fa \u00fasta a an\u00e1lny otvor sa znovu otv\u00e1ra',TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1314,'1314. Protostomia maj\u00fa \u00fastny otvor:','na chrbtovej strane tela','zaniknut\u00fd','ktor\u00fd je u v\u0161etk\u00fdch druhov s\u00fa\u010dasne aj vyvrhovac\u00ed','z prvotn\u00e9ho otvoru \u2014 prvo\u00fast a an\u00e1lny otvor sa vytv\u00e1ra druhotne','premenen\u00fd na an\u00e1lny','na prednom konci tela a vn\u00fatorn\u00fa kostru mezodermov\u00e9ho p\u00f4vodu','zo zvy\u0161ku prvo\u00fast gastruly a vyvrhovac\u00ed otvor na opa\u010dnom konci tela','na zadnom konci tela a vyvrhovac\u00ed otvor vznik\u00e1 z blastop\u00f3ru',FALSE,FALSE,FALSE,TRUE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1315,'1315. Medzi prvo\u00fastovce patria:','obr\u00fa\u010dkavce','hl\u00edstovce','k\u00f4rovce','p\u00e1somnice','m\u00e4kk\u00fd\u0161e','ploskulice','ulitn\u00edky','pijavice',TRUE,TRUE,TRUE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1316,'1316. Ploskavce (Plathelminthes):','nemaj\u00fa vo svojom v\u00fdvine larvu','maj\u00fa \u010drevo bez an\u00e1lneho otvoru','d\u00fdchaj\u00fa protonefr\u00eddiami','d\u00fdchaj\u00fa cel\u00fdm povrchom tela','s\u00fa vo\u013ene \u017eij\u00face i parazitick\u00e9 \u017eivo\u010d\u00edchy','s\u00fa dvojstranne s\u00famem\u00e9','nie s\u00fa v \u017eiadnom pr\u00edpade parazity','maj\u00fa vyvinut\u00fa hlavov\u00fa \u010das\u0165 s primit\u00edvnymi o\u010dami',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1317,'1317. Motolica pe\u010de\u0148ov\u00e1:','patr\u00ed medzi hl\u00edstovce','patr\u00ed do triedy ploskavce','patr\u00ed do kme\u0148a motolice','je dlh\u00e1 20-30 mm','na hlave a bruchu m\u00e1 pr\u00edsavky','cicia krv','\u017eije v pe\u010deni a \u017el\u010dovodoch dobytka a oviec','sa vyv\u00edja cez medzibostitera \u2014 Galba truncatula',FALSE,FALSE,FALSE,TRUE,TRUE,TRUE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1318,'1318. Vylu\u010dovacia s\u00fastava sa prv\u00fdkr\u00e1t objavuje pri kmeni:','Porifera','polypovce','ploskavce vo forme protonefr\u00eddi\u00ed','me\u0148avkobi\u010d\u00edkovce','p\u0155hlivce','m\u00e4kk\u00fd\u0161e','Plathelminthes','ploskavce vo forme nefr\u00eddi\u00ed',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1319,'1319. Tr\u00e1viacu s\u00fastavu nemaj\u00fa vyvinut\u00fa:','\u010dl\u00e1nkono\u017ece','ostnatoko\u017ece','me\u0148avkobi\u010d\u00edkovce','pijav\u00edce','m\u00e4kk\u00fd\u0161e','ploskulice','h\u00e1\u010fatk\u00e1','n\u00e1levn\u00edky',FALSE,FALSE,TRUE,FALSE,FALSE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1320,'1320. Ozna\u010dte spr\u00e1vne odpovede:','dospel\u00e9 p\u00e1somnice parazituj\u00fa v krvnom obehu, svalovine alebo v in\u00fdch org\u00e1noch','dospel\u00e9 p\u00e1somnice sp\u00f4sobuj\u00fa m\u00e1lokrvnos\u0165 a ich larvy m\u00f4\u017eu sp\u00f4sobova\u0165 z\u00e1paly a nekr\u00f3zu tkan\u00edv','p\u00e1somnica dlh\u00e1 sa do \u010dloveka m\u00f4\u017ee dosta\u0165 z nedostato\u010dne upraven\u00e9ho brav\u010dov\u00e9ho m\u00e4sa','ploskulice maj\u00fa pigmentovan\u00e9 o\u010di a \u010dasto aj statocystu','ploskulice maj\u00fa\u0009regenera\u010dn\u00fa schopnos\u0165','motolice \u017eij\u00fa paraziticky','vlasovce napadaj\u00fa lymfatick\u00e9 cievy','vlasovce napadaj\u00fa vlasov\u00e9 folikuly',FALSE,TRUE,FALSE,TRUE,TRUE,TRUE,TRUE,FALSE)
INSERT INTO BIOLOGIA VALUES(1321,'1321. Ozna\u010dte spr\u00e1vne odpovede:','Trichinella spiralis sp\u00f4sobuje zdurenie a gigantizmus \u010dast\u00ed tela','hl\u00edsty a mrle patria medzi hl\u00edstovce','pijavice patria k ektoparazitom','vlasovec u\u0161n\u00fd m\u00f4\u017ee sto\u010den\u00fd v strednom uchu sp\u00f4sobi\u0165 do\u010dasn\u00fa hluchotu','mnoh\u00e9 hl\u00edstoyce dok\u00e1\u017eu \u017ei\u0165 aj v podmienkach bez kysl\u00edka','samice hl\u00edsty detskej kladen\u00edm vaj\u00ed\u010dok na okraje kone\u010dn\u00edka vyvol\u00e1vaj\u00fa svrbenie','Ascaris lumbricoides dosahuje ve\u013ekos\u0165 do 20 mm','svalovec \u0161pir\u00e1lov\u00fd m\u00f4\u017ee vyvola\u0165 u \u010dloveka hor\u00fa\u010dkovit\u00e9 ochorenia',FALSE,TRUE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1322,'1322. P\u00e1somnice:','s\u00fa hermafrodity','potravu prij\u00edmaj\u00fa cel\u00fdm povrchom tela','maj\u00fa na prednom konci tela \u00fastny otvor, potom hltan a rozkon\u00e1ren\u00e9 \u010drevo','dosahuj\u00fa d\u013a\u017eku a\u017e 10 m','s\u00fa gonochoristy','nemaj\u00fa gastrovaskul\u00e1rnu s\u00fastavu','patria k ektoparazitom','patria k zooparazitom',TRUE,TRUE,FALSE,TRUE,FALSE,TRUE,FALSE,TRUE)
INSERT INTO BIOLOGIA VALUES(1323,'1323. Svalovec \u0161pir\u00e1lov\u00fd:','sa pren\u00e1\u0161a na \u010dloveka pohlavn\u00fdm stykom','patr\u00ed medzi hl\u00edstovce','patr\u00ed medzi ploskavce','sa do tela \u010dloveka dost\u00e1va konzum\u00e1ciou nedostato\u010dne tepelne spracovan\u00e9ho m\u00e4sa zvierat','sa do \u010dloveka dost\u00e1va nedostato\u010dne umytou zeleninou','vyvol\u00e1va hna\u010dkovit\u00e9 ochorenie','vyvol\u00e1va hor\u00fa\u010dkovit\u00e9 ochorenie','je parazitom \u010dloveka',FALSE,TRUE,FALSE,TRUE,FALSE,FALSE,TRUE,TRUE)
INSERT INTO BIOLOGIA VALUES(1324,'1324. Hirudo medicinalis:','patr\u00ed medzi \u010dl\u00e1nkono\u017ece','produkuje v slinn\u00fdch \u017e\u013eaz\u00e1ch hirud\u00edn','patr\u00ed medzi pijavice','produkuje v slinn\u00fdch \u017e\u013eaz\u00e1ch l\u00e1tky podporuj\u00face zr\u00e1\u017eanie krvi','sa pou\u017e\u00edvala u\u017e v stredoveku na ods\u00e1vanie krvi pri n\u00edzkom tlaku krvi','\u017eije v sladk\u00fdch vod\u00e1ch','\u017eije v moriach','je endoparazit',FALSE,TRUE,TRUE,FALSE,FALSE,TRUE,FALSE,FALSE)
